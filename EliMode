////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Answer", true) == 0 || strcmp(cmd, "/Ans", true) == 0)
	{
	    if(Math_ON == 0) return SendClientMessage(playerid, White, "ôòéìåú äîúîèé÷ä ìà îåôòìú");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Answer [number] :öåøú äùéîåù");
		any = strval(tmp);
		if(any != Math_Answer) return SendClientMessage(playerid, White, "äúùåáä ùäæðú ìà ðëåðä");
        Math_ON = 0;
        KillTimer(Math_Timer);
		OL_SendClientMessageToAll(Grellow, "============[{FFFFFF} îòøëú îúîèé÷ä {ADFF2F}]============");
		format(string, sizeof(string), "%s{FFFFFF} :ôåúø úøâéì äîúîèé÷ä äåà", GetName(playerid));
		OL_SendClientMessageToAll(Grellow, string);
		format(string,sizeof(string), "äúùåáä äðëåðä äéà: {ADFF2F}%s", GetNum(Math_Answer));
		OL_SendClientMessageToAll(White, string);
		if((DOF2_GetInt(File(playerid), "Bank")+Math_Price) > 99999999)
		{
			SendClientMessage(playerid, White, "! îëéååï ùñëåí äæëééä áéçã òí äëñó ùéù ìê ááð÷ òåáøéí àú îâáìú äëñó ááð÷ - äòáøú äëñó ìáð÷ ðëùìä, äëñó ðùìç éùéøåú àìééê");
	        AntiCheat_GivePlayerMoney(playerid, true, Math_Price);
	    }
	    else DOF2_SetInt(File(playerid), "Bank", DOF2_GetInt(File(playerid), "Bank")+Math_Price);
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Battle", true) == 0)
	{
  		tmp = strtok(cmdtext, idx);
  		if(!strlen(tmp))
 		{
			SendClientMessage(playerid, White, "/Battle [Invite | Join | Option]  :öåøú ùéîåù");
			SendClientMessage(playerid, Whitegrey, "/Battle Invite [playerid] [weaponid] [ammu] - ëãé ìäæîéï ùç÷ï ì÷øá");
			SendClientMessage(playerid, White, "{FF0000}[playerid]{FFFFFF} - äàéãé ùì äùç÷ï {FF0000}[weaponid]{FFFFFF} - äàéãé ùì äðù÷ {FF0000}[ammu]{FFFFFF} - îñôø äëãåøéí");
			SendClientMessage(playerid, Whitegrey, "/Battle Join [playerid] - ëãé ìàùø á÷ùä ùì ùç÷ï àìééê ì÷øá");
			SendClientMessage(playerid, White, "{FF0000}[playerid]{FFFFFF} - äàéãé ùì äùç÷ï");
			SendClientMessage(playerid, Whitegrey, "/Battle Option - ìôúåç/ìñâåø àú ÷áìú á÷ùåú ì÷øá áùøú");
			return 1;
		}
	    if(strcmp(tmp, "Invite", true) == 0)
	    {
			if(HaveBattle == 1) return SendClientMessage(playerid, White, "÷ééí ëáø ÷øá àéùé ôòéì");
			tmp1 = strtok(cmdtext, idx);
	    	if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Battle Invite [playerid] [weaponid] [ammu]  :öåøú ùéîåù");
			id = strval(tmp1);
			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		    if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(BattleOption[id] == 0) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ôúç àú àôùøåú ÷øáåú äàéùééí");
			if(IsPlayerInPvpArea(id) == 1) return SendClientMessage(playerid, White, "áùøú Pvpùç÷ï æä ðîöà áàçã îàæåøé ä");
			if(IsPlayerInMission(id) == 1) return SendClientMessage(playerid, White, "ùç÷ï æä îáöò ëøâò àú àçú îîùéîåú äùøú");
	  		if(AutomaticACT_In[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áôòéìåú àåèåîèéú");
			if(ActivityStatus[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ôòéìåú");
			if(InBattle[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà á÷øá àéùé");
			if(InBattle[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà á÷øá àéùé");
			if(inJail[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áëìà");
			if(inAFK[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ìà ðîöà");
			tmp2 = strtok(cmdtext, idx);
	    	if(!strlen(tmp2)) return SendClientMessage(playerid, White, "/Battle Invite [playerid] [weaponid] [ammu]  :öåøú ùéîåù");
			weap = strval(tmp2);
			if(weap < 1 || weap > 46) return SendClientMessage(playerid, White, "äàééãé ðù÷ ùöééðú ùâåé");
			if(weap == 38 || weap == 39 || weap == 40 || weap == 43 || weap == 44 || weap == 37 || weap == 45) return SendClientMessage(playerid, White, "äðù÷ ùöééðú çñåí ìùéîåù");
            BattleWeap = weap;
			tmp3 = strtok(cmdtext, idx);
			if(!strlen(tmp3)) return SendClientMessage(playerid, White, "/Battle Invite [playerid] [weaponid] [ammu] :öåøú ùéîåù");
			ammu = strval(tmp3);
			if(ammu < 1 || ammu > 9999) return SendClientMessage(playerid, White, "èååç äëãåøéí äåà áéï 1 ì9999");
            BattleAmmoWeap = ammu;
			format(string, sizeof(string), "òí îñôø äëãåøéí %d %s ì÷øá àéùé òí äðù÷ %s äæîðú àú äùç÷ï", ammu, WeaponNames[weap], GetName(id));
			SendClientMessage(playerid, Azure, string);
			format(string, sizeof(string), "òí îñôø äëãåøéí %d %s äæîéï àåúê ì÷øá àéùé òí äðù÷ %s äùç÷ï", ammu, WeaponNames[weap], GetName(playerid));
			SendClientMessage(id, Azure, string);
			SendClientMessage(id, White, "/Battle Join - ìàéùåø ä÷øá äùúîù áô÷åãä");
			BattleInviter[id] = playerid;
			InviteToBattle[id] = 1;
			return 1;
		}
	    if(strcmp(tmp, "Join", true) == 0)
	    {
	        if(!InviteToBattle[playerid]) return SendClientMessage(playerid, White, "ìà ÷éáìú àó äæîðä ì÷øá àéùé");
			//==================================================================
			HaveBattle = 1;
			//==================================================================
			ResetPlayerWeapons(playerid);
			ResetPlayerWeapons(BattleInviter[playerid]);
			//==================================================================
			GivePlayerWeapon(playerid, BattleWeap, BattleAmmoWeap);
			GivePlayerWeapon(BattleInviter[playerid], BattleWeap, BattleAmmoWeap);
			//==================================================================
	        SetPlayerPosEx(playerid, 1997.9924, 2356.4568, 23.8516, 90.5897);
	        SetPlayerPosEx(BattleInviter[playerid], 1956.5206, 2356.8027, 23.8516, 271.3848);
			//==================================================================
			SetCameraBehindPlayer(playerid);
			SetCameraBehindPlayer(BattleInviter[playerid]);
			//==================================================================
	        SetPlayerHealth(playerid, 100);
	        SetPlayerHealth(BattleInviter[playerid], 100);
			//==================================================================
	        SetPlayerArmour(playerid, 100);
	        SetPlayerArmour(BattleInviter[playerid], 100);
			//==================================================================
	        SetPlayerVirtualWorld(playerid, 10);
	        SetPlayerVirtualWorld(BattleInviter[playerid], 10);
			//==================================================================
			TogglePlayerControllable(playerid, 0);
	        TogglePlayerControllable(BattleInviter[playerid], 0);
			//==================================================================
	        InBattle[playerid] = 1;
	        InBattle[BattleInviter[playerid]] = 1;
			//==================================================================
			BattlePlayers = 2;
			//==================================================================
			BattlePlayer_1 = playerid;
			BattlePlayer_2 = BattleInviter[playerid];
			//==================================================================
			BattleCountDown(3);
			CountdownON = 1;
			//==================================================================
			InviteToBattle[playerid] = 0;
			//==================================================================
			return 1;
		}
		if(strcmp(tmp, "Option", true) == 0)
	    {
	    	if(BattleOption[playerid] == 0)
	    	{
				BattleOption[playerid] = 1;
				SendClientMessage(playerid, Azure, "ôúçú àú àôùøåú ÷áìú ÷øáåú àéùééí îùç÷ðéí áùøú");
				return 1;
			}
	    	if(BattleOption[playerid] == 1)
	    	{
				BattleOption[playerid] = 0;
				SendClientMessage(playerid, Azure, "ñâøú àú àôùøåú ÷áìú ÷øáåú àéùééí îùç÷ðéí áùøú");
				return 1;
			}
			return 1;
		}
		else
		{
			SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Battle {FFFFFF}àôùøåú");
		}
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Gang", true) == 0)
	{
	    tmp = strtok(cmdtext, idx);
	    if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Gang [Create | Delete | Invite | Join | Leave | Kick | Chat] :öåøú äùéîåù");
	    if(strcmp(tmp, "Create", true) == 0)
	    {
	        if(DOF2_GetInt(File(playerid), "InGang") == 1) return SendClientMessage(playerid, White, "àúä ëáø ðîöà áëðåôééä");
	        tmp1 = strtok(cmdtext, idx);
		    format(GangFile, sizeof(GangFile), "Gangs/%s.ini", tmp1);
	        if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Gang Create [name] :öåøú äùéîåù");
        	for(new i = 0; i < strlen(tmp1); i++)
 			if(tmp1[i] < 'ú' && tmp1[i] > 'à') return SendClientMessage(playerid, White, "òìéê ìáçåø ùí ìëðåôééä áàðâìéú áìáã");
		    if(DOF2_FileExists(GangFile)) return SendClientMessage(playerid, White, "äùí ùöééðú úôåñ òì éãé ëðåôééä àçøú");
			//------------------------------------------------------------------
			DOF2_CreateFile(GangFile);
		    DOF2_SetString(GangFile, "GangName", tmp1);
			DOF2_SetInt(GangFile, "Players", 1);
			DOF2_SetInt(ClanFile, "GangChat", 0);
			//------------------------------------------------------------------
		    DOF2_SetInt(File(playerid), "InGang", 1);
			DOF2_SetString(File(playerid), "Gang", tmp1);
		    DOF2_SetInt(File(playerid), "GangLevel", 2);
		    format(string, sizeof(string), "! áäöìçä \"{FFFFFF}%s{8B5A2B}\" éöøú àú äëðåôééä", tmp1);
			SendClientMessage(playerid, Whitebrown, string);
			SendClientMessage(playerid, Whitebrown, "äåâãøú àåèåîèéú ëîðäì äëðåôééä");
			SendClientMessage(playerid, White, "/Gang - ìòæøä äùúîù áô÷åãä");
			return 1;
		}
		if(strcmp(tmp, "Delete", true) == 0)
	    {
	        if(DOF2_GetInt(File(playerid), "InGang") == 0) return SendClientMessage(playerid, White, "àúä ìà ðîöà áëðåôééä");
			if(DOF2_GetInt(File(playerid), "GangLevel") != 2) return SendClientMessage(playerid, White, "àúä ìà îðäì äëðåôééä");
		    format(GangFile, sizeof(GangFile), "Gangs/%s.ini", DOF2_GetString(File(playerid), "Gang"));
			format(string, sizeof(string), "! ðîç÷ä áäöìçä \"{FFFFFF}%s{8B5A2B}\" äëðåôééä", DOF2_GetString(File(playerid), "Gang"));
			SendClientMessage(playerid, Whitebrown, string);
			format(string, sizeof(string), "! îç÷ àú äëðåôééä {FFFFFF}%s{8B5A2B} îðäì äëðåôééä", DOF2_GetString(File(playerid), "Gang"));
			for(new i=0; i<MAX_PLAYERS; i++)
			{
				if(i != playerid && DOF2_GetInt(File(i), "InGang") == 1 && strcmp(DOF2_GetString(File(playerid), "Gang"), DOF2_GetString(File(i), "Gang"), true) == 0)
				{
					SendClientMessage(i, Whitebrown, string);
					DOF2_Unset(File(i), "GangLevel");
					DOF2_Unset(File(i), "Gang");
					DOF2_SetInt(File(i), "InGang", 0);
				}
			}
			DOF2_Unset(File(playerid), "GangLevel");
			DOF2_Unset(File(playerid), "Gang");
			DOF2_SetInt(File(playerid), "InGang", 0);
		    DOF2_RemoveFile(GangFile);
			return 1;
		}
	    if(strcmp(tmp, "Invite", true) == 0)
		{
	        if(DOF2_GetInt(File(playerid), "InGang") == 0) return SendClientMessage(playerid, White, "àúä ìà ðîöà áëðåôééä");
			if(DOF2_GetInt(File(playerid), "GangLevel") != 2) return SendClientMessage(playerid, White, "àúä ìà îðäì äëðåôééä");
			tmp1 = strtok(cmdtext,idx);
			if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Gang Invite [playerid] :öåøú ùéîåù");
			id = strval(tmp1);
			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
			if(DOF2_GetInt(File(id), "InGang") == 1 && !strcmp(DOF2_GetString(File(playerid),"Gang"), DOF2_GetString(File(id), "Gang"), true)) return SendClientMessage(playerid, White, "ùç÷ï æä ëáø ðîöà áëðåôééä áå àúä ðîöà");
			if(DOF2_GetInt(File(id), "InGang") == 1) return SendClientMessage(playerid, White, "äùç÷ï äéðå çáø áëðåôééä àçøú");
			format(string, sizeof(string), "! ìëðåôééä ùìê, àðà äîúï {FFFFFF}%s{8B5A2B} äæîðú àú äùç÷ï", GetName(id));
			SendClientMessage(playerid, Whitebrown, string);
			format(string, sizeof(string), "! \"{FFFFFF}%s{8B5A2B}\" äæîéï àåúê ìëðåôééä {FFFFFF}%s{8B5A2B} äùç÷ï", DOF2_GetString(File(playerid), "Gang"), GetName(playerid));
			SendClientMessage(id, Whitebrown, string);
			SendClientMessage(id, White, "/Gang Join - ëãé ìäöèøó äùúîù áô÷åãä");
			GangInviter[id] = playerid;
			InviteToGang[id] = 1;
			return 1;
		}
	    if(strcmp(tmp, "Join", true) == 0)
		{
			if(DOF2_GetInt(File(playerid), "InGang") == 1) return SendClientMessage(playerid, White, "àúä ëáø ðîöà áëðåôééä");
			if(InviteToGang[playerid] == 0) return SendClientMessage(playerid, White, "ìà ÷éáìú äæîðä ìùåí ëðåôééä");
			//------------------------------------------------------------------
			DOF2_SetInt(File(playerid), "InGang", 1);
   			DOF2_SetString(File(playerid), "Gang", DOF2_GetString(File(GangInviter[playerid]), "Gang"));
			DOF2_SetInt(File(playerid), "GangLevel", 1);
			//------------------------------------------------------------------
		    format(GangFile, sizeof(GangFile), "Gangs/%s.ini", DOF2_GetString(File(playerid), "Gang"));
			DOF2_SetInt(GangFile, "Players", DOF2_GetInt(GangFile,"Players")+1);
			format(string, sizeof(string), "! áäöìçä \"{FFFFFF}%s{8B5A2B}\" äöèøôú ìëðåôééä", DOF2_GetString(File(playerid), "Gang"));
    		SendClientMessage(playerid, Whitebrown, string);
    		format(string, sizeof(string), "! äöèøó ìëðåôééä {FFFFFF}%s{8B5A2B} äùç÷ï", GetName(playerid));
			for(new i=0; i<MAX_PLAYERS; i++)
			{
				if(IsPlayerConnected(i))
				{
					if(Logged[i] == true)
					{
					    if(i != playerid)
					    {
					 		if(DOF2_GetInt(File(i), "InGang") == 1)
					 		{
							 	if(strcmp(DOF2_GetString(File(playerid), "Gang"), DOF2_GetString(File(i), "Gang"), true) == 0)
							 	{
									SendClientMessage(i, Whitebrown, string);
								}
							}
						}
					}
				}
			}
    		SendClientMessage(playerid, Yellow, "/Gang áøåê äáà ìëðåôééä, ìòæøä á÷ùø ìîòøëú äëðåôéåú äùúîù áô÷åãä");
			return 1;
		}
	    if(strcmp(tmp, "Leave", true) == 0)
		{
	        if(DOF2_GetInt(File(playerid), "InGang") == 0) return SendClientMessage(playerid, White, "àúä ìà ðîöà áëðåôééä");
		    format(GangFile, sizeof(GangFile), "Gangs/%s.ini", DOF2_GetString(File(playerid), "Gang"));
			DOF2_SetInt(GangFile, "Players", DOF2_GetInt(GangFile,"Players")-1);
			format(string, sizeof(string), "! áäöìçä \"{FFFFFF}%s{8B5A2B}\" òæáú àú äëðåôééä", DOF2_GetString(File(playerid), "Gang"));
    		SendClientMessage(playerid, Whitebrown, string);
    		format(string, sizeof(string), "! òæá àú äëðåôééä {FFFFFF}%s{8B5A2B} äùç÷ï", GetName(playerid));
			if(DOF2_GetInt(File(playerid), "GangLevel") == 1)
			{
				for(new i=0; i<MAX_PLAYERS; i++)
				{
					if(IsPlayerConnected(i))
					{
						if(Logged[i] == true)
						{
						    if(i != playerid)
						    {
						 		if(DOF2_GetInt(File(i), "InGang") == 1)
						 		{
								 	if(strcmp(DOF2_GetString(File(playerid), "Gang"), DOF2_GetString(File(i), "Gang"), true) == 0)
								 	{
										SendClientMessage(i, Whitebrown, string);
									}
								}
							}
						}
					}
				}
			}
			else if(DOF2_GetInt(File(playerid), "GangLevel") == 2)
			{
				format(string, sizeof(string), "! ðîç÷ä àåèåîèéú ò÷á òæéáú îðäì äëðåôééä \"{FFFFFF}%s{8B5A2B}\" äëðåôééä", DOF2_GetString(File(playerid), "Gang"));
				for(new i=0; i<MAX_PLAYERS; i++)
				{
					if(i != playerid && DOF2_GetInt(File(i), "InGang") == 1 && strcmp(DOF2_GetString(File(playerid), "Gang"), DOF2_GetString(File(i), "Gang"), true) == 0)
					{
						SendClientMessage(i, Whitebrown, string);
						DOF2_Unset(File(i), "GangLevel");
						DOF2_Unset(File(i), "Gang");
						DOF2_SetInt(File(i), "InGang", 0);
					}
				}
				format(GangFile, sizeof(GangFile), "Gangs/%s.ini", DOF2_GetString(File(playerid), "Gang"));
				DOF2_RemoveFile(GangFile);
			}
			DOF2_Unset(File(playerid), "GangLevel");
			DOF2_Unset(File(playerid), "Gang");
			DOF2_SetInt(File(playerid), "InGang", 0);
   			return 1;
		}
	    if(strcmp(tmp, "Kick", true) == 0)
		{
	        if(DOF2_GetInt(File(playerid), "InGang") == 0) return SendClientMessage(playerid, White, "àúä ìà ðîöà áëðåôééä");
			if(DOF2_GetInt(File(playerid), "GangLevel") != 2) return SendClientMessage(playerid, White, "àúä ìà îðäì äëðåôééä");
			tmp1 = strtok(cmdtext,idx);
			if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Gang Kick [playerid] :öåøú ùéîåù");
    		id = strval(tmp1);
			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
