			if(weap == 38 || weap == 39 || weap == 40 || weap == 43 || weap == 44 || weap == 45) return SendClientMessage(playerid, White, "äðù÷ ùöééðú çñåí ìùéîåù");
			tmp2 = strtok(cmdtext, idx);
