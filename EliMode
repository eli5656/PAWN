 		SendClientMessage(id, Green ,string);
		format(string, sizeof(string), "{FFFFFF}%s{33AA33} ùéâøú ìð÷åãú äñôàåï àú äùç÷ï", GetName(id));
 		SendClientMessage(playerid, Green ,string);
		SpawnPlayer(id);
  		return 1;
