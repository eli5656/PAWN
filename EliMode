////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Mute", true) == 0 || strcmp(cmd, "/M", true) == 0)
	{
		if(GetAdminLevel(playerid) < 2 && GetTempAdminLevel(playerid) < 2 && GetSpyAdminLevel(playerid) < 2) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì2");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Mute(M) [playerid] [time] [reason] :öåøú ùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
  		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		if(inAFK[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ìà ðîöà");
		if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
		if(inMute[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ëáø îåùú÷");
	 	tmp1 = strtok(cmdtext, idx);
		if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Mute(M) [playerid] [time] [reason] :öåøú ùéîåù");
		any = strval(tmp1);
		if(any < 1 || any > 10)  return SendClientMessage(playerid, White, "èååç äîéåè áã÷åú äåà î1 òã 10");
		new length = strlen(cmdtext);
		while ((idx < length) && (cmdtext[idx] <= ' '))
		{
			idx++;
		}
		new offset = idx;
		new result[64];
		while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
		{
			result[idx - offset] = cmdtext[idx];
			idx++;
		}
		result[idx - offset] = EOS;
		if(!strlen(result)) return SendClientMessage(playerid, White, "/Mute(M) [playerid] [time] [reason] :öåøú ùéîåù");
	 	format(string, sizeof(string), "ì{FFFFFF}%d{33AA33} ã÷åú {FFFFFF}%s{33AA33} äåùú÷ òì éãé äîðäì {FFFFFF}%s{33AA33} äùç÷ï", any, GetName(playerid), GetName(id));
		OL_SendClientMessageToAll(Green, string);
  		format(string, sizeof(string), "({FFFFFF}%s{33AA33})", (result));
  		OL_SendClientMessageToAll(Green, string);
		DOF2_SetInt(File(id), "Mute", DOF2_GetInt(File(id), "Mute")+1);
		DOF2_SetInt(File(id), "inMute", 1);
		inMute[id] = 1;
		DOF2_SetInt(File(id), "MuteTime", any);
		DOF2_SetString(File(id), "MuteReason", (result));
		DOF2_SetString(File(id), "MuteAdmin", GetName(playerid));
		MuteTimer[id] = SetTimerEx("AutomaticUnmute", 60*1000*any, 0, "d", id);
		MuteTimer1[id] = SetTimerEx("AutomaticMainusMuteTime", 60*1000, 0, "d", id);
 		return 1;
 	}
