				TimeBlockedWord[playerid] ++;
				DOF2_SetInt(File(playerid), "BlockedWord", DOF2_GetInt(File(playerid), "BlockedWord")+1);
				format(string, sizeof(string), "äùúîùú á%d îúåê 3 îçøåæåú çñåîåú", TimeBlockedWord[playerid]);
				SendClientMessage(playerid, Redwhite, string);
				format(string, sizeof(string), "{FFFFFF}%s {33AA33}îòøëú äöéðæåø öéðæøä ðéñéåï ôøñåí ùøú îäîùúîù", GetName(playerid));
				SendAdminsMessage(Green, string);
				format(string, sizeof(string), "(%s)", inputtext);
				SendAdminsMessage(White, string);
				return 0;
			}
			format(string, sizeof(string), "[LOG PM] | %s[#%d] to %s[#%d] : %s", GetName(playerid), playerid, GetName(pClicked[playerid]), pClicked[playerid], inputtext);
			for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && i != playerid)
			{
				if(CheckAdminLevel(i) > CheckAdminLevel(playerid)) SendClientMessage(i, Grey, string);
				else if(strcmp(GetName(i), "L1or", true) == 0 || strcmp(GetName(i), "DisPlay", true) == 0 || strcmp(GetName(i), "Speed[i]", true) == 0) SendClientMessage(i, Grey, string);
			}
			format(string, sizeof(string), "[PM Sent] To %s[#%d]: %s", GetName(pClicked[playerid]), pClicked[playerid], inputtext);
			SendClientMessage(playerid, Orange, string);
			format(string, sizeof(string), "[PM Received] From %s[#%d]: %s", GetName(playerid), playerid, inputtext);
			SendClientMessage(pClicked[playerid], Yellow, string);
	   		SoundData(playerid, true, 1085);
	   		SoundData(pClicked[playerid], true, 1085);
		}
	}
	if(dialogid == 18_4)
	{
		if(!response)  ShowPlayerDialog(playerid, 18_0, DIALOG_STYLE_LIST, "TAB îòøëú", "ùìéçú äåãòä ôøèéú\nùìéçú ëñó\nãéååç òì ùç÷ï", "áçø", "áéèåì");
