	}
	if(dialogid == 18_1)
	{
		if(!response)  ShowPlayerDialog(playerid, 18_0, DIALOG_STYLE_LIST, "TAB îòøëú", "ùìéçú äåãòä ôøèéú\nùìéçú ëñó\nãéååç òì ùç÷ï", "áçø", "áéèåì");
		else
		{
			for(new i=0; i<MAX_PLAYERS; i++)
		    {
		        if(GetAdminLevel(i) >= 2) any++;
		        else if(GetTempAdminLevel(i) >= 2) any++;
			}
			if(!any) return SendClientMessage(playerid, White, "àéï îðäìéí îçåáøéí áùøú");
			if(!IsPlayerConnected(pClicked[playerid])) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[pClicked[playerid]] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			format(string, sizeof(string), "%s àðà øùåí àú ñéáú äãéååç òì äùç÷ï\n{FF0000}òìéê ìøùåí ñéáä ëãé ìäîùéê", GetName(pClicked[playerid]));
