		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/GetVehicleHP(Gvhp) [playerid] :öåøú ùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(!IsPlayerInAnyVehicle(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðîöà áøëá");
  		GetVehicleHealth(GetPlayerVehicleID(id), Health);
  		format(string, sizeof(string), "äéðå %1.f {FFFFFF}%s{33AA33} àçåæ äçééí ùì äøëá ùì äùç÷ï", (Health/10), GetName(id));
		SendClientMessage(playerid, Green ,string);
        return 1;
    }
