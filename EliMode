		DisablePlayerRaceCheckpoint(playerid);
  		CP_ParachutingMission[playerid] = 0;
		AntiCheat_GivePlayerMoney(playerid, true, ParachutingMission_RandomPrice);
		SetPlayerVirtualWorld(playerid, 0);
	}
    return 1;
}

//------------------------------------------------------------------------------

public OnPlayerUpdate(playerid)
{
	SetTimerEx("UpdatePlayerData", 1000*1*1, 0, "d", playerid);
	if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_PASSENGER && Logged[GetDriver(GetPlayerVehicleID(playerid))] == true && !inAFK[GetDriver(GetPlayerVehicleID(playerid))])
	{
		if(!inAFK[playerid] && Logged[playerid] == true)
		{
	    	KillTimer(AutoAFK_Timer[playerid]);
   			AutoAFK_Timer[playerid] = SetTimerEx("AutomaticAFK", 1000*60*2, 0, "d", playerid);
		}
	}
	DOO_OnPlayerUpdate(playerid);
	return 1;
}

//------------------------------------------------------------------------------

public OnGameModeInit()
{
//	SetGameModeText("FxpDM V0.6.2-a");

	new sendcmd[128];
	format(sendcmd, sizeof(sendcmd), "hostname %s", SERVER_NAME);
    SendRconCommand(sendcmd);
    DisableInteriorEnterExits();
	ShowPlayerMarkers(1);
	ShowNameTags(1);
	EnableStuntBonusForAll(0);
	UsePlayerPedAnims();
	AntiDeAMX();
	//StopRobbingMyMode();
	
	if(!DOF2_FileExists("Users/None.ini")) DOF2_CreateFile("Users/None.ini");
	
	SetTimer("UpdateScore", 1000*1*1, 1);
	SetTimer("CheckPlayerWarnings", 1000*1*1, 1);
	SetTimer("AccessLess", 1000*1*1, 1);
	SetTimer("AutomaticMessage", 1000*60*20, 1);
	SetTimer("StartAutomaticActivity", 1000*60*30, 1);
	SetTimer("ActOn", 1000*1*1, 1);
	SetTimer("CheckTimeBlockedWord", 1000*1*1, 1);
	SetTimer("MoneyShip", 1000*1*3, 1);
	SetTimer("RunningAntiCheat", 1000*2*1, 1);
	SetTimer("ExclusiveBOTip", 1000*60*4, 1);
	SetTimer("AntiRcon", 1000*1*1, 1);
	SetTimer("SetPlayerGodmode", 1000*1*1, 1);
//	SetTimer("UpdateLogoColor", 1000*1*1, 1);
	PropAmountTimer = SetTimer("UpdatePropertiesOwnerAmount", 1000*60*DOF2_GetInt("Properties/Setting.ini", "AmountMinutes"), 1);
	SetTimer("security", 1000, 1);
	SetTimer("UpdateTimeNData", 1000*1*1, 1);
	//SetTimer("UpdateOnlinePlayers", 1000*1*1, 1);
	
	SetTimer("Minigun_CheckWinner", 1000*1*1, 1);
