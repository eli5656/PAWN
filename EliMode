			if(GVA(v) == 560)  DOF2_SetInt(carfile, "Price", 8000000);
			if(GVA(v) == 565) DOF2_SetInt(carfile, "Price", 8000000);
			if(GVA(v) == 533) DOF2_SetInt(carfile, "Price", 6000000);
			if(GVA(v) == 522) DOF2_SetInt(carfile, "Price", 10000000);
			if(GVA(v) == 463) DOF2_SetInt(carfile, "Price", 8000000);
