	if(dialogid == 6_0 && response)
	{
       	if(listitem == 0)
		{
			if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, White, "àúä ìà ðîöà áøëá");
			if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, White, "àúä ìà äðäâ áøëá");
			if(DOF2_GetInt(CarFile(vehicleid), "Public") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
			if(DOF2_GetInt(CarFile(vehicleid), "Private") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
			if(DOF2_GetInt(File(playerid), "HaveCar") == 1) return SendClientMessage(playerid, White, "éù ááòìåúê øëá àéùé");
			if(DOF2_GetInt(CarFile(vehicleid), "Owned") == 1) return SendClientMessage(playerid, White, "øëá àéùé æä ðîöà ááòìåú îéùäå");
			if(DOF2_GetInt(CarFile(vehicleid), "Special") == 1 && GetLevel(playerid) < 2) return SendClientMessage(playerid, White, "øëá îéåçã ðéúï ì÷ðééä îøîä 5 åîòìä");
    		if(DOF2_GetInt(CarFile(vehicleid), "Special") == 1)
			{
				if(AntiCheat_GetPlayerMoney(playerid) < GetVehiclePrice(GetPlayerVehicleID(playerid))) return SendClientMessage(playerid, White, "àéï ìê îñôé÷ ëñó ëãé ì÷ðåú øëá æä");
				DOF2_SetString(CarFile(vehicleid), "Owner", GetName(playerid));
				DOF2_SetInt(CarFile(vehicleid), "Owned", 1);
				DOF2_SetInt(CarFile(vehicleid), "Lock", 0);
				DOF2_SetInt(File(playerid), "HaveCar", 1);
				DOF2_SetInt(File(playerid), "CarID", vehicleid);
				format(string, sizeof(string), "áäöìçä {FFFFFF}%s{556b2f} ÷ðéú àú äøëá", VehicleNames[GetVehicleModel(vehicleid)-400]);
				SendClientMessage(playerid, Olive, string);
				AntiCheat_GivePlayerMoney(playerid, false, GetVehiclePrice(GetPlayerVehicleID(playerid)));
				//--------------------------------------------------------------
				Delete3DTextLabel(CarText[vehicleid]);
				format(string, sizeof(string), "øëá îéåçã\n{FFFFFF}%s{800080} ááòìåú", DOF2_GetString(CarFile(vehicleid), "Owner"));
				CarText[vehicleid] = Create3DTextLabel(string, Purple, 0, 0, 2, 30, 0, 1);
				Attach3DTextLabelToVehicle(CarText[vehicleid], vehicleid, 0, 0, 1);
			}
			else
			{
				if(AntiCheat_GetPlayerMoney(playerid) < GetVehiclePrice(GetPlayerVehicleID(playerid))) return SendClientMessage(playerid, White, "àéï ìê îñôé÷ ëñó ëãé ì÷ðåú øëá æä");
				DOF2_SetString(CarFile(vehicleid), "Owner", GetName(playerid));
				DOF2_SetInt(CarFile(vehicleid), "Owned", 1);
				DOF2_SetInt(CarFile(vehicleid), "Lock", 0);
				DOF2_SetInt(File(playerid), "HaveCar", 1);
				DOF2_SetInt(File(playerid), "CarID", vehicleid);
				format(string, sizeof(string), "áäöìçä {FFFFFF}%s{556b2f} ÷ðéú àú äøëá", VehicleNames[GetVehicleModel(vehicleid)-400]);
				SendClientMessage(playerid, Olive, string);
				AntiCheat_GivePlayerMoney(playerid, false, GetVehiclePrice(GetPlayerVehicleID(playerid)));
				//--------------------------------------------------------------
				Delete3DTextLabel(CarText[vehicleid]);
				format(string, sizeof(string), "øëá øâéì\n{FFFFFF}%s{33AA33} ááòìåú", DOF2_GetString(CarFile(vehicleid), "Owner"));
				CarText[vehicleid] = Create3DTextLabel(string, Aqua, 0, 0, 2, 30, 0, 1);
				Attach3DTextLabelToVehicle(CarText[vehicleid], vehicleid, 0, 0, 1);
			}
			Vehicle3DTimer = SetTimer("UpdateVehicle3DText", 1000*1*1, false);
			return 1;
		}
       	if(listitem == 1)
		{
			if(DOF2_GetInt(File(playerid), "HaveCar") == 0) return SendClientMessage(playerid, White, "àéï ááòìåúê øëá àéùé");
			ShowPlayerDialog(playerid, 6_15, DIALOG_STYLE_LIST, "îòøëú øëáéí - àôùøåéåú äøëá äàéùé", "îëéøú äøëá äàéùé\n{AFAFAF}/Callcar - ìùâø àú äøëá äàéùé{FFFFFF}\n{AFAFAF}/Fix - ìú÷ï àú äøëá äàéùé{FFFFFF}\n{AFAFAF}/Flip - äôéëú äøëá äàéùé{FFFFFF}\nìðòåì/ìôúåç àú äøëá äàéùé\nùéðåé ìåçéú øéùåé ìøëá äàéùé", "áçø", "çæåø");
		}
       	if(listitem == 2)
		{
			if(DOF2_GetInt(File(playerid), "HaveCar") == 0) return SendClientMessage(playerid, White, "àéï ááòìåúê øëá àéùé");
			if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, White, "àúä ìà ðîöà áøëá");
			if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, White, "àúä ìà äðäâ áøëá");
			if(DOF2_GetInt(CarFile(vehicleid), "Public") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
			if(DOF2_GetInt(CarFile(vehicleid), "Private") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
			if(vehicleid != GetPlayerCar(playerid)) return SendClientMessage(playerid, White, "øëá àéùé æä àéðå ùééê ìê");
			if(DOF2_GetInt(CarFile(vehicleid), "Special") == 0) return SendClientMessage(playerid, White, "øëá æä àéðå øëá îéåçã");
			ShowPlayerDialog(playerid, 6_1, DIALOG_STYLE_LIST, "îòøëú øëáéí - ùéôåøéí", "Nitro\nHydralics\nWheels\nColor\nNeon\nBike Nitro\nCar Spawn", "áçø", "çæåø");
			return 1;
		}
       	if(listitem == 3)
		{
			if(GetAdminLevel(playerid) < 5 && GetSpyAdminLevel(playerid) < 5 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì5");
			ShowPlayerDialog(playerid, 6_12, DIALOG_STYLE_LIST, "îòøëú øëáéí - ô÷åãåú àãîéï ìøëá", "äôéëú øëá ìöéáåøé\näôéëú øëá ìîéåçã\näôéëú øëá ìøëåù ôøèé\n÷áéòú îçéø ìøëá\náéèåì ùîéøú îé÷åí\n{FF0000}îëéøú äøëá", "áçø", "çæåø");
			return 1;
		}
	}
	if(dialogid == 6_1)
	{
		if(!response) ShowPlayerDialog(playerid, 6_0, DIALOG_STYLE_LIST, "îòøëú øëáéí", "÷ðééú äøëá\nàôùøåéåú äøëá äàéùé\n{800080}(÷ðééú/îëéøú ùéôåøéí úîéãééí ìøëá äàéùé (îéåçãéí áìáã\n{33AA33}ô÷åãåú àãîéï ìøëá", "áçø", "áéèåì");
		else
		{
        	if(listitem == 0) return ShowPlayerDialog(playerid, 6_2, DIALOG_STYLE_LIST, "Nitro - îòøëú øëáéí - ùéôåøéí", "÷ðééä\näñøä", "áçø", "çæåø");
        	if(listitem == 1) return ShowPlayerDialog(playerid, 6_4, DIALOG_STYLE_LIST, "Hydralics - îòøëú øëáéí - ùéôåøéí", "÷ðééä\näñøä", "áçø", "çæåø");
        	if(listitem == 2) return ShowPlayerDialog(playerid, 6_5, DIALOG_STYLE_LIST, "Wheels - îòøëú øëáéí - ùéôåøéí", "÷ðééä\näñøä", "áçø", "çæåø");
        	if(listitem == 3)
			{
				if(AntiCheat_GetPlayerMoney(playerid) < 100000) return SendClientMessage(playerid, White, "[100,000] - àéï ìê îñôé÷ ëñó ì÷ðééú ùéôåø úîéãé æä");
				ShowPlayerDialog(playerid, 6_7, DIALOG_STYLE_INPUT, "Colors - îòøëú øëáéí - ùéôåøéí", "àðà ëúåá áúéáä àú äöáòéí ùáøöåðê ìùéí ìøëáê", "áçø", "çæåø");
				return 1;
