/////////////////////////////////// LOGS ///////////////////////////////////////
public GiveMoney_Logs(string[])
{
	new entry[256];
	format(entry, sizeof(entry), "%s\r\n\n\n", string);
	new File:hFile;
	hFile = fopen("Logs/GiveMoney.log", io_append);
	fwrite(hFile, entry);
	fclose(hFile);
}
stock fwrite_utf8(File:handle, str[], bool:use_utf8)
{
	new x=0;
 	if(!handle) return 0;
	while(str[x] != EOS) {
		fputchar(handle, str[x], use_utf8);
		x++;
	}

	return x;
}
public PrivateMessages_Logs(string[])
{
	new entry[256];
	format(entry, sizeof(entry), "%s\r\n\n\n", string);
	new File:hFile;
	hFile = fopen("Logs/PrivateMessages.log", io_append);
	fwrite_utf8(hFile, entry, false);
	fclose(hFile);
}

