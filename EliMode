			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
			if(DOF2_GetInt(File(id), "InClan") == 0) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðîöà á÷ìàï øùîé");
			if(DOF2_GetInt(File(id), "InClan") == 1 && strcmp(DOF2_GetString(File(playerid), "Clan"), DOF2_GetString(File(id), "Clan"), true) != 0) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà á÷ìàï øùîé àçø");
			if(DOF2_GetInt(File(playerid), "ClanLevel") < DOF2_GetInt(File(id), "ClanLevel")) return SendClientMessage(playerid, White, "àéðê éëåì ìòøåú øîåú ùì ùç÷ðéí áøîä éåúø âáåää îùìê á÷ìàï");
			tmp2 = strtok(cmdtext,idx);
    		if(!strlen(tmp2)) return SendClientMessage(playerid, White, "/Clan SetLevel [playerid] [level] :öåøú ùéîåù");
			level = strval(tmp2);
			if(level < 1 || level > 3) return SendClientMessage(playerid, White, "èååç äøîåú á÷ìàï äåà áéï 1 ì3");
			if(level == DOF2_GetInt(File(playerid), "ClanLevel")) return SendClientMessage(playerid, White, "àéðê éëåì ìòøåê àú øîúå ùì ùç÷ï ìøîä ùìê");
	    	DOF2_SetInt(File(id), "ClanLevel", level);
    		format(string, sizeof(string), "ì{FFFFFF}%d{008080} {FFFFFF}%s{008080} ùéðä àú äøîä á÷ìàï ùì äùç÷ï {FFFFFF}%s{008080} äùç÷ï", level, GetName(id), GetName(playerid));
			SendMessageToClan(playerid, string);
			return 1;
		}
	    if(strcmp(tmp, "Kick", true) == 0)
		{
			if(DOF2_GetInt(File(playerid), "InClan") == 0) return SendClientMessage(playerid, White, "àúä ìà ðîöà á÷ìàï øùîé");
			if(DOF2_GetInt(File(playerid), "ClanLevel") < 2) return SendClientMessage(playerid, White, "àúä ìà áåçï ÷ìàï åîòìä á÷ìàï äøùîé áå àúä ðîöà");
			tmp1 = strtok(cmdtext,idx);
			if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Clan Kick [playerid] :öåøú ùéîåù");
    		id = strval(tmp1);
			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
			if(DOF2_GetInt(File(id), "InClan") == 0) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðîöà á÷ìàï øùîé");
			if(DOF2_GetInt(File(id), "InClan") == 1 && strcmp(DOF2_GetString(File(playerid), "Clan"), DOF2_GetString(File(id), "Clan"), true) != 0) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà á÷ìàï øùîé àçø");
			if(DOF2_GetInt(File(playerid), "ClanLevel") < DOF2_GetInt(File(id), "ClanLevel")) return SendClientMessage(playerid, White, "àúä ìà éëåì ìäòéó îéùäå îä÷ìàï áøîä æää å/àå âáåää îùìê");
  			DOF2_Unset(File(id), "ClanLevel");
			DOF2_SetInt(File(id), "InClan", 0);
    		format(string, sizeof(string), "îä÷ìàï {FFFFFF}%s{008080} äòéó àú äùç÷ï {FFFFFF}%s{008080} äùç÷ï", GetName(id), GetName(playerid));
			SendMessageToClan(playerid, string);
			DOF2_Unset(File(id), "Clan");
    		format(string, sizeof(string), "äòéó àåúê îä÷ìàï {FFFFFF}%s{008080} äùç÷ï", GetName(playerid));
			SendClientMessage(id, Bluesea, string);
			format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
			DOF2_SetInt(ClanFile, "Players", DOF2_GetInt(ClanFile, "Players")-1);
   			return 1;
		}
	    if(strcmp(tmp, "Chat", true) == 0)
		{
			if(DOF2_GetInt(File(playerid), "ClanLevel") < 3) return SendClientMessage(playerid, White, "àúä ìà ñâï îðäì ÷ìàï åîòìä á÷ìàï äøùîé áå àúä ðîöà");
			format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
			if(DOF2_GetInt(ClanFile, "ClanChat") == 0)
			{
				DOF2_SetInt(ClanFile, "ClanChat", 1);
				if(DOF2_GetInt(File(playerid), "ClanLevel") == 3)
				{
	    			format(string, sizeof(string), "ðòì àú ö'àè ä÷ìàï {FFFFFF}%s{008080} ñâï îðäì ä÷ìàï", GetName(playerid));
					SendMessageToClan(playerid, string);
				}
				else if(DOF2_GetInt(File(playerid), "ClanLevel") == 4)
				{
	    			format(string, sizeof(string), "ðòì àú ö'àè ä÷ìàï {FFFFFF}%s{008080} îðäì ä÷ìàï", GetName(playerid));
					SendMessageToClan(playerid, string);
				}
			}
			else if(DOF2_GetInt(ClanFile, "ClanChat") == 1)
			{
				DOF2_SetInt(ClanFile, "ClanChat", 0);
				if(DOF2_GetInt(File(playerid), "ClanLevel") == 3)
				{
	    			format(string, sizeof(string), "ôúç àú ö'àè ä÷ìàï {FFFFFF}%s{008080} ñâï îðäì ä÷ìàï", GetName(playerid));
					SendMessageToClan(playerid, string);
				}
				else if(DOF2_GetInt(File(playerid), "ClanLevel") == 4)
				{
	    			format(string, sizeof(string), "ôúç àú ö'àè ä÷ìàï {FFFFFF}%s{008080} îðäì ä÷ìàï", GetName(playerid));
					SendMessageToClan(playerid, string);
				}
			}
   			return 1;
		}
	    if(strcmp(tmp, "Msg", true) == 0)
		{
			if(DOF2_GetInt(File(playerid), "InClan") == 0) return SendClientMessage(playerid, White, "àúä ìà ðîöà á÷ìàï øùîé");
			if(DOF2_GetInt(File(playerid), "ClanLevel") < 4) return SendClientMessage(playerid, White, "àúä ìà îðäì ä÷ìàï á÷ìàï äøùîé áå àúä ðîöà");
		    if(AntiCheat_GetPlayerMoney(playerid) < 100000) return SendClientMessage(playerid, White, "[100,000] - àéï ìê îñôé÷ ëñó ëãé ìùðåú äåãòú ëðéñä ì÷ìàï äøùîé");
			new length = strlen(cmdtext);
			while ((idx < length) && (cmdtext[idx] <= ' '))
			{
				idx++;
			}
			new offset = idx;
			new result[64];
			while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
			{
				result[idx - offset] = cmdtext[idx];
				idx++;
			}
			result[idx - offset] = EOS;
			if(!strlen(result)) return SendClientMessage(playerid, White, "/Clan Msg [text] :öåøú ùéîåù");
			format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
			if(DOF2_GetInt(ClanFile, "HaveMsg") == 0)
			{
				DOF2_SetInt(ClanFile, "HaveMsg", 1);
			}
			DOF2_SetString(ClanFile, "ClanMsg", (result));
    		format(string, sizeof(string), "ùéðä àú äåãòú ä÷ìàï ì: {FFFFFF}%s{008080} {FFFFFF}%s{008080} îðäì ä÷ìàï", (result), GetName(playerid));
			SendMessageToClan(playerid, string);
			AntiCheat_GivePlayerMoney(playerid, false, 100000);
   			return 1;
		}
	    if(strcmp(tmp, "Color", true) == 0)
		{
			if(DOF2_GetInt(File(playerid), "InClan") == 0) return SendClientMessage(playerid, White, "àúä ìà ðîöà á÷ìàï øùîé");
			if(DOF2_GetInt(File(playerid), "ClanLevel") < 4) return SendClientMessage(playerid, White, "àúä ìà îðäì ä÷ìàï á÷ìàï äøùîé áå àúä ðîöà");
		    if(AntiCheat_GetPlayerMoney(playerid) < 500000) return SendClientMessage(playerid, White, "[500,000] - àéï ìê îñôé÷ ëñó ëãé ìùðåú äåãòú ëðéñä ì÷ìàï äøùîé");
			tmp1 = strtok(cmdtext,idx);
		    if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Clan Color [Chat | Player] :öåøú ùéîåù");
		    if(strcmp(tmp1, "Chat", true) == 0)
			{
				tmp2 = strtok(cmdtext,idx);
				if(!strlen(tmp2))return SendClientMessage(playerid, White, "/Clan Color Chat [Red 1 - 255] [Green 1 - 255] [Blue 1 - 255] :öåøú ùéîåù");
				new R = strval(tmp2);
				if(R > 255 || 0 > R) return SendClientMessage(playerid, White, "äåà áéï 0 ì255 Red èååç äöáòéí ùì");
				tmp3 = strtok(cmdtext,idx);
				if(!strlen(tmp3))return SendClientMessage(playerid, White, "/Clan Color Chat [Red 1 - 255] [Green 1 - 255] [Blue 1 - 255] :öåøú ùéîåù");
				new G = strval(tmp3);
				if(G > 255 || 0 > G) return SendClientMessage(playerid, White, "äåà áéï 0 ì255 Green èååç äöáòéí ùì");
				tmp4 = strtok(cmdtext,idx);
				if(!strlen(tmp4))return SendClientMessage(playerid, White, "/Clan Color Chat [Red 1 - 255] [Green 1 - 255] [Blue 1 - 255] :öåøú ùéîåù");
				new B = strval(tmp4);
				if(B > 255 || 0 > B) return SendClientMessage(playerid, White, "äåà áéï 0 ì255 Blue èååç äöáòéí ùì");
				SendClientMessage(playerid, Bluesea, "! öáò ä÷ìàï ìö'àè ðùîø áäöìçä");
	    		format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
			    DOF2_SetInt(ClanFile, "HaveClanChatColor", 1);
			    DOF2_SetInt(ClanFile, "ClanChatColorR", R);
			    DOF2_SetInt(ClanFile, "ClanChatColorG", G);
			    DOF2_SetInt(ClanFile, "ClanChatColorB", B);
			    AntiCheat_GivePlayerMoney(playerid, false, 500000);
			   	return 1;
			}
		    if(strcmp(tmp1, "Player", true) == 0)
			{
				tmp2 = strtok(cmdtext,idx);
				if(!strlen(tmp2))return SendClientMessage(playerid, White, "/Clan Color Player [Red 1 - 255] [Green 1 - 255] [Blue 1 - 255] :öåøú ùéîåù");
				new R = strval(tmp2);
				if(R > 255 || 0 > R) return SendClientMessage(playerid, White, "äåà áéï 0 ì255 Red èååç äöáòéí ùì");
				tmp3 = strtok(cmdtext,idx);
				if(!strlen(tmp3))return SendClientMessage(playerid, White, "/Clan Color Player [Red 1 - 255] [Green 1 - 255] [Blue 1 - 255] :öåøú ùéîåù");
				new G = strval(tmp3);
				if(G > 255 || 0 > G) return SendClientMessage(playerid, White, "äåà áéï 0 ì255 Green èååç äöáòéí ùì");
				tmp4 = strtok(cmdtext,idx);
				if(!strlen(tmp4))return SendClientMessage(playerid, White, "/Clan Color Player [Red 1 - 255] [Green 1 - 255] [Blue 1 - 255] :öåøú ùéîåù");
				new B = strval(tmp4);
				if(B > 255 || 0 > B) return SendClientMessage(playerid, White, "äåà áéï 0 ì255 Blue èååç äöáòéí ùì");
				SendClientMessage(playerid, Bluesea, "! öáò ä÷ìàï ìùç÷ï ðùîø áäöìçä");
	    		format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
			    DOF2_SetInt(ClanFile, "HaveClanColor", 1);
			    DOF2_SetInt(ClanFile, "ClanColorR", R);
			    DOF2_SetInt(ClanFile, "ClanColorG", G);
			    DOF2_SetInt(ClanFile, "ClanColorB", B);
			    AntiCheat_GivePlayerMoney(playerid, false, 500000);
			   	return 1;
			}
			else
			{
				SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Clan Color {FFFFFF}àôùøåú");
			}
  			return 1;
		}
	    if(strcmp(tmp, "Data", true) == 0)
		{
		    tmp1 = strtok(cmdtext,idx);
		    if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Clan Data [Info | Players] :öåøú ùéîåù");
		    if(strcmp(tmp1, "Info", true) == 0)
			{
				tmp2 = strtok(cmdtext, idx);
				if(strlen(tmp2))
			    {
				    format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", tmp2);
				    if(!DOF2_FileExists(ClanFile)) return SendClientMessage(playerid, White, "ùí ä÷ìàï äøùîé ùöééðú àéððå ÷ééí");
				    format(string, sizeof(string), ":{FFFFFF}%s{008080} ôøèé ä÷ìàï", tmp2);
					SendClientMessage(playerid, Grellow, string);
				    format(string, sizeof(string), "{FFFFFF}%d{008080} :ëîåú äùç÷ðéí á÷ìàï", DOF2_GetInt(ClanFile, "Players"));
					SendClientMessage(playerid, White, string);
					new ClanPlayers;
					for(new i = 0; i < MAX_PLAYERS; i++)
					{
						if(IsPlayerConnected(i))
						{
							if(Logged[i] == true)
							{
								if(DOF2_GetInt(File(i), "InClan") == 1)
								{
									if(strcmp(tmp2, DOF2_GetString(File(i), "Clan"), true) == 0)
					 				{
										ClanPlayers +=1;
									}
								}
							}
						}
					}
				    format(string, sizeof(string), "{FFFFFF}%d{008080} :îñôø ùç÷ðé ä÷ìàï äîçåáøéí áøâò æä", ClanPlayers);
					SendClientMessage(playerid, White, string);
				    format(string, sizeof(string), "äåãòú ä÷ìàï: {FFFFFF}%s{008080}", DOF2_GetString(ClanFile, "ClanMsg"));
					SendClientMessage(playerid, White, string);
				    format(string, sizeof(string), "{FFFFFF}%s${008080} :ëîåú äëñó ááð÷ ä÷ìàï", GetNum(DOF2_GetInt(ClanFile, "Bank")));
					SendClientMessage(playerid, White, string);
				}
				else if(!strlen(tmp2))
				{
				    if(DOF2_GetInt(File(playerid), "InClan") == 0) return SendClientMessage(playerid, White, "àúä ìà çáø á÷ìàï øùîé");
					format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
					SendClientMessage(playerid, Grellow, ":ôøèé ä÷ìàï ùìê");
				    format(string, sizeof(string), "{FFFFFF}%d{008080} :ëîåú äùç÷ðéí á÷ìàï", DOF2_GetInt(ClanFile, "Players"));
					SendClientMessage(playerid, White, string);
					new ClanPlayers;
					for(new i = 0; i < MAX_PLAYERS; i++)
					{
						if(IsPlayerConnected(i))
						{
							if(Logged[i] == true)
							{
								if(DOF2_GetInt(File(i), "InClan") == 1)
								{
									if(strcmp(DOF2_GetString(File(playerid), "Clan"), DOF2_GetString(File(i), "Clan"), true) == 0)
					 				{
										ClanPlayers +=1;
									}
								}
							}
						}
					}
				    format(string, sizeof(string), "{FFFFFF}%d{008080} :îñôø ùç÷ðé ä÷ìàï äîçåáøéí áøâò æä", ClanPlayers);
					SendClientMessage(playerid, White, string);
				    format(string, sizeof(string), "äåãòú ä÷ìàï: {FFFFFF}%s{008080}", DOF2_GetString(ClanFile, "ClanMsg"));
					SendClientMessage(playerid, White, string);
				    format(string, sizeof(string), "{FFFFFF}%s${008080} :ëîåú äëñó ááð÷ ä÷ìàï", GetNum(DOF2_GetInt(ClanFile, "Bank")));
					SendClientMessage(playerid, White, string);
				}
				return 1;
			}
			if(strcmp(tmp1, "Players", true) == 0)
			{
				tmp2 = strtok(cmdtext, idx);
				if(strlen(tmp2))
			    {
				    format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", tmp2);
				    if(!DOF2_FileExists(ClanFile)) return SendClientMessage(playerid, White, "ùí ä÷ìàï äøùîé ùöééðú àéððå ÷ééí");
				    format(string, sizeof(string), ":îçåáøéí {FFFFFF}%s{008080} ùç÷ðé ä÷ìàï", tmp2);
				    SendClientMessage(playerid, Grellow, string);
					new ClanPlayers;
					for(new i = 0; i < MAX_PLAYERS; i++)
					{
						if(IsPlayerConnected(i))
						{
							if(Logged[i] == true)
							{
								if(DOF2_GetInt(File(i), "InClan") == 1)
								{
									if(strcmp(tmp2, DOF2_GetString(File(i), "Clan"), true) == 0)
					 				{
										ClanPlayers +=1;
									}
								}
							}
						}
					}
				    format(string, sizeof(string), "{FFFFFF}%d{008080} :îñôø ùç÷ðé ä÷ìàï äîçåáøéí áøâò æä", ClanPlayers);
					SendClientMessage(playerid, White, string);
					for(new i = 0; i < MAX_PLAYERS; i++)
					{
						if(IsPlayerConnected(i))
						{
							if(Logged[i] == true)
							{
								if(DOF2_GetInt(File(i), "InClan") == 1)
								{
									if(strcmp(tmp2, DOF2_GetString(File(i), "Clan"), true) == 0)
					 				{
					 				    if(DOF2_GetInt(File(i), "ClanLevel")  == 1)
					 				    {
											format(string, sizeof(string), "[#{FFFFFF}%d{008080}] Name: {FFFFFF}%s{008080} | Level: ùç÷ï ÷ìàï", i, GetName(i));
											SendClientMessage(playerid, Bluesea, string);
										}
					 				    else if(DOF2_GetInt(File(i), "ClanLevel")  == 2)
					 				    {
											format(string, sizeof(string), "[#{FFFFFF}%d{008080}] Name: {FFFFFF}%s{008080} | Level: áåçï ÷ìàï", i, GetName(i));
											SendClientMessage(playerid, Bluesea, string);
										}
					 				    else if(DOF2_GetInt(File(i), "ClanLevel")  == 3)
					 				    {
											format(string, sizeof(string), "[#{FFFFFF}%d{008080}] Name: {FFFFFF}%s{008080} | Level: ñâï îðäì ÷ìàï", i, GetName(i));
											SendClientMessage(playerid, Bluesea, string);
										}
					 				    else if(DOF2_GetInt(File(i), "ClanLevel")  == 4)
					 				    {
											format(string, sizeof(string), "[#{FFFFFF}%d{008080}] Name: {FFFFFF}%s{008080} | Level: îðäì ÷ìàï", i, GetName(i));
											SendClientMessage(playerid, Bluesea, string);
										}
									}
								}
							}
						}
					}
				}
				else if(!strlen(tmp2))
				{
				    if(DOF2_GetInt(File(playerid), "InClan") == 0) return SendClientMessage(playerid, White, "àúä ìà çáø á÷ìàï øùîé");
					format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetInt(File(playerid), "Clan"));
				    SendClientMessage(playerid, Grellow, ":ùç÷ðé ä÷ìàï ùìê îçåáøéí");
					new ClanPlayers;
					for(new i = 0; i < MAX_PLAYERS; i++)
					{
						if(IsPlayerConnected(i))
						{
							if(Logged[i] == true)
							{
								if(DOF2_GetInt(File(i), "InClan") == 1)
								{
									if(strcmp(DOF2_GetString(File(playerid), "Clan"), DOF2_GetString(File(i), "Clan"), true) == 0)
					 				{
										ClanPlayers +=1;
									}
								}
							}
						}
					}
				    format(string, sizeof(string), "{FFFFFF}%d{008080} :îñôø ùç÷ðé ä÷ìàï äîçåáøéí áøâò æä", ClanPlayers);
					SendClientMessage(playerid, White, string);
					for(new i = 0; i < MAX_PLAYERS; i++)
					{
						if(IsPlayerConnected(i))
						{
							if(Logged[i] == true)
							{
								if(DOF2_GetInt(File(i), "InClan") == 1)
								{
									if(strcmp(DOF2_GetString(File(playerid), "Clan"), DOF2_GetString(File(i), "Clan"), true) == 0)
					 				{
					 				    if(DOF2_GetInt(File(i), "ClanLevel")  == 1)
					 				    {
											format(string, sizeof(string), "[#{FFFFFF}%d{008080}] Name: {FFFFFF}%s{008080} | Level: ùç÷ï ÷ìàï", i, GetName(i));
											SendClientMessage(playerid, White, string);
										}
					 				    else if(DOF2_GetInt(File(i), "ClanLevel")  == 2)
					 				    {
											format(string, sizeof(string), "[#{FFFFFF}%d{008080}] Name: {FFFFFF}%s{008080} | Level: áåçï ÷ìàï", i, GetName(i));
											SendClientMessage(playerid, White, string);
										}
					 				    else if(DOF2_GetInt(File(i), "ClanLevel")  == 3)
					 				    {
											format(string, sizeof(string), "[#{FFFFFF}%d{008080}] Name: {FFFFFF}%s{008080} | Level: ñâï îðäì ÷ìàï", i, GetName(i));
											SendClientMessage(playerid, White, string);
										}
					 				    else if(DOF2_GetInt(File(i), "ClanLevel")  == 4)
					 				    {
											format(string, sizeof(string), "[#{FFFFFF}%d{008080}] Name: {FFFFFF}%s{008080} | Level: îðäì ÷ìàï", i, GetName(i));
											SendClientMessage(playerid, White, string);
										}
									}
								}
							}
						}
					}
				}
				return 1;
			}
			else
			{
				SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Clan Setting {FFFFFF}àôùøåú");
			}
			return 1;
		}
		else
		{
			SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Clan {FFFFFF}àôùøåú");
		}
		return 1;
	}
