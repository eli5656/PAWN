		weap = strval(tmp1);
		if(weap < 1 || weap > 46) return SendClientMessage(playerid, White, "äàééãé ðù÷ ùöééðú ùâåé");
		//if(weap == 38 || weap == 39 || weap == 40 || weap == 43 || weap == 44 || weap == 45) return SendClientMessage(playerid, White, "äðù÷ ùöééðú çñåí ìùéîåù");
		tmp2 = strtok(cmdtext, idx);
		if(!strlen(tmp2)) return SendClientMessage(playerid, White, "/GiveWeapon(Gw) [playerid] [weapon] [ammu] :öåøú ùéîåù");
		ammu = strval(tmp2);
		if(ammu < 1 || ammu > 9999) return SendClientMessage(playerid, White, "èååç ëîåú äëãåøéí ìðù÷ äåà áéï 1 ì9999");
 		format(string, sizeof(string), "{FFFFFF}%d{33AA33} òí îñôø ëãåøéí {FFFFFF}%s{33AA33} ðúï ìê àú äðù÷ {FFFFFF}%s{33AA33} äîðäì", ammu, WeaponNames[weap],GetName(playerid));
	 	SendClientMessage(id, Green ,string);
		format(string, sizeof(string), "{FFFFFF}%d{33AA33} òí îñôø ëãåøéí {FFFFFF}%s{33AA33} àú äðù÷ {FFFFFF}%s{33AA33} ðúú ìùç÷ï", ammu,WeaponNames[weap],GetName(id));
	 	SendClientMessage(playerid, Green ,string);
		GivePlayerWeapon(id, weap, ammu);
		return 1;
	}
