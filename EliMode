				ShowPlayerDialog(playerid, 1_7, DIALOG_STYLE_LIST, "îòøëú áð÷ ÷ìàï", "îùéëú ñëåí îñåééí\nîùéëú ëì äëñó ùéù ìê ááð÷", "áçø", "çæåø");
			}
			format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
