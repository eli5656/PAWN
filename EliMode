		while ((idx < length) && (cmdtext[idx] <= ' '))
		{
			idx++;
		}
		new offset = idx;
		new result[64];
		while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
		{
			result[idx - offset] = cmdtext[idx];
			idx++;
		}
		result[idx - offset] = EOS;
		if(!strlen(result)) return SendClientMessage(playerid, White, "/Ban(B) [playerid] [reason] :öåøú ùéîåù");
	 	format(string, sizeof(string), "{FFFFFF}%s{33AA33} ðçñí òì éãé äîðäì {FFFFFF}%s{33AA33} äùç÷ï", GetName(playerid), GetName(id));
