//------------------------------------------------------------------------------

public ExclusiveBOTip()
{
	switch(random(13))
	{
		case 0: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}äéãòú? ìøåá äô÷åãåú áùøú éù ÷éöåø! çôùå áñåâøééí åàåúéåú âãåìåú");
		case 1: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}éù îâååï àôùøåéåú ìäøååéç ëñó ÷ì áùøú, úðñå ìùãåã áð÷ àå ììëú ìñôéðú äëñó áìàñ åðèåøàñ ëãé ìäøååéç ëñó ÷ì");
		case 2: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}åäöååú éùîç ìòæåø /Report | /Helpme ðú÷ìúí ááòéä äùúîùå áô÷åãåú");
		case 3: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}ùáî÷ìãú \"F\" ä÷ìé÷å òì î÷ù Pvp ëãé ìöàú îàæåøé !");
		case 4: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}/Check ëãé ìáãå÷ îùúîùéí àí äí ðîöàéí á\"îöáéí\" ùåðéí äùúîùå áô÷åãä");
		case 5: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}/Help àçìä ô÷åãä ëãàé ùúùîùå áä îéãé ôòí :-)");
		case 6: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}/ChangePassword(Cp) ëãàé îéãé ôòí ìùðåú ñéñîà ìîùúîù ùìëí áòæøú äô÷åãä");
		case 7: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}\"éù ìëí ÷ìàï åäéðëí îòåðééðéí áîô÷ãä?! äñúëìå áôåøåí \"ãéåðé ÷ìàðéí");
		case 9: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}\"îöàúí áàâ? úãååçå ìðå áôåøåí \"îùåá åúîéëä");
		case 10: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}www.Fxp.Co.il òåã ìà ðøùîúí ìôåøåí? äëúåáú ùìðå äéà");
		case 11: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}àì úáäìå!!! äëñó îäôòéìåéåú äàåèåîèéåú äåìê éùø ìáð÷");
		case 12: OL_SendClientMessageToAll(Gold, "FxpDM_BOTip: {FFFFFF}81.218.109.61:7778 - äàéôé ùì äèéîñôé÷ 3 ùì ä÷äéìä äéðå");
	}
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerClickPlayer(playerid, clickedplayerid, source)
{
	pClicked[playerid] = clickedplayerid;
	if(IsYou(pClicked[playerid], playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ôòåìä æå òì òöîê");
    ShowPlayerDialog(playerid, 18_0, DIALOG_STYLE_LIST, "TAB îòøëú", "ùìéçú äåãòä ôøèéú\nùìéçú ëñó\nãéååç òì ùç÷ï", "áçø", "áéèåì");
    return 1;
}

//------------------------------------------------------------------------------

public StartAutomaticActivity()
{
	if(Minigun_ACT_Start == 1 && Minigun_ACT_Running == 0 && Minigun_ACT_On == 0) Minigun_End(2);
	else if(Minigun_ACT_Start == 0 && Minigun_ACT_Running == 1 && Minigun_ACT_On == 0) Minigun_End(2);
	else if(Minigun_ACT_Start == 0 && Minigun_ACT_Running == 0 && Minigun_ACT_On == 1) Minigun_End(2);
	
	if(RandomWeapon_ACT_Start == 1 && RandomWeapon_ACT_Running == 0 && RandomWeapon_ACT_On == 0) RandomWeapon_End(2);
	else if(RandomWeapon_ACT_Start == 0 && RandomWeapon_ACT_Running == 1 && RandomWeapon_ACT_On == 0) RandomWeapon_End(2);
	else if(RandomWeapon_ACT_Start == 0 && RandomWeapon_ACT_Running == 0 && RandomWeapon_ACT_On == 1) RandomWeapon_End(2);
	
	if(Monster_ACT_Start == 1 && Monster_ACT_Running == 0 && Monster_ACT_On == 0) Monster_End(2);
	else if(Monster_ACT_Start == 0 && Monster_ACT_Running == 1 && Monster_ACT_On == 0) Monster_End(2);
	else if(Monster_ACT_Start == 0 && Monster_ACT_Running == 0 && Monster_ACT_On == 1) Monster_End(2);
	
	if(Karting_ACT_Start == 1 && Karting_ACT_Running == 0 && Karting_ACT_On == 0) Karting_End(2);
	else if(Karting_ACT_Start == 0 && Karting_ACT_Running == 1 && Karting_ACT_On == 0) Karting_End(2);
	else if(Karting_ACT_Start == 0 && Karting_ACT_Running == 0 && Karting_ACT_On == 1) Karting_End(2);

	if(Drift_ACT_Start == 1 && Drift_ACT_Running == 0 && Drift_ACT_On == 0) Drift_End(2);
	else if(Drift_ACT_Start == 0 && Drift_ACT_Running == 1 && Drift_ACT_On == 0) Drift_End(2);
	else if(Drift_ACT_Start == 0 && Drift_ACT_Running == 0 && Drift_ACT_On == 1) Drift_End(2);

	if(SawnoffKing_ACT_Start == 1 && SawnoffKing_ACT_Running == 0 && SawnoffKing_ACT_On == 0) SawnoffKing_End(2);
	else if(SawnoffKing_ACT_Start == 0 && SawnoffKing_ACT_Running == 1 && SawnoffKing_ACT_On == 0) SawnoffKing_End(2);
	else if(SawnoffKing_ACT_Start == 0 && SawnoffKing_ACT_Running == 0 && SawnoffKing_ACT_On == 1) SawnoffKing_End(2);

	switch(random(5))
	{
		case 0:
		{
			Minigun_ACT_Timer_Start = SetTimer("Minigun_Start", 1000*1, 1);
   			Minigun_ACT_Start = 1;
			AutomaticACT_AlreadyON = 1;
		}
		case 1:
		{
			RandomWeapon_ACT_Timer_Start = SetTimer("RandomWeapon_Start", 1000*1, 1);
            RandomWeapon_ACT_Start = 1;
			AutomaticACT_AlreadyON = 1;
		}
		case 2:
		{
			Monster_ACT_Timer_Start = SetTimer("Monster_Start", 1000*1, 1);
 			Monster_ACT_Start = 1;
			AutomaticACT_AlreadyON = 1;
		}
		case 3:
		{
			Karting_ACT_Timer_Start = SetTimer("Karting_Start", 1000*1, 1);
 			Karting_ACT_Start = 1;
			AutomaticACT_AlreadyON = 1;
		}

		case 4:
		{
			SawnoffKing_ACT_Timer_Start = SetTimer("SawnoffKing_Start", 1000*1, 1);
 			SawnoffKing_ACT_Start = 1;
			AutomaticACT_AlreadyON = 1;
		}
		case 5:
		{
			Drift_ACT_Timer_Start = SetTimer("Drift_Start", 1000*1, 1);
 			Drift_ACT_Start = 1;
			AutomaticACT_AlreadyON = 1;
		}
	}
}

