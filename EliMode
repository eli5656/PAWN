		 	SendClientMessage(playerid, Green ,string);
		 	AddVehicleComponent(GetPlayerVehicleID(id), 1010);
		}
		else if(!strlen(tmp))
	    {
			if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, White, "àúä ìà ðîöà áøëá");
			SendClientMessage(playerid, Green, "äåñôú ðéèøå ìøëáê");
