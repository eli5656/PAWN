			else
			{
				format(string, sizeof(string), "[Bank Charge Sent] %s[#%d] to %s[#%d]: %s$", GetName(playerid), playerid, GetName(pClicked[playerid]), pClicked[playerid], GetNum(any));
				SendClientMessage(playerid, Orange, string);
				format(string, sizeof(string), "[Bank Charge Received] %s[#%d] to %s[#%d]: %s$", GetName(playerid), playerid, GetName(pClicked[playerid]), pClicked[playerid], GetNum(any));
				SendClientMessage(pClicked[playerid], Yellow, string);
