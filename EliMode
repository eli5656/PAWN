  		format(string, sizeof(string), "({FFFFFF}%s{33AA33})", (result));
  		OL_SendClientMessageToAll(Green, string);
 		DOF2_SetInt(File(id), "Ban", DOF2_GetInt(File(id), "Ban")+1);
 		getdate(Year, Month, Day);
 		gettime(Hour, Minute, Second);
		DOF2_SetInt(File(id), "HaveBan", 1);
		DOF2_SetString(File(id), "ReasonBan", (result));
		DOF2_SetString(File(id), "AdminBan", GetName(playerid));
		DOF2_SetString(File(playerid), "SupporterBan", "None");
		DOF2_SetInt(File(id), "YearBan", Year);
		DOF2_SetInt(File(id), "MonthBan", Month);
		DOF2_SetInt(File(id), "DayBan", Day);
		DOF2_SetInt(File(id), "HourBan", Hour);
		DOF2_SetInt(File(id), "MinuteBan", Minute);
		DOF2_SetInt(File(id), "SecondBan", Second);
		DOF2_SetInt("/Setting.ini", "InBans", DOF2_GetInt("/BansIP.ini", "InBans")+1);
		DOF2_SetInt("/BansIP.ini", GetIP(id), 1);
  		disreason[id] = 1;
  		kixkx[id] = SetTimerEx("kickx", 1000, 1, "d", id);
 		//---------------------------------------------------------------- [LOG]
		format(string, sizeof(string), "%s gave ban to player %s (reason: %s) | Time: %d:%d:%d - Date: %d/%d/%d", GetName(playerid), GetName(id), (result), Hour, Minute, Second, Day, Month, Year);
  		AdminCommands_Logs(string);
		//----------------------------------------------------------------------
 		return 1;
 	}
