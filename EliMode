 			if(GetAdminLevel(i) >= 2 || GetTempAdminLevel(i) >= 2 || GetSpyAdminLevel(i) >= 2)
 			{
				new lang1text[20];
				if(DOF2_GetInt(File(playerid), "AdminLevel") == 2) { lang1text = "îðäì áðéñéåï"; }
				else if(DOF2_GetInt(File(playerid), "AdminLevel") == 3) { lang1text = "îðäì"; }
				else if(DOF2_GetInt(File(playerid), "AdminLevel") == 4) { lang1text = "îðäì áëéø"; }
				else if(DOF2_GetInt(File(playerid), "AdminLevel") == 5) { lang1text = "àçøàé ÷äéìä"; } 
		 		format(string, sizeof(string), "îëéååï ùäéðê {FFFFFF}%s{33AA33} áùøú, äö'àè ìà ðå÷ä òáåøê ìöåøê èéôåì áâåøîéí äáòééúééí", lang1text);
