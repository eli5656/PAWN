public RobberyMission_Countdown(cd)
{
	new string[256];
	format(string, sizeof(string), "~r~Must more ~w~%d ~r~seconds", cd);
	GameTextForPlayer(RobberyMission_Player, string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		RobberyMission_Timer_1 = SetTimerEx("RobberyMission_Countdown", 1000, 0, "%d", cd);
	}
	else
	{
		KillTimer(RobberyMission_Timer_1);
	}
	return 1;
}

public RobberyMission_End(playerid)
{
	new string[256];
	RobberyMission_In[playerid] = 0;
	format(string, sizeof(string), "• òáøú àú îùéîú ùãéãú äáð÷\n%s$ äøååçú -", GetNum(RobberyMission_RandomPrice));
	ShowPlayerDialog(playerid, 20_2, DIALOG_STYLE_MSGBOX, "îùéîú ùãéãú äáð÷", string, "áéèåì", "");
	DestroyVehicle(RobberyMission_Car);
	KillTimer(RobberyMission_Timer_1);
	KillTimer(RobberyMission_Timer_2);
	format(string, sizeof(string), "{FFFFFF}[Robbery Mission]{AA3333} ñééí àú îùéîú ùãéãú äëñó áäöìçä {FFFFFF}%s{AA3333} äùç÷ï", GetName(RobberyMission_Player));
	OL_SendClientMessageToAllEx(playerid, Red, string);
	AntiCheat_GivePlayerMoney(playerid, true, RobberyMission_RandomPrice);
	return 1;
}

public RobberyMission_Toggle()
{
	RobberyMission_Start = 0;
	OL_SendClientMessageToAll(Red, "{FFFFFF}[Robbery Mission]{AA3333} òáøå 15 ã÷åú îàæ ùãéãú äáð÷ äàçøåðä - ðéúï ìùãåã àú äáð÷ ùåá");
	KillTimer(RobberyMission_Switch);
	return 1;
}

