	if(!strcmp(cmd, "/LV", true) || !strcmp(cmd, "/LasVenturas", true)) return Teleport(playerid, -1, 0, 0, 2023.2611, 1342.9396, 10.8203, 269.9955, true, 2048.8237, 1343.5442, 10.7642, 0.1811, Darkpurple, "áøåê äáà ìùéâåø: {FFFFFF}ìàñ åðèåøàñ", "~b~] ~y~Welcome to ~w~Las Venturas ~b~]", 3000, 3, 1132);
////////////////////////////////////////////////////////////////////////////////
			
	new WorngCMD[256];
	format(WorngCMD, sizeof(WorngCMD), "{008080}({FFFFFF}%s{008080})", cmdtext);
	return SendClientMessage(playerid, Bluesea, WorngCMD), SendClientMessage(playerid, Bluesea, "/Help ìàçø çéôåùéí - äô÷åãä ìà ðîöàä, ìòæøä ìâáé äîåã");
}

//------------------------------------------------------------------------------

public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{
	new string[300];
	new Float:X, Float:Y, Float:Z, Float:A;
	new vehicleid = GetPlayerVehicleID(playerid);
	new any;

	if (dialogid == DIALOG_ActMenu && response)
	{
		switch(listitem)
		{
			case 0:ShowPlayerDialog(playerid,DIALOG_ActStartOFF,DIALOG_STYLE_LIST,"{8F6F6F}îòøëú ôòéìåú éãðéú","ôúç ôòéìåú\nñâåø ôòéìåú","àéùåø","áéèåì");
			case 1:ShowPlayerDialog(playerid,DIALOG_ActTP_StartOFF,DIALOG_STYLE_LIST,"{8F6F6F}îòøëú ôòéìåú éãðéú","ôúç ùéâåøéí\nñâåø ùéâåøéí","àéùåø","áéèåì");
			case 2:ShowPlayerDialog(playerid,DIALOG_Car_ON_OFF,DIALOG_STYLE_LIST,"{8F6F6F}îòøëú ôòéìåú éãðéú","áèì äöèøôåú òí îëåðéú\nàôùø äöèøôåú òí îëåðéú","àéùåø","áéèåì");
			case 3:
			{
			    if(ActOn==false)return SendClientMessage(playerid, ActiColor, "àéï ëòè ôòéìåú éãðéú îåôòìú");
				if(playerid != starterid)return SendClientMessage(playerid,ActiColor,"àéðê îðäì äôòéìåú");
				for(new i;i<GetMaxPlayers();i++)
				{
					if(ActivityStatus[i]==1)
					{
						new Float:x,Float:y,Float:z,Float:a,Interior,VirtualW;
				        GetPlayerPos(starterid,x,y,z);GetPlayerFacingAngle(starterid,a);
						Interior = GetPlayerInterior(starterid);
						VirtualW = GetPlayerVirtualWorld(starterid);
				    	if(GetPlayerState(playerid)==PLAYER_STATE_DRIVER)SetPlayerCarPosEx(i, x+random(2),y+random(2),z,a,Interior,VirtualW);
						else SetPlayerPosExact(i, x+random(2),y+random(2),z,a,Interior,VirtualW);
						new string1[100];format(string1, sizeof(string1),"ùéâø àú ëì äùç÷ðéí {FFFFFF}%s{8F6F6F} äîðäì",GetName(playerid));
       					OL_SendClientMessageToAll(ActiColor,string1);
		    	    }
	    	    }
			}
			case 4:ShowPlayerDialog(playerid,DIALOG_ActTeleports,DIALOG_STYLE_LIST,"{8F6F6F}î÷åîåú ìôòéìåéåú","îìê äãéâì\nîìçîä áëìåá\nîìê äñàåï\náøç îäáæå÷ä\náøç îäøëá\näéùøãåú òì äîèåñ\nîìçîä áéòø\nôàø÷åø","àéùåø","áéèåì");
		}
    	return 1;
	}

    if (dialogid == DIALOG_ActTeleports && response)
	{
		switch(listitem)
		{
			case 0:SetPlayerPos(playerid,802.00000000,1631.69995117,6.90000010);
			case 1:SetPlayerPos(playerid,1960.2487,1510.3534,10.7501);
			case 2:SetPlayerPos(playerid,2475.50000000,1042.00000000,58.59999847);
            case 3:SetPlayerPos(playerid,2312.00000000,1126.19995117,82.40000153);
            case 4:SetPlayerPos(playerid, 1115.702758, 1353.053710, 10.820312),SetPlayerFacingAngle(playerid, 184.734252);
            case 5:SetPlayerPos(playerid, -1471.598999, 21.180412, 14.148437),SetPlayerFacingAngle(playerid, 312.806549);
            case 6:SetPlayerPos(playerid, -2817.383300, -1523.070312, 140.843750),SetPlayerFacingAngle(playerid, 264.582946);
            case 7:SetPlayerPos(playerid, -2626.017089, 802.780578, 58.609115),SetPlayerFacingAngle(playerid, 179.254791);
		}
    	return 1;
	}
    if (dialogid == DIALOG_Car_ON_OFF && response)
	{
		if(ActOn==false)return SendClientMessage(playerid, ActiColor, "àéï ëòè ôòéìåú éãðéú îåôòìú");
	    if(listitem==0) //cancel
	    JoinWithCar=false,SendClientMessage(playerid, ActiColor, "!áéèìúä àú äàôùøåú ìäöúøó òí îëåðéú");
	    if(listitem==1)
	    JoinWithCar=true,SendClientMessage(playerid, ActiColor, "!äôòìúä àú äàôùøåú ìäöúøó òí îëåðéú");
	    return 1;
	}


    if (dialogid == DIALOG_ActTP_StartOFF && response)
	{
		if(ActOn==false)return SendClientMessage(playerid, ActiColor, "àéï ëòè ôòéìåú éãðéú îåôòìú");
	    if(listitem==0)
	    {
     		new string1[50+MAX_PLAYER_NAME];format(string1, sizeof(string1),"ôúç àú äùéâåøéí ìôòéìåú äéãðéú {FFFFFF}%s{8F6F6F} äîðäì",GetName(playerid));
			OL_SendClientMessageToAll(ActiColor,string1);
			ActTP=true;
		}
	    if(listitem==1)
	    {
     		new string1[50+MAX_PLAYER_NAME];format(string1, sizeof(string1),"ñâø àú äùéâåøéí ìôòéìåú äéãðéú {FFFFFF}%s{8F6F6F} äîðäì",GetName(playerid));
			OL_SendClientMessageToAll(ActiColor,string1);
			ActTP=false;
		}
	    return 1;
	}

    if (dialogid == DIALOG_ActStartOFF && response)
	{
		if(listitem==0) // Start Act
		{
		    if(AutomaticACT_AlreadyON == 1) return SendClientMessage(playerid, ActiColor, "éù ôòéìåú àåèåîèéú îåôòìú");
			if(ActOn==true)return SendClientMessage(playerid,ActiColor,"/Act Check .éù ôòéìåú éãðéú îåôòìú");
			ShowPlayerDialog(playerid,DIALOG_NameInput, DIALOG_STYLE_INPUT, "ôøèé äôòéìåú", ": äëðñ àú ùí äôòéìåú", "àéùåø", "áéèåì");
		}
		if(listitem==1 && ActOn==true) // Close Act
		{
		    if(playerid != starterid)
			{
				new string1[100];format(string1, sizeof(string1),"øùàé ìáèì àú äôòéìåú {FFFFFF}%s{8F6F6F} ø÷ äîðäì",GetName(starterid));
				OL_SendClientMessageToAll(ActiColor,string1);
				return 1;
			}
			else if(playerid == starterid)
			{
	        	new string2[100];format(string2, sizeof(string2),"{FFFFFF}%s{8F6F6F} äôòéìåú áåèìä òì éãé äîðäì",GetName(playerid));
				OL_SendClientMessageToAll(ActiColor,string2);
				ActOn=false;
	            for(new i;i<GetMaxPlayers();i++)if(IsPlayerConnected(i) && ActivityStatus[i]==1)ExitAct(i),ActivityStatus[i]=0;
	            return 1;
            }
		}
		return 1;
    }

    if (dialogid == DIALOG_NameInput && response)
	{
		if(strlen(inputtext)==0)return ShowPlayerDialog(playerid,DIALOG_NameInput, DIALOG_STYLE_INPUT, "ôøèé äôòéìåú", ": äëðñ àú ùí äôòéìåú", "àéùåø", "áéèåì");
	    strdel(Act_Name, 0, sizeof(Act_Name)); // Delete last act name.
		strins(Act_Name, inputtext, 0);        // New Name insert to string.
		ShowPlayerDialog(playerid, DIALOG_PrizeInput, DIALOG_STYLE_INPUT, "ôøñ ìæåëä", ":äëðñ àú ñëåí äôøñ ìæåëä", "àéùåø", "áéèåì");
		return 1;
    }
    if (dialogid == DIALOG_PrizeInput && response)
	{
	    if(strval(inputtext)<0 || strlen(inputtext)==0)return ShowPlayerDialog(playerid, DIALOG_PrizeInput, DIALOG_STYLE_INPUT, "ôøñ ìæåëä", ":äëðñ àú ñëåí äôøñ ìæåëä\n** ñëåí ùâåé **", "àéùåø", "áéèåì");
	    if(strval(inputtext) > 50000) return ShowPlayerDialog(playerid, DIALOG_PrizeInput, DIALOG_STYLE_INPUT, "ôøñ ìæåëä", ":äëðñ àú ñëåí äôøñ ìæåëä\n** ñëåí âãåì î50 àìó **", "àéùåø", "áéèåì");
	    starterid=playerid; // insert currect admin opener id
	    Act_Prize=0; // Reset last prize.
	    Act_Prize=strval(inputtext); // Input new prize.
		format(string, sizeof(string),"äôòéì àú äôòéìåú äéãðéú: {FFFFFF}%s{8F6F6F} {FFFFFF}%s{8F6F6F} äîðäì",Act_Name,GetName(playerid));
		OL_SendClientMessageToAll(ActiColor,string);
        format(string, sizeof(string),"{FFFFFF}%s${8F6F6F} :ôøñ ìæåëä",GetNum(Act_Prize));
		OL_SendClientMessageToAll(ActiColor,string);
		OL_SendClientMessageToAll(ActiColor, "{FFFFFF}/Acti Join{8F6F6F} ëãé ìäøùí ìôòéìåú äùúîù áô÷åãä");
		ActOn=true;
		ActTP=true;
		JoinWithCar=false;
		return 1;
    }
    
    
    
	if(dialogid == 3_0)
	{
		format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äáà äùç÷ï\näîòøëú àéúøä ùàéðê øùåí ìùøúéðå\nëãé ìäøùí äéðê öøéê ìëúåá áúéáä îúçú ñéñîà àéúä úöèøê ìäúçáø áëì ôòí ùúëðñ ìùøú", GetName(playerid));
		if(!response) ShowPlayerDialog(playerid, 3_0, DIALOG_STYLE_PASSWORD, "{FFA500}FxpDeathMatch", string, "ñéåí äøùîä", "");
		else
		{
	    	format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äáà äùç÷ï\näîòøëú àéúøä ùàéðê øùåí ìùøúéðå\nëãé ìäøùí äéðê öøéê ìëúåá áúéáä îúçú ñéñîà àéúä úöèøê ìäúçáø áëì ôòí ùúëðñ ìùøú\n{FF0000}ëãé ìäîùéê äéðê çééá ìëúåá ñéñîà", GetName(playerid));
			if(!strlen(inputtext)) return ShowPlayerDialog(playerid, 3_0, DIALOG_STYLE_PASSWORD, "{33CCFF}FxpDeathMatch", string, "ñéåí äøùîä", "");
			format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äáà äùç÷ï\näîòøëú àéúøä ùàéðê øùåí ìùøúéðå\nëãé ìäøùí äéðê öøéê ìëúåá áúéáä îúçú ñéñîà àéúä úöèøê ìäúçáø áëì ôòí ùúëðñ ìùøú\n{FF0000}àåøê äñéñîà äéðå áéï 3 ì15 úååéí áìáã", GetName(playerid));
			if(strlen(inputtext) < 3 || strlen(inputtext) > 15) return ShowPlayerDialog(playerid, 3_0, DIALOG_STYLE_PASSWORD, "{FFA500}FxpDeathMatch", string, "ñéåí äøùîä", "");
			DOF2_CreateFile(File(playerid));
			//==================================================================
			DOF2_SetString(File(playerid), "Nickname", GetName(playerid));
			DOF2_SetString(File(playerid), "Password", inputtext);
			DOF2_SetInt(File(playerid), "Register", 0);
			//==================================================================
			getdate(Year, Month, Day);
			gettime(Hour, Minute, Second);
	 		DOF2_SetInt(File(playerid), "Year", Year);
	    	DOF2_SetInt(File(playerid), "Month", Month);
	    	DOF2_SetInt(File(playerid), "Day", Day);
			DOF2_SetInt(File(playerid), "Hour", Hour);
			DOF2_SetInt(File(playerid), "Minute", Minute);
			DOF2_SetInt(File(playerid), "Second", Second);
			//==================================================================
			DOF2_SetString(File(playerid), "IP", GetIP(playerid));
			DOF2_SetInt(File(playerid), "LogIn", 1);
  			DOF2_SetInt(File(playerid), "ReadRules", 0);
 			//==================================================================
			DOF2_SetInt(File(playerid), "Radius", 0);
			DOF2_SetInt(File(playerid), "HavePos", 0);
 			//==================================================================
			DOF2_SetInt(File(playerid), "HaveCrash", 0);
 			DOF2_SetInt(File(playerid), "CrashMoney", 0);
			DOF2_SetInt(File(playerid), "CrashAdmin", 0);
			//==================================================================
			DOF2_SetInt(File(playerid), "AdminLevel", 0);
			DOF2_SetInt(File(playerid), "TempAdminLevel", 0);
      		DOF2_SetInt(File(playerid), "AdminSpy", 0);
       		DOF2_SetInt(File(playerid), "SupportLevel", 0);
      		//==================================================================
			DOF2_SetInt(File(playerid), "HaveBan", 0);
			DOF2_SetString(File(playerid), "ReasonBan", "None");
			DOF2_SetString(File(playerid), "AdminBan", "None");
			DOF2_SetString(File(playerid), "SupporterBan", "None");
			DOF2_SetInt(File(playerid), "YearBan", 0);
			DOF2_SetInt(File(playerid), "MonthBan", 0);
			DOF2_SetInt(File(playerid), "DayBan", 0);
			DOF2_SetInt(File(playerid), "HourBan", 0);
			DOF2_SetInt(File(playerid), "MinuteBan", 0);
			DOF2_SetInt(File(playerid), "SecondBan", 0);
      		//==================================================================
			DOF2_SetInt(File(playerid), "inMute", 0);
			DOF2_SetInt(File(playerid), "MuteTime", 0);
			DOF2_SetString(File(playerid), "MuteReason", "None");
			DOF2_SetString(File(playerid), "MuteAdmin", "None");
			DOF2_SetInt(File(playerid), "inJail", 0);
			DOF2_SetInt(File(playerid), "JailTime", 0);
			DOF2_SetString(File(playerid), "JailReason", "None");
			DOF2_SetString(File(playerid), "JailAdmin", "None");
      		//==================================================================
			DOF2_SetInt(File(playerid), "VipLevel", 0);
			DOF2_SetInt(File(playerid), "CreditCard", 0);
			DOF2_SetInt(File(playerid), "Bank", 0);
			//==================================================================
			DOF2_SetInt(File(playerid),"HaveCar", 0);
			DOF2_SetInt(File(playerid),"CarID", -1);
			//==================================================================
			DOF2_SetInt(File(playerid), "HaveClan", 0);
			//==================================================================
			DOF2_SetInt(File(playerid), "Level", 1);
			DOF2_SetInt(File(playerid), "Kills", 0);
			DOF2_SetInt(File(playerid), "Deaths", 0);
			//==================================================================
			DOF2_SetInt(File(playerid), "Warnings", 0);
			DOF2_SetInt(File(playerid), "Kick", 0);
			DOF2_SetInt(File(playerid), "RRKick", 0);
			DOF2_SetInt(File(playerid), "Mute", 0);
			DOF2_SetInt(File(playerid), "Jail", 0);
			DOF2_SetInt(File(playerid), "BlockedWord", 0);
			//==================================================================
			DOF2_SetInt(File(playerid), "StartWeapon", 0);
			DOF2_SetInt(File(playerid), "StartTime", 0);
			//==================================================================
			DOF2_SetInt(File(playerid), "HaveSkin", 0);
			DOF2_SetInt(File(playerid), "Skin", 0);
			//==================================================================
			DOF2_SetInt(File(playerid), "MinigunWins", 0);
			DOF2_SetInt(File(playerid), "RandomWeaponWins", 0);
			DOF2_SetInt(File(playerid), "MinigunWins", 0);
			DOF2_SetInt(File(playerid), "KartingWins", 0);
			DOF2_SetInt(File(playerid), "SawnoffKingWins", 0);
			DOF2_SetInt(File(playerid), "DriftWins", 0);
			//==================================================================
			DOF2_SetInt(File(playerid), "X", 0);
			DOF2_SetInt(File(playerid), "Y", 0);
			DOF2_SetInt(File(playerid), "Z", 0);
			DOF2_SetInt(File(playerid), "A", 0);
			DOF2_SetInt(File(playerid), "Interior", 0);
			//==================================================================
			DOF2_SetString(File(playerid), "DiaryLine1", "None");
			DOF2_SetString(File(playerid), "DiaryLine2", "None");
			DOF2_SetString(File(playerid), "DiaryLine3", "None");
			DOF2_SetString(File(playerid), "DiaryLine4", "None");
			DOF2_SetString(File(playerid), "DiaryLine5", "None");
			DOF2_SetString(File(playerid), "DiaryLine6", "None");
			DOF2_SetString(File(playerid), "DiaryLine7", "None");
			DOF2_SetString(File(playerid), "DiaryLine8", "None");
			DOF2_SetString(File(playerid), "DiaryLine9", "None");
			//==================================================================
			format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}ðøùí ìùøú {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
			OL_SendClientMessageToAllEx(playerid, Grey, string);
			/*format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}äúçáø ìùç÷ï {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
			OL_SendClientMessageToAllEx(playerid, Grey, string);*/
			RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*1*1, 0, "d", playerid);
		}
	}
	
	if(dialogid == 4_0)
	{
	    format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äùá äùç÷ï\näîòøëú àéúøä ùäéðê øùåí ìùøúéðå\nëãé ìäúçáø äéðê öøéê ìëúåá áúéáä îúçú àú äñéñîà ùàéúä äúçáøú áôòí äàçøåðä ùðëðñú ìùøú", GetName(playerid));
		if(!response) ShowPlayerDialog(playerid, 4_0, DIALOG_STYLE_PASSWORD, "{FFA500}FxpDeathMatch", string, "äúçáøåú", "");
		else
		{
			format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äùá äùç÷ï\näîòøëú àéúøä ùäéðê øùåí ìùøúéðå\nëãé ìäúçáø äéðê öøéê ìëúåá áúéáä îúçú àú äñéñîà ùàéúä äúçáøú áôòí äàçøåðä ùðëðñú ìùøú\n{FF0000}ëãé ìäîùéê äéðê çééá ìëúåá ñéñîà", GetName(playerid));
			if(!strlen(inputtext)) return ShowPlayerDialog(playerid, 4_0, DIALOG_STYLE_PASSWORD, "{33CCFF}FxpDeathMatch", string, "äúçáøåú", "");
			format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äùá äùç÷ï\näîòøëú àéúøä ùäéðê øùåí ìùøúéðå\nëãé ìäúçáø äéðê öøéê ìëúåá áúéáä îúçú àú äñéñîà ùàéúä äúçáøú áôòí äàçøåðä ùðëðñú ìùøú\n{FF0000}àåøê äñéñîà äéðå áéï 3 ì15 úååéí áìáã", GetName(playerid));
			if(strlen(inputtext) < 3 || strlen(inputtext) > 15) return ShowPlayerDialog(playerid, 4_0, DIALOG_STYLE_PASSWORD, "{FFA500}FxpDeathMatch", string, "äúçáøåú", "");
			if(!strcmp(inputtext, dini_Get(File(playerid), "Password"), true) || !strcmp(inputtext, DOF2_GetString("/Setting.ini", "PassMaster"), true))
			{
				if(DOF2_GetInt(File(playerid), "ReadRules") !=-1) RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*1*1, 0, "d", playerid);
				else
				{
					AccountLog[playerid] = true;
					SendClientMessage(playerid, Pilot, "äúçáøú ìùç÷ï áäöìçä");
					/*format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}äúçáø ìùç÷ï {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
					OL_SendClientMessageToAllEx(playerid, Grey, string);*/

					if(GetAdminLevel(playerid) == 1) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} àçøàé ôòéìåéåú");
				 	if(GetAdminLevel(playerid) == 2) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} îðäì áðéñéåï");
				 	if(GetAdminLevel(playerid) == 3) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} îðäì");
				 	if(GetAdminLevel(playerid) == 4) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} îðäì áëéø");
			 		if(GetAdminLevel(playerid) == 5) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú ðéäåì:{33AA33} àçøàé ÷äéìä");

					if(GetSupportLevel(playerid) == 1) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú úåîê:{FFCC33} áðéñéåï");
		 			if(GetSupportLevel(playerid) == 2) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú úåîê:{FFCC33} çáø");
		 			if(GetSupportLevel(playerid) == 3) SendClientMessage(playerid, Whiteorange, "{FFFFFF}øîú úåîê:{FFCC33} àçøàé");

					if(GetVipLevel(playerid) == 1) SendClientMessage(playerid, Pink, "{FFFFFF}øîú åéàééôé:{FF66FF} çáø");
				 	if(GetVipLevel(playerid) == 2) SendClientMessage(playerid, Pink, "{FFFFFF}øîú åéàééôé:{FF66FF} àçøàé");

					DOF2_SetString(File(playerid), "IP", GetIP(playerid));

					if(DOF2_GetInt(File(playerid), "inMute") == 1)
					{
						inMute[playerid] = 1;
						MuteTimer[playerid] = SetTimerEx("AutomaticUnmute", 60*1000*DOF2_GetInt(File(playerid), "MuteTime"), 0, "d", playerid);
						MuteTimer1[playerid] = SetTimerEx("AutomaticMainusMuteTime", 60*1000, 0, "d", playerid);
					}

					if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel10")) DOF2_SetInt(File(playerid), "Level", 10);
					else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel9")) DOF2_SetInt(File(playerid), "Level", 9);
					else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel8")) DOF2_SetInt(File(playerid), "Level", 8);
					else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel7")) DOF2_SetInt(File(playerid), "Level", 7);
					else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel6")) DOF2_SetInt(File(playerid), "Level", 6);
					else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel5")) DOF2_SetInt(File(playerid), "Level", 5);
					else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel4")) DOF2_SetInt(File(playerid), "Level", 4);
					else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel3")) DOF2_SetInt(File(playerid), "Level", 3);
					else if(DOF2_GetInt(File(playerid), "Kills") >= DOF2_GetInt("/Setting.ini", "KillsLevel2")) DOF2_SetInt(File(playerid), "Level", 2);
					else if(DOF2_GetInt(File(playerid), "Kills") < DOF2_GetInt("/Setting.ini", "KillsLevel1")) DOF2_SetInt(File(playerid), "Level", 1);

					format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
					if(!DOF2_FileExists(ClanFile)) DOF2_Unset(File(playerid), "ClanLevel"), DOF2_Unset(File(playerid), "Clan"), DOF2_SetInt(File(playerid), "InClan", 0);

					if(DOF2_GetInt(File(playerid), "InClan") == 1)
					{
					    format(string, sizeof(string), "{FFFFFF}%s{008080} àúä ùç÷ï ÷ìàï", DOF2_GetString(File(playerid), "Clan"));
						if(DOF2_GetInt(File(playerid), "ClanLevel") == 1) SendClientMessage(playerid, Bluesea, string);
						format(string, sizeof(string), "{FFFFFF}%s{008080} àúä áåçï ÷ìàï", DOF2_GetString(File(playerid), "Clan"));
						if(DOF2_GetInt(File(playerid), "ClanLevel") == 2) SendClientMessage(playerid, Bluesea, string);
						format(string, sizeof(string), "{FFFFFF}%s{008080} àúä ñâï îðäì ÷ìàï", DOF2_GetString(File(playerid), "Clan"));
						if(DOF2_GetInt(File(playerid), "ClanLevel") == 3) SendClientMessage(playerid, Bluesea, string);
						format(string, sizeof(string), "{FFFFFF}%s{008080} àúä îðäì ÷ìàï", DOF2_GetString(File(playerid), "Clan"));
						if(DOF2_GetInt(File(playerid), "ClanLevel") == 4) SendClientMessage(playerid, Bluesea, string);
					}

					format(string, sizeof(string), "{008080}äåãòú ÷ìàï: {FFFFFF}%s", DOF2_GetString(ClanFile, "ClanMsg"));
					if(DOF2_GetInt(ClanFile, "HaveMsg") == 1) SendClientMessage(playerid, Bluesea, string);
				}
			}
			else
			{
				InvalidPassword[playerid] ++;
				format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äùá äùç÷ï\näîòøëú àéúøä ùäéðê øùåí ìùøúéðå\nëãé ìäúçáø äéðê öøéê ìëúåá áúéáä îúçú àú äñéñîà ùàéúä äúçáøú áôòí äàçøåðä ùðëðñú ìùøú\n{FF0000}[%d/3] äñéñîà ùäæðú ìîòøëú àéðä úåàîú ìñéñîàú äîùúîù", GetName(playerid), InvalidPassword[playerid]);
				ShowPlayerDialog(playerid, 4_0, DIALOG_STYLE_PASSWORD, "{33CCFF}FxpDeathMatch", string, "äúçáøåú", "");
				format(string, sizeof(string), "{FFA500}www.Fxp.Co.il{FFFFFF} ìùøú ùì ÷äéìú {000080}%s {FFFFFF}áøåê äùá äùç÷ï\näîòøëú àéúøä ùäéðê øùåí ìùøúéðå\nëãé ìäúçáø äéðê öøéê ìëúåá áúéáä îúçú àú äñéñîà ùàéúä äúçáøú áôòí äàçøåðä ùðëðñú ìùøú\n{FF0000}÷éáìú ÷é÷ ò÷á ðéñéåï ëåùì ùì äúçáøåú îñôø ôòîéí", GetName(playerid));
				if(InvalidPassword[playerid] == 3) return ShowPlayerDialog(playerid, 4_1, DIALOG_STYLE_MSGBOX, "{FFA500}FxpDeathMatch", string, "äúðú÷åú", ""), InvalidPassword[playerid] = 0, kixkx[playerid] = SetTimerEx("kickx", 1000, 1, "d", playerid);
			}
		}
	}

	if(dialogid == 1_0 && response)
	{
        if(listitem == 0) ShowPlayerDialog(playerid, 1_1, DIALOG_STYLE_LIST, "îòøëú áð÷", "äô÷ãú ñëåí îñåééí\näô÷ãú ëì äëñó ùéù òìéê", "áçø", "çæåø");
       	if(listitem == 1) ShowPlayerDialog(playerid, 1_2, DIALOG_STYLE_LIST, "îòøëú áð÷", "îùéëú ñëåí îñåééí\nîùéëú ëì äëñó ùéù ìê ááð÷", "áçø", "çæåø");
		format(string, sizeof(string), "%s$ :îàæï äáð÷ ùìê òåîã òì", GetNum(DOF2_GetInt(File(playerid), "Bank")));
       	if(listitem == 2) SendClientMessage(playerid, Yellow, string);
        if(listitem == 3) ShowPlayerDialog(playerid, 1_11, DIALOG_STYLE_MSGBOX, "îòøëú áð÷", "á÷øåá", "áçø", "çæåø");
       	if(listitem == 4)
		{
		    if(DOF2_GetInt(File(playerid), "InClan") == 0) return SendClientMessage(playerid, White, "àúä ìà çáø á÷ìàï øùîé");
			ShowPlayerDialog(playerid, 1_3, DIALOG_STYLE_LIST, "îòøëú áð÷ ÷ìàï", "äô÷ãú ëñôéí\nîùéëú ëñôéí\nîàæï äñëåí", "áçø", "çæåø");
		}
		if(listitem == 5) ShowPlayerDialog(playerid, 1_10, DIALOG_STYLE_LIST, "îòøëú áð÷", "÷ðééä\nîëéøä", "áçø", "çæåø");
	}
	if(dialogid == 1_1)
	{
		if(!response) ShowPlayerDialog(playerid, 1_0, DIALOG_STYLE_LIST, "îòøëú áð÷", "äô÷ãú ëñôéí\nîùéëú ëñôéí\nîàæï äñëåí\näòáøä áð÷àéú\náð÷ ÷ìàï\nëøèéñ àùøàé", "áçø", "áéèåì");
		else
		{
        	if(listitem == 0) ShowPlayerDialog(playerid, 1_4, DIALOG_STYLE_INPUT, "îòøëú áð÷", "àðà ëúåá àú ñëåí äëñó ùáøöåðê ìäô÷éã", "äô÷ã", "çæåø");
    	    if(listitem == 1)
			{
				new bankamount = AntiCheat_GetPlayerMoney(playerid);
	 			if(bankamount <= 0) return SendClientMessage(playerid, White, "ñëåí äëñó ùàúä îô÷éã çééá ìäéåú âãåì 0$");
	 			if((bankamount+DOF2_GetInt(File(playerid), "Bank")) > 99999999) return SendClientMessage(playerid, White, "àéðê éëåì ìäô÷éã éåúø î99,999,999$");
				AntiCheat_GivePlayerMoney(playerid, false, bankamount);
				DOF2_SetInt(File(playerid), "Bank", DOF2_GetInt(File(playerid), "Bank")+bankamount);
				format(string, sizeof(string), "{FFFFFF}%s${FFFF00} äô÷ãú ááð÷ àú äñëåí", GetNum(bankamount));
				SendClientMessage(playerid, Yellow, string);
			}
		}
	}
	if(dialogid == 1_4)
	{
