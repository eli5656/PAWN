			{
   				if(i != playerid)
				{
					if(IsPlayerNearPlayer(GetRadius(playerid), playerid, i))
					{
						GivePlayerWeapon(i, weap, ammu);
					}
				}
			}
			format(string, sizeof(string), "òí îñôø äëãåøéí {FFFFFF}%d{33AA33} ìùç÷ðéí áøãéåñ ùì {FFFFFF}%d.0{33AA33} öòãéí îîðå {FFFFFF}%s{33AA33} ðúï àú äðù÷ {FFFFFF}%s{33AA33} äîðäì", ammu, GetRadius(playerid), WeaponNames[weap], GetName(playerid));
