		while ((idx < length) && (cmdtext[idx] <= ' '))
		{
			idx++;
		}
		new offset = idx;
		new result[64];
		while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
		{
			result[idx - offset] = cmdtext[idx];
			idx++;
		}
		result[idx - offset] = EOS;
		if(!strlen(result)) return SendClientMessage(playerid, White, "/Report(Rep) [playerid] [reason] :öåøú ùéîåù");
		SendClientMessage(playerid, Green, "úåãä øáä, äãéååç ðùìç áäöìçä");
		format(string, sizeof(string), "**Reporter (%s): %s äãéååç ðùìç òì äùç÷ï", GetName(playerid), GetName(id));
		SendAdminsMessage(Orange, string);
