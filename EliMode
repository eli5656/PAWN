       	if(listitem == 1)
		{
	  		for(new i=0; i<MAX_ATMS; i++)
			{
				format(AtmFile, sizeof(AtmFile), "ATMs/%d.ini", i);
	   			if(DOF2_FileExists(AtmFile) && PlayerToPoint(1, playerid, DOF2_GetFloat(AtmFile, "X"), DOF2_GetFloat(AtmFile, "Y"), DOF2_GetFloat(AtmFile, "Z")))
		   		{
					DOF2_RemoveFile(AtmFile);
					Delete3DTextLabel(AtmText[i]);
					DestroyPickup(AtmPickup[i]);
				 	new Setting[256];
					format(Setting, sizeof(Setting), "ATMs/Setting.ini");
					new AtmID = DOF2_GetInt(Setting, "ATMs")-1;
					DOF2_SetInt(Setting, "ATMs", AtmID);
					SendClientMessage(playerid, Yellow, "! îç÷ú ëñôåîè æä áäöìçä");
				}
			}
		}
        if(listitem == 2) ShowPlayerDialog(playerid, 10_1, DIALOG_STYLE_INPUT, "îòøëú áð÷ - ôàðì ëñôåîè", "àðà äëðñ àú äàéãé ùì äëñôåîè\nòì îðú ìäæéæ àåúå ìîé÷åîê äðåëçé", "áçø", "çæåø");
	}
	if(dialogid == 10_1)
	{
		if(!response) ShowPlayerDialog(playerid, 10_0, DIALOG_STYLE_LIST, "îòøëú áð÷ - ôàðì ëñôåîè", "éöéøú òîãú ëñôåîè çãùä\nîçé÷ú òîãú ëñôåîè ÷ééîú\näææú îé÷åí ùì òîãú ëñôåîè ÷ééîú", "áçø", "áéèåì");
		else
		{
			if(!IsNumeric(inputtext)) return ShowPlayerDialog(playerid, 10_1, DIALOG_STYLE_INPUT, "îòøëú áð÷ - ôàðì ëñôåîèè", "àðà äëðñ àú äàéãé ùì äëñôåîè\nòì îðú ìäæéæ àåúå ìîé÷åîê äðåëçé\n{FF0000}äàéãé ùäæðú ùâåé", "áçø", "çæåø");
			if(!strlen(inputtext)) return ShowPlayerDialog(playerid, 10_1, DIALOG_STYLE_INPUT, "îòøëú áð÷ - ôàðì ëñôåîè", "àðà äëðñ àú äàéãé ùì äëñôåîè\nòì îðú ìäæéæ àåúå ìîé÷åîê äðåëçé\n{FF0000}àðà äëðñ àéãé áëãé ìäîùéê", "áçø", "çæåø");
