	if(strcmp(cmd, "/RespawnAllVehicles", true) == 0 || strcmp(cmd, "/Rav", true) == 0)
	{
		if(GetAdminLevel(playerid) < 3 && GetTempAdminLevel(playerid) < 3 && GetSpyAdminLevel(playerid) < 3) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì3");
		for(new i=0; i<MAX_PLAYERS; i++)
		{
		    if(IsPlayerInAnyVehicle(i))
		    {
		        DontReturnThisVehicle[GetPlayerVehicleID(i)] = 1;
		    }
		}
		for(new vv=0; vv<MAX_VEHICLES; vv++)
		{
			if(!DontReturnThisVehicle[vv]) SetVehicleToRespawn(vv);
			DontReturnThisVehicle[vv] = 0;
		}
 		format(string, sizeof(string), "ñéãø ëì äøëáéí ùìà äéå áùéîåù {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
    	OL_SendClientMessageToAll(Green, string);
      	return 1;
  	}
////////////////////////////////////////////////////////////////////////////////
  	if(strcmp(cmd, "/RespawnVehicle", true) == 0 || strcmp(cmd, "/Rv", true) == 0)
	{
		if(GetAdminLevel(playerid) < 3 && GetTempAdminLevel(playerid) < 3 && GetSpyAdminLevel(playerid) < 3) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì3");
		if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, White, "àúä ìà ðîöà áøëá");
		if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, White, "àúä ìà äðäâ áøëá");
		SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		SendClientMessage(playerid, Green, "ñéãøú àú äøëá ùäééú áå");
      	return 1;
  	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "//", true) == 0)
	{
		if(GetAdminLevel(playerid) < 2 && GetTempAdminLevel(playerid) < 2 && GetSpyAdminLevel(playerid) < 2) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì2");
		new length = strlen(cmdtext);
		while ((idx < length) && (cmdtext[idx] <= ' '))
		{
			idx++;
		}
		new offset = idx;
		new result[64];
		while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
		{
			result[idx - offset] = cmdtext[idx];
			idx++;
		}
		result[idx - offset] = EOS;
		if(!strlen(result)) return SendClientMessage(playerid, White, "// [text] :öåøú ùéîåù");
		new lang1text[20];
		if(DOF2_GetInt(File(playerid), "AdminLevel") == 2) { lang1text = "îðäì áðéñéåï"; }
		else if(DOF2_GetInt(File(playerid), "AdminLevel") == 3) { lang1text = "îðäì"; }
		else if(DOF2_GetInt(File(playerid), "AdminLevel") == 4) { lang1text = "îðäì áëéø"; }
		else if(DOF2_GetInt(File(playerid), "AdminLevel") == 5) { lang1text = "àçøàé ÷äéìä"; }
 		format(string, sizeof(string), "[{FFFFFF}Admins{33AA33} Chat] | {FFFFFF}%s{33AA33} | {FFFFFF}%s{33AA33}[#{FFFFFF}%d{33AA33}]: %s", lang1text, GetName(playerid), playerid, (result));
		SendAdminsMessage(Green, string);
		//---------------------------------------------------------------- [LOG]
		format(string1, sizeof(string1), "%s wrote to AdminChat: %s | Time: %d:%d:%d - Date: %d/%d/%d", GetName(playerid), (result), Hour, Minute, Second, Day, Month, Year);
		AdminsChat_Logs(string1);
		//----------------------------------------------------------------------
		return 1;
	}
