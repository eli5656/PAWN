						SendClientMessage(playerid, White, string);
					}
				}
			}
			return 1;
 		}
		else if(inHardcore[playerid] == 1)
	    {
			SendClientMessage(playerid, Grellow, ":ùç÷ðé ääàøã÷åø äîçåáøéí");
