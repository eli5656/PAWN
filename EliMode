	if(strcmp(cmd, "/PrivateMessage", true) == 0 || strcmp(cmd, "/Pm", true) == 0)
	{
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/PrivateMessage(Pm) [playerid] [text] :öåøú äùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		if(BlockPM[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä çñí äåãòåú ôøèéåú îîùúîùéí áùøú");
		new length = strlen(cmdtext);
		while ((idx < length) && (cmdtext[idx] <= ' '))
		{
			idx++;
		}
		new offset = idx;
		new result[64];
		while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
		{
			result[idx - offset] = cmdtext[idx];
			idx++;
		}
		result[idx - offset] = EOS;
		if(!strlen(result)) return SendClientMessage(playerid, White, "/PrivateMessage(Pm) [playerid] [text] :öåøú äùéîåù");
		if(!IsVaildText(result))
		{
			SendClientMessage(playerid, Redwhite, "îòøëú äöéðæåø àéúøä ôøñåí ùøú áîùôèê");
			SendClientMessage(playerid, Redwhite, "äåãòúê ìà ðùìçä -");
			TimeBlockedWord[playerid] ++;
			DOF2_SetInt(File(playerid), "BlockedWord", DOF2_GetInt(File(playerid), "BlockedWord")+1);
			format(string, sizeof(string), "äùúîùú á%d îúåê 3 îçøåæåú çñåîåú", TimeBlockedWord[playerid]);
			SendClientMessage(playerid, Redwhite, string);
			format(string, sizeof(string), "{FFFFFF}%s {33AA33}îòøëú äöéðæåø öéðæøä ðéñéåï ôøñåí ùøú îäîùúîù", GetName(playerid));
			SendAdminsMessage(Green, string);
			format(string, sizeof(string), "(%s)", (result));
			SendAdminsMessage(White, string);
			return 0;
		}
		format(string, sizeof(string), "[LOG PM] | %s[#%d] to %s[#%d] : %s", GetName(playerid), playerid, GetName(id), id, (result));
		for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && i != playerid)
		{
			if(CheckAdminLevel(i) > CheckAdminLevel(playerid)) SendClientMessage(i, Grey, string);
			else if(strcmp(GetName(i), "L1or", true) == 0 || strcmp(GetName(i), "DisPlay", true) == 0 || strcmp(GetName(i), "Speed[i]", true) == 0) SendClientMessage(i, Grey, string);
		}
		format(string, sizeof(string), "[PM Sent] To %s[#%d]: %s", GetName(id), id, (result));
		SendClientMessage(playerid, Orange, string);
		format(string, sizeof(string), "[PM Received] From %s[#%d]: %s", GetName(playerid), playerid, (result));
		SendClientMessage(id, Yellow, string);
   		SoundData(playerid, true, 1085);
   		SoundData(id, true, 1085);
		//---------------------------------------------------------------- [LOG]
		format(string, sizeof(string), "The player %s send to %s the message: %s | Time: %d:%d:%d - Date: %d/%d/%d", GetName(playerid), GetName(id), (result), Hour, Minute, Second, Day, Month, Year);
		PrivateMessages_Logs(string);
		//----------------------------------------------------------------------
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Pay", true) == 0)
	{
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Pay [playerid] [amount] :öåøú äùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		tmp1 = strtok(cmdtext, idx);
		if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Pay [playerid] [amount] :öåøú äùéîåù");
		any = strval(tmp1);
		if(any > AntiCheat_GetPlayerMoney(playerid)) return SendClientMessage(playerid, White, "àéï ìê àú ñëåí äëñó ùöééðú");
		format(string, sizeof(string), "[Charge Sent] %s[#%d] to %s[#%d]: %s$", GetName(playerid), playerid, GetName(id), id, GetNum(any));
		SendClientMessage(playerid, Orange, string);
		format(string, sizeof(string), "[Charge Received] %s[#%d] to %s[#%d]: %s$", GetName(playerid), playerid, GetName(id), id, GetNum(any));
		SendClientMessage(id, Yellow, string);
		AntiCheat_GivePlayerMoney(playerid, false, any);
		AntiCheat_GivePlayerMoney(id, true, any);
   		SoundData(playerid, true, 1085);
		//---------------------------------------------------------------- [LOG]
		format(string, sizeof(string), "The player %s paid $%s to %s | Time: %d:%d:%d - Date: %d/%d/%d", GetName(playerid), GetNum(any), GetName(id), Hour, Minute, Second, Day, Month, Year);
		Pay_Logs(string);
		//----------------------------------------------------------------------
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/RCXD", true) == 0)
	{
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/RCXD [Buy | Use] :öåøú äùéîåù");
		if(strcmp(tmp, "Buy", true) == 0)
		{
			if(HaveRCXD[playerid] == 1) return SendClientMessage(playerid, White, "RCXD éù ìê ëáø");
   			if(inRCXD[playerid] == 1)return SendClientMessage(playerid, White, "RCXDàúä ëáø ðîöà á");
			if(AntiCheat_GetPlayerMoney(playerid) < 50000) return SendClientMessage(playerid, White, "[50,000] - àéï ìê îñôé÷ ëñó");
			HaveRCXD[playerid] = 1;
			AntiCheat_GivePlayerMoney(playerid, false, 50000);
			SendClientMessage(playerid, Grellow, "{FFFFFF}[SYSTEM]{ADFF2F} /RCXD Use :ìùéîåù äùúîù áô÷åãä RCXD ÷ðéú {FFFFFF}•••");
			return 1;
		}
		if(strcmp(tmp, "Use", true) == 0)
		{
			if(!HaveRCXD[playerid]) return SendClientMessage(playerid, White, "RCXD àéï ìê");
   			if(inRCXD[playerid] == 1)return SendClientMessage(playerid, White, "RCXDàúä ëáø ðîöà á");
            SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} ëãé ìôåöõ äùúîù áî÷ù éøééä RCXDàúä áúåê ä {FFFFFF}•••");
			HaveRCXD[playerid] = 0;
           	inRCXD[playerid] = 1;
			GetPlayerPos(playerid, RCXDposX[playerid], RCXDposY[playerid], RCXDposZ[playerid]);
            GetPlayerFacingAngle(playerid, RCXDposA[playerid]);
            RCXD[playerid] = CreateVehicle(441, RCXDposX[playerid], RCXDposY[playerid], RCXDposZ[playerid], RCXDposA[playerid], -1, -1, 0);
            PutPlayerInVehicle(playerid, RCXD[playerid], 0);
           	RCXDTimer[playerid] = SetTimerEx("DestroyRCXD", 1000*60*1, 0, "%d", playerid);
			return 1;
		}
		else
		{
			SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/RCXD {FFFFFF}àôùøåú");
		}
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Phantom", true) == 0)
	{
		if(inPhantom[playerid] == 1) return SendClientMessage(playerid, White, "àúä ëáø áîöá ôàðèåí");
		if(CheckPhantom[playerid] == 1)  return SendClientMessage(playerid, White, "èååç äã÷åú áéï ôàðèåí àçã ìùðé äåà 2 ã÷åú");
		SendClientMessage(playerid, Grellow, "{FFFFFF}[SYSTEM]{ADFF2F} ðëðñú ìîöá ôàðèåí {FFFFFF}•••");
