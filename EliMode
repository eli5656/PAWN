    	if(StuntsMission_WasFinish[playerid] == 1) return SendClientMessage(playerid, White, "ðéúï ìòùåú àú îùéîú äñèàðèéí ôòí áëðéñä ìîùç÷");
		if(AntiCheat_GetPlayerMoney(playerid) < 1000) return SendClientMessage(playerid, White, "àéï ìê 1000$ ëãé ìäúçéì àú äîùéîä");
		AntiCheat_GivePlayerMoney(playerid, false, 1000);
		StuntsMission_Start = 1;
		StuntsMission_In[playerid] = 1;
		StuntsMission_RandomPrice = randomex(10000, 20000);
		format(string, sizeof(string), "• äúçìú ëòú àú îùéîú äñèàðèéí\néù ìê ã÷ä ìäâéò ìö'÷ôåééðè ùðîöà òì äââ\náîéãä åúöà îäàåôðåò úôñì àåèåîèéú îäîùéîä\n%s$ áîéãä åúâéò ìö'÷ôåééðè ú÷áì", GetNum(StuntsMission_RandomPrice));
		ShowPlayerDialog(playerid, 21_1, DIALOG_STYLE_MSGBOX, "îùéîú äñèàðèéí", string, "äîùê", "");
		GetPlayerPos(playerid, X, Y, Z);
		GetPlayerFacingAngle(playerid, A);
		StuntsMission_Car = CreateVehicle(522, X, Y, Z, A, -1, -1, 50000);
		PutPlayerInVehicle(playerid, StuntsMission_Car, 0);
		StuntsMission_Player = playerid;
		StuntsMission_Countdown(60);
		StuntsMission_Timer_2 = SetTimerEx("StuntsMission_End", 1000*60*1, 0, "%d", playerid);
		format(string, sizeof(string), "{FFFFFF}[Stunts Mission]{AA3333} îúçéì áãøëå ìéòã ùì îùéîú äñèàðèéí {FFFFFF}%s{AA3333} äùç÷ï", GetName(StuntsMission_Player));
  		OL_SendClientMessageToAllEx(playerid, Red, string);
	   	SetVehicleVirtualWorld(StuntsMission_Car, 11);
		SetVehiclePosEx(playerid, 1967.6538, -1425.2203, 13.2155, 89.6771);
		SetPlayerVirtualWorld(playerid, 11);
	}
 	if(dialogid == 22_0 && response)
	{
		format(string, sizeof(string), "%s îùéîú äãøéôè ëáø ðòùú òì éãé äùç÷ï", GetName(DriftMission_Player));
		if(DriftMission_Start == 1) return SendClientMessage(playerid, White, string);
		if(DriftMission_WasFinish[playerid] == 1) return SendClientMessage(playerid, White, "ðéúï ìòùåú àú îùéîú äãøéôè ôòí áëðéñä ìîùç÷");
		if(AntiCheat_GetPlayerMoney(playerid) < 1000) return SendClientMessage(playerid, White, "àéï ìê 1000$ ëãé ìäúçéì àú äîùéîä");
		AntiCheat_GivePlayerMoney(playerid, false, 1000);
		DriftMission_Start = 1;
		DriftMission_In[playerid] = 1;
		DriftMission_RandomPrice = randomex(10000, 20000);
		format(string, sizeof(string), "• äúçìú ëòú àú îùéîú äãøéôè\néù ìê çöé ã÷ä ìäùìéí àú äîñìåì\náîéãä åúöà îäøëá úôñì àåèåîèéú îäîùéîä\n%s$ áîéãä åúâéò ìö'÷ôåééðè äñéåí ú÷áì", GetNum(DriftMission_RandomPrice));
		ShowPlayerDialog(playerid, 22_1, DIALOG_STYLE_MSGBOX, "îùéîú äãøéôè", string, "äîùê", "");
		GetPlayerPos(playerid, X, Y, Z);
		GetPlayerFacingAngle(playerid, A);
		DriftMission_Car = CreateVehicle(451, X, Y, Z, A, -1, -1, 50000);
		PutPlayerInVehicle(playerid, DriftMission_Car, 0);
		SetPlayerRaceCheckpoint(playerid,2,1415.0321,-2034.9058,52.8209,        0,0,0,15);
		CP_DriftMission[playerid]++;
		DriftMission_Player = playerid;
		DriftMission_Countdown(30);
		DriftMission_Timer_2 = SetTimerEx("DriftMission_End", 1000*30*1, 0, "%d", playerid);
		SetVehicleVirtualWorld(DriftMission_Car, 11);
		SetVehiclePosEx(playerid, 1280.2069, -2056.4087, 58.5319, 268.94249);
		SetPlayerVirtualWorld(playerid, 11);
		format(string, sizeof(string), "{FFFFFF}[Drift Mission]{AA3333} äúçéì àú îùéîú äãøéôè {FFFFFF}%s{AA3333} äùç÷ï", GetName(DriftMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
	}
	if(dialogid == 23_0 && response)
	{
		format(string, sizeof(string), "%s îùéîú äöðéçä ëáø ðòùú òì éãé äùç÷ï", GetName(ParachutingMission_Player));
  		if(ParachutingMission_Start == 1) return SendClientMessage(playerid, White, string);
    	if(ParachutingMission_WasFinish[playerid] == 1) return SendClientMessage(playerid, White, "ðéúï ìòùåú àú îùéîú äöðéçä ôòí áëðéñä ìîùç÷");
		if(AntiCheat_GetPlayerMoney(playerid) < 1000) return SendClientMessage(playerid, White, "àéï ìê 1000$ ëãé ìäúçéì àú äîùéîä");
		AntiCheat_GivePlayerMoney(playerid, false, 1000);
		ParachutingMission_Start = 1;
		ParachutingMission_In[playerid] = 1;
		ParachutingMission_RandomPrice = randomex(10000, 20000);
		format(string, sizeof(string), "• äúçìú ëòú àú îùéîú äöðéçä\néù ìê ã÷ä åçöé ìäâéò ìö'÷ôåééðè ùðîöà òì äââ ááðééï îîåì\n%s$ áîéãä åúâéò ìö'÷ôåééðè ú÷áì", GetNum(ParachutingMission_RandomPrice));
		ShowPlayerDialog(playerid, 21_1, DIALOG_STYLE_MSGBOX, "îùéîú äöðéçä", string, "äîùê", "");
		SetPlayerPosEx(playerid, 1544.5236, -1374.6577, 330.0591, 178.6192);
		GivePlayerWeapon(playerid, 46, 1);
		ParachutingMission_Player = playerid;
		ParachutingMission_Countdown(90);
		ParachutingMission_Timer_2 = SetTimerEx("ParachutingMission_End", 1000*90*1, 0, "%d", playerid);
		format(string, sizeof(string), "{FFFFFF}[Parachuting Mission]{AA3333} îúçéì áãøëå ìéòã ùì îùéîú äöðéçä {FFFFFF}%s{AA3333} äùç÷ï", GetName(ParachutingMission_Player));
  		OL_SendClientMessageToAllEx(playerid, Red, string);
  		SetPlayerRaceCheckpoint(playerid,1,1481.2938,-1790.2223,156.7533,       0,0,0,2.5);
		CP_ParachutingMission[playerid]++;
		SetPlayerVirtualWorld(playerid, 11);
	}
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerText(playerid, text[])
{
    new string[256];
    
	if(!inAFK[playerid] && Logged[playerid] == true)
	{
	    KillTimer(AutoAFK_Timer[playerid]);
   		AutoAFK_Timer[playerid] = SetTimerEx("AutomaticAFK", 1000*60*2, 0, "d", playerid);
	}
    
	if(DOF2_FileExists(File(playerid)) && AccountLog[playerid] == false) return SendClientMessage(playerid, Redwhite, "àúä çééá ìäëðñ ìîùç÷ ëãé ìëúåá áö'àè"), 0;
	else if(!DOF2_FileExists(File(playerid))) return SendClientMessage(playerid, Redwhite, "àúä çééá ìäøùí ìùøú ëãé ìëúåá áö'àè"), 0;


	if(inMute[playerid] == 1) return SendClientMessage(playerid, Redwhite, "àúä îåùú÷, àéðê éëåì ìëúåá áö'àè"), 0;

	if(inAFK[playerid] == 1) return SendClientMessage(playerid, Redwhite, "àúä áîöá ìà ðîöà, àéðê éëåì ìëúåá áö'àè"), 0;

	if(CheckChatBlock == 1 && CheckAdminLevel(playerid) <=  2) return SendClientMessage(playerid, Redwhite, "äö'àè ðòåì, àéðê éëåì ìëúåá áö'àè"), 0;

	if(Survey_Have == 1 && CheckAdminLevel(playerid) <=  2) return SendClientMessage(playerid, Redwhite, "äñ÷ø îåôòì, àéðê éëåì ìëúåá áö'àè"), 0;
	
	if(CheckFlooding(playerid) && !GetAdminLevel(playerid) && !GetTempAdminLevel(playerid) && !GetSpyAdminLevel(playerid)) return SendClientMessage(playerid, Redwhite, "òáøú àú îñôø ääåãòåú äîåøùä ì2 ùðéåú, àðà äîúï îñôø ùðéåú åäîùê áëúéáä"), 0;


	for(new word; word<sizeof(BlockedWords); word++)
	{
    	if(strfind(text, BlockedWords[word]) != -1)
		{
			format(string, sizeof(string), "\"îòøëú äöéðæåø àéúøä îéìä çñåîä áîùôè \"%s", BlockedWords[word]);
			SendClientMessage(playerid, Redwhite, string);
			SendClientMessage(playerid, Redwhite, "äåãòúê ìà ðùìçä -");
			TimeBlockedWord[playerid] ++;
			DOF2_SetInt(File(playerid), "BlockedWord", DOF2_GetInt(File(playerid), "BlockedWord")+1);
			format(string, sizeof(string), "äùúîùú á%d îúåê 3 îçøåæåú çñåîåú", TimeBlockedWord[playerid]);
			SendClientMessage(playerid, Redwhite, string);
			format(string, sizeof(string), "{FFFFFF}%s {33AA33}îòøëú äöéðæåø öéðæøä ðéñéåï ôøñåí îéìä çñåîä îäîùúîù", GetName(playerid));
			SendAdminsMessage(Green, string);
			format(string, sizeof(string), "(%s)", text);
			SendAdminsMessage(White, string);
			return 0;
		}
	}

	if(!IsVaildText(text))
	{
		SendClientMessage(playerid, Redwhite, "îòøëú äöéðæåø àéúøä ôøñåí ùøú áîùôèê");
		SendClientMessage(playerid, Redwhite, "äåãòúê ìà ðùìçä -");
		TimeBlockedWord[playerid] ++;
		DOF2_SetInt(File(playerid), "BlockedWord", DOF2_GetInt(File(playerid), "BlockedWord")+1);
		format(string, sizeof(string), "äùúîùú á%d îúåê 3 îçøåæåú çñåîåú", TimeBlockedWord[playerid]);
		SendClientMessage(playerid, Redwhite, string);
		format(string, sizeof(string), "{FFFFFF}%s {33AA33}îòøëú äöéðæåø öéðæøä ðéñéåï ôøñåí ùøú îäîùúîù", GetName(playerid));
		SendAdminsMessage(Green, string);
		format(string, sizeof(string), "(%s)", text);
		SendAdminsMessage(White, string);
		return 0;
	}

	if(text[0] == '*')
	{
	    if(GetSupportLevel(playerid) >= 1)
  		{
			new ac[128], lang1text[20];
			strmid(ac, text, 1, strlen(text));
			if(DOF2_GetInt(File(playerid), "SupportLevel") == 1) lang1text = "áðéñéåï";
			else if(DOF2_GetInt(File(playerid), "SupportLevel") == 2) lang1text = "çáø";
			else if(DOF2_GetInt(File(playerid), "SupportLevel") == 3) lang1text = "àçøàé";
 			format(string, sizeof(string), "[{FFFFFF}Supporters{FFA500} Chat] | {FFFFFF}%s{FFA500} | {FFFFFF}%s{FFA500}[#{FFFFFF}%d{FFA500}]: %s", lang1text, GetName(playerid), playerid, ac);
			SendSupportsMessage(Orange, string);
	  	}
		else SendClientMessage(playerid, White, "àúä ìà úåîê");
		return 0;
	}

	if(text[0] == '#')
	{
	    if(GetVipLevel(playerid) >= 1)
  		{
			new ac[128], lang1text[20];
			strmid(ac, text, 1, strlen(text));
			if(DOF2_GetInt(File(playerid), "VipLevel") == 1) lang1text = "çáø";
			else if(DOF2_GetInt(File(playerid), "VipLevel") == 2) lang1text = "àçøàé";
 			format(string, sizeof(string), "[{FFFFFF}VIP{FF66FF} Chat] | {FFFFFF}%s{FF66FF} | {FFFFFF}%s{FF66FF}[#{FFFFFF}%d{FF66FF}]: %s", lang1text, GetName(playerid), playerid, ac);
			SendVipMessage(Pink, string);
	  	}
		else SendClientMessage(playerid, White, "àúä ìà çáø åéàééôé");
		return 0;
	}

	if(text[0] == '@')
	{
		if(DOF2_GetInt(File(playerid), "InClan") >= 1)
		{
			format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
			if(DOF2_GetInt(ClanFile, "ClanChat") == 0)
			{
			    new lang1text[20];
			    if(DOF2_GetInt(File(playerid), "ClanLevel") == 1) lang1text = "ùç÷ï";
				else if(DOF2_GetInt(File(playerid), "ClanLevel") == 2) lang1text = "áåçï";
				else if(DOF2_GetInt(File(playerid), "ClanLevel") == 3) lang1text = "ñâï îðäì";
				else if(DOF2_GetInt(File(playerid), "ClanLevel") == 4) lang1text = "îðäì";
				format(string, sizeof(string), "[%s Chat] | %s | %s[#%d]: %s", DOF2_GetString(File(playerid), "Clan"), lang1text, GetName(playerid), playerid, text[1]);
				for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && DOF2_GetInt(File(i), "InClan") == 1 && strcmp(DOF2_GetString(File(playerid), "Clan"), DOF2_GetString(File(i), "Clan"), true) == 0)
				{
					if(DOF2_GetInt(ClanFile, "HaveClanChatColor") == 1) SendClientMessage(i, rgba2hex(DOF2_GetInt(ClanFile, "ClanChatColorR"), DOF2_GetInt(ClanFile,"ClanChatColorG"), DOF2_GetInt(ClanFile,"ClanChatColorB"), 255), string);
					else SendClientMessage(i, Bluesea, string);
				}
			}
			else SendClientMessage(playerid, White, "ö'àè ä÷ìàï ðòåì, àéðê éëåì ìëúåá áö'àè ä÷ìàï");
		}
		else SendClientMessage(playerid, White, "àúä ìà çáø ÷ìàï øùîé");
 		return 0;
	}
	
	if(text[0] == '!')
	{
		if(DOF2_GetInt(File(playerid), "InGang") >= 1)
		{
			format(GangFile, sizeof(GangFile), "Gangs/%s.ini", DOF2_GetString(File(playerid), "Gang"));
			if(DOF2_GetInt(GangFile, "GangChat") == 0)
			{
			    new lang1text[20];
			    if(DOF2_GetInt(File(playerid), "GangLevel") == 1) lang1text = "ùç÷ï";
				else if(DOF2_GetInt(File(playerid), "GangLevel") == 2) lang1text = "îðäì";
				format(string, sizeof(string), "[%s Chat] | %s | %s[#%d]: %s", DOF2_GetString(File(playerid), "Gang"), lang1text, GetName(playerid), playerid, text[1]);
				for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && DOF2_GetInt(File(i), "InGang") == 1 && strcmp(DOF2_GetString(File(playerid), "Gang"), DOF2_GetString(File(i), "Gang"), true) == 0) SendClientMessage(i, Whitebrown, string);
			}
			else SendClientMessage(playerid, White, "ö'àè äëðåôééä ðòåì, àéðê éëåì ìëúåá áö'àè äëðåôééä");
		}
		else SendClientMessage(playerid, White, "àúä ìà çáø ëðåôééä");
 		return 0;
	}
	
	////////////////////////////////////////////////////////////////////////////
	format(string, sizeof(string), "[Activity Manager] %s:[{FFFFFF}#%d] {FFFFFF}%s", GetName(playerid), playerid,text);
 	if(GetAdminLevel(playerid) == 1 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Whitegrey, string), 0;
	format(string, sizeof(string), "[Activity Manager] %s:[{FFFFFF}#%d][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
	if(GetAdminLevel(playerid) == 1 && inAdminSpy[playerid] == 0 && DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Whitegrey, string), 0;
	//==========================================================================
	format(string, sizeof(string), "[Attemp Admin] %s:[{FFFFFF}#%d{33AA33}] {FFFFFF}%s", GetName(playerid), playerid,text);
 	if(GetAdminLevel(playerid) == 2 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string),0;
	format(string, sizeof(string), "[Attemp Admin] %s:[{FFFFFF}#%d{33AA33}][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
	if(GetAdminLevel(playerid) == 2 && inAdminSpy[playerid] == 0 && DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	//==========================================================================
 	format(string, sizeof(string), "[Admin] %s:[{FFFFFF}#%d{33AA33}] {FFFFFF}%s", GetName(playerid), playerid,text);
	if(GetAdminLevel(playerid) == 3 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	format(string, sizeof(string), "[Admin] %s:[{FFFFFF}#%d{33AA33}][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
	if(GetAdminLevel(playerid) == 3 && inAdminSpy[playerid] == 0 && DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	//==========================================================================
	format(string, sizeof(string), "[Master Admin] %s:[{FFFFFF}#%d{33AA33}] {FFFFFF}%s", GetName(playerid), playerid,text);
 	if(GetAdminLevel(playerid) == 4 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	format(string, sizeof(string), "[Master Admin] %s:[{FFFFFF}#%d{33AA33}][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
	if(GetAdminLevel(playerid) == 4 && inAdminSpy[playerid] == 0 && DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	//==========================================================================
	format(string, sizeof(string), "[Main Admin] %s:[{FFFFFF}#%d{33AA33}] {FFFFFF}%s", GetName(playerid), playerid,text);
 	if(GetAdminLevel(playerid) == 5 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	format(string, sizeof(string), "[Main Admin] %s:[{FFFFFF}#%d{33AA33}][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
	if(GetAdminLevel(playerid) == 5 && inAdminSpy[playerid] == 0 && DOF2_IsSet(File(playerid), "Tag"))  return OL_SendClientMessageToAll(Green, string), 0;
	////////////////////////////////////////////////////////////////////////////
	format(string, sizeof(string), "[Temp Activity Manager] %s:[{FFFFFF}#%d{33AA33}] {FFFFFF}%s", GetName(playerid), playerid,text);
 	if(GetTempAdminLevel(playerid) == 1 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	format(string, sizeof(string), "[Temp Activity Manager] %s:[{FFFFFF}#%d{33AA33}][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
	if(GetTempAdminLevel(playerid) == 1 && inAdminSpy[playerid] == 0 && DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	//==========================================================================
	format(string, sizeof(string), "[Temp Attemp Admin] %s:[{FFFFFF}#%d{33AA33}] {FFFFFF}%s", GetName(playerid), playerid,text);
 	if(GetTempAdminLevel(playerid) == 2 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string),0;
	format(string, sizeof(string), "[Temp Attemp Admin] %s:[{FFFFFF}#%d{33AA33}][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
	if(GetTempAdminLevel(playerid) == 2 && inAdminSpy[playerid] == 0 && DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	//==========================================================================
 	format(string, sizeof(string), "[Temp Admin] %s:[{FFFFFF}#%d{33AA33}] {FFFFFF}%s", GetName(playerid), playerid,text);
	if(GetTempAdminLevel(playerid) == 3 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	format(string, sizeof(string), "[Temp Admin] %s:[{FFFFFF}#%d{33AA33}][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
	if(GetTempAdminLevel(playerid) == 3 && inAdminSpy[playerid] == 0 && DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	//==========================================================================
	format(string, sizeof(string), "[Temp Master Admin] %s:[{FFFFFF}#%d{33AA33}] {FFFFFF}%s", GetName(playerid), playerid,text);
 	if(GetTempAdminLevel(playerid) == 4 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	format(string, sizeof(string), "[Temp Master Admin] %s:[{FFFFFF}#%d{33AA33}][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
	if(GetTempAdminLevel(playerid) == 4 && inAdminSpy[playerid] == 0 && DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Green, string), 0;
	////////////////////////////////////////////////////////////////////////////
	format(string, sizeof(string), "%s [%d]", text, playerid);
	if(GetSpyAdminLevel(playerid) >= 1) return SendPlayerMessageToAll(playerid, string), 0;
	////////////////////////////////////////////////////////////////////////////
	format(string, sizeof(string), "%s [{33CCFF}#%d{FFFFFF} | %s]", text, playerid, DOF2_GetString(pfile,"Tag"));
	if(DOF2_IsSet(File(playerid), "Tag")) return SendPlayerMessageToAll(playerid, string), 0;
	//==========================================================================
	format(string, sizeof(string), "%s [{33CCFF}#%d{FFFFFF}]", text, playerid);
	if(!DOF2_IsSet(File(playerid), "Tag")) return SendPlayerMessageToAll(playerid, string), 0;
	////////////////////////////////////////////////////////////////////////////
	return 1;
}

//------------------------------------------------------------------------------

public UpdateVehiclePos()
{
	for(new v; v<MAX_VEHICLES; v++)
	{
		format(carfile, sizeof(carfile), "Cars/%d.ini", v);
		if(DOF2_FileExists(carfile) && DOF2_GetInt(carfile, "HaveSpawn") == 1) SetVehiclePos(v, DOF2_GetFloat(carfile, "vX"), DOF2_GetFloat(carfile, "vY"), DOF2_GetFloat(carfile, "vZ")), SetVehicleZAngle(v, DOF2_GetFloat(carfile, "vA"));
	}
 	KillTimer(UpdateVehiclePos_Timer);
	return 1;
}

//------------------------------------------------------------------------------

public OnVehicleSpawn(vehicleid)
{
	new string[256];
	DestroyObject(ANeon[vehicleid]);
	DestroyObject(BikeNitroObj[vehicleid]);
	Vehicle3DTimer = SetTimer("UpdateVehicle3DText", 1000*1*1, false);

	if(DOF2_GetInt(CarFile(vehicleid), "HaveSpawn") == 1) SetVehiclePos(vehicleid, DOF2_GetFloat(CarFile(vehicleid), "vX"), DOF2_GetFloat(CarFile(vehicleid), "vY"), DOF2_GetFloat(CarFile(vehicleid), "vZ")), SetVehicleZAngle(vehicleid, DOF2_GetFloat(CarFile(vehicleid), "vA"));
	
	format(string, sizeof(string), "%s", DOF2_GetString(CarFile(vehicleid), "Plate"));
	if(DOF2_GetInt(CarFile(vehicleid), "HavePlate") == 1) SetVehicleNumberPlate(vehicleid, string);
			
	if(DOF2_GetInt(CarFile(vehicleid), "HaveColor") == 1) if(DOF2_GetInt(CarFile(vehicleid), "Color1") >= 0 && DOF2_GetInt(CarFile(vehicleid), "Color2") >= 0) ChangeVehicleColor(vehicleid, DOF2_GetInt(CarFile(vehicleid), "Color1"), DOF2_GetInt(CarFile(vehicleid), "Color2"));
	
    if(DOF2_GetInt(CarFile(vehicleid), "Nitro") == 1) AddVehicleComponent(vehicleid, 1009);
    else if(DOF2_GetInt(CarFile(vehicleid), "Nitro") == 2) AddVehicleComponent(vehicleid, 1008);
    else if(DOF2_GetInt(CarFile(vehicleid), "Nitro") == 3) AddVehicleComponent(vehicleid, 1010);
	
	if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1073) AddVehicleComponent(vehicleid, 1073);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1074) AddVehicleComponent(vehicleid, 1074);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1075) AddVehicleComponent(vehicleid, 1075);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1076) AddVehicleComponent(vehicleid, 1076);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1077) AddVehicleComponent(vehicleid, 1077);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1078) AddVehicleComponent(vehicleid, 1078);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1079) AddVehicleComponent(vehicleid, 1079);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1080) AddVehicleComponent(vehicleid, 1080);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1081) AddVehicleComponent(vehicleid, 1081);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1082) AddVehicleComponent(vehicleid, 1082);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1083) AddVehicleComponent(vehicleid, 1083);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1084) AddVehicleComponent(vehicleid, 1084);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1085) AddVehicleComponent(vehicleid, 1085);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1096) AddVehicleComponent(vehicleid, 1096);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1097) AddVehicleComponent(vehicleid, 1097);
	else if(DOF2_GetInt(CarFile(vehicleid), "Wheels") == 1098) AddVehicleComponent(vehicleid, 1098);
	
	if(DOF2_GetInt(CarFile(vehicleid), "Hydralics") > 0) AddVehicleComponent(vehicleid, 1087);
	
	if(DOF2_GetInt(CarFile(vehicleid), "Neon") == 1)
	{
		ANeon[vehicleid] = CreateObject(18652, 0, 0, 0, 0, 0, 0, 500);
		AttachObjectToVehicle(ANeon[vehicleid],vehicleid, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
	}
	else if(DOF2_GetInt(CarFile(vehicleid), "Neon") == 2)
	{
		ANeon[vehicleid] = CreateObject(18648, 0, 0, 0, 0, 0, 0, 500);
		AttachObjectToVehicle(ANeon[vehicleid],vehicleid, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
	}
	else if(DOF2_GetInt(CarFile(vehicleid), "Neon") == 3)
	{
		ANeon[vehicleid] = CreateObject(18647, 0, 0, 0, 0, 0, 0, 500);
		AttachObjectToVehicle(ANeon[vehicleid],vehicleid, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
	}
	else if(DOF2_GetInt(CarFile(vehicleid), "Neon") == 4)
	{
		ANeon[vehicleid] = CreateObject(18649, 0, 0, 0, 0, 0, 0, 500);
		AttachObjectToVehicle(ANeon[vehicleid],vehicleid, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
	}
	else if(DOF2_GetInt(CarFile(vehicleid), "Neon") == 5)
	{
		ANeon[vehicleid] = CreateObject(18650, 0, 0, 0, 0, 0, 0, 500);
		AttachObjectToVehicle(ANeon[vehicleid],vehicleid, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
	}
	else if(DOF2_GetInt(CarFile(vehicleid), "Neon") == 6)
	{
		ANeon[vehicleid] = CreateObject(18651, 0, 0, 0, 0, 0, 0, 500);
		AttachObjectToVehicle(ANeon[vehicleid],vehicleid, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
	}
	
	if(DOF2_GetInt(CarFile(vehicleid), "BikeNitro") == 1)
	{
		BikeNitroObj[vehicleid] = CreateObject(18693,0,0,10000000,0,0,250);
		AttachObjectToVehicle(BikeNitroObj[vehicleid],vehicleid, 0.2, -1.2, -1.45, 0.0, 0.5, 0.5);
	}
	
	Delete3DTextLabel(CarText[vehicleid]);
	if(DOF2_GetInt(CarFile(vehicleid), "Special") == 0 && DOF2_GetInt(CarFile(vehicleid), "Public") == 0 && DOF2_GetInt(CarFile(vehicleid), "Private") == 0 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 1)
	{
		format(string, sizeof(string), "øëá øâéì\n{FFFFFF}%s{33AA33} ááòìåú", DOF2_GetString(CarFile(vehicleid), "Owner"));
		CarText[vehicleid] = Create3DTextLabel(string, Aqua, 0, 0, 2, 30, 0, 1);
		Attach3DTextLabelToVehicle(CarText[vehicleid], vehicleid, 0, 0, 1);
	}
	if(DOF2_GetInt(CarFile(vehicleid), "Special") == 1 && DOF2_GetInt(CarFile(vehicleid), "Public") == 0 && DOF2_GetInt(CarFile(vehicleid), "Private") == 0 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 1)
	{
		format(string, sizeof(string), "øëá îéåçã\n{FFFFFF}%s{800080} ááòìåú", DOF2_GetString(CarFile(vehicleid), "Owner"));
		CarText[vehicleid] = Create3DTextLabel(string, Purple, 0, 0, 2, 30, 0, 1);
		Attach3DTextLabelToVehicle(CarText[vehicleid], vehicleid, 0, 0, 1);
	}
	if(DOF2_GetInt(CarFile(vehicleid), "Special") == 0 && DOF2_GetInt(CarFile(vehicleid), "Public") == 0 && DOF2_GetInt(CarFile(vehicleid), "Private") == 0 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 0)
	{
		CarText[vehicleid] = Create3DTextLabel("øëá øâéì\n{33AA33}ôðåé ìøëéùä", Aqua, 0, 0, 2, 30, 0, 1);
		Attach3DTextLabelToVehicle(CarText[vehicleid], vehicleid, 0, 0, 1);
	}
	if(DOF2_GetInt(CarFile(vehicleid), "Special") == 1 && DOF2_GetInt(CarFile(vehicleid), "Public") == 0 && DOF2_GetInt(CarFile(vehicleid), "Private") == 0 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 0)
	{
		CarText[vehicleid] = Create3DTextLabel("øëá îéåçã\n{800080}ôðåé ìøëéùä", Purple, 0, 0, 2, 30, 0, 1);
		Attach3DTextLabelToVehicle(CarText[vehicleid], vehicleid, 0, 0, 1);
	}
	if(DOF2_GetInt(CarFile(vehicleid), "Special") == 0 && DOF2_GetInt(CarFile(vehicleid), "Public") == 1 && DOF2_GetInt(CarFile(vehicleid), "Private") == 0 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 0)
	{
		CarText[vehicleid] = Create3DTextLabel("øëá öéáåøé", Brightred, 0, 0, 2, 30, 0, 1);
		Attach3DTextLabelToVehicle(CarText[vehicleid], vehicleid, 0, 0, 1);
	}
	if(DOF2_GetInt(CarFile(vehicleid), "Special") == 0 && DOF2_GetInt(CarFile(vehicleid), "Public") == 0 && DOF2_GetInt(CarFile(vehicleid), "Private") == 1 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 0)
	{
		CarText[vehicleid] = Create3DTextLabel("øëåù ôøèé", Bluesea, 0, 0, 2, 30, 0, 1);
		Attach3DTextLabelToVehicle(CarText[vehicleid], vehicleid, 0, 0, 1);
	}
	return 1;
}

forward kickx(playerid);
public kickx(playerid)
{
	Kick(playerid);
	KillTimer(kixkx[playerid]);
	return 1;
}
//------------------------------------------------------------------------------

public OnVehicleDeath(vehicleid, killerid)
{
	for(new i=0; i<MAX_PLAYERS; i++) if(RCXD[i] == vehicleid) KillTimer(RCXDTimer[i]), DestroyRCXD(i);
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerSpawn(playerid)
{
	//StopSoundData(playerid, true, 1097);
	StopAudioStreamForPlayer(playerid);
	TextDrawShowForPlayer(playerid, WebSiteLogo[playerid]);
	TextDrawShowForPlayer(playerid, Clock);
 	TextDrawShowForPlayer(playerid, PlayeridName[playerid]);
//	TextDrawShowForPlayer(playerid, Logo);
//	TextDrawShowForPlayer(playerid, OnlinePlayers);
//	TextDrawShowForPlayer(playerid, Version);
	TextDrawShowForPlayer(playerid, Bar_Health[playerid]);
	TextDrawShowForPlayer(playerid, Bar_Armour[playerid]);
	TextDrawShowForPlayer(playerid, Bar_Money[playerid]);
//	TextDrawShowForPlayer(playerid, VehID[playerid]);
 	
	SetPlayerInterior(playerid, 0);
	
	new string[256];

	if(!inAFK[playerid] && Logged[playerid] == true)
	{
	    KillTimer(AutoAFK_Timer[playerid]);
   		AutoAFK_Timer[playerid] = SetTimerEx("AutomaticAFK", 1000*60*2, 0, "d", playerid);
	}
	
	if(Logged[playerid] == true)
	{
		if(!strcmp(GetName(playerid), "L1or1", true) && !strcmp(GetName(playerid), "DisPlay", true) && !strcmp(GetName(playerid), "Speed[i]", true) && !strcmp(GetName(playerid), "DeadDevil", true) && !strcmp(GetName(playerid), "Fervor", true) && !strcmp(GetName(playerid), "Tomer", true) && !strcmp(GetName(playerid), "Amit_Mizrahi", true))
		{
		}
		else
		{
			format(string, sizeof(string), "%s\núåãä òì ôòéìåúê áùøú", GetName(playerid));
			CreatePlayer3DTextLabel(playerid, string, Gold, 1039.0315, -1133.7107, 22.8281, 20.0, 0); // Hollywood Walk of Fame
		}
	}
	
	new RandomSpawn = random(sizeof(RandomPlayerSpawns));
	SetPlayerPos(playerid, RandomPlayerSpawns[RandomSpawn][0], RandomPlayerSpawns[RandomSpawn][1], RandomPlayerSpawns[RandomSpawn][2]);

	if(DOF2_GetInt(File(playerid), "Register") == 0) DOF2_SetInt(File(playerid), "Register", 1);

	format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}ðëðñ ìîùç÷ {FF0000}%s {FFFFFF}äîùúîù [#%d]", GetName(playerid),playerid);
	if(AccountLog[playerid] == true && Logged[playerid] == false) OL_SendClientMessageToAllEx(playerid, Grey, string), Logged[playerid] = true;
	//if(DOF2_GetInt(File(playerid), "ResetCar") == 0) DOF2_SetInt(File(playerid), "HaveCar", 0), DOF2_SetString(File(playerid), "CarID", "-1"), DOF2_SetInt(File(playerid), "ResetCar", 1);

	if(DOF2_GetInt(File(playerid), "HaveCrash") == 1)
	{
		if(DOF2_GetInt(File(playerid), "CrashMoney") >= 1) AntiCheat_GivePlayerMoney(playerid, true, DOF2_GetInt(File(playerid), "CrashMoney")), SendClientMessage(playerid, Grey, "{FFA500}[FxpDeathMatch] {FFFFFF}áäúðú÷åú äàçøåðä ùìê ÷éáìú ÷øàù, {FF0000}ëñôê {FFFFFF}ðùîø åäçåæø àìééê");
		format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}áäúðú÷åú äàçøåðä ùìê ÷éáìú ÷øàù, åøîú äðéäåì äæîðé ùìê äééúä {FF0000}%d{FFFFFF}, øîú äðéäåì äæîðé äåçæøä", DOF2_GetInt(File(playerid), "TempAdminLevel"));
		if(DOF2_GetInt(File(playerid), "CrashAdmin") >= 1) DOF2_SetInt(File(playerid), "TempAdminLevel", DOF2_GetInt(File(playerid), "CrashAdmin")), SendClientMessage(playerid, Grey, string);
		DOF2_SetInt(File(playerid), "HaveCrash", 0), DOF2_SetInt(File(playerid), "CrashMoney", 0), DOF2_SetInt(File(playerid), "CrashAdmin", 0);
	}

	Delete3DTextLabel(ActLevel[playerid]);

	ResetPlayerWeapons(playerid);
	
	if(DOF2_GetInt(File(playerid), "StartWeapon") > 0) GivePlayerWeapon(playerid, DOF2_GetInt(File(playerid), "StartWeapon"), 50);
	else GivePlayerWeapon(playerid, 24, 50);
	
	SetPlayerTime(playerid, 20, 0);
	if(DOF2_GetInt(File(playerid), "StartTime") > 0) SetPlayerTime(playerid, DOF2_GetInt(File(playerid), "StartTime"), 0);
	
	if(DOF2_GetInt(File(playerid), "Knife") > 0) GivePlayerWeapon(playerid, 4, DOF2_GetInt(File(playerid), "Knife_Ammo"));
	if(DOF2_GetInt(File(playerid), "Chainsaw") > 0) GivePlayerWeapon(playerid, 9, DOF2_GetInt(File(playerid), "Chainsaw_Ammo"));
	if(DOF2_GetInt(File(playerid), "Grenade") > 0) GivePlayerWeapon(playerid, 16, DOF2_GetInt(File(playerid), "Grenade_Ammo"));
	if(DOF2_GetInt(File(playerid), "MolotovCocktail") > 0) GivePlayerWeapon(playerid, 18, DOF2_GetInt(File(playerid), "MolotovCocktail_Ammo"));
	if(DOF2_GetInt(File(playerid), "9mm") > 0) GivePlayerWeapon(playerid, 22, DOF2_GetInt(File(playerid), "9mm_Ammo"));
	if(DOF2_GetInt(File(playerid), "SawnoffShotgun") > 0) GivePlayerWeapon(playerid, 26, DOF2_GetInt(File(playerid), "SawnoffShotgun_Ammo"));
	if(DOF2_GetInt(File(playerid), "CombatShotgun") > 0) GivePlayerWeapon(playerid, 27, DOF2_GetInt(File(playerid), "CombatShotgun_Ammo"));
	if(DOF2_GetInt(File(playerid), "MicroSMG") > 0) GivePlayerWeapon(playerid, 28, DOF2_GetInt(File(playerid), "MicroSMG_Ammo"));
	if(DOF2_GetInt(File(playerid), "Tec9") > 0) GivePlayerWeapon(playerid, 32, DOF2_GetInt(File(playerid), "Tec9_Ammo"));
	if(DOF2_GetInt(File(playerid), "AK47") > 0) GivePlayerWeapon(playerid, 30, DOF2_GetInt(File(playerid), "AK47_Ammo"));
	if(DOF2_GetInt(File(playerid), "M4") > 0) GivePlayerWeapon(playerid, 31, DOF2_GetInt(File(playerid), "M4_Ammo"));
	if(DOF2_GetInt(File(playerid), "SniperRifle") > 0) GivePlayerWeapon(playerid, 34, DOF2_GetInt(File(playerid), "SniperRifle_Ammo"));
	if(DOF2_GetInt(File(playerid), "HaveArmour") == 1) SetPlayerArmour(playerid, 100);
	
	if(GetLevel(playerid) == 1) SetPlayerArmour(playerid, 10);
	else if(GetLevel(playerid) == 2) SetPlayerArmour(playerid, 20);
	else if(GetLevel(playerid) == 3) SetPlayerArmour(playerid, 30);
	else if(GetLevel(playerid) == 4) SetPlayerArmour(playerid, 40);
	else if(GetLevel(playerid) == 5) SetPlayerArmour(playerid, 50);
	else if(GetLevel(playerid) == 6) SetPlayerArmour(playerid, 60);
	else if(GetLevel(playerid) == 7) SetPlayerArmour(playerid, 70);
	else if(GetLevel(playerid) == 8) SetPlayerArmour(playerid, 80);
	else if(GetLevel(playerid) == 9) SetPlayerArmour(playerid, 90);
	else if(GetLevel(playerid) == 10) SetPlayerArmour(playerid, 100);

    GangZoneShowForPlayer(playerid, GangZone_Vip, 0xFF00FF96);

	if(DOF2_GetInt(File(playerid),"InClan") == 1)
	{
    	format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
	    if(DOF2_GetInt(ClanFile, "HaveSpawn") == 1) SetPlayerPosEx(playerid, DOF2_GetFloat(ClanFile, "SpawnX"), DOF2_GetFloat(ClanFile, "SpawnY"), DOF2_GetFloat(ClanFile, "SpawnZ"), DOF2_GetFloat(ClanFile, "SpawnA"));
		if(DOF2_GetInt(ClanFile, "HaveClanColor") == 1) SetPlayerColor(playerid, rgba2hex(DOF2_GetInt(ClanFile, "ClanColorR"), DOF2_GetInt(ClanFile,"ClanColorG"), DOF2_GetInt(ClanFile,"ClanColorB"), 255));
	}

	if(DOF2_GetInt(File(playerid), "inJail") == 1)
	{
		inJail[playerid] = 1;
		JailTimer[playerid] = SetTimerEx("AutomaticUnjail", 60*1000*DOF2_GetInt(File(playerid), "JailTime"), 0, "d", playerid);
		JailTimer1[playerid] = SetTimerEx("AutomaticMainusJailTime", 60*1000, 0, "d", playerid);
		SetPlayerVirtualWorld(playerid, 2);
  		SetPlayerInterior(playerid, 3);
		SetPlayerPosEx(playerid, 340.3327, 164.0451, 1019.9912, 358.6400);
	}

    if(GetSkin(playerid) == 1) SetPlayerSkin(playerid, DOF2_GetInt(File(playerid), "Skin"));
    if(inWar[playerid] == 1)
    {
		SetPlayerInterior(playerid, 16);
		SetPlayerVirtualWorld(playerid, 9);
      	ResetPlayerWeapons(playerid);
   		SetPlayerHealth(playerid, 100);
   		SetPlayerArmour(playerid, 100);
	    GivePlayerWeapon(playerid, 26, 100000);
	    GivePlayerWeapon(playerid, 31, 100000);
	    GivePlayerWeapon(playerid, 32, 100000);
		switch(random(4))
		{
			case 0: SetPlayerPos(playerid, -1368.2813,1256.2673,1039.8672);
			case 1: SetPlayerPos(playerid, -1426.9152,1236.1774,1039.8741);
			case 2: SetPlayerPos(playerid, -1414.3704,1269.9659,1039.8741);
			case 3: SetPlayerPos(playerid, -1378.8235,1221.9016,1039.8672);
		}
		SetCameraBehindPlayer(playerid);
	}

    if(inFunbattle[playerid] == 1)
    {
  		SetPlayerInterior(playerid, 0);
		SetPlayerVirtualWorld(playerid, 5);
		SetCameraBehindPlayer(playerid);
      	ResetPlayerWeapons(playerid);
   		SetPlayerHealth(playerid, 100);
   		SetPlayerArmour(playerid, 100);
	    GivePlayerWeapon(playerid, 4, 1);
		switch(random(10))
		{
			case 0: SetPlayerPos(playerid, 1955.7610, 664.7080, 10.8203);
			case 1: SetPlayerPos(playerid, 1955.8861, 741.7211, 10.8203);
			case 2: SetPlayerPos(playerid, 1897.5166, 742.0504, 10.8198);
			case 3: SetPlayerPos(playerid, 1897.4633, 664.1781, 10.8203);
			case 4: SetPlayerPos(playerid, 1898.3416, 685.2984, 14.2756);
			case 5: SetPlayerPos(playerid, 1898.6339, 720.7966, 14.2761);
			case 6: SetPlayerPos(playerid, 1918.8018, 703.2986, 11.1328);
			case 7: SetPlayerPos(playerid, 1930.1067, 703.1287, 11.1328);
			case 8: SetPlayerPos(playerid, 1969.3967, 728.6649, 19.3468);
			case 9: SetPlayerPos(playerid, 1969.9930, 679.1732, 19.3469);
		}
	}

	if(inHardcore[playerid] == 1)
	{
  		SetPlayerInterior(playerid, 0);
		SetPlayerVirtualWorld(playerid, 6);
		SetCameraBehindPlayer(playerid);
 		ResetPlayerWeapons(playerid);
  		SetPlayerHealth(playerid, 100);
  		SetPlayerArmour(playerid, 100);
		switch(random(2))
		{
			case 0: GivePlayerWeapon(playerid, 35, 100000);
			case 1: GivePlayerWeapon(playerid, 38, 100000);
		}
		switch(random(10))
		{
			case 0: SetPlayerPos(playerid, 1136.4252,2850.5906,10.8203);
			case 1: SetPlayerPos(playerid, 1177.7346,2813.9194,10.8203);
			case 2: SetPlayerPos(playerid, 1319.1849,2855.9282,10.8203);
			case 3: SetPlayerPos(playerid, 1347.8789,2871.0122,10.4318);
			case 4: SetPlayerPos(playerid, 1395.0508,2845.8088,10.8203);
			case 5: SetPlayerPos(playerid, 1368.7010,2803.8027,10.8203);
			case 6: SetPlayerPos(playerid, 1366.6128,2731.2175,10.8203);
			case 7: SetPlayerPos(playerid, 1314.8970,2752.1975,10.8203);
			case 8: SetPlayerPos(playerid, 1257.0466,2725.1821,10.8203);
			case 9: SetPlayerPos(playerid, 1280.4435,2795.5291,10.8125);
		}
	}

	if(inDM[playerid] == 1)
	{
  		SetPlayerInterior(playerid, 0);
		SetPlayerVirtualWorld(playerid, 7);
		SetCameraBehindPlayer(playerid);
		ResetPlayerWeapons(playerid);
	 	SetPlayerHealth(playerid, 100);
	  	SetPlayerArmour(playerid, 100);
		GivePlayerWeapon(playerid, 24, 10000);
   	    GivePlayerWeapon(playerid, 27, 10000);
		GivePlayerWeapon(playerid, 31, 10000);
	   	GivePlayerWeapon(playerid, 32, 10000);
		switch(random(15))
		{
			case 0: SetPlayerPos(playerid, 1368.1095, 2169.7573, 11.0234);
			case 1: SetPlayerPos(playerid, 1356.4377, 2177.4951, 11.0156);
			case 2: SetPlayerPos(playerid, 1350.7726, 2161.5620, 11.0156);
			case 3: SetPlayerPos(playerid, 1332.7174, 2150.3665, 11.0156);
			case 4: SetPlayerPos(playerid, 1324.3278, 2129.3279, 11.0156);
			case 5: SetPlayerPos(playerid, 1329.4720, 2113.6460, 11.0156);
			case 6: SetPlayerPos(playerid, 1317.3594, 2119.2644, 11.0156);
			case 7: SetPlayerPos(playerid, 1343.9983, 2113.5471, 11.0156);
			case 8: SetPlayerPos(playerid, 1355.1498, 2119.7510, 11.0156);
			case 9: SetPlayerPos(playerid, 1369.3339, 2117.8230, 11.0156);
			case 10: SetPlayerPos(playerid, 1381.8855, 2119.5391, 11.0156);
			case 11: SetPlayerPos(playerid, 1387.0352, 2136.3342, 11.0234);
			case 12: SetPlayerPos(playerid, 1378.2015, 2163.3113, 11.0234);
			case 13: SetPlayerPos(playerid, 1384.2388, 2184.8020, 11.0234);
			case 14: SetPlayerPos(playerid, 1340.7930, 2136.9504, 11.0156);
		}
	}
	KillTimer(PropTimer);
	PropTimer = SetTimer("UpdateProperties", 1000*1*1, 1);
	KillTimer(AtmTimer);
	AtmTimer = SetTimer("UpdateAtm", 1000*1*1, 1);
	KillTimer(Vehicle3DTimer);
	Vehicle3DTimer = SetTimer("UpdateVehicle3DText", 1000*1*1, false);
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerExitVehicle(playerid, vehicleid)
{
	if(Monster_ACT_In[playerid] == 1)
	{
		Monster_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Monster_ACT_AllPlayers -=1;
	    SetPlayerVirtualWorld(playerid, 0);
	    SpawnPlayer(playerid);
     	SetPlayerInterior(playerid, 0);
	    AutomaticACT_In[playerid] = 0;
	    Monster_ACT_In[playerid] = 0;
	}

	if(Karting_ACT_In[playerid] == 1)
	{
		Karting_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Karting_ACT_AllPlayers -=1;
	    SetPlayerVirtualWorld(playerid, 0);
	    SpawnPlayer(playerid);
     	SetPlayerInterior(playerid, 0);
	    AutomaticACT_In[playerid] = 0;
	    Karting_ACT_In[playerid] = 0;
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_KartingActivity[playerid] = 0;
	}
	if(Drift_ACT_In[playerid] == 1)
	{
		Drift_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Drift_ACT_AllPlayers -=1;
	    SetPlayerVirtualWorld(playerid, 0);
	    SpawnPlayer(playerid);
     	SetPlayerInterior(playerid, 0);
	    AutomaticACT_In[playerid] = 0;
	    Drift_ACT_In[playerid] = 0;
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_DriftActivity[playerid] = 0;
	}
	if(RaceMission_In[playerid] == 1)
	{
		new string[256];
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_RaceMission[playerid] = 0;
		RaceMission_Start = 0;
		RaceMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äîéøåõ");
 		SendClientMessage(playerid, White, "éöàú îøëá äîùéîä -");
 		DestroyVehicle(RaceMission_Car);
 		KillTimer(RaceMission_Timer_1);
 		KillTimer(RaceMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Race Mission]{AA3333} ìà äöìéç ìäâéò ì÷å äñéåí ùì îùéîú äîéøåõ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RaceMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï éöà îäøëá äîùéîä -");
   		SetPlayerVirtualWorld(playerid, 0);
 	}
 	if(StuntsMission_In[playerid] == 1)
	{
		new string[256];
		StuntsMission_Start = 0;
		StuntsMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äñèàðèéí");
 		SendClientMessage(playerid, White, "éöàú îøëá äîùéîä -");
 		DestroyVehicle(StuntsMission_Car);
 		KillTimer(StuntsMission_Timer_1);
 		KillTimer(StuntsMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Stunts Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äñèàðèéí {FFFFFF}%s{AA3333} äùç÷ï", GetName(StuntsMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï éöà îäøëá äîùéîä -");
	 	SetPlayerVirtualWorld(playerid, 0);
 	}
	if(DriftMission_In[playerid] == 1)
	{
		new string[256];
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_DriftMission[playerid] = 0;
		DriftMission_Start = 0;
		DriftMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äãøéôè");
 		SendClientMessage(playerid, White, "éöàú îøëá äîùéîä -");
 		DestroyVehicle(DriftMission_Car);
 		KillTimer(DriftMission_Timer_1);
 		KillTimer(DriftMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Drift Mission]{AA3333} ìà äöìéç ìäâéò ì÷å äñéåí ùì îùéîú äãøéôè {FFFFFF}%s{AA3333} äùç÷ï", GetName(DriftMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï éöà îäøëá äîùéîä -");
   		SetPlayerVirtualWorld(playerid, 0);
 	}
	if(MoneyMission_In[playerid] == 1)
	{
		new string[256];
		MoneyMission_Start = 0;
		MoneyMission_In[playerid] = 0;
	 	SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äëñó");
	 	SendClientMessage(playerid, White, "éöàúä îäøëá -");
	 	KillTimer(MoneyMission_Timer_1);
	 	DestroyVehicle(MoneyMission_Bike);
	 	KillTimer(MoneyMission_Timer_2);
	 	format(string, sizeof(string), "{FFFFFF}[Money Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äëñó {FFFFFF}%s{AA3333} äùç÷ï", GetName(MoneyMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï éöà îøëáå -");
		RemovePlayerAttachedObject(playerid, 2); // MoneyBag
 	}
 	if(inGodmode[playerid] == 1) SetVehicleHealth(vehicleid, 1000);
    return 1;
}

//------------------------------------------------------------------------------

public OnPlayerDeath(playerid, killerid, reason)
{
    new string[256];

    new playercash;
	if(killerid == INVALID_PLAYER_ID)
	{
        SendDeathMessage(INVALID_PLAYER_ID,playerid,reason);

		if(MoneyMission_In[playerid] == 1)
		{
			MoneyMission_Start = 0;
			MoneyMission_In[playerid] = 0;
	 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äëñó");
	 		SendClientMessage(playerid, White, "îúú áîäìê äîùéîä -");
	 		KillTimer(MoneyMission_Timer_1);
	 		DestroyVehicle(MoneyMission_Bike);
	 		KillTimer(MoneyMission_Timer_2);
	 		format(string, sizeof(string), "{FFFFFF}[Money Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äëñó {FFFFFF}%s{AA3333} äùç÷ï", GetName(MoneyMission_Player));
			OL_SendClientMessageToAllEx(playerid, Red, string);
			OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï îú áãøëå ìéòã -");
			RemovePlayerAttachedObject(playerid, 2); // MoneyBag
		}
	 	if(RaceMission_In[playerid] == 1)
		{
	    	DisablePlayerRaceCheckpoint(playerid);
	      	CP_RaceMission[playerid] = 0;
			RaceMission_Start = 0;
			RaceMission_In[playerid] = 0;
	 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äîéøåõ");
	 		SendClientMessage(playerid, White, "îúú áîäìê äîùéîä -");
	 		DestroyVehicle(RaceMission_Car);
	 		KillTimer(RaceMission_Timer_1);
	 		KillTimer(RaceMission_Timer_2);
	 		format(string, sizeof(string), "{FFFFFF}[Race Mission]{AA3333} ìà äöìéç ìäâéò ì÷å äñéåí ùì îùéîú äîéøåõ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RaceMission_Player));
			OL_SendClientMessageToAllEx(playerid, Red, string);
			OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï îú áãøëå ìéòã -");
	 		SetPlayerVirtualWorld(playerid, 0);
	 	}
	 	if(RobberyMission_In[playerid] == 1)
		{
			RobberyMission_In[playerid] = 0;
	 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú ùãéãú äáð÷");
	 		SendClientMessage(playerid, White, "îúú áîäìê äîùéîä -");
	 		DestroyVehicle(RobberyMission_Car);
	 		KillTimer(RobberyMission_Timer_1);
	 		KillTimer(RobberyMission_Timer_2);
	 		format(string, sizeof(string), "{FFFFFF}[Robbery Mission]{AA3333} ìà äöìéç ìùãåã àú äáð÷ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RobberyMission_Player));
			OL_SendClientMessageToAllEx(playerid, Red, string);
			OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï îú áîäìê äùãéãä -");
	 	}
		if(StuntsMission_In[playerid] == 1)
		{
			StuntsMission_Start = 0;
			StuntsMission_In[playerid] = 0;
	 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äñèàðèéí");
	 		SendClientMessage(playerid, White, "îúú áîäìê äîùéîä -");
	 		DestroyVehicle(StuntsMission_Car);
	 		KillTimer(StuntsMission_Timer_1);
	 		KillTimer(StuntsMission_Timer_2);
	 		format(string, sizeof(string), "{FFFFFF}[Stunts Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äñèàðèéí {FFFFFF}%s{AA3333} äùç÷ï", GetName(StuntsMission_Player));
			OL_SendClientMessageToAllEx(playerid, Red, string);
			OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï îú áãøëå ìéòã -");
	 		SetPlayerVirtualWorld(playerid, 0);
		}
	 	if(DriftMission_In[playerid] == 1)
		{
	    	DisablePlayerRaceCheckpoint(playerid);
	      	CP_DriftMission[playerid] = 0;
			DriftMission_Start = 0;
			DriftMission_In[playerid] = 0;
	 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äãøéôè");
	 		SendClientMessage(playerid, White, "îúú áîäìê äîùéîä -");
	 		DestroyVehicle(DriftMission_Car);
	 		KillTimer(DriftMission_Timer_1);
	 		KillTimer(DriftMission_Timer_2);
	 		format(string, sizeof(string), "{FFFFFF}[Drift Mission]{AA3333} ìà äöìéç ìäâéò ì÷å äñéåí ùì îùéîú äãøéôè {FFFFFF}%s{AA3333} äùç÷ï", GetName(DriftMission_Player));
			OL_SendClientMessageToAllEx(playerid, Red, string);
			OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï îú áãøëå ìéòã -");
	 		SetPlayerVirtualWorld(playerid, 0);
	 	}
		if(ParachutingMission_In[playerid] == 1)
		{
	    	DisablePlayerRaceCheckpoint(playerid);
	      	CP_ParachutingMission[playerid] = 0;
			ParachutingMission_Start = 0;
			ParachutingMission_In[playerid] = 0;
	 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äöðéçä");
	 		SendClientMessage(playerid, White, "îúú áîäìê äîùéîä -");
	 		KillTimer(ParachutingMission_Timer_1);
	 		KillTimer(ParachutingMission_Timer_2);
	 		format(string, sizeof(string), "{FFFFFF}[Parachuting Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äöðéçä {FFFFFF}%s{AA3333} äùç÷ï", GetName(ParachutingMission_Player));
			OL_SendClientMessageToAllEx(playerid, Red, string);
			OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï îú áãøëå ìéòã -");
	 		SetPlayerVirtualWorld(playerid, 0);
		}
	}
	else
	{
 		SendDeathMessage(killerid,playerid,reason);
		playercash = AntiCheat_GetPlayerMoney(playerid);
		if(playercash > 0) AntiCheat_GivePlayerMoney(killerid, true, playercash), ResetPlayerMoney(playerid), AntiCheat_Money[playerid] = 0;
	}

 	ActivityStatus[playerid] = 0;

	if(InvisibleON[playerid] == 1) SetPlayerColor(playerid, RandomColor()), InvisibleON[playerid] = 0;
	
	if(inGodmode[playerid] == 1) SendClientMessage(playerid, Green, "äâåãîåã ðëáä àåèåîèéú"), inGodmode[playerid] = 0;

    if(InBattle[playerid] == 1)
  	{
		BattlePlayers -=1;
		SetPlayerVirtualWorld(playerid, 0);
		BattleWinner = killerid;
		BattleLosser = playerid;
		CheckBattleWinner(0);
	}

  	if(Minigun_ACT_On == 1 && Minigun_ACT_In[playerid] == 1)
	{
		Minigun_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Minigun_ACT_AllPlayers -=1;
		SetPlayerVirtualWorld(playerid, 0);
	}

  	if(RandomWeapon_ACT_On == 1 && RandomWeapon_ACT_In[playerid] == 1)
	{
		RandomWeapon_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		RandomWeapon_ACT_AllPlayers -=1;
		SetPlayerVirtualWorld(playerid, 0);
	}
	
  	if(Monster_ACT_On == 1 && Monster_ACT_In[playerid] == 1)
	{
		Monster_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Monster_ACT_AllPlayers -=1;
		SetPlayerVirtualWorld(playerid, 0);
	}

  	if(Karting_ACT_On == 1 && Karting_ACT_In[playerid] == 1)
	{
		Karting_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Karting_ACT_AllPlayers -=1;
	    SetPlayerVirtualWorld(playerid, 0);
	   	DisablePlayerRaceCheckpoint(playerid);
	   	CP_KartingActivity[playerid] = 0;
	}
	if(Drift_ACT_On == 1 && Drift_ACT_In[playerid] == 1)
	{
		Drift_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		Drift_ACT_AllPlayers -=1;
	    SetPlayerVirtualWorld(playerid, 0);
	   	DisablePlayerRaceCheckpoint(playerid);
	   	CP_DriftActivity[playerid] = 0;
	}
  	if(SawnoffKing_ACT_On == 1 && SawnoffKing_ACT_In[playerid] == 1)
	{
		SawnoffKing_ACT_In[playerid] = 0;
		AutomaticACT_In[playerid] = 0;
		SawnoffKing_ACT_AllPlayers -=1;
		SetPlayerVirtualWorld(playerid, 0);
	}

	if(inWar[killerid] == 1)
	{
	    new Float:Health;
	    GetPlayerHealth(killerid, Health);
		if(Health < 100) SetPlayerHealth(killerid, 100);
	    else SetPlayerArmour(killerid, 100);
	}

	if(MoneyMission_In[playerid] == 1)
	{
		MoneyMission_Start = 0;
		MoneyMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äëñó");
 		SendClientMessage(playerid, White, "ðøöçú áîäìê äîùéîä -");
 		KillTimer(MoneyMission_Timer_1);
 		KillTimer(MoneyMission_Timer_2);
 		DestroyVehicle(MoneyMission_Bike);
 		format(string, sizeof(string), "{FFFFFF}[Money Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äëñó {FFFFFF}%s{AA3333} äùç÷ï", GetName(MoneyMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðøöç áãøëå ìéòã -");
		AntiCheat_GivePlayerMoney(killerid, true, MoneyMission_RandomPrice);
		RemovePlayerAttachedObject(playerid, 2); // MoneyBag
	}
 	if(RobberyMission_In[playerid] == 1)
	{
		RobberyMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú ùãéãú äáð÷");
 		SendClientMessage(playerid, White, "ðøöçú áîäìê äîùéîä -");
 		DestroyVehicle(RobberyMission_Car);
 		KillTimer(RobberyMission_Timer_1);
 		KillTimer(RobberyMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Robbery Mission]{AA3333} ìà äöìéç ìùãåã àú äáð÷ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RobberyMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðøöç áîäìê äùãéãä -");
		AntiCheat_GivePlayerMoney(killerid, true, RobberyMission_RandomPrice);
 	}
 	
	DOF2_SetInt(File(killerid), "Kills", DOF2_GetInt(File(killerid), "Kills")+1);
	DOF2_SetInt(File(playerid), "Deaths", DOF2_GetInt(File(playerid), "Deaths")+1);

 	Kills[playerid] = 0;
	Kills[killerid] ++;

	for(new i=0; i<MAX_PLAYERS; i++) if(inRCXD[i]) KillTimer(RCXDTimer[i]), DestroyRCXD(i);

	switch(Kills[killerid])
	{
	    case 5:
	    {
			KS[killerid] = 1;
			format(string, sizeof(string), "òìä ìøîú îáå÷ùåú 1 òí 5 äøéâåú ììà îååú {FFFFFF}%s{458B74} äùç÷ï", GetName(killerid));
			OL_SendClientMessageToAll(Aquamarine, string);
			OL_SendClientMessageToAll(Lightaquamarine, "• ääøéâä ùì äùç÷ï úæëä àåúëí á5000$");
			SetPlayerWantedLevel(killerid, 1);
		}
	    case 10:
	    {
			KS[killerid] = 2;
			format(string, sizeof(string), "òìä ìøîú îáå÷ùåú 2 òí 10 äøéâåú ììà îååú {FFFFFF}%s{458B74} äùç÷ï", GetName(killerid));
			OL_SendClientMessageToAll(Aquamarine, string);
			OL_SendClientMessageToAll(Lightaquamarine, "• äøéâä ùì äùç÷ï úæëä àåúëí á10000$");
			SetPlayerWantedLevel(killerid, 2);
		}
	    case 15:
	    {
			KS[killerid] = 3;
			format(string, sizeof(string), "òìä ìøîú îáå÷ùåú 3 òí 15 äøéâåú ììà îååú {FFFFFF}%s{458B74} äùç÷ï", GetName(killerid));
			OL_SendClientMessageToAll(Aquamarine, string);
			OL_SendClientMessageToAll(Lightaquamarine, "• äøéâä ùì äùç÷ï úæëä àåúëí á15000$");
			SetPlayerWantedLevel(killerid, 3);
		}
	    case 20:
	    {
			KS[killerid] = 4;
			format(string, sizeof(string), "òìä ìøîú îáå÷ùåú 4 òí 20 äøéâåú ììà îååú {FFFFFF}%s{458B74} äùç÷ï", GetName(killerid));
			OL_SendClientMessageToAll(Aquamarine, string);
			OL_SendClientMessageToAll(Lightaquamarine, "• äøéâä ùì äùç÷ï úæëä àåúëí á20000$");
			SetPlayerWantedLevel(killerid, 4);
		}
	    case 25:
	    {
			KS[killerid] = 5;
			format(string, sizeof(string), "òìä ìøîú îáå÷ùåú 5 òí 25 äøéâåú ììà îååú {FFFFFF}%s{458B74} äùç÷ï", GetName(killerid));
			OL_SendClientMessageToAll(Aquamarine, string);
			OL_SendClientMessageToAll(Lightaquamarine, "• äøéâä ùì äùç÷ï úæëä àåúëí á25000$");
			SetPlayerWantedLevel(killerid, 5);
		}
	    case 30:
	    {
			KS[killerid] = 6;
			format(string, sizeof(string), "òìä ìøîú äîáå÷ùåú äî÷ñéîàìéú 6 òí 30 äøéâåú ììà îååú {FFFFFF}%s{458B74} äùç÷ï", GetName(killerid));
			OL_SendClientMessageToAll(Aquamarine, string);
			OL_SendClientMessageToAll(Lightaquamarine, "• äøéâä ùì äùç÷ï úæëä àåúëí á30000$");
			SetPlayerWantedLevel(killerid, 6);
		}
	}

	switch(KS[playerid])
	{
	    case 1:
	    {
			format(string, sizeof(string), "å÷éáì 5000$ {FFFFFF}%s{FFCC33} äøâ àú äîáå÷ù {FFFFFF}%s{FFCC33} äùç÷ï", GetName(playerid), GetName(killerid));
			OL_SendClientMessageToAll(Whiteorange, string);
			SetPlayerWantedLevel(playerid, 0);
			AntiCheat_GivePlayerMoney(killerid, true, 5000);
			KS[playerid] = 0;
	    }
	    case 2:
	    {
			format(string, sizeof(string), "å÷éáì 10000$ {FFFFFF}%s{FFCC33} äøâ àú äîáå÷ù {FFFFFF}%s{FFCC33} äùç÷ï", GetName(playerid), GetName(killerid));
			OL_SendClientMessageToAll(Whiteorange, string);
			SetPlayerWantedLevel(playerid, 0);
			AntiCheat_GivePlayerMoney(killerid, true, 10000);
			KS[playerid] = 0;
	    }
	    case 3:
	    {
			format(string, sizeof(string), "å÷éáì 15000$ {FFFFFF}%s{FFCC33} äøâ àú äîáå÷ù {FFFFFF}%s{FFCC33} äùç÷ï", GetName(playerid), GetName(killerid));
			OL_SendClientMessageToAll(Whiteorange, string);
			SetPlayerWantedLevel(playerid, 0);
			AntiCheat_GivePlayerMoney(killerid, true, 15000);
			KS[playerid] = 0;
	    }
	    case 4:
	    {
			format(string, sizeof(string), "å÷éáì 20000$ {FFFFFF}%s{FFCC33} äøâ àú äîáå÷ù {FFFFFF}%s{FFCC33} äùç÷ï", GetName(playerid), GetName(killerid));
			OL_SendClientMessageToAll(Whiteorange, string);
			SetPlayerWantedLevel(playerid, 0);
			AntiCheat_GivePlayerMoney(killerid, true, 20000);
			KS[playerid] = 0;
	    }
	    case 5:
	    {
			format(string, sizeof(string), "å÷éáì 25000$ {FFFFFF}%s{FFCC33} äøâ àú äîáå÷ù {FFFFFF}%s{FFCC33} äùç÷ï", GetName(playerid), GetName(killerid));
			OL_SendClientMessageToAll(Whiteorange, string);
			SetPlayerWantedLevel(playerid, 0);
			AntiCheat_GivePlayerMoney(killerid, true, 25000);
			KS[playerid] = 0;
	    }
	    case 6:
	    {
			format(string, sizeof(string), "å÷éáì 30000$ {FFFFFF}%s{FFCC33} äøâ àú äîáå÷ù {FFFFFF}%s{FFCC33} äùç÷ï", GetName(playerid), GetName(killerid));
			OL_SendClientMessageToAll(Whiteorange, string);
			SetPlayerWantedLevel(playerid, 0);
			AntiCheat_GivePlayerMoney(killerid, true, 25000);
			KS[playerid] = 0;
	    }
	}
	TextDrawDestroy(Bar_Health[playerid]);
	Bar_Health[playerid] = TextDrawCreate(569.5, 68.5, "0%");
	TextDrawFont(Bar_Health[playerid], 3);
	TextDrawSetOutline(Bar_Health[playerid], 1);
	TextDrawLetterSize(Bar_Health[playerid], 0.199999, 0.555555);
	TextDrawColor(Bar_Health[playerid], 0xffffffff);
	
	
	if(DOF2_GetInt(File(killerid), "Kills") == DOF2_GetInt("/Setting.ini", "KillsLevel2")) return DOF2_SetInt(File(killerid), "Level", 2), SendClientMessage(killerid, Whitegrey, "îæì èåá òìéú øîä! äøîä äðåëçéú ùìê äéà: 2"), SendClientMessage(killerid, Aquamarine, "! îëéååï ùøîúê äðåëçéú äéà 2 - ìà ú÷áì éåúø ñàåï-àåó ùåèâàï åòåæé áäúçìä - úöèøê ì÷ðåú, áäöìçä"), SoundData(playerid, true, 1149);
	if(DOF2_GetInt(File(killerid), "Kills") == DOF2_GetInt("/Setting.ini", "KillsLevel3")) return DOF2_SetInt(File(killerid), "Level", 3), SendClientMessage(killerid, Whitegrey, "îæì èåá òìéú øîä! äøîä äðåëçéú ùìê äéà: 3"), SoundData(playerid, true, 1149);
	if(DOF2_GetInt(File(killerid), "Kills") == DOF2_GetInt("/Setting.ini", "KillsLevel4")) return DOF2_SetInt(File(killerid), "Level", 4), SendClientMessage(killerid, Whitegrey, "îæì èåá òìéú øîä! äøîä äðåëçéú ùìê äéà: 4"), SoundData(playerid, true, 1149);
	if(DOF2_GetInt(File(killerid), "Kills") == DOF2_GetInt("/Setting.ini", "KillsLevel5")) return DOF2_SetInt(File(killerid), "Level", 5), SendClientMessage(killerid, Whitegrey, "îæì èåá òìéú øîä! äøîä äðåëçéú ùìê äéà: 5"), SoundData(playerid, true, 1149);
	if(DOF2_GetInt(File(killerid), "Kills") == DOF2_GetInt("/Setting.ini", "KillsLevel6")) return DOF2_SetInt(File(killerid), "Level", 6), SendClientMessage(killerid, Whitegrey, "îæì èåá òìéú øîä! äøîä äðåëçéú ùìê äéà: 6"), SoundData(playerid, true, 1149);
	if(DOF2_GetInt(File(killerid), "Kills") == DOF2_GetInt("/Setting.ini", "KillsLevel7")) return DOF2_SetInt(File(killerid), "Level", 7), SendClientMessage(killerid, Whitegrey, "îæì èåá òìéú øîä! äøîä äðåëçéú ùìê äéà: 7"), SoundData(playerid, true, 1149);
	if(DOF2_GetInt(File(killerid), "Kills") == DOF2_GetInt("/Setting.ini", "KillsLevel8")) return DOF2_SetInt(File(killerid), "Level", 8), SendClientMessage(killerid, Whitegrey, "îæì èåá òìéú øîä! äøîä äðåëçéú ùìê äéà: 8"), SoundData(playerid, true, 1149);
	if(DOF2_GetInt(File(killerid), "Kills") == DOF2_GetInt("/Setting.ini", "KillsLevel9")) return DOF2_SetInt(File(killerid), "Level", 9), SendClientMessage(killerid, Whitegrey, "îæì èåá òìéú øîä! äøîä äðåëçéú ùìê äéà: 9"), SoundData(playerid, true, 1149);
	if(DOF2_GetInt(File(killerid), "Kills") == DOF2_GetInt("/Setting.ini", "KillsLevel10")) return DOF2_SetInt(File(killerid), "Level", 10), SendClientMessage(killerid, Whitegrey, "îæì èåá òìéú øîä! äøîä äðåëçéú ùìê äéà: 10"), SoundData(playerid, true, 1149);
 	return 1;
}

//------------------------------------------------------------------------------
	
public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
	if(!inAFK[playerid] && Logged[playerid] == true)
	{
	    KillTimer(AutoAFK_Timer[playerid]);
   		AutoAFK_Timer[playerid] = SetTimerEx("AutomaticAFK", 1000*60*2, 0, "d", playerid);
	}
	
    if(inAFK[playerid] == 1)
	{
  		    new string[256];
  		    SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} çæøú îîöá ìà ðîöà {FFFFFF}•••");
   			format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}çæø îîöá ìà ðîöà {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
			OL_SendClientMessageToAllEx(playerid, Grey, string);
			TextDrawHideForPlayer(playerid, Afk);
			TextDrawHideForPlayer(playerid, Afk1);
  			TogglePlayerControllable(playerid, 1);
			SetPlayerVirtualWorld(playerid, afkvirtual);
 			inAFK[playerid] = 0;
	}
	//new string[256];
    /*format(string, sizeof(string), "[%d] >> [%d]", oldkeys, newkeys);
	SendClientMessage(playerid, White, string);*/

	if(UsedVelocity[playerid] == 1 && IsKeyJustDown(KEY_FIRE, newkeys, oldkeys))
	{
	    if(IsPlayerInAnyVehicle(playerid))
	    {
			new Float:X, Float:Y, Float:Z;
			GetVehicleVelocity(GetPlayerVehicleID(playerid), X, Y, Z);
			SetVehicleVelocity(GetPlayerVehicleID(playerid), X*5.0, Y*5.0, Z);
		}
		else SendClientMessage(playerid, White, "! ìöåøê äàöú äøëá òìéê ìäéåú áúåê øëá");
	}

	if(IsKeyJustDown(KEY_FIRE, newkeys, oldkeys) && inRCXD[playerid]) KillTimer(RCXDTimer[playerid]), DestroyRCXD(playerid);

	if(IsPlayerInArea(playerid, 2140.9573, 1627.5625, 2147.5081, 1642.9884))
	{
		if(IsKeyJustDown(KEY_FIRE, newkeys, oldkeys))
		{
		    SetPlayerArmour(playerid, 0);
		    SetPlayerHealth(playerid, 0);
		    SendClientMessage(playerid, White, "! {FFFFFF}àñåø ìäøáéõ ìùç÷ðéí {FFD720}ááð÷");
		}
	}
	else if(IsPlayerInArea(playerid, 302.9533, -143.8248, 317.3446, -126.0462))
	{
		if(IsKeyJustDown(KEY_FIRE, newkeys, oldkeys))
		{
		    SetPlayerArmour(playerid, 0);
		    SetPlayerHealth(playerid, 0);
		    SendClientMessage(playerid, White, "! {FFFFFF}àñåø ìäøáéõ ìùç÷ðéí {FFD720}áçðåú äðù÷éí");
		}
	}
	
	if(PRESSED(KEY_SECONDARY_ATTACK))
	{
		for(new i=0; i<MAX_ATMS; i++)
		{
	   		format(AtmFile, sizeof(AtmFile), "ATMs/%d.ini", i);
		   	if(DOF2_FileExists(AtmFile) && PlayerToPoint(1, playerid, DOF2_GetFloat(AtmFile, "X"), DOF2_GetFloat(AtmFile, "Y"), DOF2_GetFloat(AtmFile, "Z")))
		   	{
 				if(GetCreditCard(playerid) == 0) return SendClientMessage(playerid, White, "àéï áøùåúê ëøèéñ àùøàé, ì÷ðééú ëøèéñ öåø ÷ùø òí äáð÷");
				ShowPlayerDialog(playerid, 10_2, DIALOG_STYLE_LIST, "îòøëú áð÷ - ëñôåîè", "äô÷ãú ëñó\nîùéëú ëñó", "áçø", "áéèåì");
			}
		}
	}
	
	if(PRESSED(KEY_SECONDARY_ATTACK))
	{
		for(new i=0; i<MAX_PROPS; i++)
		{
	   		format(PropFile, sizeof(PropFile), "Properties/%d.ini", i);
		   	if(DOF2_FileExists(PropFile) && PlayerToPoint(1, playerid, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z"))) ShowPlayerDialog(playerid, 15_2, DIALOG_STYLE_LIST, "îòøëú ðëñéí", "÷ðééú äðëñ\nîëéøú äðëñ", "áçø", "áéèåì");
		}
	}
	
	if(PRESSED(KEY_YES))
	{
		if(Survey_Have == 1)
		{
			if(Survey_Yes[playerid] == 1) return SendClientMessage(playerid, White, "äöáòú ëáø áñ÷ø æä");
			if(Survey_No[playerid] == 1) return SendClientMessage(playerid, White, "äöáòú ëáø áñ÷ø æä");
			Survey_Yes[playerid] = 1;
			Survey_AllYes ++;
			SendClientMessage(playerid, Green, "äöáòúê ð÷ìèä áîòøëú, úåãä");
		}
		return 1;
	}
	if(PRESSED(KEY_NO))
	{
		if(Survey_Have == 1)
		{
			if(Survey_Yes[playerid] == 1) return SendClientMessage(playerid, White, "äöáòú ëáø áñ÷ø æä");
			if(Survey_No[playerid] == 1) return SendClientMessage(playerid, White, "äöáòú ëáø áñ÷ø æä");
			Survey_No[playerid] = 1;
			Survey_AllNo ++;
			SendClientMessage(playerid, Green, "äöáòúê ð÷ìèä áîòøëú, úåãä");
		}
		return 1;
	}
	
	if(PRESSED(KEY_SECONDARY_ATTACK))
	{
		if(PlayerToPoint(1, playerid, 2146.4558, 1640.1334, 993.5761))
		{
            if(RobberyMission_In[playerid] == 1) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ôòåìä æå ùàúä áîùéîú ùãéãú äáð÷");
			ShowPlayerDialog(playerid, 1_0, DIALOG_STYLE_LIST, "îòøëú áð÷", "äô÷ãú ëñôéí\nîùéëú ëñôéí\nîàæï äñëåí\näòáøä áð÷àéú\náð÷ ÷ìàï\nëøèéñ àùøàé", "áçø", "áéèåì");
		}
		else if(PlayerToPoint(1, playerid, 313.7229, -133.6927, 999.6016))
		{
            if(RobberyMission_In[playerid] == 1) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ôòåìä æå ùàúä áîùéîú ùãéãú äáð÷");
			ShowPlayerDialog(playerid, 2_0, DIALOG_STYLE_LIST, "çðåú äðù÷éí", "÷ðééú ðù÷éí\nîëéøú ðù÷éí\nøùéîú äðù÷éí", "áçø", "áéèåì");
		}
	}
	
	if(PRESSED(KEY_SECONDARY_ATTACK))
	{
	    if(IsPlayerInPvpArea(playerid) == 1)
	    {
	        new Float:pHealth, Float:pArmour;
			GetPlayerHealth(playerid, pHealth);
			GetPlayerArmour(playerid, pArmour);
	        if(pHealth != 100 && pArmour != 100) return SendClientMessage(playerid, White, "àéðê éëåì ìöàú îàæåøé äìçéîä ëùàéï ìê çééí åîâï îìàéí");
		    new string[256];
			if(inWar[playerid])
			{
				SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} éöàú îàæåø äîìçîä {FFFFFF}•••");
				SpawnPlayer(playerid);
	   			SetPlayerInterior(playerid, 0);
				SetPlayerVirtualWorld(playerid, 0);
				SetPlayerHealth(playerid, BeforePvpHealth[playerid]);
				SetPlayerArmour(playerid, BeforePvpArmour[playerid]);
				SoundData(playerid, true, 1137);
				format(string, sizeof(string), "éöà îàæåø äîìçîä {FFFFFF}%s{4B351B} äùç÷ï", GetName(playerid));
				for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && inWar[i] == 1 && i != playerid) SendClientMessage(i, Brown, string);
				inWar[playerid] = 0;
			}
			else if(inDM[playerid])
			{
				SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} éöàú îàæåø äðù÷éí äëáãéí {FFFFFF}•••");
				SpawnPlayer(playerid);
	   			SetPlayerInterior(playerid, 0);
				SetPlayerVirtualWorld(playerid, 0);
				SetPlayerHealth(playerid, BeforePvpHealth[playerid]);
				SetPlayerArmour(playerid, BeforePvpArmour[playerid]);
				SoundData(playerid, true, 1137);
				format(string, sizeof(string), "éöà îàæåø äðù÷éí äëáãéí {FFFFFF}%s{4B351B} äùç÷ï", GetName(playerid));
				for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && inDM[i] == 1 && i != playerid) SendClientMessage(i, Brown, string);
				inDM[playerid] = 0;
			}
			else if(inHardcore[playerid])
			{
				SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} éöàú îàæåø ääàøã÷åø {FFFFFF}•••");
				SpawnPlayer(playerid);
	   			SetPlayerInterior(playerid, 0);
				SetPlayerVirtualWorld(playerid, 0);
				SetPlayerHealth(playerid, BeforePvpHealth[playerid]);
				SetPlayerArmour(playerid, BeforePvpArmour[playerid]);
				SoundData(playerid, true, 1137);
				format(string, sizeof(string), "éöà îàæåø ääàøã÷åø {FFFFFF}%s{4B351B} äùç÷ï", GetName(playerid));
				for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && inHardcore[i] == 1 && i != playerid) SendClientMessage(i, Brown, string);
				inHardcore[playerid] = 0;
			}
			else if(inFunbattle[playerid])
			{
				if(strcmp(GetPlayerAnimationName(playerid), "Jetpack_Idle", true) == 0)
				{
				}
				else
				{
					SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} \"éöàú îàæåø ä\"÷øá äëéó {FFFFFF}•••");
					SpawnPlayer(playerid);
	   				SetPlayerInterior(playerid, 0);
					SetPlayerVirtualWorld(playerid, 0);
					SetPlayerHealth(playerid, BeforePvpHealth[playerid]);
					SetPlayerArmour(playerid, BeforePvpArmour[playerid]);
					SoundData(playerid, true, 1137);
					format(string, sizeof(string), "\"éöà îàæåø ä\"÷øá äëéó {FFFFFF}%s{4B351B} äùç÷ï", GetName(playerid));
					for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && inFunbattle[i] == 1 && i != playerid) SendClientMessage(i, Brown, string);
					inFunbattle[playerid] = 0;
				}
			}
		}
	}
 	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerStateChange(playerid, newstate, oldstate)
{
	if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER)
	{
	    if(IsABike(GetPlayerVehicleID(playerid)))
	    {
			switch(GetPlayerSkin(playerid))
			{
		        #define SPAO{%0,%1,%2,%3,%4,%5} SetPlayerAttachedObject(playerid, 1, 18645, 2, (%0), (%1), (%2), (%3), (%4), (%5));
				case 0, 65, 74, 149, 208, 273:  SPAO{0.070000, 0.000000, 0.000000, 88.000000, 75.000000, 0.000000}
				case 1..6, 8, 14, 16, 22, 27, 29, 33, 41..49, 82..84, 86, 87, 119, 289: SPAO{0.070000, 0.000000, 0.000000, 88.000000, 77.000000, 0.000000}
				case 7, 10: SPAO{0.090000, 0.019999, 0.000000, 88.000000, 90.000000, 0.000000}
				case 9: SPAO{0.059999, 0.019999, 0.000000, 88.000000, 90.000000, 0.000000}
				case 11..13: SPAO{0.070000, 0.019999, 0.000000, 88.000000, 90.000000, 0.000000}
				case 15: SPAO{0.059999, 0.000000, 0.000000, 88.000000, 82.000000, 0.000000}
				case 17..21: SPAO{0.059999, 0.019999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 23..26, 28, 30..32, 34..39, 57, 58, 98, 99, 104..118, 120..131: SPAO{0.079999, 0.019999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 40: SPAO{0.050000, 0.009999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 50, 100..103, 148, 150..189, 222: SPAO{0.070000, 0.009999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 51..54: SPAO{0.100000, 0.009999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 55, 56, 63, 64, 66..73, 75, 76, 78..81, 133..143, 147, 190..207, 209..219, 221, 247..272, 274..288, 290..293: SPAO{0.070000, 0.019999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 59..62: SPAO{0.079999, 0.029999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 77: SPAO{0.059999, 0.019999, 0.000000, 87.000000, 82.000000, 0.000000}
				case 85, 88, 89: SPAO{0.070000, 0.039999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 90..97: SPAO{0.050000, 0.019999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 132: SPAO{0.000000, 0.019999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 144..146: SPAO{0.090000, 0.000000, 0.000000, 88.000000, 82.000000, 0.000000}
				case 220: SPAO{0.029999, 0.019999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 223, 246: SPAO{0.070000, 0.050000, 0.000000, 88.000000, 82.000000, 0.000000}
				case 224..245: SPAO{0.070000, 0.029999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 294: SPAO{0.070000, 0.019999, 0.000000, 91.000000, 84.000000, 0.000000}
				case 295: SPAO{0.050000, 0.019998, 0.000000, 86.000000, 82.000000, 0.000000}
				case 296..298: SPAO{0.064999, 0.009999, 0.000000, 88.000000, 82.000000, 0.000000}
				case 299: SPAO{0.064998, 0.019999, 0.000000, 88.000000, 82.000000, 0.000000}
		    }
		}
	}
	else RemovePlayerAttachedObject(playerid, 1);
	

    if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER)
    {
        new vehicleid = GetPlayerVehicleID(playerid);
        new Vehicle[30];
        format(Vehicle, sizeof(Vehicle), "~y~%s", VehicleNames[GetVehicleModel(vehicleid)-400]);
        GameTextForPlayer(playerid, Vehicle, 3000, 1);

        if(DOF2_GetInt(CarFile(vehicleid), "Lock") == 1 && strcmp(GetName(playerid), DOF2_GetString(CarFile(vehicleid), "Owner"), true) && GetAdminLevel(playerid) <= 4 && GetTempAdminLevel(playerid) <= 4 && GetSpyAdminLevel(playerid) <= 4)
		{
			RemovePlayerFromVehicle(playerid);
			SendClientMessage(playerid, Olive, "øëá àéùé æä ðòåì");
			return 1;
		}

		new string[256];
		
		if(!DOF2_FileExists(CarFile(vehicleid))) return 1;
        format(string, sizeof(string), "/Car -{33CCFF} àí áøöåðê ì÷ðåú øëá æä [{FFFFFF}%s${33CCFF}], ä÷ù/é", GetNum(DOF2_GetInt(CarFile(vehicleid), "Price")));
		if(DOF2_GetInt(CarFile(vehicleid), "Public") == 0 && DOF2_GetInt(CarFile(vehicleid), "Special") == 0 && DOF2_GetInt(CarFile(vehicleid), "Private") == 0 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 0) return SendClientMessage(playerid, White, string);
		else if(DOF2_GetInt(CarFile(vehicleid), "Public") == 0 && DOF2_GetInt(CarFile(vehicleid), "Special") == 1 && DOF2_GetInt(CarFile(vehicleid), "Private") == 0 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 0) return SendClientMessage(playerid, White, string);

		format(string, sizeof(string), "[àì øëáê äøâéì, ðñéòä ðòéîä {FFFFFF}%s{7CFC00} áøåê äùá äùç÷ï]", DOF2_GetString(CarFile(vehicleid), "Owner"));
		if(DOF2_GetInt(CarFile(vehicleid), "Special") == 0 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 1 && GetPlayerCar(playerid) == GetPlayerVehicleID(playerid)) return SendClientMessage(playerid, Aqua, string);

		format(string, sizeof(string), "{800080}[àì øëáê äîéåçã, ðñéòä ðòéîä {FFFFFF}%s{800080} áøåê äùá äùç÷ï]", DOF2_GetString(CarFile(vehicleid), "Owner"));
		if(DOF2_GetInt(CarFile(vehicleid), "Special") == 1 && DOF2_GetInt(CarFile(vehicleid), "Owned") == 1 && GetPlayerCar(playerid) == GetPlayerVehicleID(playerid)) return SendClientMessage(playerid, Purple, string);

        if(vehicleid == RobberyMission_Car && !RobberyMission_In[playerid])
		{
			RemovePlayerFromVehicle(playerid);
			SendClientMessage(playerid, Red, "{FFFFFF}[Robbery Mission]{AA3333} øëá æä ùééê ìîáöò îùéîú ùãéãú äáð÷ áìáã");
			return 1;
		}
	}
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerClickMap(playerid, Float:fX, Float:fY, Float:fZ)
{
    if(GetAdminLevel(playerid) >= 4 && AllowTeleport[playerid] == 1 && !inAFK[playerid]) SetPlayerPosFindZ(playerid, fX, fY, fZ);
    return 1;
}

//------------------------------------------------------------------------------

public UpdatePlayerData(playerid)
{
	new string[256];
	if(dini_Int(File(playerid),"Bank") < 10)
	{
	    TextDrawShowForPlayer(playerid, Bar_Money[playerid]);
		format(string, sizeof(string), "$0000000%i", dini_Int(File(playerid),"Bank"));
	}
	else if(dini_Int(File(playerid),"Bank") <= 99)
	{
	    TextDrawShowForPlayer(playerid, Bar_Money[playerid]);
		format(string, sizeof(string), "$000000%i", dini_Int(File(playerid),"Bank"));
	}
	else if(dini_Int(File(playerid),"Bank") <= 999)
	{
	    TextDrawShowForPlayer(playerid, Bar_Money[playerid]);
		format(string, sizeof(string), "$00000%i", dini_Int(File(playerid),"Bank"));
	}
	else if(dini_Int(File(playerid),"Bank") <= 9999)
	{
	    TextDrawShowForPlayer(playerid, Bar_Money[playerid]);
		format(string, sizeof(string), "$0000%i", dini_Int(File(playerid),"Bank"));
	}
	else if(dini_Int(File(playerid),"Bank") <= 99999)
	{
	    TextDrawShowForPlayer(playerid, Bar_Money[playerid]);
		format(string, sizeof(string), "$000%i", dini_Int(File(playerid),"Bank"));
	}
	else if(dini_Int(File(playerid),"Bank") <= 999999)
	{
	    TextDrawShowForPlayer(playerid, Bar_Money[playerid]);
		format(string, sizeof(string), "$00%i", dini_Int(File(playerid),"Bank"));
	}
	else if(dini_Int(File(playerid),"Bank") <= 9999999)
	{
	    TextDrawShowForPlayer(playerid, Bar_Money[playerid]);
		format(string, sizeof(string), "$0%i", dini_Int(File(playerid),"Bank"));
	}
	else if(dini_Int(File(playerid),"Bank") <= 99999999)
	{
	    TextDrawShowForPlayer(playerid, Bar_Money[playerid]);
		format(string, sizeof(string), "$%i", dini_Int(File(playerid),"Bank"));
	}
	TextDrawSetString(Bar_Money[playerid], string);
    if(!inGodmode[playerid])
    {
        new Float:pHealth;
    	GetPlayerHealth(playerid, pHealth);
    	if(pHealth > 1) format(string, sizeof(string),"%.0f%", pHealth), TextDrawSetString(Bar_Health[playerid], string);
	
    	new Float:pArmour;
    	GetPlayerArmour(playerid, pArmour);
		if(pArmour > 1)
		{
			if(HiddenBarArmour[playerid] == 1)
			{
				TextDrawShowForPlayer(playerid, Bar_Armour[playerid]);
				format(string, sizeof(string),"%.0f%", pArmour);
				TextDrawSetString(Bar_Armour[playerid], string);
				HiddenBarArmour[playerid] = 0;
			}
			else format(string, sizeof(string),"%.0f%", pArmour), TextDrawSetString(Bar_Armour[playerid], string);
		}
		else TextDrawHideForPlayer(playerid, Bar_Armour[playerid]), HiddenBarArmour[playerid] = 1;
	}
	else
	{
		TextDrawSetString(Bar_Health[playerid], "non");
		TextDrawSetString(Bar_Armour[playerid], "non");
	}
/*	new Float:VidHealth;
    GetVehicleHealth(GetPlayerVehicleID(playerid), VidHealth);
	if(VidHealth >= 250) format(string, sizeof(string),"~y~Health: ~w~%0.f ~y~Kph: ~w~%d", VidHealth, GetPlayerSpeed(playerid));
	else format(string, sizeof(string),"~y~Health: ~r~BOOM! ~y~Kph: ~w~%d", GetPlayerSpeed(playerid));
	if(IsPlayerInAnyVehicle(playerid)) TextDrawSetString(VehID[playerid], string);
	else TextDrawSetString(VehID[playerid], "] ~y~V~w~ehicle ~y~S~w~tatus ]");*/
	return 1;
}

//------------------------------------------------------------------------------
	
public UpdateVehicle3DText()
{
	new string[256];
 	new VCount = CreateVehicle(411, 0, 0, 0, 0, 0, 0, -1);
 	DestroyVehicle(VCount);
 	for(new i=1;i<MAX_VEHICLES;i++)
 		Delete3DTextLabel(CarText[i]);
 	for(new v=1; v<VCount; v++)
 	{

  		if(DOF2_GetInt(CarFile(v), "Special") == 0 && DOF2_GetInt(CarFile(v), "Public") == 0 && DOF2_GetInt(CarFile(v), "Private") == 0 && DOF2_GetInt(CarFile(v), "Owned") == 1)
		{
   			format(string, sizeof(string), "øëá øâéì\n{FFFFFF}%s{33AA33} ááòìåú", DOF2_GetString(CarFile(v), "Owner"));
			CarText[v] = Create3DTextLabel(string, Aqua, 0, 0, 2, 30, 0, 1);
   			Attach3DTextLabelToVehicle(CarText[v], v, 0, 0, 1);
   		}
  		if(DOF2_GetInt(CarFile(v), "Special") == 1 && DOF2_GetInt(CarFile(v), "Public") == 0 && DOF2_GetInt(CarFile(v), "Private") == 0 && DOF2_GetInt(CarFile(v), "Owned") == 1)
		{
      		format(string, sizeof(string), "øëá îéåçã\n{FFFFFF}%s{800080} ááòìåú", DOF2_GetString(CarFile(v), "Owner"));
			CarText[v] = Create3DTextLabel(string, Purple, 0, 0, 2, 30, 0, 1);
			Attach3DTextLabelToVehicle(CarText[v], v, 0, 0, 1);
  		}
  		if(DOF2_GetInt(CarFile(v), "Special") == 0 && DOF2_GetInt(CarFile(v), "Public") == 0 && DOF2_GetInt(CarFile(v), "Private") == 0 && DOF2_GetInt(CarFile(v), "Owned") == 0)
		{
			CarText[v] = Create3DTextLabel("øëá øâéì\n{33AA33}ôðåé ìøëéùä", Aqua, 0, 0, 2, 30, 0, 1);
		 	Attach3DTextLabelToVehicle(CarText[v], v, 0, 0, 1);
   		}
  		if(DOF2_GetInt(CarFile(v), "Special") == 1 && DOF2_GetInt(CarFile(v), "Public") == 0 && DOF2_GetInt(CarFile(v), "Private") == 0 && DOF2_GetInt(CarFile(v), "Owned") == 0)
		{
			CarText[v] = Create3DTextLabel("øëá îéåçã\n{800080}ôðåé ìøëéùä", Purple, 0, 0, 2, 30, 0, 1);
			Attach3DTextLabelToVehicle(CarText[v], v, 0, 0, 1);
  		}
  		if(DOF2_GetInt(CarFile(v), "Special") == 0 && DOF2_GetInt(CarFile(v), "Public") == 1 && DOF2_GetInt(CarFile(v), "Private") == 0 && DOF2_GetInt(CarFile(v), "Owned") == 0)
		{
			CarText[v] = Create3DTextLabel("øëá öéáåøé", Brightred, 0, 0, 2, 30, 0, 1);
			Attach3DTextLabelToVehicle(CarText[v], v, 0, 0, 1);
  		}
  		if(DOF2_GetInt(CarFile(v), "Special") == 0 && DOF2_GetInt(CarFile(v), "Public") == 0 && DOF2_GetInt(CarFile(v), "Private") == 1 && DOF2_GetInt(CarFile(v), "Owned") == 0)
		{
			CarText[v] = Create3DTextLabel("øëåù ôøèé", Bluesea, 0, 0, 2, 30, 0, 1);
			Attach3DTextLabelToVehicle(CarText[v], v, 0, 0, 1);
  		}
	}
 	KillTimer(Vehicle3DTimer);
 	return 1;
}

//------------------------------------------------------------------------------

public OnGameModeExit()
{
	TextDrawDestroy(Clock);
	for(new i=0; i<MAX_PLAYERS; i++)
	{
	 	TextDrawDestroy(PlayeridName[i]);
		TextDrawDestroy(Bar_Health[i]);
	//	TextDrawDestroy(VehID[i]);
		TextDrawDestroy(Bar_Armour[i]);
		TextDrawDestroy(Bar_Money[i]);
		RemovePlayerAttachedObject(i, 1); // Helmet
		RemovePlayerAttachedObject(i, 3); // Crash
	}
	TextDrawDestroy(Afk);
	TextDrawDestroy(Afk1);
	TextDrawDestroy(Clock);
	TextDrawDestroy(Logo);
//	TextDrawDestroy(OnlinePlayers);
//	TextDrawDestroy(Version);
 	for(new i;i<GetMaxPlayers();i++)
	{
		ActivityStatus[i]=0;
		if(ActivityStatus[i]==1)ExitAct(i);
	}
	for(new i=0;i<sizeof(obj)+1;i++)DestroyObject(obj[i]);
	ActOn=false;
	ActTP=false;
 	JoinWithCar=false;
	DOF2_SaveFile();
	return 1;
}

//------------------------------------------------------------------------------

public CountDown(cd)
{
	new string[256];
	format(string, sizeof(string), "%d", cd);
	GameTextForAll(string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		cdtimer = SetTimerEx("CountDown", 1000, 0, "%d", cd);
	}
	else
	{
		GameTextForAll("~r~Go! ~y~Go! ~g~Go!", 1000, 3);
		KillTimer(cdtimer);
		CountdownON = 0;
	}
	return 1;
}

public StopFreeze()
{
    for(new i=0; i<MAX_PLAYERS; i++)
	{
	    TogglePlayerControllable(i, 1);
		inFreeze[i] = 0;
		KillTimer(cdfreezetimer);
	}
	return 1;
}

//------------------------------------------------------------------------------

public SetPlayerGodmode()
{
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(!IsPlayerInAnyVehicle(i) && inGodmode[i] == 1) SetPlayerHealth(i, 10000), SetPlayerArmour(i, 10000);
		else if(IsPlayerInAnyVehicle(i) && inGodmode[i] == 1) SetPlayerHealth(i, 10000), SetPlayerArmour(i, 10000), SetVehicleHealth(GetPlayerVehicleID(i), 1000);
	}
	return 1;
}

public SetPlayerSpecOnOtherPlayer(playerid)
{
	if(inSpec[playerid] == 1)
	{
    	if(IsPlayerInAnyVehicle(pSpecting[playerid])) PlayerSpectateVehicle(playerid, GetPlayerVehicleID(pSpecting[playerid]));
		else PlayerSpectatePlayer(playerid, pSpecting[playerid]);
	}
	return 1;
}

//------------------------------------------------------------------------------

public AutomaticAFK(playerid)
{
	new string[256];
	if(AutomaticACT_In[playerid] == 1)
	{
		if(Minigun_ACT_In[playerid] == 1)
		{
			SetPlayerVirtualWorld(playerid, 0);
			SpawnPlayer(playerid);
			Minigun_ACT_In[playerid] = 0;
			AutomaticACT_In[playerid] = 0;
			Minigun_ACT_AllPlayers -=1;
		}
		if(RandomWeapon_ACT_In[playerid] == 1)
		{
			SetPlayerVirtualWorld(playerid, 0);
			SpawnPlayer(playerid);
			RandomWeapon_ACT_In[playerid] = 0;
			AutomaticACT_In[playerid] = 0;
			RandomWeapon_ACT_AllPlayers -=1;
		}
		if(Monster_ACT_In[playerid] == 1)
		{
			Monster_ACT_In[playerid] = 0;
			AutomaticACT_In[playerid] = 0;
			Monster_ACT_AllPlayers -=1;
		    SetPlayerVirtualWorld(playerid, 0);
		    SpawnPlayer(playerid);
	     	SetPlayerInterior(playerid, 0);
		    AutomaticACT_In[playerid] = 0;
		    Monster_ACT_In[playerid] = 0;
		}
		if(Karting_ACT_In[playerid] == 1)
		{
			Karting_ACT_In[playerid] = 0;
			AutomaticACT_In[playerid] = 0;
			Karting_ACT_AllPlayers -=1;
		    SetPlayerVirtualWorld(playerid, 0);
		    SpawnPlayer(playerid);
	     	SetPlayerInterior(playerid, 0);
		    AutomaticACT_In[playerid] = 0;
		    Karting_ACT_In[playerid] = 0;
	    	DisablePlayerRaceCheckpoint(playerid);
	      	CP_KartingActivity[playerid] = 0;
		}
		if(Drift_ACT_In[playerid] == 1)
		{
			Drift_ACT_In[playerid] = 0;
			AutomaticACT_In[playerid] = 0;
			Drift_ACT_AllPlayers -=1;
		    SetPlayerVirtualWorld(playerid, 0);
		    SpawnPlayer(playerid);
	     	SetPlayerInterior(playerid, 0);
		    AutomaticACT_In[playerid] = 0;
		    Drift_ACT_In[playerid] = 0;
	    	DisablePlayerRaceCheckpoint(playerid);
	      	CP_DriftActivity[playerid] = 0;
		}
		if(SawnoffKing_ACT_In[playerid] == 1)
		{
			SetPlayerVirtualWorld(playerid, 0);
			SpawnPlayer(playerid);
			SawnoffKing_ACT_In[playerid] = 0;
			SetPlayerInterior(playerid, 0);
			AutomaticACT_In[playerid] = 0;
			SawnoffKing_ACT_AllPlayers -=1;
		}
	}
	
	if(MoneyMission_In[playerid] == 1)
	{
		MoneyMission_Start = 0;
		MoneyMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äëñó");
 		SendClientMessage(playerid, White, "ðëðñú àåèåîèéú ìîöá ìà ðîöà -");
 		KillTimer(MoneyMission_Timer_1);
 		DestroyVehicle(MoneyMission_Bike);
 		KillTimer(MoneyMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Money Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äëñó {FFFFFF}%s{AA3333} äùç÷ï", GetName(MoneyMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðëðñ àåèåîèéú ìîöá ìà ðîöà -");
		RemovePlayerAttachedObject(playerid, 2); // MoneyBag
	}
	if(RaceMission_In[playerid] == 1)
	{
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_RaceMission[playerid] = 0;
		RaceMission_Start = 0;
		RaceMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äîéøåõ");
 		SendClientMessage(playerid, White, "ðëðñú àåèåîèéú ìîöá ìà ðîöà -");
 		DestroyVehicle(RaceMission_Car);
 		KillTimer(RaceMission_Timer_1);
 		KillTimer(RaceMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Race Mission]{AA3333} ìà äöìéç ìäâéò ì÷å äñéåí ùì îùéîú äîéøåõ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RaceMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðëðñ àåèåîèéú ìîöá ìà ðîöà -");
   		SetPlayerVirtualWorld(playerid, 0);
	}
	if(RobberyMission_In[playerid] == 1)
	{
		RobberyMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú ùãéãú äáð÷");
 		SendClientMessage(playerid, White, "ðëðñú àåèåîèéú ìîöá ìà ðîöà -");
 		DestroyVehicle(RobberyMission_Car);
 		KillTimer(RobberyMission_Timer_1);
 		KillTimer(RobberyMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Robbery Mission]{AA3333} ìà äöìéç ìùãåã àú äáð÷ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RobberyMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðëðñ àåèåîèéú ìîöá ìà ðîöà -");
	}
	if(StuntsMission_In[playerid] == 1)
	{
		StuntsMission_Start = 0;
		StuntsMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äñèàðèéí");
 		SendClientMessage(playerid, White, "ðëðñú àåèåîèéú ìîöá ìà ðîöà -");
 		DestroyVehicle(StuntsMission_Car);
 		KillTimer(StuntsMission_Timer_1);
 		KillTimer(StuntsMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Stunts Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äñèàðèéí {FFFFFF}%s{AA3333} äùç÷ï", GetName(StuntsMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðëðñ àåèåîèéú ìîöá ìà ðîöà -");
	 	SetPlayerVirtualWorld(playerid, 0);
	}
	if(DriftMission_In[playerid] == 1)
	{
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_DriftMission[playerid] = 0;
		DriftMission_Start = 0;
		DriftMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äãøéôè");
 		SendClientMessage(playerid, White, "ðëðñú àåèåîèéú ìîöá ìà ðîöà -");
 		DestroyVehicle(DriftMission_Car);
 		KillTimer(DriftMission_Timer_1);
 		KillTimer(DriftMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Drift Mission]{AA3333} ìà äöìéç ìäâéò ì÷å äñéåí ùì îùéîú äãøéôè {FFFFFF}%s{AA3333} äùç÷ï", GetName(DriftMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðëðñ àåèåîèéú ìîöá ìà ðîöà -");
   		SetPlayerVirtualWorld(playerid, 0);
	}
	if(ParachutingMission_In[playerid] == 1)
	{
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_ParachutingMission[playerid] = 0;
		ParachutingMission_Start = 0;
		ParachutingMission_In[playerid] = 0;
 		SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äöðéçä");
 		SendClientMessage(playerid, White, "ðëðñú àåèåîèéú ìîöá ìà ðîöà -");
 		KillTimer(ParachutingMission_Timer_1);
 		KillTimer(ParachutingMission_Timer_2);
 		format(string, sizeof(string), "{FFFFFF}[Parachuting Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äöðéçä {FFFFFF}%s{AA3333} äùç÷ï", GetName(ParachutingMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ðëðñ àåèåîèéú ìîöá ìà ðîöà -");
	 	SetPlayerVirtualWorld(playerid, 0);
	}
	
	if(!inWar[playerid] && !inDM[playerid] && !inHardcore[playerid] && !inFunbattle[playerid] && !InBattle[playerid] && !ActivityStatus[playerid] && !inSpec[playerid])
	{
		SendClientMessage(playerid, Grellow, "{FFFFFF}[SYSTEM]{ADFF2F} òáøú ìîöá ìà ðîöà àåèåîèé òì éãé äîòøëú {FFFFFF}•••");
		format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}òáø ìîöá ìà ðîöà àåèåîèé òì éãé äîòøëú {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
		OL_SendClientMessageToAllEx(playerid, Grey, string);
		TextDrawShowForPlayer(playerid, Afk);
		TextDrawShowForPlayer(playerid, Afk1);
		TogglePlayerControllable(playerid, 0);
		afkvirtual = GetPlayerVirtualWorld(playerid);
		SetPlayerVirtualWorld(playerid, 1);
		inAFK[playerid] = 1;
	}
	return 1;
}

//------------------------------------------------------------------------------

public AutomaticUnmute(playerid)
{
	new string[256];
	SendClientMessage(playerid, Green, "ùåçøøú îäùú÷úê áàåôï àåèåîèé");
	format(string, sizeof(string), "•.•.•.• ùåçøø îäùú÷úå áàåôï àåèåîèé {FFFFFF}%s{33AA33} äùç÷ï •.•.•.•", GetName(playerid));
	OL_SendClientMessageToAll(Green, string);
	KillTimer(MuteTimer[playerid]);
	KillTimer(MuteTimer1[playerid]);
	DOF2_SetInt(File(playerid), "inMute", 0);
	inMute[playerid] = 0;
	DOF2_SetInt(File(playerid), "MuteTime", 0);
	DOF2_SetString(File(playerid), "MuteReason", "None");
	DOF2_SetString(File(playerid), "MuteAdmin", "None");
	return 1;
}

public AutomaticMainusMuteTime(playerid) return DOF2_SetInt(File(playerid), "MuteTime", DOF2_GetInt(File(playerid), "MuteTime")-1);

//------------------------------------------------------------------------------

public AutomaticUnjail(playerid)
{
	new string[256];
	SendClientMessage(playerid, Green, "ùåçøøú îäëìà áàåôï àåèåîèé");
	format(string, sizeof(string), "•.•.•.• ùåçøø îäëìà áàåôï àåèåîèé {FFFFFF}%s{33AA33} äùç÷ï •.•.•.•", GetName(playerid));
	OL_SendClientMessageToAll(Green, string);
	SetPlayerVirtualWorld(playerid, 0);
	SpawnPlayer(playerid);
	KillTimer(JailTimer[playerid]);
	KillTimer(JailTimer1[playerid]);
	DOF2_SetInt(File(playerid), "inJail", 0);
	inJail[playerid] = 0;
	DOF2_SetInt(File(playerid), "JailTime", 0);
	DOF2_SetString(File(playerid), "JailReason", "None");
	DOF2_SetString(File(playerid), "JailAdmin", "None");
 	return 1;
}

public AutomaticMainusJailTime(playerid) return DOF2_SetInt(File(playerid), "JailTime", DOF2_GetInt(File(playerid), "JailTime")-1);

//------------------------------------------------------------------------------

public SurveyTimer()
{
	new string[256];
	OL_SendClientMessageToAll(Green, ":äñ÷ø ðñâø áàåôï àåèåîèé òì éãé äîòøëú, äúåöàåú äí");
	format(string, sizeof(string), "{FFFFFF}%d{33AA33} :îñôø äàðùéí ùäöáéòå áòã", Survey_AllYes);
	OL_SendClientMessageToAll(Green, string);
	format(string, sizeof(string), "{FFFFFF}%d{33AA33} :îñôø äàðùéí ùäöáéòå ðâã", Survey_AllNo);
	OL_SendClientMessageToAll(Green, string);
	if(Survey_AllYes == Survey_AllNo) OL_SendClientMessageToAll(Green, "îñôø ëîåú ääöáòåú ùååä, àéï úåöàä îåçìèú");
	format(string, sizeof(string), "äøåá äöáéò áòã áäôøù ùì {FFFFFF}%d{33AA33} äöáòåú", (Survey_AllYes-Survey_AllNo));
	if(Survey_AllYes > Survey_AllNo) OL_SendClientMessageToAll(Green, string);
	format(string, sizeof(string), "äøåá äöáéò ðâã áäôøù ùì {FFFFFF}%d{33AA33} äöáòåú", (Survey_AllNo-Survey_AllYes));
	if(Survey_AllNo > Survey_AllYes) OL_SendClientMessageToAll(Green, string);
	for(new i=1; i<MAX_PLAYERS; i++) Survey_Yes[i] = 0, Survey_No[i] = 0;
	Survey_AllYes = 0;
	Survey_AllNo = 0;
    Survey_Have = 0;
    KillTimer(Survey_Timer);
 	return 1;
}

//------------------------------------------------------------------------------

public CheckPlayerWarnings()
{
	for(new i=0; i<MAX_PLAYERS; i++) if(GetPlayerWarnings(i) == 3)
	{
	    new string[256];
		format(string, sizeof(string), "ðçñí òì éãé îòøëú äàæäøåú {FFFFFF}%s{33AA33} äùç÷ï", GetName(i));
		OL_SendClientMessageToAll(Green, string);
		OL_SendClientMessageToAll(Green, "({FFFFFF}äéðê áòì 3 àæäøåú{33AA33})");
		DOF2_SetInt(File(i), "Warnings", 0);
		DOF2_SetInt(File(i), "Ban", DOF2_GetInt(File(i), "Ban")+1);
		getdate(Year, Month, Day);
		gettime(Hour, Minute, Second);
		DOF2_SetInt(File(i), "HaveBan", 1);
		DOF2_SetString(File(i), "ReasonBan", "äéðê áòì 3 àæäøåú");
		DOF2_SetString(File(i), "AdminBan", "Warnings System");
		DOF2_SetString(File(i), "SupporterBan", "None");
		DOF2_SetInt(File(i), "YearBan", Year);
		DOF2_SetInt(File(i), "MonthBan", Month);
		DOF2_SetInt(File(i), "DayBan", Day);
		DOF2_SetInt(File(i), "HourBan", Hour);
		DOF2_SetInt(File(i), "MinuteBan", Minute);
		DOF2_SetInt(File(i), "SecondBan", Second);
		DOF2_SetInt("/BansIP.ini", GetIP(i), 1);
	 	disreason[i] = 1;
	 	kixkx[i] = SetTimerEx("kickx", 1000, 1, "d", i);
	}
 	return 1;
}

//------------------------------------------------------------------------------

public CheckTimeBlockedWord()
{
	for(new i=0; i<MAX_PLAYERS; i++) if(TimeBlockedWord[i] >= 3)
	{
		new string[256];
		format(string, sizeof(string), "ðáòè òì éãé îòøëú äöéðæåø {FFFFFF}%s{33AA33} äùç÷ï", GetName(i));
		OL_SendClientMessageToAll(Green, string);
		OL_SendClientMessageToAll(Green, "({FFFFFF}ùéîåù áéåúø î3 îçøåæåú çñåîåú ìùéîåù{33AA33})");
		DOF2_SetInt(File(i), "Kick", DOF2_GetInt(File(i), "Kick")+1);
		kixkx[i] = SetTimerEx("kickx", 1000, 1, "d", i);
	}
 	return 1;
}

//------------------------------------------------------------------------------

public RemovePhantom(playerid)
{
	SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} éöàú îîöá ôàðèåí {FFFFFF}•••");
	inPhantom[playerid] = 0;
	SetPlayerVirtualWorld(playerid, 0);
	SetPlayerWeather(playerid, 0);
	SetTimerEx("CheckBetweenPhantom", 1000*60*2, 0, "d", playerid);
   	SoundData(playerid, true, 1137);
	return 1;
}

public CheckBetweenPhantom(playerid) return CheckPhantom[playerid] = 0;

//------------------------------------------------------------------------------

public DestroyRCXD(playerid)
{
    new Float:X, Float:Y, Float:Z;
    GetVehiclePos(RCXD[playerid], X, Y, Z);
    SetPlayerPosEx(playerid, RCXDposX[playerid], RCXDposY[playerid], RCXDposZ[playerid], RCXDposA[playerid]);
  	DestroyVehicle(RCXD[playerid]);
    CreateExplosion(X, Y, Z, 6, 100.0);
    inRCXD[playerid] = 0;
   	HaveRCXD[playerid] = 0;
	return 1;
}

//------------------------------------------------------------------------------

public AccessLess()
{
	for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerInArea(i, -2382.287, 2207.118, -2113.696, 2522.421) && !GetVipLevel(i)) SetPlayerPosEx(i, -2424.6399, 2445.8030, 13.2374, 179.8548), SetCameraBehindPlayer(i);
	return 1;
}

//------------------------------------------------------------------------------

public ReadRules(playerid)
{
	if(DOF2_GetInt(File(playerid), "ReadRules") == 0)
	{
		for(new i=0; i<100; i++) SendClientMessage(playerid, White, "");
		SendClientMessage(playerid, White, "[Fxp DM] {FFD720}áøåê äáà ìùøú, á÷øåá úåöâ áôðéê øùéîú äçå÷éí ùì äùøú");
		SendClientMessage(playerid, Gold, "àñåø ìäôø ùåí çå÷ ùéåôéò ìôðéê");
		SendClientMessage(playerid, Gold, "øùéîú äçå÷éí ðåòãä ìîðåò àé-ðòéîåú áòúéã");
		SendClientMessage(playerid, Brightred, "! àé éãéòú äçå÷ -{FF0000} àéðä ôåúøú îòåðù{FFFFFF}");
		DOF2_SetInt(File(playerid), "ReadRules", 1);
		KillTimer(RulesTimer[playerid]);
		RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*10*1, 0, "d", playerid);
		return 1;
	}
	if(DOF2_GetInt(File(playerid), "ReadRules") == 1)
	{
		for(new i=0; i<100; i++) SendClientMessage(playerid, White, "");
		SendClientMessage(playerid, Gold, "#1 - {FFFFFF}çì àéñåø îåçìè ìäùúîù áùôä áåèä áùøú");
		SendClientMessage(playerid, Gold, "àéï ì÷ìì/ìäòìéá/ìôâåò áëì àçã îùç÷ðé ä÷äéìä òì ø÷ò ãú/âæò/îéï");
		SendClientMessage(playerid, Gold, "àéï ìðñåú ìò÷åó àú îòøëú äöéðæåø áùåí ãøê ùäéà");
		SendClientMessage(playerid, Gold, "Punishment: {FFFFFF}Mute {FFD720}> {FFFFFF}Kick {FFD720}> {FFFFFF}ReadRules {FFD720}> {FFFFFF}Ban");
		SendClientMessage(playerid, Brightred, "- àðà äîúï {FFFFFF}10{FF0000} ùðéåú ì÷øéàú äçå÷ äáà -");
		DOF2_SetInt(File(playerid), "ReadRules", 2);
		KillTimer(RulesTimer[playerid]);
		RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*10*1, 0, "d", playerid);
		return 1;
	}
	if(DOF2_GetInt(File(playerid), "ReadRules") == 2)
	{
		for(new i=0; i<100; i++) SendClientMessage(playerid, White, "");
		SendClientMessage(playerid, Gold, "#2 - {FFFFFF}çåáä ìäùîò ìäåøàåúéå ùì äîðäìéí áùøú");
		SendClientMessage(playerid, Gold, "éù ìäùîò ìãáøéå ùì äîðäì âí àí æä ìà îåöà çï áòéðéëí");
		SendClientMessage(playerid, Gold, "áîéãä åäéðëí çåùáéí ùäîðäì èåòä, éù ìäùîò ìå áëì î÷øä - åìúòã àú äîàåøò");
		SendClientMessage(playerid, Gold, "Punishment: {FFFFFF}Jail {FFD720}> {FFFFFF}Kick {FFD720}> {FFFFFF}ReadRules {FFD720}> {FFFFFF}Ban");
		SendClientMessage(playerid, Brightred, "- àðà äîúï {FFFFFF}10{FF0000} ùðéåú ì÷øéàú äçå÷ äáà -");
