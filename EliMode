  		AdminCommands_Logs(string);
		//----------------------------------------------------------------------
 		return 1;
 	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Getcar", true) == 0 || strcmp(cmd, "/Gc", true) == 0)
	{
		if(GetAdminLevel(playerid) < 5 && GetSpyAdminLevel(playerid) < 5 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì5");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Getcar(GC) [400-611 | part of name] :öåøú äùéîåù");
		if(IsNumeric(tmp))
		{
			v = strval(tmp);
			if(v < 400 || v > 611) return SendClientMessage(playerid, White, "äàééãé ùì äøëá ùäæðú ùâåé");
			GetPlayerPos(playerid, X, Y, Z);
			GetPlayerFacingAngle(playerid, A);
			vid = CreateVehicle(v, X, Y, Z, A, -1, -1, 50000);
			PutPlayerInVehicle(playerid, vid, 0);
			LinkVehicleToInterior(GetPlayerVehicleID(playerid), GetPlayerInterior(playerid));
 			format(string, sizeof(string), "àìééê {FFFFFF}%s{33AA33} ùéâøú àú äøëá", VehicleNames[GetVehicleModel(vid)-400]);
    		SendClientMessage(playerid, Green, string);
		}
		else
		{
		    if(GetVehicleID(tmp) == -1) return SendClientMessage(playerid, White, "ìà ðîöàå äúàîåú òí äøëáéí áùøú");
			else
			{
				GetPlayerPos(playerid, X, Y, Z);
				GetPlayerFacingAngle(playerid, A);
				vid = CreateVehicle(GetVehicleID(tmp), X, Y, Z, A, -1, -1, 50000);
				PutPlayerInVehicle(playerid, vid, 0);
				LinkVehicleToInterior(GetPlayerVehicleID(playerid), GetPlayerInterior(playerid));
	 			format(string, sizeof(string), "àìééê {FFFFFF}%s{33AA33} ùéâøú àú äøëá", VehicleNames[GetVehicleModel(vid)-400]);
	    		SendClientMessage(playerid, Green, string);
	    	}
	    }
		return 1;
	}
