			any = strval(inputtext);
			if(any > AntiCheat_GetPlayerMoney(playerid)) return SendClientMessage(playerid, White, "àéï ìê àú ñëåí äëñó ùöééðú");
			format(string, sizeof(string), "[Charge Sent] %s[#%d] to %s[#%d]: %s$", GetName(playerid), playerid, GetName(pClicked[playerid]), pClicked[playerid], GetNum(any));
			SendClientMessage(playerid, Orange, string);
			format(string, sizeof(string), "[Charge Received] %s[#%d] to %s[#%d]: %s$", GetName(playerid), playerid, GetName(pClicked[playerid]), pClicked[playerid], GetNum(any));
			SendClientMessage(pClicked[playerid], Yellow, string);
			AntiCheat_GivePlayerMoney(playerid, false, any);
			AntiCheat_GivePlayerMoney(pClicked[playerid], true, any);
 			SoundData(playerid, true, 1085);
		}
	}
	if(dialogid == 18_3)
	{
