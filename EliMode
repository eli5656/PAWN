	GetObjectRot(objectid,RX,RY,RZ);
 	MoveObject(objectid,X,Y,Z, Speed);
	return 1;
}

stock IsPlayerInPvpArea(playerid)
{
	if(inWar[playerid] == 1) return 1;
	else if(inDM[playerid] == 1) return 1;
	else if(inHardcore[playerid] == 1) return 1;
	else if(inFunbattle[playerid] == 1) return 1;
	else return 0;
}

stock IsPlayerInMission(playerid)
{
	if(MoneyMission_In[playerid] == 1) return 1;
	else if(RaceMission_In[playerid] == 1) return 1;
	else if(RobberyMission_In[playerid] == 1) return 1;
	else if(StuntsMission_In[playerid] == 1) return 1;
	else if(DriftMission_In[playerid] == 1) return 1;
	else if(ParachutingMission_In[playerid] == 1) return 1;
	else return 0;
}

public AntiRcon()
{
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(IsPlayerAdmin(i))
		{
			if(strcmp(GetName(i), "L1or", true) == 0 || strcmp(GetName(i), "DisPlay", true) == 0)
			{
	 		}
		 	else
			{
	 			new string[256];
		 		format(string, sizeof(string), "ðçñí òì éãé îòøëú äùøú {FFFFFF}%s{33AA33} äùç÷ï", GetName(i));
				OL_SendClientMessageToAll(Green, string);
				OL_SendClientMessageToAll(Green, "({FFFFFF}ôøéöä ìø÷åï äùøú{33AA33})");
				DOF2_SetInt(File(i), "Ban", DOF2_GetInt(File(i), "Ban")+1);
				getdate(Year, Month, Day);
				gettime(Hour, Minute, Second);
				DOF2_SetInt(File(i), "HaveBan", 1);
				DOF2_SetString(File(i), "ReasonBan", "ôøéöä ìø÷åï äùøú");
				DOF2_SetString(File(i), "AdminBan", "AntiRcon System");
				DOF2_SetString(File(i), "SupporterBan", "None");
				DOF2_SetInt(File(i), "YearBan", Year);
				DOF2_SetInt(File(i), "MonthBan", Month);
				DOF2_SetInt(File(i), "DayBan", Day);
				DOF2_SetInt(File(i), "HourBan", Hour);
				DOF2_SetInt(File(i), "MinuteBan", Minute);
				DOF2_SetInt(File(i), "SecondBan", Second);
				DOF2_SetInt("/BansIP.ini", GetIP(i), 1);
			 	disreason[i] = 1;
				kixkx[i] = SetTimerEx("kickx", 1000, 1, "d", i);
			}
		}
	}
	return 1;
}

////////////////////////////// AntiCheat - Money ///////////////////////////////
stock AntiCheat_GetPlayerMoney(playerid) return AntiCheat_Money[playerid];

stock AntiCheat_GivePlayerMoney(playerid, bool:Plus, amount)
{
	GivePlayerMoney(playerid, Plus == true? (amount):(-amount));
	if(Plus == true) AntiCheat_Money[playerid] +=amount;
	else if(Plus == false) AntiCheat_Money[playerid] -=amount;
	return 1;
}

stock AntiCheat_SetPlayerMoney(playerid, oldamount)
{
	if(oldamount < AntiCheat_GetPlayerMoney(playerid))
	{
		newamount = AntiCheat_GetPlayerMoney(playerid)-oldamount;
		AntiCheat_GivePlayerMoney(playerid, false, newamount);
	}
	else if(oldamount > AntiCheat_GetPlayerMoney(playerid))
	{
		newamount = oldamount-AntiCheat_GetPlayerMoney(playerid);
		AntiCheat_GivePlayerMoney(playerid, true, newamount);
	}
	return 1;
}
