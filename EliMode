	  		tmp1 = strtok(cmdtext, idx);
			if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Radius GiveWeapon(Rad Gw) [weapon] [ammu] :öåøú ùéîåù");
			weap = strval(tmp1);
			if(weap < 1 || weap > 46) return SendClientMessage(playerid, White, "äàééãé ðù÷ ùöééðú ùâåé");
			if(weap == 38 || weap == 39 || weap == 40 || weap == 43 || weap == 44 || weap == 45) return SendClientMessage(playerid, White, "äðù÷ ùöééðú çñåí ìùéîåù");
			tmp2 = strtok(cmdtext, idx);
			if(!strlen(tmp2)) return SendClientMessage(playerid, White, "/Radius GiveWeapon(Rad Gw) [weapon] [ammu] :öåøú ùéîåù");
			ammu = strval(tmp2);
			if(ammu < 1 || ammu > 9999) return SendClientMessage(playerid, White, "èååç äëãåøéí äåà áéï 1 ì9999");
