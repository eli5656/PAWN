					Delete3DTextLabel(PropText[i]);
					DestroyPickup(PropPickup[i]);
					format(string, sizeof(string), "{AA3333}%s {FFFFFF}:ùí äðëñ\n{AA3333}%s {FFFFFF}:áòìéí\n{AA3333}%s$ {FFFFFF}:îçéø äðëñ\n{AA3333}%s$ {FFFFFF}:øååç ì%s ã÷åú\n{AA3333}ùáî÷ìãú {FFFFFF}\"F\"{AA3333} ìùéîåù ìçõ òì î÷ù", DOF2_GetString(PropFile, "Name"), DOF2_GetString(PropFile, "Owner"), GetNum(DOF2_GetInt(PropFile, "Value")), GetNum(DOF2_GetInt(PropFile, "Amount")), GetNum(DOF2_GetInt("Properties/Setting.ini", "AmountMinutes")));
					PropText[i] = Create3DTextLabel(string, Gold, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z")+0.5, 60.0, 0, 1);
					PropPickup[i] = CreatePickup(1273, 1, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z"), 0);
					SendClientMessage(playerid, Red, "! îëøú ðëñ æä áäöìçä");
					return 1;
			   	}
			}
		}
	}

	if(dialogid == 16_0 && response)
	{
		if(listitem == 0) return ShowPlayerDialog(playerid, 16_1, DIALOG_STYLE_LIST, "îòøëú ùéðåé äåãòåú äùøú - äåãòú ëðéñä", "ùéðåé äåãòä\nîçé÷ú ääåãòä", "áçø", "çæåø");
 		if(listitem == 1) return ShowPlayerDialog(playerid, 16_2, DIALOG_STYLE_LIST, "îòøëú ùéðåé äåãòåú äùøú - äåãòä îñôø 1", "ùéðåé äåãòä\nîçé÷ú ääåãòä", "áçø", "çæåø");
       	if(listitem == 2) return ShowPlayerDialog(playerid, 16_3, DIALOG_STYLE_LIST, "îòøëú ùéðåé äåãòåú äùøú - äåãòä îñôø 2", "ùéðåé äåãòä\nîçé÷ú ääåãòä", "áçø", "çæåø");
  		if(listitem == 3) return ShowPlayerDialog(playerid, 16_4, DIALOG_STYLE_LIST, "îòøëú ùéðåé äåãòåú äùøú - äåãòä îñôø 3", "ùéðåé äåãòä\nîçé÷ú ääåãòä", "áçø", "çæåø");
  		if(listitem == 4) return ShowPlayerDialog(playerid, 16_5, DIALOG_STYLE_LIST, "îòøëú ùéðåé äåãòåú äùøú - äåãòä îñôø 4", "ùéðåé äåãòä\nîçé÷ú ääåãòä", "áçø", "çæåø");
  		if(listitem == 5) return ShowPlayerDialog(playerid, 16_6, DIALOG_STYLE_LIST, "îòøëú ùéðåé äåãòåú äùøú - äåãòä îñôø 5", "ùéðåé äåãòä\nîçé÷ú ääåãòä", "áçø", "çæåø");
  		if(listitem == 6)
		{
			SendClientMessage(playerid, Blue, "{0000BB}-_-_-_-_-_-_-_-_-_-_- {FFFFFF}äåãòåú äùøú - úöåâä î÷ãéîä{0000BB} -_-_-_-_-_-_-_-_-_-_-");
			format(string, sizeof(string), "{0087FF}[#1] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message1"));
			if(DOF2_IsSet("/AutomaticMessage.ini", "Message1")) SendClientMessage(playerid, White, string);
			format(string, sizeof(string), "{0087FF}[#2] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message2"));
			if(DOF2_IsSet("/AutomaticMessage.ini", "Message2")) SendClientMessage(playerid, White, string);
			format(string, sizeof(string), "{0087FF}[#3] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message3"));
			if(DOF2_IsSet("/AutomaticMessage.ini", "Message3")) SendClientMessage(playerid, White, string);
			format(string, sizeof(string), "{0087FF}[#4] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message4"));
			if(DOF2_IsSet("/AutomaticMessage.ini", "Message4")) SendClientMessage(playerid, White, string);
			format(string, sizeof(string), "{0087FF}[#5] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message5"));
			if(DOF2_IsSet("/AutomaticMessage.ini", "Message5")) SendClientMessage(playerid, White, string);
			SendClientMessage(playerid, Blue, "-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-");
		}
	}
	if(dialogid == 16_1)
	{
		if(!response) ShowPlayerDialog(playerid, 16_0, DIALOG_STYLE_LIST, "îòøëú ùéðåé äåãòåú äùøú", "äåãòú ëðéñä\näåãòä îñôø 1\näåãòä îñôø 2\näåãòä îñôø 3\näåãòä îñôø 4\näåãòä îñôø 5\núöåâä î÷ãéîä", "áçø", "áéèåì");
