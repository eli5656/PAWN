	if(strcmp(cmd, "/Ban", true) == 0 || strcmp(cmd, "/B", true) == 0)
	{
		if(GetAdminLevel(playerid) < 3 && GetTempAdminLevel(playerid) < 3 && GetSpyAdminLevel(playerid) < 3) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì3");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Ban(B) [playerid] [reason] :öåøú ùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
  		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(strcmp(GetName(id), "L1or", true) == 0 || strcmp(GetName(id), "DisPlay", true) == 0 || strcmp(GetName(id), "Speed[i]", true) == 0) return SendClientMessage(playerid, White, "àéðê éëåì ìçñåí ùç÷ï æä îäùøú");
