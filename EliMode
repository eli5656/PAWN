				format(string, sizeof(string), "äô÷éã àú ëì äñëåí ùäéä òìéå ááð÷ ä÷ìàï %s äùç÷ï", GetName(playerid));
				for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && i != playerid && DOF2_GetInt(File(i), "InClan") == 1 && strcmp(DOF2_GetString(File(playerid), "Clan"), DOF2_GetString(File(i), "Clan"), true) == 0) SendClientMessage(i, Bluesea, string);
				SendClientMessage(playerid, Yellow, "äô÷ãú ááð÷ ä÷ìàï àú ëì äëñó ùäéä òìéê");
			}
		}
	}
