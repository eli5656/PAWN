			{
				if(IsPlayerConnected(i))
				{
					if(Logged[i] == true)
					{
		 				if(DOF2_GetInt(File(i), "InGang") == 1)
				 		{
						 	if(strcmp(DOF2_GetString(File(playerid), "Gang"), DOF2_GetString(File(i), "Gang"), true) == 0)
						 	{
								SendClientMessage(i, Whitebrown, string);
							}
						}
					}
				}
			}
			DOF2_Unset(File(id), "Gang");
   			format(string, sizeof(string), "äòéó àåúê îäëðåôééä {FFFFFF}%s{8B5A2B} îðäì äëðåôééä", GetName(playerid));
			SendClientMessage(id, Whitebrown, string);
			format(GangFile, sizeof(GangFile), "Gangs/%s.ini", DOF2_GetString(File(playerid), "Gang"));
			DOF2_SetInt(GangFile, "Players", DOF2_GetInt(GangFile, "Players")-1);
   			return 1;
		}
	    if(strcmp(tmp, "Chat", true) == 0)
		{
	        if(DOF2_GetInt(File(playerid), "InGang") == 0) return SendClientMessage(playerid, White, "àúä ìà ðîöà áëðåôééä");
			if(DOF2_GetInt(File(playerid), "GangLevel") != 2) return SendClientMessage(playerid, White, "àúä ìà îðäì äëðåôééä");
