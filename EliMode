			pinterior = GetPlayerInterior(id);
 			format(string, sizeof(string), "{FFFFFF}%s{33AA33} ùéâø àåúê ìùç÷ï {FFFFFF}%s{33AA33} äîðäì", GetName(id1), GetName(playerid));
