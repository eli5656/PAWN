			if(GetSupportLevel(id) > GetSupportLevel(playerid)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì úåîê áøîú úîéëä ùååä å/àå âáåä îùìê");
