//------------------------------------------------------------------------------

public RunningAntiCheat()
{
	new string[256];
 	for(new i=0; i<MAX_PLAYERS; i++) if(Logged[i] == true && GetPlayerMoney(i) > AntiCheat_GetPlayerMoney(i)) return PlayerMoney[i] = GetPlayerMoney(i)-AntiCheat_GetPlayerMoney(i), GivePlayerMoney(i, -PlayerMoney[i]);

	for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerInAnyVehicle(i) && GetPlayerSpeed(i) >= 300 && GetVipLevel(i) == 0)
	{
	    if(!GetAdminLevel(i))
		{
			format(string, sizeof(string), "{33AA33}ðáòè òì éãé îòøëú äàðèé ö'éè {FFFFFF}%s {33AA33}äùç÷ï", GetName(i));
			OL_SendClientMessageToAll(Green, string);
			OL_SendClientMessageToAll(Green, "({33AA33}ùéîåù áö'éè {FFFFFF} ñôéã äà÷{33AA33})");
			Kick(i);
		}
		else if(GetAdminLevel(i) >= 1) SendClientMessage(i, Green, "{FFFFFF}[AntiCheat]{33AA33} àðà äôñ÷ ìäùúîù áîééãé áö'éè ñôéã äà÷");
	}
	
	for(new i=0; i<MAX_PLAYERS; i++)
	{
	    format(string, sizeof(string), "ðáòè òì éãé îòøëú äâáìú äôéðâ {FFFFFF}%s{33AA33} äùç÷ï", GetName(i));
		if(Logged[i] == true && GetPlayerPing(i) >= 500) OL_SendClientMessageToAll(Green, string), OL_SendClientMessageToAll(Green, "({FFFFFF}ëîåú äôéðâ ùìê òå÷ôú àú äëîåú äôéðâ äî÷ñéîàìéú áùøú{33AA33})"), Kick(i);
	}
	return 1;
}

//------------------------------------------------------------------------------

public AutomaticMessage()
{
	new string[256];
//	for(new i=0; i<100; i++) OL_SendClientMessageToAll(White, "");
	OL_SendClientMessageToAll(Blue, "{0000BB}-_-_-_-_-_-_-_-_-_-_- {FFFFFF}äåãòåú äùøú {0000BB}-_-_-_-_-_-_-_-_-_-_-");
	format(string, sizeof(string), "{0087FF}[#1] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message1"));
	if(DOF2_IsSet("/AutomaticMessage.ini", "Message1")) OL_SendClientMessageToAll(White, string);
	format(string, sizeof(string), "{0087FF}[#2] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message2"));
	if(DOF2_IsSet("/AutomaticMessage.ini", "Message2")) OL_SendClientMessageToAll(White, string);
	format(string, sizeof(string), "{0087FF}[#3] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message3"));
	if(DOF2_IsSet("/AutomaticMessage.ini", "Message3")) OL_SendClientMessageToAll(White, string);
	format(string, sizeof(string), "{0087FF}[#4] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message4"));
	if(DOF2_IsSet("/AutomaticMessage.ini", "Message4")) OL_SendClientMessageToAll(White, string);
	format(string, sizeof(string), "{0087FF}[#5] -{FFFFFF} %s", DOF2_GetString("/AutomaticMessage.ini", "Message5"));
	if(DOF2_IsSet("/AutomaticMessage.ini", "Message5")) OL_SendClientMessageToAll(White, string);
	OL_SendClientMessageToAll(Blue, "-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-");
	return 1;
}

//------------------------------------------------------------------------------

public UpdateAtm()
{
	if(AtmNum == 255) return KillTimer(AtmTimer);
	AtmNum++;
	format(AtmFile, sizeof(AtmFile), "ATMs/%d.ini", AtmNum);
	new string[256];
	if(DOF2_FileExists(AtmFile))
	{
		format(string, sizeof(string), "{FFFF00}ATM ID: {FFFFFF}#%d\n{FFFF00}áøåëéí äáàéí{FFFFFF} ìëñôåîè\n{FFFF00}ùáî÷ìãú {FFFFFF}\"F\"{FFFF00} ìùéîåù ìçõ òì î÷ù", DOF2_GetInt(AtmFile, "ID"));
		AtmText[AtmNum] = Create3DTextLabel(string, Gold, DOF2_GetFloat(AtmFile, "X"), DOF2_GetFloat(AtmFile, "Y"), DOF2_GetFloat(AtmFile, "Z")+0.5, 60.0, 0, 1);
		AtmPickup[AtmNum] = CreatePickup(1274, 1, DOF2_GetFloat(AtmFile, "X"), DOF2_GetFloat(AtmFile, "Y"), DOF2_GetFloat(AtmFile, "Z"), 0);
	}
	return 1;
}

//------------------------------------------------------------------------------

public UpdateProperties()
{
	if(PropNum == 255) return KillTimer(PropTimer);
	PropNum++;
	format(PropFile, sizeof(PropFile), "Properties/%d.ini", PropNum);
	new string[256];
	if(DOF2_FileExists(PropFile))
	{
		format(string, sizeof(string), "{AA3333}%s {FFFFFF}:ùí äðëñ\n{AA3333}%s {FFFFFF}:áòìéí\n{AA3333}%s$ {FFFFFF}:îçéø äðëñ\n{AA3333}%s$ {FFFFFF}:øååç ì%s ã÷åú\n{AA3333}ùáî÷ìãú {FFFFFF}\"F\"{AA3333} ìùéîåù ìçõ òì î÷ù", DOF2_GetString(PropFile, "Name"), DOF2_GetString(PropFile, "Owner"), GetNum(DOF2_GetInt(PropFile, "Value")), GetNum(DOF2_GetInt(PropFile, "Amount")), GetNum(DOF2_GetInt("Properties/Setting.ini", "AmountMinutes")));
		PropText[PropNum] = Create3DTextLabel(string, Gold, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z")+0.5, 60.0, 0, 1);
		PropPickup[PropNum] = CreatePickup(1273, 1, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z"), 0);
	}
	return 1;
}

public UpdatePropertiesOwnerAmount()
{
	for(new i=0; i<MAX_PROPS; i++)
	{
		format(PropFile, sizeof(PropFile), "Properties/%d.ini", i);
		if(strcmp(DOF2_GetString(PropFile, "Owner"), "None", true) == 0)
		{
		}
		else
		{
			if(DOF2_FileExists(PropFile) && DOF2_FileExists(File(DOF2_GetInt(PropFile, "Owner"))) && inAFK[DOF2_GetInt(PropFile, "Owner")] == 0 && Logged[DOF2_GetInt(PropFile, "Owner")] == true) DOF2_SetInt(File(DOF2_GetInt(PropFile, "Owner")), "Bank", DOF2_GetInt(File(DOF2_GetInt(PropFile, "Owner")), "Bank")+DOF2_GetInt(PropFile, "Amount"));
		}
	}
	KillTimer(PropAmountTimer);
	PropAmountTimer = SetTimer("UpdatePropertiesOwnerAmount", 1000*60*DOF2_GetInt("Properties/Setting.ini", "AmountMinutes"), 1);
	return 1;
}

//------------------------------------------------------------------------------

public UpdateVehiclesComponent()
{
	new string[256];
   	new VCount = CreateVehicle(411, 0, 0, 0, 0, 0, 0, -1);
	DestroyVehicle(VCount);
	for(new v=1; v<VCount; v++)
	{
	    format(string, sizeof(string), "%s", DOF2_GetString(CarFile(v), "Plate"));
		if(DOF2_GetInt(CarFile(v), "HavePlate") == 1) SetVehicleNumberPlate(v, string);
	
		if(DOF2_GetInt(CarFile(v), "HaveColor") == 1) if(DOF2_GetInt(CarFile(v), "Color1") >= 0 && DOF2_GetInt(CarFile(v), "Color2") >= 0) ChangeVehicleColor(v, DOF2_GetInt(CarFile(v), "Color1"), DOF2_GetInt(CarFile(v), "Color2"));

	    if(DOF2_GetInt(CarFile(v), "Nitro") == 1) AddVehicleComponent(v, 1009);
	    else if(DOF2_GetInt(CarFile(v), "Nitro") == 2) AddVehicleComponent(v, 1008);
	    else if(DOF2_GetInt(CarFile(v), "Nitro") == 3) AddVehicleComponent(v, 1010);

		if(DOF2_GetInt(CarFile(v), "Wheels") == 1073) AddVehicleComponent(v, 1073);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1074) AddVehicleComponent(v, 1074);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1075) AddVehicleComponent(v, 1075);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1076) AddVehicleComponent(v, 1076);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1077) AddVehicleComponent(v, 1077);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1078) AddVehicleComponent(v, 1078);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1079) AddVehicleComponent(v, 1079);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1080) AddVehicleComponent(v, 1080);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1081) AddVehicleComponent(v, 1081);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1082) AddVehicleComponent(v, 1082);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1083) AddVehicleComponent(v, 1083);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1084) AddVehicleComponent(v, 1084);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1085) AddVehicleComponent(v, 1085);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1096) AddVehicleComponent(v, 1096);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1097) AddVehicleComponent(v, 1097);
		else if(DOF2_GetInt(CarFile(v), "Wheels") == 1098) AddVehicleComponent(v, 1098);

		if(DOF2_GetInt(CarFile(v), "Hydralics") > 0) AddVehicleComponent(v, 1087);
		
		if(DOF2_GetInt(CarFile(v), "Neon") == 1)
		{
	       	ANeon[v] = CreateObject(18652, 0, 0, 0, 0, 0, 0, 500);
	        AttachObjectToVehicle(ANeon[v],v, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
  		}
		else if(DOF2_GetInt(CarFile(v), "Neon") == 2)
		{
  			ANeon[v] = CreateObject(18648, 0, 0, 0, 0, 0, 0, 500);
	    	AttachObjectToVehicle(ANeon[v],v, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
     	}
		else if(DOF2_GetInt(CarFile(v), "Neon") == 3)
		{
  			ANeon[v] = CreateObject(18647, 0, 0, 0, 0, 0, 0, 500);
  			AttachObjectToVehicle(ANeon[v],v, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
  		}
		else if(DOF2_GetInt(CarFile(v), "Neon") == 4)
		{
			ANeon[v] = CreateObject(18649, 0, 0, 0, 0, 0, 0, 500);
			AttachObjectToVehicle(ANeon[v],v, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
		}
		else if(DOF2_GetInt(CarFile(v), "Neon") == 5)
		{
  			ANeon[v] = CreateObject(18650, 0, 0, 0, 0, 0, 0, 500);
  			AttachObjectToVehicle(ANeon[v],v, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
		}
		else if(DOF2_GetInt(CarFile(v), "Neon") == 6)
		{
  			ANeon[v] = CreateObject(18651, 0, 0, 0, 0, 0, 0, 500);
  			AttachObjectToVehicle(ANeon[v],v, 0.0, 0.0, -0.70, 0.0, 0.0, 0.0);
  		}

		if(DOF2_GetInt(CarFile(v), "BikeNitro") == 1)
		{
			BikeNitroObj[v] = CreateObject(18693,0,0,10000000,0,0,250);
			AttachObjectToVehicle(BikeNitroObj[v],v, 0.2, -1.2, -1.45, 0.0, 0.5, 0.5);
		}
	}
	KillTimer(CompoentTimer);
	return 1;
}

//------------------------------------------------------------------------------

public CheckExistsVehicles()
{
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		format(carfile, sizeof(carfile), "Cars/%d.ini", DOF2_GetInt(File(i), "CarID"));
	    if(!DOF2_FileExists(carfile)) DOF2_SetInt(File(i), "HaveCar", 0), DOF2_SetString(File(i), "CarID", "-1");
	}
	return 1;
}
		
//------------------------------------------------------------------------------

public LoadCarsFiles()
{
	new VCount = CreateVehicle(411, 0, 0, 0, 0, 0, 0, -1);
	DestroyVehicle(VCount);
	for(new v=1; v<VCount; v++)
	{
		format(carfile, sizeof(carfile), "Cars/%d.ini", v);
		if(!DOF2_FileExists(carfile))
		{
			new Float:X, Float:Y, Float:Z, Float:A;
			GetVehiclePos(v, X, Y, Z);
			GetVehicleZAngle(v, A);
			DOF2_CreateFile(carfile);
			DOF2_SetInt(carfile, "ID", v);
			DOF2_SetInt(carfile, "Model", GetVehicleModel(v));
			DOF2_SetString(carfile, "Owner", "None");
			DOF2_SetInt(carfile, "Owned", 0);
			DOF2_SetInt(carfile, "Lock", 0);
			DOF2_SetInt(carfile, "Special", 0);
			DOF2_SetInt(carfile, "Private", 0);
			DOF2_SetInt(carfile, "Public", 0);
			DOF2_SetInt(carfile, "HavePlate", 0);
			DOF2_SetInt(carfile, "Plate", 0);
			DOF2_SetInt(carfile, "HaveColor", 0);
			DOF2_SetInt(carfile, "Color1", 0);
			DOF2_SetInt(carfile, "Color2", 0);
			DOF2_SetInt(carfile, "Nitro", 0);
			DOF2_SetInt(carfile, "Hydralics", 0);
			DOF2_SetInt(carfile, "Wheels", 0);
			DOF2_SetInt(carfile, "Neon", 0);
			DOF2_SetInt(carfile, "BikeNitro", 0);
			DOF2_SetInt(carfile, "HaveSpawn", 0);
			DOF2_SetFloat(carfile, "vX", X);
			DOF2_SetFloat(carfile, "vY", Y);
			DOF2_SetFloat(carfile, "vZ", Z);
			DOF2_SetFloat(carfile, "vA", A);

			#define GVA GetVehicleModel

			/*==============================================================
			==================== [ Air Vehicles ] ==========================
			==============================================================*/

			//////////////////////////////////////////////////////////////// Airplanes
			if(GVA(v) == 592 || GVA(v) == 577 || GVA(v) == 511 || GVA(v) == 512 || GVA(v) == 593 ||
			GVA(v) == 520 || GVA(v) == 553 || GVA(v) == 476 || GVA(v) == 519 || GVA(v) == 460 ||
			GVA(v) == 513) DOF2_SetInt(carfile, "Price", 1500000);

			//////////////////////////////////////////////////////////////// Helicopters
			if(GVA(v) == 548 || GVA(v) == 425 || GVA(v) == 417 || GVA(v) == 487 || GVA(v) == 488 ||
			GVA(v) == 497 || GVA(v) == 563 || GVA(v) == 447 || GVA(v) == 469) DOF2_SetInt(carfile, "Price", 1000000);

			////////////////////////////////////////////////////////////////

			/*==============================================================
			==================== [ Land Vehicles ] =========================
			==============================================================*/

			//////////////////////////////////////////////////////////////// Bikes
			if(GVA(v) == 509 || GVA(v) == 481 || GVA(v) == 510 || GVA(v) == 462 || GVA(v) == 448 ||
			GVA(v) == 581 || GVA(v) == 416 || GVA(v) == 523 || GVA(v) == 521 ||
			GVA(v) == 463 || GVA(v) == 586 || GVA(v) == 468 || GVA(v) == 471 || GVA(v) == 461) DOF2_SetInt(carfile, "Price", 100000);
			
			if(GVA(v) == 522) DOF2_SetInt(carfile, "Price", 5000000);
			//////////////////////////////////////////////////////////////// Convertibles
			if(GVA(v) == 480 || GVA(v) == 533 || GVA(v) == 439 || GVA(v) == 555) DOF2_SetInt(carfile, "Price", 300000);
			
			//////////////////////////////////////////////////////////////// Industrial
			if(GVA(v) == 499 || GVA(v) == 422 || GVA(v) == 482 || GVA(v) == 498 || GVA(v) == 609 ||
			GVA(v) == 524 || GVA(v) == 578 || GVA(v) == 455 || GVA(v) == 403 || GVA(v) == 414 ||
			GVA(v) == 482 || GVA(v) == 443 || GVA(v) == 514 || GVA(v) == 600 || GVA(v) == 413 ||
			GVA(v) == 515 || GVA(v) == 440 || GVA(v) == 543 || GVA(v) == 605 || GVA(v) == 459 ||
			GVA(v) == 531 || GVA(v) == 408 || GVA(v) == 552 || GVA(v) == 478 || GVA(v) == 456 ||
			GVA(v) == 554) DOF2_SetInt(carfile, "Price", 450000);

			//////////////////////////////////////////////////////////////// Lowriders
			if(GVA(v) == 536 || GVA(v) == 575 || GVA(v) == 534 || GVA(v) == 567 || GVA(v) == 535 ||
			GVA(v) == 566 || GVA(v) == 576 || GVA(v) == 412) DOF2_SetInt(carfile, "Price", 250000);

			//////////////////////////////////////////////////////////////// Off Road
			if(GVA(v) == 468 || GVA(v) == 424 || GVA(v) == 573 || GVA(v) == 579 || GVA(v) == 400 ||
			GVA(v) == 500 || GVA(v) == 444 || GVA(v) == 556 || GVA(v) == 557 || GVA(v) == 470 ||
			GVA(v) == 489 || GVA(v) == 505 || GVA(v) == 495 || GVA(v) == 568) DOF2_SetInt(carfile, "Price", 150000);

			//////////////////////////////////////////////////////////////// Public Service Vehicles
			if(GVA(v) == 416 || GVA(v) == 433 || GVA(v) == 431 || GVA(v) == 438 || GVA(v) == 437 ||
			GVA(v) == 523 || GVA(v) == 427 || GVA(v) == 490 || GVA(v) == 528 || GVA(v) == 407 ||
			GVA(v) == 544 || GVA(v) == 596 || GVA(v) == 598 || GVA(v) == 597 || GVA(v) == 599 ||
			GVA(v) == 432 || GVA(v) == 601 || GVA(v) == 420) DOF2_SetInt(carfile, "Price", 350000);

			//////////////////////////////////////////////////////////////// Saloons
			if(GVA(v) == 445 || GVA(v) == 504 || GVA(v) == 401 || GVA(v) == 518 || GVA(v) == 427 ||
			GVA(v) == 542 || GVA(v) == 507 || GVA(v) == 562 || GVA(v) == 585 || GVA(v) == 419 ||
			GVA(v) == 526 || GVA(v) == 604 || GVA(v) == 466 || GVA(v) == 492 || GVA(v) == 474 ||
			GVA(v) == 546 || GVA(v) == 517 || GVA(v) == 410 || GVA(v) == 551 || GVA(v) == 516 ||
			GVA(v) == 467 || GVA(v) == 426 || GVA(v) == 436 || GVA(v) == 547 || GVA(v) == 405 ||
			GVA(v) == 580 || GVA(v) == 550 || GVA(v) == 549 || GVA(v) == 540 || GVA(v) == 560 ||
			GVA(v) == 491 || GVA(v) == 529 || GVA(v) == 421 || GVA(v) == 527) DOF2_SetInt(carfile, "Price", 200000);

			//////////////////////////////////////////////////////////////// Sport Vehicles
			if( GVA(v) == 602 || GVA(v) == 429 || GVA(v) == 496 || GVA(v) == 402 || GVA(v) == 411 ||
			GVA(v) == 415 || GVA(v) == 451 || GVA(v) == 541 || GVA(v) == 589 || GVA(v) == 587 ||
			GVA(v) == 565 || GVA(v) == 494 || GVA(v) == 502 || GVA(v) == 503 || GVA(v) == 559 ||
			GVA(v) == 603 || GVA(v) == 475 || GVA(v) == 506 || GVA(v) == 558 || GVA(v) == 477) DOF2_SetInt(carfile, "Price", 3000000);

			//////////////////////////////////////////////////////////////// Station Wagons
			if(GVA(v) == 418 || GVA(v) == 404 || GVA(v) == 479 || GVA(v) == 458 || GVA(v) == 561) DOF2_SetInt(carfile, "Price", 225000);

			////////////////////////////////////////////////////////////////

			/*==============================================================
			==================== [ Sea Vehicles ] ==========================
			==============================================================*/

			//////////////////////////////////////////////////////////////// Boats
			if(GVA(v) == 472 || GVA(v) == 473 || GVA(v) == 493 || GVA(v) == 595 || GVA(v) == 484 ||
			GVA(v) == 430 || GVA(v) == 453 || GVA(v) == 452 || GVA(v) == 446 || GVA(v) == 454) DOF2_SetInt(carfile, "Price", 500000);

			////////////////////////////////////////////////////////////////

			/*==============================================================
			=============== [ Miscellaneous Vehicles ] =====================
			==============================================================*/

			//////////////////////////////////////////////////////////////// Trailers
			if(GVA(v) == 435 || GVA(v) == 450 || GVA(v) == 561 || GVA(v) == 606 || GVA(v) == 607 ||
			GVA(v) == 610 || GVA(v) == 569 || GVA(v) == 590 || GVA(v) == 584 || GVA(v) == 570 ||
		 	GVA(v) == 608 || GVA(v) == 611) DOF2_SetInt(carfile, "Price", 0);

			//////////////////////////////////////////////////////////////// Unique Vehicles
			if(GVA(v) == 485 || GVA(v) == 538 || GVA(v) == 475 || GVA(v) == 483 || GVA(v) == 508 ||
			GVA(v) == 532 || GVA(v) == 486 || GVA(v) == 406 || GVA(v) == 530 || GVA(v) == 537 ||
		 	GVA(v) == 434 || GVA(v) == 545 || GVA(v) == 588 || GVA(v) == 571 || GVA(v) == 572 ||
		 	GVA(v) == 423 || GVA(v) == 442 || GVA(v) == 428 || GVA(v) == 409 || GVA(v) == 574 ||
		 	GVA(v) == 449 || GVA(v) == 525 || GVA(v) == 583 || GVA(v) == 539 || GVA(v) == 457) DOF2_SetInt(carfile, "Price", 125000);

			//////////////////////////////////////////////////////////////// Unique Vehicles
			if(GVA(v) == 485 || GVA(v) == 538 || GVA(v) == 475 || GVA(v) == 483 || GVA(v) == 508 ||
			GVA(v) == 532 || GVA(v) == 486 || GVA(v) == 406 || GVA(v) == 530 || GVA(v) == 537 ||
		 	GVA(v) == 434 || GVA(v) == 545 || GVA(v) == 588 || GVA(v) == 571 || GVA(v) == 572 ||
		 	GVA(v) == 423 || GVA(v) == 442 || GVA(v) == 428 || GVA(v) == 409 || GVA(v) == 574 ||
		 	GVA(v) == 449 || GVA(v) == 525 || GVA(v) == 583 || GVA(v) == 539) DOF2_SetInt(carfile, "Price", 125000);
		 	
			//////////////////////////////////////////////////////////////// RC Vehicles
			if(GVA(v) == 441 || GVA(v) == 464 || GVA(v) == 465 || GVA(v) == 501 || GVA(v) == 564 ||
			GVA(v) == 594) DOF2_SetInt(carfile, "Price", 50000);
			////////////////////////////////////////////////////////////////

			/*==============================================================
			============== [ Exclusive Vehicles Prices ] ===================
			==============================================================*/

			////////////////////////////////////////////////////////////////
			/*if(GVA(v) == 434) DOF2_SetInt(carfile, "Price", 2000000);
			if(GVA(v) == 506) DOF2_SetInt(carfile, "Price", 7000000);
