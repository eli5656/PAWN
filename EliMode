	       		AntiCheat_GivePlayerMoney(playerid, true, 50000);
	       		DestroyObject(BikeNitroObj[vehicleid]);
			  	SendClientMessage(playerid, Olive, "îëøú àú äùéôåø ðéèøå ùäéä áàåôðåòê - ÷éáìú çöé îñëåí ä÷ðééä");
				return 1;
			}
		}
	}
	if(dialogid == 6_17)
	{
		if(!response) ShowPlayerDialog(playerid, 6_1, DIALOG_STYLE_LIST, "îòøëú øëáéí - ùéôåøéí", "Nitro\nHydralics\nWheels\nColor\nNeon\nBike Nitro\nCar Spawn", "áçø", "çæåø");
		else
		{
        	if(listitem == 0)
			{
				if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, White, "àúä ìà ðîöà áøëá");
				if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, White, "àúä ìà äðäâ áøëá");
				if(DOF2_GetInt(CarFile(vehicleid), "Public") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
				if(DOF2_GetInt(CarFile(vehicleid), "Private") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
				if(vehicleid != GetPlayerCar(playerid)) return SendClientMessage(playerid, White, "øëá àéùé æä àéðå ùééê ìê");
				GetVehiclePos(vehicleid, X, Y, Z);
				GetVehicleZAngle(vehicleid, A);
				DOF2_SetInt(CarFile(vehicleid), "HaveSpawn", 1);
				DOF2_SetFloat(CarFile(vehicleid), "vX", X);
				DOF2_SetFloat(CarFile(vehicleid), "vY", Y);
				DOF2_SetFloat(CarFile(vehicleid), "vZ", Z);
				DOF2_SetFloat(CarFile(vehicleid), "vA", A);
			  	SendClientMessage(playerid, Olive, "ùîøú àú îé÷åí æä ìøëáê äàéùé");
				return 1;
			}
        	if(listitem == 1)
			{
				if(!DOF2_GetInt(CarFile(vehicleid), "HaveSpawn")) return SendClientMessage(playerid, White, "ìøëá àéùé æä àéï îé÷åí ùîåø");
				DOF2_SetInt(CarFile(vehicleid), "HaveSpawn", 0);
			  	SendClientMessage(playerid, Olive, "áéèìú àú ùîéøú äîé÷åí ìøëáê äàéùé");
				return 1;
			}
		}
	}
	
	if(dialogid == 7_0 && response)
	{
       	if(listitem == 0)
		{
			if(BlockPM[playerid] == 0) SendClientMessage(playerid, White, "[BlockPM] {33CCFF}äôòìú àú àôùøåú çñéîú ÷áìú äåãòåú ôøèéåú îîùúîùéí áùøú"), BlockPM[playerid] = 1;
			else if(BlockPM[playerid] == 1) SendClientMessage(playerid, White, "[BlockPM] {33CCFF}ëéáéú àú àôùøåú çñéîú ÷áìú äåãòåú ôøèéåú îîùúîùéí áùøú"), BlockPM[playerid] = 0;
	   		SoundData(playerid, true, 1137);
			return 1;
		}
       	if(listitem == 1)
		{
			if(GetLogIn(playerid) == 0) SendClientMessage(playerid, White, "[LogIP] {33CCFF}äôòìú àú äàôùøåú ùì äúçáøåú ìôé àééôé"), DOF2_SetInt(File(playerid), "LogIn", 1);
			else if(GetLogIn(playerid) == 1) SendClientMessage(playerid, White, "[LogIP] {33CCFF}ëéáéú àú äàôùøåú ùì äúçáøåú ìôé àééôé"), DOF2_SetInt(File(playerid), "LogIn", 0);
	   		SoundData(playerid, true, 1137);
			return 1;
		}
       	if(listitem == 2)
		{
			if(GetSkin(playerid) == 0) SendClientMessage(playerid, White, "[Skin] {33CCFF}ùîøú àú äñ÷éï äðåëçé ùìê"), DOF2_SetInt(File(playerid), "HaveSkin", 1), DOF2_SetInt(File(playerid), "Skin", GetPlayerSkin(playerid));
			else if(GetSkin(playerid) == 1) SendClientMessage(playerid, White, "[Skin] {33CCFF}áéèìú àú ùîéøú àú äñ÷éï äðåëçé ùìê"), DOF2_SetInt(File(playerid), "HaveSkin", 0);
	   		SoundData(playerid, true, 1137);
   			return 1;
		}
       	if(listitem == 3) return ShowPlayerDialog(playerid, 7_1, DIALOG_STYLE_INPUT, "îòøëú ùéðåé äâãøåú ùç÷ï - äâãøú ùòä úîéãéú", "àðà øùåí àú äùòä ùáøöåðê ìùéí ëùòä úîéãéú", "÷áò ùòä", "çæåø");
       	if(listitem == 4)
		{
			new year, month, day, hour, minute, second;
			getdate(year, month, day);
			gettime(hour, minute, second);
			format(string, sizeof(string), "[Time&Date] {33CCFF}%d/%d/%d - %d:%d:%d", day, month, year, hour, minute, second);
			SendClientMessage(playerid, White, string);
	   		SoundData(playerid, true, 1137);
  			return 1;
		}
 		if(listitem == 5) return ShowPlayerDialog(playerid, 7_2, DIALOG_STYLE_LIST, "îòøëú ùéðåé äâãøåú ùç÷ï - ùéðåé ðù÷ äúçìúé", "9mm\nSilenced 9mm\nDesert Eagle", "áçø", "çæåø");
	}
	if(dialogid == 7_1)
	{
		if(!response) ShowPlayerDialog(playerid, 7_0, DIALOG_STYLE_LIST, "îòøëú ùéðåé äâãøåú ùç÷ï", "çñéîä/áéèåì çñéîä ì÷áìú äåãòåú ôøèéåú\náéèåì/äôòìú äúçáøåú àåèåîèéú ìôé àéôé\nùîéøä/áéèåì ñ÷éï\näâãøú ùòä úîéãéú\näöâú ùòä åúàøéê\nùéðåé ðù÷ äúçìúé", "áçø", "áéèåì");
		else
		{
			any = strval(inputtext);
			if(any < 1 || any > 24) return SendClientMessage(playerid, White, "èååç äùòåú äåà áéï 1 ì24");
			DOF2_SetInt(File(playerid), "StartTime", any);
