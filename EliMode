			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(CheckAdminLevel(id) > 1) return SendClientMessage(playerid, White, "! àéðê éëåì ìáöò ô÷åãä æå òì îðäì");
			if(GetSupportLevel(id) > GetSupportLevel(playerid)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì úåîê áøîú úîéëä ùååä å/àå âáåä îùìê");
			if(!inJail[id]) return SendClientMessage(playerid, White, "ùç÷ï æä ìà áëìà");
			format(string, sizeof(string), "ùéçøø àåúê îäëìà %s äúåîê", GetName(playerid));
			SendClientMessage(id, Whiteorange, string);
			format(string, sizeof(string), "îäëìà %s ùéçøøú àú äùç÷ï", GetName(id));
			SendClientMessage(playerid, Whiteorange, string);
			format(string, sizeof(string), "{FFFFFF}%s{FFCC33} ùåçøø îäëìà òì éãé äúåîê {FFFFFF}%s{FFCC33} äùç÷ï", GetName(playerid), GetName(id));
			OL_SendClientMessageToAll(Whiteorange, string);
			DOF2_SetInt(File(id), "Jail", DOF2_GetInt(File(id), "Jail")-1);
			KillTimer(JailTimer[id]);
			KillTimer(JailTimer1[id]);
		 	SetPlayerVirtualWorld(id, 0);
			SpawnPlayer(id);
			DOF2_SetInt(File(id), "inJail", 0);
			inJail[id] = 0;
			DOF2_SetInt(File(id), "JailTime", 0);
			DOF2_SetString(File(id), "JailReason", "None");
			DOF2_SetString(File(id), "JailAdmin", "None");
			return 1;
		}
		if(strcmp(tmp, "Kick", true) == 0 || strcmp(tmp, "K", true) == 0)
		{
			if(GetSupportLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú úåîê ùìê ðîåëä å/àå ìà ùååä ì1");
			tmp1 = strtok(cmdtext, idx);
			if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Support Kick(Sup K) [playerid] [reason] :öåøú ùéîåù");
			if(!IsNumeric(tmp1)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
			id = strval(tmp1);
		 	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(CheckAdminLevel(id) > 1) return SendClientMessage(playerid, White, "! àéðê éëåì ìáöò ô÷åãä æå òì îðäì");
			if(GetSupportLevel(id) > GetSupportLevel(playerid)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì úåîê áøîú úîéëä ùååä å/àå âáåä îùìê");
		 	new length = strlen(cmdtext);
			while ((idx < length) && (cmdtext[idx] <= ' '))
			{
				idx++;
			}
			new offset = idx;
			new result[64];
			while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
			{
				result[idx - offset] = cmdtext[idx];
				idx++;
			}
			result[idx - offset] = EOS;
			if(!strlen(result)) return SendClientMessage(playerid, White, "/Support Kick(Sup K) [playerid] [reason] :öåøú ùéîåù");
		 	format(string, sizeof(string), "{FFFFFF}%s{FFCC33} ðáòè òì éãé äúåîê {FFFFFF}%s{FFCC33} äùç÷ï", GetName(playerid), GetName(id));
			OL_SendClientMessageToAll(Whiteorange, string);
		 	format(string, sizeof(string), "({FFFFFF}%s{FFCC33})", (result));
		 	OL_SendClientMessageToAll(Whiteorange, string);
			DOF2_SetInt(File(id), "Kick", DOF2_GetInt(File(id), "Kick")+1);
		 	kixkx[id] = SetTimerEx("kickx", 1000, 1, "d", id);
			return 1;
		}
		if(strcmp(tmp, "Ban", true) == 0 || strcmp(tmp, "B", true) == 0)
		{
			if(GetSupportLevel(playerid) < 2) return SendClientMessage(playerid, White, "äøîú úåîê ùìê ðîåëä å/àå ìà ùååä ì2");
			tmp1 = strtok(cmdtext, idx);
			if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Support Ban(Sup B) [playerid] [reason] :öåøú ùéîåù");
			if(!IsNumeric(tmp1)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
			id = strval(tmp1);
  			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
   			if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
			new length = strlen(cmdtext);
			while ((idx < length) && (cmdtext[idx] <= ' '))
			{
				idx++;
			}
			new offset = idx;
			new result[64];
			while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
			{
				result[idx - offset] = cmdtext[idx];
				idx++;
			}
			result[idx - offset] = EOS;
			if(!strlen(result)) return SendClientMessage(playerid, White, "/Support Ban(Sup B) [playerid] [reason] :öåøú ùéîåù");
		 	format(string, sizeof(string), "{FFFFFF}%s{FFCC33} ðçñí òì éãé äúåîê {FFFFFF}%s{FFCC33} äùç÷ï", GetName(playerid), GetName(id));
			OL_SendClientMessageToAll(Whiteorange, string);
		 	format(string, sizeof(string), "({FFFFFF}%s{FFCC33})", (result));
		 	OL_SendClientMessageToAll(Whiteorange, string);
	 		DOF2_SetInt(File(id), "Ban", DOF2_GetInt(File(id), "Ban")+1);
	 		getdate(Year, Month, Day);
	 		gettime(Hour, Minute, Second);
			DOF2_SetInt(File(id), "HaveBan", 1);
			DOF2_SetString(File(id), "ReasonBan", (result));
			DOF2_SetString(File(id), "AdminBan", "None");
			DOF2_SetString(File(id), "SupporterBan", GetName(playerid));
			DOF2_SetInt(File(id), "YearBan", Year);
			DOF2_SetInt(File(id), "MonthBan", Month);
			DOF2_SetInt(File(id), "DayBan", Day);
			DOF2_SetInt(File(id), "HourBan", Hour);
			DOF2_SetInt(File(id), "MinuteBan", Minute);
			DOF2_SetInt(File(id), "SecondBan", Second);
			DOF2_SetInt("/Setting.ini", "InBans", DOF2_GetInt("/BansIP.ini", "InBans")+1);
			DOF2_SetInt("/BansIP.ini", GetIP(id), 1);
	  		disreason[id] = 1;
	  		kixkx[id] = SetTimerEx("kickx", 1000, 1, "d", id);
	 		return 1;
	 	}
		else
		{
			SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Support {FFFFFF}àôùøåú");
		}
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/War", true) == 0)
	{
		format(string, sizeof(string), "ðëðñ ìàæåø äîìçîä {FFFFFF}%s{4B351B} äùç÷ï", GetName(playerid));
		for(new i=0; i<MAX_PLAYERS; i++)
		{
			if(IsPlayerConnected(i) && Logged[i] == true)
			{
				if(inWar[i] == 1)
				{
					if(i != playerid)
					{
						SendClientMessage(i, Brown, string);
					}
				}
			}
		}
	  	ResetPlayerWeapons(playerid);
  		SetPlayerHealth(playerid, 100);
  		SetPlayerArmour(playerid, 100);
	    GivePlayerWeapon(playerid, 26, 100000);
	    GivePlayerWeapon(playerid, 31, 100000);
	    GivePlayerWeapon(playerid, 32, 100000);
		SetPlayerInterior(playerid, 16);
		SetPlayerVirtualWorld(playerid, 9);
		GetPlayerHealth(playerid, BeforePvpHealth[playerid]);
		GetPlayerArmour(playerid, BeforePvpArmour[playerid]);
		switch(random(4))
		{
			case 0:{SetPlayerPosEx(playerid, -1368.2813,1256.2673,1039.8672,109.1435);}
			case 1:{SetPlayerPosEx(playerid, -1426.9152,1236.1774,1039.8741,288.6618);}
			case 2:{SetPlayerPosEx(playerid, -1414.3704,1269.9659,1039.8741,215.0279);}
			case 3:{SetPlayerPosEx(playerid, -1378.8235,1221.9016,1039.8672,35.1961);}
		}
		SetCameraBehindPlayer(playerid);
		inWar[playerid] = 1;
		SendClientMessage(playerid, Grellow, "{FFFFFF}[SYSTEM]{ADFF2F} áøåê äáà ìàæåø äîìçîä {FFFFFF}•••");
		SoundData(playerid, true, 1137);
		return 1;
	}
