		KillTimer(RulesTimer[playerid]);
		RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*10*1, 0, "d", playerid);
		return 1;
	}
	if(DOF2_GetInt(File(playerid), "ReadRules") == 3)
	{
		for(new i=0; i<100; i++) SendClientMessage(playerid, White, "");
		SendClientMessage(playerid, Gold, "#3 - {FFFFFF}çì àéñåø îåçìè ìäùúîù áëì úåñó ùäåà ìîùç÷/ðéöåì áàâéí");
		SendClientMessage(playerid, Gold, "çì àéñåø îåçìè ìäùúîù áëì ñåâ ùäåà ùì ö'éèéí/äà÷éí/èøééðøéí/îåãéí/ñåáééèéí");
		SendClientMessage(playerid, Gold, "\"áîéãä åîöàúí áàâ áîåã ùðéúï ìðöìå ìîèøåú àéùéåú - éù ìãååç áôåøåí \"îùåá åúîéëä");
		SendClientMessage(playerid, Gold, "áîéãä åùç÷ï ééúôñ äåà éåøç÷ îäùøú ìöîéúåú ììà ëì äúøàä îøàù åáìé æëåú ìòøòåø");
		SendClientMessage(playerid, Gold, "Punishment: {FFFFFF}Ban");
		SendClientMessage(playerid, Brightred, "- àðà äîúï {FFFFFF}10{FF0000} ùðéåú ì÷øéàú äçå÷ äáà -");
		DOF2_SetInt(File(playerid), "ReadRules", 4);
		KillTimer(RulesTimer[playerid]);
		RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*10*1, 0, "d", playerid);
		return 1;
	}
	if(DOF2_GetInt(File(playerid), "ReadRules") == 4)
	{
		for(new i=0; i<100; i++) SendClientMessage(playerid, White, "");
		SendClientMessage(playerid, Gold, "#4 - {FFFFFF}(àñåø ìáöò áùøú ëì äñëí ùì äùâú äøéâåú áîøîä (ñôàåï/ñååàô ÷éì");
		SendClientMessage(playerid, Gold, "äâãøú ñôàåï ÷éì: áøâò ùùç÷ï ðåçú áð÷åãú äñôàåï, àéï ìäéåú îåëï òí äëååðú àì äð÷åãä");
		SendClientMessage(playerid, Gold, "äâãøú ñååàô ÷éì: àúä äåøâ àåúå, åäåà äåøâ àåúê áúåøåú");
		SendClientMessage(playerid, Gold, "Punishment: {FFFFFF}Jail {FFD720}> {FFFFFF}Kick {FFD720}> {FFFFFF}ReadRules {FFD720}> {FFFFFF}Ban");
		SendClientMessage(playerid, Brightred, "- àðà äîúï {FFFFFF}10{FF0000} ùðéåú ì÷øéàú äçå÷ äáà -");
		DOF2_SetInt(File(playerid), "ReadRules", 5);
		KillTimer(RulesTimer[playerid]);
		RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*10*1, 0, "d", playerid);
		return 1;
	}
	if(DOF2_GetInt(File(playerid), "ReadRules") == 5)
	{
		for(new i=0; i<100; i++) SendClientMessage(playerid, White, "");
		SendClientMessage(playerid, Gold, "#5 - {FFFFFF}àéï ìôøñí áùøúéðå àó ÷äéìä/ùøú/ôåøåí îúçøä àçø");
		SendClientMessage(playerid, Gold, "àñåø ìôøñí ëúåáú ùì ÷äéìä/ôåøåí àçø, áðåñó àñåø ìôøñí âí àééôé ùì ÷äéìä îúçøä");
		SendClientMessage(playerid, Gold, "áîéãä å÷ééí ìôåøåí ùøúé îùç÷ ðåñôéí - äçå÷éí äîöåééðéí ìîòìä çìéí âí ôä");
		SendClientMessage(playerid, Gold, "Punishment: {FFFFFF}Ban");
		SendClientMessage(playerid, Brightred, "- àðà äîúï {FFFFFF}10{FF0000} ùðéåú ì÷øéàú äçå÷ äáà -");
		DOF2_SetInt(File(playerid), "ReadRules", 6);
		KillTimer(RulesTimer[playerid]);
		RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*10*1, 0, "d", playerid);
		return 1;
	}
	if(DOF2_GetInt(File(playerid), "ReadRules") == 6)
	{
		for(new i=0; i<100; i++) SendClientMessage(playerid, White, "");
		SendClientMessage(playerid, Gold, "#6 - {FFFFFF} çì àéñåø îåçìè ìäéåú ùåúó ìôùò áî÷øéí áùøú");
		SendClientMessage(playerid, Gold, "áîéãä åøàéúí ö'éèø åìà ãéååçúí æä ðçùá ùåúôåú ìôùò");
		SendClientMessage(playerid, Gold, "/Report | /HelpMe - áîéãä åðú÷ìúí áö'éèø äùúîùå áô÷åãåú äáàåú åúãååçå òì ëê");
		SendClientMessage(playerid, Gold, "Punishment: {FFFFFF}Ban");
		SendClientMessage(playerid, Brightred, "- àðà äîúï {FFFFFF}10{FF0000} ùðéåú ìñéåí äçå÷éí -");
		DOF2_SetInt(File(playerid), "ReadRules", 7);
		KillTimer(RulesTimer[playerid]);
		RulesTimer[playerid] = SetTimerEx("ReadRules", 1000*10*1, 0, "d", playerid);
		return 1;
	}
	if(DOF2_GetInt(File(playerid), "ReadRules") == 7)
	{
		for(new i=0; i<100; i++) SendClientMessage(playerid, White, "");
 		new string[256];
		/*format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}ñééí ì÷øåà àú äçå÷éí {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
		OL_SendClientMessageToAllEx(playerid, Grey, string);*/
		SendClientMessage(playerid, Orange, "äîùê îùç÷ îäðä, îöååú äùøú :)");
		SendClientMessage(playerid, Grellow, "/Help àí àúä ðú÷ì ááòéä/ùàìä àúä úîéã úåëì ìôðåú ìîðäì àå ìäòæø áô÷åãä");
		if(DOF2_GetInt(File(playerid), "Register") == 0)
		{
			SendClientMessage(playerid, Pilot, "ðøùîú ìùøú áäöìçä");
		    format(string, sizeof(string), "%s {FFD720}:ùí îùúîù", DOF2_GetString(File(playerid), "Nickname"));
			SendClientMessage(playerid, White, string);
		    format(string, sizeof(string), "%s {FFD720}:ñéñîà", DOF2_GetString(File(playerid), "Password"));
			SendClientMessage(playerid, White, string);
			SendClientMessage(playerid, Grey, "/Setting {FFFFFF}:ëòú ôåòìú îòøëú çéáåø àåèåîèé ìôé àééôé áîùúîù ùìê ìáéèåì");
			SendClientMessage(playerid, Brightred, "(Fàðà öìí îñê ëãé ìîðåò áòéåú òúéãéåú (î÷ù äöéìåí äéðå 8 *");
			SendClientMessage(playerid, Green, "! îëéååï ùàúä çãù ÷éáìú 10 àìó ãåìø îúðú äùøú éùéøåú ìáð÷");
			SendClientMessage(playerid, Grey, "/Bank {FFFFFF}- ëãé ìäâéò ìáð÷ äùúîù áô÷åãä");
			DOF2_SetInt(File(playerid), "Bank", DOF2_GetInt(File(playerid), "Bank")+10000);
		}
		else SendClientMessage(playerid, Pilot, "! î÷ååéí ùùéððú àú äçå÷éí ëîå ùöøéê - ùìà úöèøê ôòí äáàä ì÷øåà ùåá àú äçå÷éí");
		DOF2_SetInt(File(playerid), "ReadRules", -1);
		KillTimer(RulesTimer[playerid]);
		AccountLog[playerid] = true;
		return 1;
	}
	return 1;
}

//------------------------------------------------------------------------------

public CreateBarrels(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		new Float:X, Float:Y, Float:Z;
		GetPlayerPos(playerid, X, Y, Z);
		Barrel[playerid][0] = CreateObject(1225, X, Y+2.5, Z, 0.0, 0.0, 0.0, 500);
		Barrel[playerid][1] = CreateObject(1225, X+2.5, Y, Z, 0.0, 0.0, 90.0, 500);
		Barrel[playerid][2] = CreateObject(1225, X-2.5, Y, Z, 0.0, 0.0, 270.0, 500);
		Barrel[playerid][3] = CreateObject(1225, X, Y-2.5, Z, 0.0, 0.0, 180.0, 500);
	}
	SetTimerEx("DestroyBarrels", 1000*10*1, 0, "d", playerid);
 	return 1;
}

public DestroyBarrels(playerid)
{
	DestroyObject(Barrel[playerid][0]);
	DestroyObject(Barrel[playerid][1]);
	DestroyObject(Barrel[playerid][2]);
	DestroyObject(Barrel[playerid][3]);
 	return 1;
}

//------------------------------------------------------------------------------

public SendRocketObject(playerid)
{
	new Float:X, Float:Y, Float:Z;
	new Float:X1, Float:Y1, Float:Z1;
	GetPlayerPos(playerid, X, Y, Z);
	GetObjectPos(RocketObj[playerid], X1, Y1, Z1);
    MoveObjectEx(RocketObj[playerid], X, Y, Z, 300);
	if(PlayerToPoint(5.0, playerid, X1, Y1, Z1))
	{
        CreateExplosion(X, Y, Z, 6, 10);
		KillTimer(RocketTimer[playerid]);
		DestroyObject(RocketObj[playerid]);
		SetPlayerHealth(playerid, 0);
		RocketOn = 0;
	}
	else if (X != X1 && Y != Y1 && Z != Z1)
	{
	}
	return 1;
}


public StopSendRocketObject(playerid)
{
	new Float:X, Float:Y, Float:Z;
	GetObjectPos(RocketObj[playerid], X, Y, Z);
    CreateExplosion(X, Y, Z, 6, 10);
	KillTimer(RocketTimer[playerid]);
	KillTimer(RocketEndTimer[playerid]);
	DestroyObject(RocketObj[playerid]);
	RocketOn = 0;
	return 1;
}

//------------------------------------------------------------------------------

public MoneyShip()
{
	for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerInArea(i, 810.8079, -2121.3257, 863.0656, -2111.0884) && Logged[i] == true && inAFK[i] == 0)
	{
		if(GetLevel(i) == 1) AntiCheat_GivePlayerMoney(i, true, 50);
  		else if(GetLevel(i) == 2) AntiCheat_GivePlayerMoney(i, true, 100);
  		else if(GetLevel(i) == 3) AntiCheat_GivePlayerMoney(i, true, 150);
		else if(GetLevel(i) == 4) AntiCheat_GivePlayerMoney(i, true, 200);
		else if(GetLevel(i) == 5) AntiCheat_GivePlayerMoney(i, true, 250);
		else if(GetLevel(i) == 6) AntiCheat_GivePlayerMoney(i, true, 300);
		else if(GetLevel(i) == 7) AntiCheat_GivePlayerMoney(i, true, 350);
		else if(GetLevel(i) == 8) AntiCheat_GivePlayerMoney(i, true, 400);
		else if(GetLevel(i) == 9) AntiCheat_GivePlayerMoney(i, true, 450);
		else if(GetLevel(i) == 10) AntiCheat_GivePlayerMoney(i, true, 500);
	}
}

//------------------------------------------------------------------------------

public BattleCountDown(cd)
{
	new string[256];
	format(string, sizeof(string), "%d", cd);
	GameTextForPlayer(BattlePlayer_1, string, 1000, 3);
	GameTextForPlayer(BattlePlayer_2, string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		cdbattletimer = SetTimerEx("BattleCountDown", 1000, 0, "%d", cd);
	}
	else
	{
		for(new i=0; i<MAX_PLAYERS; i++) if(InBattle[i] == 1) TogglePlayerControllable(i, 1), inFreeze[i] = 0;
		GameTextForPlayer(BattlePlayer_1, "~r~Go! ~y~Go! ~g~Go!", 1000, 3);
		GameTextForPlayer(BattlePlayer_2, "~r~Go! ~y~Go! ~g~Go!", 1000, 3);
		KillTimer(cdbattletimer);
		CountdownON = 0;
	}
	return 1;
}

public CheckBattleWinner(endid)
{
	if(endid == 0)
	{
		new string[256];
		format(string, sizeof(string), "ðéöç àåúê á÷øá äàéùé %s äùç÷ï", GetName(BattleWinner));
		SendClientMessage(BattleLosser, Azure, string);
		format(string, sizeof(string), "á÷øá äàéùé %s ðéöçú àú äùç÷ï", GetName(BattleLosser));
		SendClientMessage(BattleWinner, Azure, string);
		SpawnPlayer(BattleWinner);
		InBattle[BattleWinner] = 0;
		InBattle[BattleLosser] = 0;
		SetPlayerVirtualWorld(BattleWinner, 0);
		BattlePlayers = 0;
		HaveBattle = 0;
	}
	else if(endid == 1)
	{
		for(new i=0; i<MAX_PLAYERS; i++) if(HaveBattle == 1 && BattlePlayers == 1 && InBattle[i] == 1)
		{
			new string[256];
			format(string, sizeof(string), "á÷øá äàéùé ò÷á äúðú÷åúå îäùøú %s ðéöçú àú äùç÷ï", BattleLosser_Disconnect);
			SendClientMessage(i, Azure, string);
			SpawnPlayer(i);
			InBattle[i] = 0;
			SetPlayerVirtualWorld(i, 0);
			BattlePlayers = 0;
			HaveBattle = 0;
		}
	}
	return 1;
}

//------------------------------------------------------------------------------

public UpdateTimeNData()
{
	new string[256];
    gettime(Hour, Minute, Second);
    getdate(Year, Month, Day);
   	format(string, 32, "%02d:%02d:%02d - %d/%d/%d", Hour, Minute, Second, Day, Month, Year);
   	TextDrawSetString(Clock, string);
}

