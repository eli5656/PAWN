//------------------------------------------------------------------------------

public Math_Start()
{
	new string[256];
    Math_Num[0] = randomex(1, 10);
    Math_Num[1] = randomex(1, 10);
    Math_Num[2] = randomex(1, 10);
    Math_Num[3] = randomex(1, 10);
	Math_ON = 1;
	OL_SendClientMessageToAll(Grellow, "============[{FFFFFF} îòøëú îúîèé÷ä {ADFF2F}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}æîï ìôúøåï äúøâéì:{ADFF2F} ã÷ä åçöé");
	Math_RandomPrice = randomex(20000, 30000);
	Math_Price = Math_RandomPrice;
	format(string,sizeof(string), "ôøñ: {ADFF2F}%s$", GetNum(Math_Price));
	OL_SendClientMessageToAll(White, string);
	OL_SendClientMessageToAll(White, "/{ADFF2F}ANS{FFFFFF}wer :ô÷åãä ìáãé÷ú úùåáä");
    switch(random(2))
	{
		case 0:
		{
			Math_Answer = (Math_Num[0]*Math_Num[1])+(Math_Num[2]*Math_Num[3]);
			format(string, sizeof(string), "(%d*%d)+(%d*%d)=? {FFFFFF}:úøâéì äîúîèé÷ä äåà", Math_Num[0], Math_Num[1], Math_Num[2], Math_Num[3]);
			OL_SendClientMessageToAll(Grellow, string);
		}
		case 1:
		{
			Math_Answer = (Math_Num[0]+Math_Num[1])*(Math_Num[2]+Math_Num[3]);
			format(string, sizeof(string), "(%d+%d)*(%d+%d)=? {FFFFFF}:úøâéì äîúîèé÷ä äåà", Math_Num[0], Math_Num[1], Math_Num[2], Math_Num[3]);
			OL_SendClientMessageToAll(Grellow, string);
		}
	}
	Math_Timer = SetTimer("Math_End", 1000*90*1, 1);
	return ;
}

public Math_End()
{
	new string[256];
	OL_SendClientMessageToAll(Grellow, "============[{FFFFFF} îòøëú îúîèé÷ä {ADFF2F}]============");
	OL_SendClientMessageToAll(White, "ôåúø úøâéì äîúîèé÷ä äåà: {ADFF2F}àéï ôåúø");
	format(string,sizeof(string), "äúùåáä äðëåðä äéà: {ADFF2F}%s", GetNum(Math_Answer));
	OL_SendClientMessageToAll(White, string);
	KillTimer(Math_Timer);
	Math_Answer = 0;
	Math_ON = 0;
	Math_Timer1 = SetTimer("Math_Start", 1000*60*20, 0);
	return 1;
}

//------------------------------------------------------------------------------

public MoneyMission_Countdown(cd)
{
	new string[256];
	format(string, sizeof(string), "~r~Must more ~w~%d ~r~seconds", cd);
	GameTextForPlayer(MoneyMission_Player, string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		MoneyMission_Timer_1 = SetTimerEx("MoneyMission_Countdown", 1000, 0, "%d", cd);
	}
	else
	{
		KillTimer(MoneyMission_Timer_1);
	}
	return 1;
}

public MoneyMission_End(playerid)
{
	new string[256];
	MoneyMission_Start = 0;
	MoneyMission_In[playerid] = 0;
	SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äëñó");
 	SendClientMessage(playerid, White, "ìà äñô÷ú ìäâéò ìéòã áæîï -");
 	KillTimer(MoneyMission_Timer_1);
 	KillTimer(MoneyMission_Timer_2);
 	DestroyVehicle(MoneyMission_Bike);
 	format(string, sizeof(string), "{FFFFFF}[Money Mission]{AA3333} ìà äöìéç ìäâéò ìéòã ùì îùéîú äëñó {FFFFFF}%s{AA3333} äùç÷ï", GetName(MoneyMission_Player));
	OL_SendClientMessageToAllEx(playerid, Red, string);
	OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ìà äñôé÷ ìäâéò ìéòã áæîï -");
	RemovePlayerAttachedObject(playerid, 2); // MoneyBag
	return 1;
}

//------------------------------------------------------------------------------

public RaceMission_Countdown(cd)
{
	new string[256];
	format(string, sizeof(string), "~r~Must more ~w~%d ~r~seconds", cd);
	GameTextForPlayer(RaceMission_Player, string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		RaceMission_Timer_1 = SetTimerEx("RaceMission_Countdown", 1000, 0, "%d", cd);
	}
	else
	{
		KillTimer(RaceMission_Timer_1);
	}
	return 1;
}

public RaceMission_End(playerid)
{
	new string[256];
    DisablePlayerRaceCheckpoint(playerid);
    CP_RaceMission[playerid] = 0;
	RaceMission_Start = 0;
	RaceMission_In[playerid] = 0;
	SendClientMessage(playerid, Red, "• ðëùìú áîùéîú äîéøåõ");
 	SendClientMessage(playerid, White, "ìà äñô÷ú ìäâéò ìéòã áæîï -");
 	DestroyVehicle(RaceMission_Car);
 	KillTimer(RaceMission_Timer_1);
 	KillTimer(RaceMission_Timer_2);
 	SetPlayerVirtualWorld(playerid, 0);
 	format(string, sizeof(string), "{FFFFFF}[Race Mission]{AA3333} ìà äöìéç ìäâéò ì÷å äñéåí ùì îùéîú äîéøåõ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RaceMission_Player));
	OL_SendClientMessageToAllEx(playerid, Red, string);
	OL_SendClientMessageToAllEx(playerid, White, "äùç÷ï ìà äñôé÷ ìäâéò ìéòã áæîï -");
	return 1;
}

