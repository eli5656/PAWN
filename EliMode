	if(strcmp(cmd, "/Tag", true) == 0 || strcmp(cmd, "/T", true) == 0)
	{
		if(GetAdminLevel(playerid) < 4 && GetTempAdminLevel(playerid) < 4 && GetSpyAdminLevel(playerid) < 4) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì4");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Tag(T) [Set | Remove] :öåøú ùéîåù");
	 	if(strcmp(tmp, "Set", true) == 0 || strcmp(tmp, "S", true) == 0)
		{
			tmp1 = strtok(cmdtext, idx);
			if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Tag Set(T s) [playerid] [text] :öåøú ùéîåù");
			if(!IsNumeric(tmp1)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
	  		id = strval(tmp1);
	    	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			new length = strlen(cmdtext);
			while ((idx < length) && (cmdtext[idx] <= ' '))
			{
				idx++;
			}
			new offset = idx;
			new result[64];
			while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
			{
				result[idx - offset] = cmdtext[idx];
				idx++;
			}
			result[idx - offset] = EOS;
			if(!strlen(result)) return SendClientMessage(playerid, White, "/Tag Set(T s) [playerid] [text] :öåøú ùéîåù");
			format(string, sizeof(string), "{FFFFFF}%s{33AA33}ùéðä ìê àú äúàâ ì {FFFFFF}%s{33AA33} äîðäì", (result), GetName(playerid));
			SendClientMessage(id, Green ,string);
			format(string, sizeof(string), "{FFFFFF}%s{33AA33}àú äúàâ ì {FFFFFF}%s{33AA33} ùéðéú ìùç÷ï", (result), GetName(id));
			SendClientMessage(playerid, Green ,string);
			DOF2_SetString(File(id), "Tag", (result));
			return 1;
		}
	 	if(strcmp(tmp, "Remove", true) == 0 || strcmp(tmp, "R", true) == 0)
		{
			if(GetAdminLevel(playerid) < 4 && GetTempAdminLevel(playerid) < 4 && GetSpyAdminLevel(playerid) < 4) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì4");
			tmp = strtok(cmdtext, idx);
	    	if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Tag Remove(T r) [playerid] :öåøú ùéîåù");
			if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
	  		id = strval(tmp);
	    	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			format(string, sizeof(string), "äñéø ìê àú äúàâ {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
			SendClientMessage(id, Green ,string);
			format(string, sizeof(string), "àú äúàâ {FFFFFF}%s{33AA33} äñøú ìùç÷ï", GetName(id));
			SendClientMessage(playerid, Green ,string);
			DOF2_Unset(File(id), "Tag");
			return 1;
		}
		else
		{
			SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Tag {FFFFFF}àôùøåú");
		}
		return 1;
	}
