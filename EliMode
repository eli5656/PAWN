							 	{
									format(string, sizeof(string), "ðòì àú ö'àè äëðåôééä {FFFFFF}%s{8B5A2B} îðäì äëðåôééä", GetName(playerid));
									SendClientMessage(i, Whitebrown, string);
								}
							}
						}
					}
				}
			}
			else if(DOF2_GetInt(GangFile, "GangChat") == 1)
			{
