	if(strcmp(cmd, "/Kill", true) == 0)
	{
  		tmp = strtok(cmdtext, idx);
		if(strlen(tmp))
		{
			if(GetAdminLevel(playerid) < 2 && GetTempAdminLevel(playerid) < 2 && GetSpyAdminLevel(playerid) < 2) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì2");
			if(GetAdminLevel(id) > GetAdminLevel(playerid)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
			if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
			id = strval(tmp);
  			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
   			if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
			format(string, sizeof(string), "àéôñ ìê àú äçééí {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
 			SendClientMessage(id, Green ,string);
			format(string, sizeof(string), "àú äçééí {FFFFFF}%s{33AA33} àéôñú ìùç÷ï", GetName(id));
 			SendClientMessage(playerid, Green ,string);
 		    SetPlayerHealth(id, 0);
		    SetPlayerArmour(id, 0);
		}
		else if(!strlen(tmp))
		{
		    SetPlayerHealth(playerid, 0);
		    SetPlayerArmour(playerid, 0);
			//SendClientMessage(playerid, Grellow, "{FFFFFF}[SYSTEM]{ADFF2F} äúàáãú {FFFFFF}•••");
   			SoundData(playerid, true, 1137);
		}
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Bomb", true) == 0)
	{
	    tmp = strtok(cmdtext , idx);
	    if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Bomb [Buy | Use] :öåøú äùéîåù");
	    if(strcmp(tmp, "Buy", true) == 0)
	    {
	        tmp1 = strtok(cmdtext, idx);
	        if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Bomb Buy [1-3] :öåøú äùéîåù");
			if(HaveBomb[playerid] == 1) return SendClientMessage(playerid, White, "éù ááòìåúê ëáø ôööä");
			if(strcmp(tmp1, "1", true) == 0)
			{
			    if(AntiCheat_GetPlayerMoney(playerid) < 50000) return SendClientMessage(playerid, White, "[50,000] - àéï ìê îñôé÷ ëñó");
				HaveBomb[playerid] = 1;
				BombSize[playerid][0] = 1;
				SendClientMessage(playerid, Indigo, "{FFFFFF}Easy{4B00B0} ÷ðéú ôööä áâåãì");
				AntiCheat_GivePlayerMoney(playerid, false, 50000);
			}
			if(strcmp(tmp1, "2", true) == 0)
			{
			    if(AntiCheat_GetPlayerMoney(playerid) < 100000) return SendClientMessage(playerid, White, "[100,000] - àéï ìê îñôé÷ ëñó");
				HaveBomb[playerid] = 1;
				BombSize[playerid][1] = 1;
				SendClientMessage(playerid, Indigo, "{FFFFFF}Medium{4B00B0} ÷ðéú ôööä áâåãì");
				AntiCheat_GivePlayerMoney(playerid, false, 100000);
			}
			if(strcmp(tmp1, "3", true) == 0)
			{
			    if(AntiCheat_GetPlayerMoney(playerid) < 150000) return SendClientMessage(playerid, White, "[150,000] - àéï ìê îñôé÷ ëñó");
				HaveBomb[playerid] = 1;
				BombSize[playerid][2] = 1;
				SendClientMessage(playerid, Indigo, "{FFFFFF}Large{4B00B0} ÷ðéú ôööä áâåãì");
				AntiCheat_GivePlayerMoney(playerid, false, 150000);
			}
			return 1;
		}
		if(strcmp(tmp, "Use", true) == 0)
	    {
			if(HaveBomb[playerid] == 0) return SendClientMessage(playerid, White, "àéï ááòìåúê ôööä");
			if(UseBomb[playerid] == 0)
			{
			    if(IsPlayerInAnyVehicle(playerid))
			    {
				    BombVehicle[playerid] = vehicleid;
				    if(BombSize[playerid][0] == 1)
					{
				    	SendClientMessage(playerid, Indigo, "ùäééúä ìê áîëåðéú {FFFFFF}Easy{4B00B0} äùúìú àú äôööä áâåãì");
				    }
				    else if(BombSize[playerid][1] == 1)
					{
				    	SendClientMessage(playerid, Indigo, "ùäééúä ìê áîëåðéú {FFFFFF}Medium{4B00B0} äùúìú àú äôööä áâåãì");
				    }
				    else if(BombSize[playerid][2] == 1)
					{
				    	SendClientMessage(playerid, Indigo, "ùäééúä ìê áîëåðéú {FFFFFF}Large{4B00B0} äùúìú àú äôööä áâåãì");
				    }
				}
				else
			    {
			        GetPlayerPos(playerid, BombX[playerid], BombY[playerid], BombZ[playerid]);
					BombVehicle[playerid] = -1;
				    if(BombSize[playerid][0] == 1)
					{
				    	SendClientMessage(playerid, Indigo, "ùäééúä ìê òì äøöôä {FFFFFF}Easy{4B00B0} äùúìú àú äôööä áâåãì");
				    }
				    else if(BombSize[playerid][1] == 1)
					{
				    	SendClientMessage(playerid, Indigo, "ùäééúä ìê òì äøöôä {FFFFFF}Medium{4B00B0} äùúìú àú äôööä áâåãì");
				    }
				    else if(BombSize[playerid][2] == 1)
					{
				    	SendClientMessage(playerid, Indigo, "ùäééúä ìê òì äøöôä {FFFFFF}Large{4B00B0} äùúìú àú äôööä áâåãì");
				    }
				    ApplyAnimation(playerid, "PED", "bomber", 4.1, 0, 0, 0, 0, 0);
				    GetPlayerPos(playerid, X, Y, Z);
				    BombObject[playerid] = CreateObject(1654, X, Y, Z, 0.0, 0.0, 0.0, 500);
			    }
				UseBomb[playerid] = 1;
			}
			else if(UseBomb[playerid] == 1)
			{
			    if(BombVehicle[playerid] == -1)
			    {
				    if(BombSize[playerid][0] == 1)
					{
					    CreateExplosion(BombX[playerid], BombY[playerid], BombZ[playerid], 6, 5);
					    SendClientMessage(playerid, Indigo, "ùäééúä òì äøöôä {FFFFFF}Easy{4B00B0} ôåööú àú äôööä áâåãì");
					    BombSize[playerid][0] = 0;
					}
				    if(BombSize[playerid][1] == 1)
					{
					    CreateExplosion(BombX[playerid], BombY[playerid], BombZ[playerid], 6, 10);
					    SendClientMessage(playerid, Indigo, "ùäééúä òì äøöôä {FFFFFF}Medium{4B00B0} ôåööú àú äôööä áâåãì");
					    BombSize[playerid][1] = 0;
					}
				    if(BombSize[playerid][2] == 1)
					{
					    CreateExplosion(BombX[playerid], BombY[playerid], BombZ[playerid], 6, 15);
					    SendClientMessage(playerid, Indigo, "ùäééúä òì äøöôä {FFFFFF}Large{4B00B0} ôåööú àú äôööä áâåãì");
					    BombSize[playerid][2] = 0;
					}
					DestroyObject(BombObject[playerid]);
				}
                else
			    {
				    if(BombSize[playerid][0] == 1)
					{
    			 		GetVehiclePos(BombVehicle[playerid], X, Y, Z);
     					CreateExplosion(X, Y, Z, 6, 5);
					    SendClientMessage(playerid, Indigo, "ùäééúä áîëåðéú {FFFFFF}Easy{4B00B0} ôåööú àú äôööä áâåãì");
					    BombSize[playerid][0] = 0;
		   			}
				    if(BombSize[playerid][1] == 1)
					{
    			 		GetVehiclePos(BombVehicle[playerid], X, Y, Z);
     					CreateExplosion(X, Y, Z, 6, 5);
					    SendClientMessage(playerid, Indigo, "ùäééúä áîëåðéú {FFFFFF}Medium{4B00B0} ôåööú àú äôööä áâåãì");
					    BombSize[playerid][1] = 0;
		   			}
				    if(BombSize[playerid][2] == 1)
					{
    			 		GetVehiclePos(BombVehicle[playerid], X, Y, Z);
     					CreateExplosion(X, Y, Z, 6, 5);
					    SendClientMessage(playerid, Indigo, "ùäééúä áîëåðéú {FFFFFF}Large{4B00B0} ôåööú àú äôööä áâåãì");
					    BombSize[playerid][2] = 0;
		   			}
				}
				UseBomb[playerid] = 0;
    			HaveBomb[playerid] = 0;
			}
			return 1;
		}
		else
		{
			SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Bomb {FFFFFF}àôùøåú");
		}
		return 1;
	}
