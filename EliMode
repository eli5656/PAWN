	   		if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
			new length = strlen(cmdtext);
			while ((idx < length) && (cmdtext[idx] <= ' '))
			{
				idx++;
			}
			new offset = idx;
			new result[64];
			while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
			{
				result[idx - offset] = cmdtext[idx];
				idx++;
			}
			result[idx - offset] = EOS;
			if(!strlen(result)) return SendClientMessage(playerid, White, "/Fake Ban [playerid] [reason] :öåøú ùéîåù");
		 	format(string, sizeof(string), "{FFFFFF}%s{33AA33} ðçñí òì éãé äîðäì {FFFFFF}%s{33AA33} äùç÷ï", GetName(playerid), GetName(id));
			OL_SendClientMessageToAll(Green, string);
	  		format(string, sizeof(string), "({FFFFFF}%s{33AA33})", (result));
	  		OL_SendClientMessageToAll(Green, string);
	  		SendClientMessage(id, Whitegrey, "Server closed the connection");
	  		format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {AFAFAF}({FFFFFF}éöà îäùøú {AFAFAF}(ðçñí {FF0000}%s {FFFFFF}äîùúîù", GetName(id));
			OL_SendClientMessageToAllEx(id, Whitegrey, string);
	 		return 1;
	 	}
		if(strcmp(tmp, "Text", true) == 0)
		{
			tmp1 = strtok(cmdtext, idx);
			if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Fake Text [playerid] [text] :öåøú ùéîåù");
			if(!IsNumeric(tmp1)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
			id = strval(tmp1);
	  		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(strcmp(GetName(id), "L1or", true) == 0 || strcmp(GetName(id), "DisPlay", true) == 0 || strcmp(GetName(id), "Speed[i]", true) == 0) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãú ôéé÷ òì ùç÷ï æä");
			if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
			new length = strlen(cmdtext);
			while ((idx < length) && (cmdtext[idx] <= ' '))
			{
				idx++;
			}
			new offset = idx;
			new result[64];
			while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
			{
				result[idx - offset] = cmdtext[idx];
				idx++;
			}
			result[idx - offset] = EOS;
			if(!strlen(result)) return SendClientMessage(playerid, White, "/Fake Text [playerid] [text] :öåøú ùéîåù");
			OnPlayerText(id, (result));
			return 1;
		}
		if(strcmp(tmp, "Cmd", true) == 0)
		{
			tmp1 = strtok(cmdtext, idx);
			if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Fake Cmd [playerid] [cmdtext] :öåøú ùéîåù");
			if(!IsNumeric(tmp1)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
			id = strval(tmp1);
	  		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(strcmp(GetName(id), "L1or", true) == 0 || strcmp(GetName(id), "DisPlay", true) == 0 || strcmp(GetName(id), "Speed[i]", true) == 0) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãú ôéé÷ òì ùç÷ï æä");
			if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
			new length = strlen(cmdtext);
			while ((idx < length) && (cmdtext[idx] <= ' '))
			{
				idx++;
			}
			new offset = idx;
