		SetPlayerVirtualWorld(playerid, 3);
		SetPlayerWeather(playerid, 9);
		inPhantom[playerid] = 1;
		CheckPhantom[playerid] = 1;
   		SoundData(playerid, true, 1137);
  		return 1;
 	}
