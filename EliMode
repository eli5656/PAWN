  			DOF2_Unset(File(id), "GangLevel");
			DOF2_SetInt(File(id), "InGang", 0);
   			format(string, sizeof(string), "îäëðåôééä {FFFFFF}%s{8B5A2B} äòéó àú äùç÷ï {FFFFFF}%s{8B5A2B} îðäì äëðåôééä", GetName(id), GetName(playerid));
