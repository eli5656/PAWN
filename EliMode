			{
				if(Minigun_ACT_Start == 0 && Minigun_ACT_Running == 0 && Minigun_ACT_On == 1) return SendClientMessage(playerid, White, "äôòéìåú îéðéâàï ëáø îåôòìú");
				if(Minigun_ACT_Start == 0 && Minigun_ACT_Running == 1 && Minigun_ACT_On == 0) return SendClientMessage(playerid, White, "äôòéìåú îéðéâàï ëáø äçìä");
				if(Minigun_ACT_In[playerid] == 1) return SendClientMessage(playerid, White, "ëáø ðøùîú ìôòéìåú äîéðéâàï");
				if(Minigun_ACT_AllPlayers == MAX_MINIGUN_PLAYERS) return SendClientMessage(playerid, White, "ëì î÷åîåú äôòéìåú ðúôñå");
				Minigun_ACT_In[playerid] = 1;
				Minigun_ACT_AllPlayers +=1;
				format(string, sizeof(string), "#{FFFFFF}%d{99FFFF} ðøùîú áäöìçä ìôòéìåú äîéðéâàï, àúä îùúúó îñôø", Minigun_ACT_AllPlayers);
