		    new Setting[256];
			format(Setting, sizeof(Setting), "ATMs/Setting.ini");
			new AtmID = DOF2_GetInt(Setting, "ATMs")+1;
			DOF2_SetInt(Setting, "ATMs", AtmID);
			format(AtmFile, sizeof(AtmFile), "ATMs/%d.ini", AtmID);
			GetPlayerPos(playerid, X, Y, Z);
			DOF2_CreateFile(AtmFile);
			DOF2_SetInt(AtmFile, "ID", AtmID);
	     	DOF2_SetFloat(AtmFile, "X", X);
	      	DOF2_SetFloat(AtmFile, "Y", Y);
	      	DOF2_SetFloat(AtmFile, "Z", Z);
			format(string, sizeof(string), "{FFFF00}ATM ID: {FFFFFF}#%d\n{FFFF00}áøåëéí äáàéí{FFFFFF} ìëñôåîè\n{FFFF00}ùáî÷ìãú {FFFFFF}\"F\"{FFFF00} ìùéîåù ìçõ òì î÷ù", DOF2_GetInt(AtmFile, "ID"));
			AtmText[AtmID] = Create3DTextLabel(string, Gold, DOF2_GetFloat(AtmFile, "X"), DOF2_GetFloat(AtmFile, "Y"), DOF2_GetFloat(AtmFile, "Z")+0.5, 60.0, 0, 1);
			AtmPickup[AtmID] = CreatePickup(1274, 1, DOF2_GetFloat(AtmFile, "X"), DOF2_GetFloat(AtmFile, "Y"), DOF2_GetFloat(AtmFile, "Z"), 0);
			SendClientMessage(playerid, Yellow, "! éöøú ëñôåîè çãù áäöìçä");
