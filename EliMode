		X = strval(tmp);
		tmp1 = strtok(cmdtext, idx);
		if(!strlen(tmp1)) return SendClientMessage(playerid,White, "/GotoXyZ [X] [Y] [Z] :öåøú äùéîåù");
		Y = strval(tmp1);
		tmp2 = strtok(cmdtext, idx);
		if(!strlen(tmp2)) return SendClientMessage(playerid,White, "/GotoXyZ [X] [Y] [Z] :öåøú äùéîåù");
		Z = strval(tmp2);
		SetPlayerPos(playerid, X,Y,Z);
