		format(string, sizeof(string), "{FFFFFF}[Robbery Mission]{AA3333} äúçéì àú îùéîú ùãéãú äáð÷ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RobberyMission_Player));
   		OL_SendClientMessageToAllEx(playerid, Red, string);
		RobberyMission_Switch = SetTimer("RobberyMission_Toggle", 1000*60*15, 1);
	}
	if(dialogid == 21_0 && response)
	{
		format(string, sizeof(string), "%s îùéîú äñèàðèéí ëáø ðòùú òì éãé äùç÷ï", GetName(StuntsMission_Player));
