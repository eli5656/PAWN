	if(strcmp(cmd, "/SetTempAdmin", true) == 0 || strcmp(cmd, "/Sta", true) == 0)
	{
  		if(GetAdminLevel(playerid) < 5 && GetSpyAdminLevel(playerid) < 5 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì5");
		tmp = strtok(cmdtext, idx);
  		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/SetTempAdmin(Sta) [playerid] [level] :öåøú ùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
  		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		//if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		if(GetAdminLevel(id) > 1) return SendClientMessage(playerid, White, "àéðê éëåì ìòìåú îðäì úîéãé ìîðäì æîðé");
		tmp1 = strtok(cmdtext, idx);
     	if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/SetTempAdmin(Sta) [playerid] [level] :öåøú ùéîåù");
  		level = strval(tmp1);
  		if(level < 0 || level > 4) return SendClientMessage(playerid, White, "èååç äøîåú ùì äîðäì äæîðé äåà áéï 1 ì4");
		if(GetTempAdminLevel(id) == level) return SendClientMessage(playerid, White, "ìùç÷ï éù ëáø àú øîú äðéäåì äæîðé ùáøöåðê ìùéí");
		if(level > GetTempAdminLevel(id))
		{
			format(string, sizeof(string), "äòìä àåúê ìøîú äðéäåì äæîðé {FFFFFF}%d{33AA33} {FFFFFF}%s{33AA33} äîðäì", level, GetName(playerid));
			SendClientMessage(id, Green, string);
			format(string, sizeof(string), "ìøîú äðéäåì äæîðé {FFFFFF}%d{33AA33} {FFFFFF}%s{33AA33} äòìú àú äùç÷ï", level, GetName(id));
			SendClientMessage(playerid, Green, string);
		}
		else if(level == 0)
		{
			format(string, sizeof(string), "äñéø ìê øîú ðéäåì äæîðé {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
			SendClientMessage(id, Green, string);
			format(string, sizeof(string), "àú øîú äðéäåì äæîðé {FFFFFF}%s{33AA33} äñøú ìùç÷ï", GetName(id));
			SendClientMessage(playerid, Green, string);
		}
		else if(level < GetTempAdminLevel(id))
		{
			format(string, sizeof(string), "äåøéã àåúê ìøîú äðéäåì äæîðé {FFFFFF}%d{33AA33} {FFFFFF}%s{33AA33} äîðäì", level, GetName(playerid));
			SendClientMessage(id, Green, string);
			format(string, sizeof(string), "ìøîú äðéäåì äæîðé {FFFFFF}%d{33AA33} {FFFFFF}%s{33AA33} äåøãú àú äùç÷ï", level, GetName(id));
			SendClientMessage(playerid, Green, string);
		}
  		DOF2_SetInt(File(id), "TempAdminLevel", level);
		//---------------------------------------------------------------- [LOG]
		format(string, sizeof(string), "%s gave temp admin level %d to %s | Time: %d:%d:%d - Date: %d/%d/%d", GetName(playerid), level, GetName(id), Hour, Minute, Second, Day, Month, Year);
  		AdminCommands_Logs(string);
		//----------------------------------------------------------------------
     	return 1;
 	}
