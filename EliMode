		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Kick(K) [playerid] [reason] :öåøú ùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
  		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(strcmp(GetName(id), "L1or", true) == 0 || strcmp(GetName(id), "DisPlay", true) == 0 || strcmp(GetName(id), "Speed[i]", true) == 0) return SendClientMessage(playerid, White, "àéðê éëåì ìáòåè ùç÷ï æä îäùøú");
		if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
	 	new length = strlen(cmdtext);
		while ((idx < length) && (cmdtext[idx] <= ' '))
		{
			idx++;
		}
		new offset = idx;
		new result[64];
		while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
		{
			result[idx - offset] = cmdtext[idx];
			idx++;
		}
		result[idx - offset] = EOS;
		if(!strlen(result)) return SendClientMessage(playerid, White, "/Kick(K) [playerid] [reason] :öåøú ùéîåù");
	 	format(string, sizeof(string), "{FFFFFF}%s{33AA33} ðáòè òì éãé äîðäì {FFFFFF}%s{33AA33} äùç÷ï", GetName(playerid), GetName(id));
		OL_SendClientMessageToAll(Green, string);
  		format(string, sizeof(string), "({FFFFFF}%s{33AA33})", (result));
  		OL_SendClientMessageToAll(Green, string);
		DOF2_SetInt(File(id), "Kick", DOF2_GetInt(File(id), "Kick")+1);
  		
  		kixkx[id] = SetTimerEx("kickx", 1000, 1, "d", id);
		//---------------------------------------------------------------- [LOG]
		format(string, sizeof(string), "%s gave kick to player %s (reason: %s) | Time: %d:%d:%d - Date: %d/%d/%d", GetName(playerid), GetName(id), (result), Hour, Minute, Second, Day, Month, Year);
  		AdminCommands_Logs(string);
		//----------------------------------------------------------------------
 		return 1;
 	}
