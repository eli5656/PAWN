	}
	
    if(dialogid == 19_0 && response)
	{
		format(string, sizeof(string), "%s îùéîú äîéøåõ ëáø ðòùú òì éãé äùç÷ï", GetName(RaceMission_Player));
  		if(RaceMission_Start == 1) return SendClientMessage(playerid, White, string);
    	if(RaceMission_WasFinish[playerid] == 1) return SendClientMessage(playerid, White, "ðéúï ìòùåú àú îùéîú äîéøåõ ôòí áëðéñä ìîùç÷");
		if(AntiCheat_GetPlayerMoney(playerid) < 1000) return SendClientMessage(playerid, White, "àéï ìê 1000$ ëãé ìäúçéì àú äîùéîä");
		AntiCheat_GivePlayerMoney(playerid, false, 1000);
		RaceMission_Start = 1;
		RaceMission_In[playerid] = 1;
		RaceMission_RandomPrice = randomex(10000, 20000);
		format(string, sizeof(string), "• äúçìú ëòú àú îùéîú äîéøåõ\néù ìê 2 ã÷åú ìäùìéí àú äîñìåì\náîéãä åúöà îäøëá úôñì àåèåîèéú îäîùéîä\n%s$ áîéãä åúâéò ìñåó äîñìåì ú÷áì", GetNum(RaceMission_RandomPrice));
		ShowPlayerDialog(playerid, 19_1, DIALOG_STYLE_MSGBOX, "îùéîú äîéøåõ", string, "äîùê", "");
		GetPlayerPos(playerid, X, Y, Z);
		GetPlayerFacingAngle(playerid, A);
		RaceMission_Car = CreateVehicle(411, X, Y, Z, A, -1, -1, 50000);
		PutPlayerInVehicle(playerid, RaceMission_Car, 0);
		SetPlayerRaceCheckpoint(playerid,2,1348.0542,-2217.9360,12.9464,        0,0,0,15);
		CP_RaceMission[playerid]++;
		RaceMission_Player = playerid;
		RaceMission_Countdown(120);
		RaceMission_Timer_2 = SetTimerEx("RaceMission_End", 1000*120*1, 0, "%d", playerid);
		SetVehicleVirtualWorld(RaceMission_Car, 11);
		SetVehiclePosEx(playerid, 1346.7539, -2266.7354, 13.4960, 0.5440);
		SetPlayerVirtualWorld(playerid, 11);
		format(string, sizeof(string), "{FFFFFF}[Race Mission]{AA3333} äúçéì àú îùéîú äîéøåõ {FFFFFF}%s{AA3333} äùç÷ï", GetName(RaceMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
	}
    if(dialogid == 20_0 && response)
	{
		format(string, sizeof(string), "áîäìê ä15 ã÷åú äàçøåðåú %s îùéîú ùãéãú äáð÷ ëáø ðòùú òì éãé äùç÷ï", GetName(RobberyMission_Player));
  		if(RobberyMission_Start == 1) return SendClientMessage(playerid, White, string);
