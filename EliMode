		{
			ShowPlayerNameTagForPlayer(i, id, false);
		}
		Delete3DTextLabel(ActLevel[id]);
		format(string, sizeof(string), "äôê àåúê ìáìúé ðøàä {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
		SendClientMessage(id, Green ,string);
		format(string, sizeof(string), "{FFFFFF}%s{33AA33} äôëú ìáìúé ðøàä àú äùç÷ï", GetName(id));
		SendClientMessage(playerid, Green ,string);
		SetPlayerColor(id, Invisible);
        InvisibleON[playerid] = 1;
        return 1;
    }
