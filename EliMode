			if(Logged[pClicked[playerid]] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			if(BlockPM[pClicked[playerid]] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä çñí äåãòåú ôøèéåú îîùúîùéí áùøú");
			format(string, sizeof(string), "%s àðà øùåí àú ääåãòä ùáøöåðê ìùìåç ìùç÷ï\n{FF0000}òìéê ìøùåí äåãòä ëãé ìäîùéê", GetName(pClicked[playerid]));
