////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/ChangePassword", true) == 0 || strcmp(cmd, "/Cp", true) == 0)
	{
		oldpass = strtok(cmdtext, idx);
	    if(!strlen(oldpass)) return SendClientMessage(playerid, White, "/ChangePassword(Cp) [oldpassword] [newpassword] [confirmpassword] :öåøú ùéîåù");
		if(strlen(oldpass) < 3 || strlen(oldpass) > 15) return SendClientMessage(playerid, White, "äñéñîà çééáú ìäéåú áéï 3 ì15 úååéí");
		if(strcmp(oldpass, DOF2_GetString(File(playerid), "Password"), true)) return SendClientMessage(playerid, White, "äñéñîà ùäæðú àéðä úåàîú ìñéñîà äðåëçéú");
		newpass = strtok(cmdtext, idx);
	    if(!strlen(newpass)) return SendClientMessage(playerid, White, "/ChangePassword(Cp) [oldpassword] [newpassword] [confirmpassword] :öåøú ùéîåù");
		if(strlen(newpass) < 3 || strlen(newpass) > 15) return SendClientMessage(playerid, White, "äñéñîà çééáú ìäéåú áéï 3 ì15 úååéí");
		if(!strcmp(newpass, DOF2_GetString(File(playerid), "Password"), true)) return SendClientMessage(playerid, White, "äñéñîà äçãùä ùäæðú úåàîú ìñéñîà äéùðä");
		confrimnewpass = strtok(cmdtext, idx);
	    if(!strlen(confrimnewpass)) return SendClientMessage(playerid, White, "/ChangePassword(Cp) [oldpassword] [newpassword] [confirmpassword] :öåøú ùéîåù");
		if(strlen(confrimnewpass) < 3 || strlen(confrimnewpass) > 15) return SendClientMessage(playerid, White, "äñéñîà çééáú ìäéåú áéï 3 ì15 úååéí");
		if(strval(newpass) != strval(confrimnewpass)) return SendClientMessage(playerid, White, "äñéñîàåú ùäæðú àéðï úåàîåú");
		format(string, sizeof(string), "{FFFFFF}[SYSTEM]{ADFF2F} %säñéñîà ùìê ùåðúä áäöìçä ì {FFFFFF}•••", newpass);
		SendClientMessage(playerid, Grellow, string);
		DOF2_SetString(File(playerid), "Password", newpass);
		SoundData(playerid, true, 1137);
		return 1;
    }
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/ChangeNick", true) == 0 || strcmp(cmd, "/Cn", true) == 0)
	{
		confirmpass = strtok(cmdtext, idx);
	    if(!strlen(confirmpass)) return SendClientMessage(playerid, White, "/ChangeNick(Cn) [password] [newnickname] :öåøú ùéîåù");
		if(strlen(confirmpass) < 3 || strlen(confirmpass) > 15) return SendClientMessage(playerid, White, "äñéñîà çééáú ìäéåú áéï 3 ì15 úååéí");
		if(strcmp(confirmpass, DOF2_GetString(File(playerid), "Password"), true)) return SendClientMessage(playerid, White, "äñéñîà ùäæðú àéðä úåàîú ìñéñîà äðåëçéú");
		tmp1 = strtok(cmdtext, idx);
	    if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/ChangeNick(Cn) [password] [newnickname] :öåøú ùéîåù");
		if(!IsNameValid(tmp1)) return SendClientMessage(playerid, White, "ùí îùúîù àéðå ú÷éï");
		format(string, sizeof(string), "Users/%s.ini", tmp1);
		if(DOF2_FileExists(string))
		{
		    format(string, sizeof(string), "ëáø øùåí ìùøú %s äîùúîù", string);
			SendClientMessage(playerid, White, string);
			return 1;
		}
		//======================================================================
		format(string1, sizeof(string1), "! \"%s\"ùéðä àú ùîå áùøú ì \"%s\" äîùúîù", tmp1, GetName(playerid));
		OL_SendClientMessageToAllEx(playerid, Lightaquamarine, string1);
		format(string1, sizeof(string1), "{FFFFFF}[SYSTEM]{ADFF2F} ! àðà äúçáø îçãù ,%sùîê ùåðä áäöìçä ì {FFFFFF}•••", tmp1);
		SendClientMessage(playerid, Grellow, string1);
		//----------------------------------------------------------------------
        for(new i=0; i<MAX_PROPS; i++)
		{
		   	format(PropFile, sizeof(PropFile), "Properties/%d.ini", i);
		   	if(DOF2_FileExists(PropFile) && strcmp(DOF2_GetString(PropFile, "Owner"), GetName(playerid), true) == 0)
		   	{
				DOF2_SetString(PropFile, "Owner", tmp1);
				Delete3DTextLabel(PropText[i]);
				DestroyPickup(PropPickup[i]);
				format(string, sizeof(string), "{AA3333}%s {FFFFFF}:ùí äðëñ\n{AA3333}%s {FFFFFF}:áòìéí\n{AA3333}%s$ {FFFFFF}:îçéø äðëñ\n{AA3333}%s$ {FFFFFF}:øååç ì%s ã÷åú\n{AA3333}ùáî÷ìãú {FFFFFF}\"F\"{AA3333} ìùéîåù ìçõ òì î÷ù", DOF2_GetString(PropFile, "Name"), DOF2_GetString(PropFile, "Owner"), GetNum(DOF2_GetInt(PropFile, "Value")), GetNum(DOF2_GetInt(PropFile, "Amount")), GetNum(DOF2_GetInt("Properties/Setting.ini", "AmountMinutes")));
				PropText[i] = Create3DTextLabel(string, Gold, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z")+0.5, 60.0, 0, 1);
				PropPickup[i] = CreatePickup(1273, 1, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z"), 0);
			}
		}
		//----------------------------------------------------------------------
		DOF2_CopyFile(File(playerid), string);
		DOF2_SetString(File(playerid), "Nickname", string);
		//======================================================================
		if(DOF2_GetInt(File(playerid), "HaveCar") == 1)
		{
			format(string1, sizeof(string1), "Cars/%d.ini", DOF2_GetInt(File(playerid), "CarID"));
			DOF2_CreateFile(string1);
			DOF2_SetString(string1, "Owner", tmp1);
			if(DOF2_GetInt(CarFile(GetPlayerCar(playerid)), "Special") == 1)
			{
				Delete3DTextLabel(CarText[GetPlayerCar(playerid)]);
				format(string, sizeof(string), "øëá îéåçã\n{FFFFFF}%s{800080} ááòìåú", tmp1);
				CarText[GetPlayerCar(playerid)] = Create3DTextLabel(string, Purple, 0, 0, 2, 30, 0, 1);
				Attach3DTextLabelToVehicle(CarText[GetPlayerCar(playerid)], GetPlayerCar(playerid), 0, 0,1);
			}
			else
			{
				Delete3DTextLabel(CarText[GetPlayerCar(playerid)]);
				format(string, sizeof(string), "øëá øâéì\n{FFFFFF}%s{33AA33} ááòìåú", tmp1);
				CarText[GetPlayerCar(playerid)] = Create3DTextLabel(string, Aqua, 0, 0, 2, 30, 0, 1);
				Attach3DTextLabelToVehicle(CarText[GetPlayerCar(playerid)], GetPlayerCar(playerid), 0, 0, 1);
			}
			Vehicle3DTimer = SetTimer("UpdateVehicle3DText", 1000*1*1, false);
		}
		DOF2_RemoveFile(File(playerid));
		disreason[playerid] = 3;
		kixkx[playerid] = SetTimerEx("kickx", 1000, 1, "d", playerid);
		return 1;
	}
