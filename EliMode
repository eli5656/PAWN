			format(AtmFile, sizeof(AtmFile), "ATMs/%d.ini", aid);
			for(new i=0; i<MAX_ATMS; i++)
			{
   			 	if(!DOF2_FileExists(AtmFile)) return ShowPlayerDialog(playerid, 10_1, DIALOG_STYLE_INPUT, "îòøëú áð÷ - ôàðì ëñôåîè", "àðà äëðñ àú äàéãé ùì äëñôåîè\nòì îðú ìäæéæ àåúå ìîé÷åîê äðåëçé\n{FF0000}äàéãé ùì äëñôåîè ùöééðú àéðå ÷ééí", "áçø", "çæåø");
				else
