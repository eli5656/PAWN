	if(dialogid == 10_2_1)
	{
		if(!response) ShowPlayerDialog(playerid, 10_2, DIALOG_STYLE_LIST, "îòøëú áð÷ - ëñôåîè", "äô÷ãú ëñó\nîùéëú ëñó", "áçø", "áéèåì");
		else
		{
			if(listitem == 0) ShowPlayerDialog(playerid, 10_2_1_1, DIALOG_STYLE_INPUT, "îòøëú áð÷ - ëñôåîè", "àðà ëúåá àú ñëåí äëñó ùáøöåðê ìäô÷éã", "äô÷ã", "çæåø");
	  		if(listitem == 1)
			{
				new bankamount = AntiCheat_GetPlayerMoney(playerid);
		 		if(bankamount <= 0) return SendClientMessage(playerid, White, "ñëåí äëñó ùàúä îô÷éã çééá ìäéåú âãåì 0$");
		 		if((bankamount+DOF2_GetInt(File(playerid), "Bank")) > 99999999) return SendClientMessage(playerid, White, "àéðê éëåì ìäô÷éã éåúø î99,999,999$");
				AntiCheat_GivePlayerMoney(playerid, false, bankamount);
				DOF2_SetInt(File(playerid), "Bank", DOF2_GetInt(File(playerid), "Bank")+bankamount);
				format(string, sizeof(string), "{FFFFFF}%s${FFFF00} äô÷ãú ááð÷ àú äñëåí", GetNum(bankamount));
				SendClientMessage(playerid, Yellow, string);
			}
		}
	}
	if(dialogid == 10_2_1_1)
	{
		if(!response) ShowPlayerDialog(playerid, 10_2_1, DIALOG_STYLE_LIST, "îòøëú áð÷ - ëñôåîè", "äô÷ãú ñëåí îñåééí\näô÷ãú ëì äëñó ùéù òìéê", "áçø", "çæåø");
		else
		{
			new bankamount = strval(inputtext);
			if(!bankamount) return SendClientMessage(playerid, White, "äéðê öøéê ìëúåá ñëåí ëìùäå");
			if(bankamount <= 0) return SendClientMessage(playerid, White, "ñëåí äëñó ùàúä îô÷éã çééá ìäéåú âãåì 0$");
			if(bankamount > AntiCheat_GetPlayerMoney(playerid)) return SendClientMessage(playerid, White, "àéï ìê àú ñëåí äëñó ùáøöåðê ìäô÷éã");
	 		if((bankamount+DOF2_GetInt(File(playerid), "Bank")) > 99999999) return SendClientMessage(playerid, White, "àéðê éëåì ìäô÷éã éåúø î99,999,999$");
			AntiCheat_GivePlayerMoney(playerid, false, bankamount);
			DOF2_SetInt(File(playerid), "Bank", DOF2_GetInt(File(playerid), "Bank")+bankamount);
			SendClientMessage(playerid, Yellow, "äô÷ãú ááð÷ àú ëì äëñó ùäéä òìéê");
		}
	}
	if(dialogid == 10_2_2)
	{
		if(!response) ShowPlayerDialog(playerid, 10_2, DIALOG_STYLE_LIST, "îòøëú áð÷ - ëñôåîè", "äô÷ãú ëñó\nîùéëú ëñó", "áçø", "áéèåì");
		else
		{
	        if(listitem == 0) ShowPlayerDialog(playerid, 10_2_2_1, DIALOG_STYLE_INPUT, "îòøëú áð÷ - ëñôåîè", "àðà ëúåá àú ñëåí äëñó ùáøöåðê ìîùåê", "îùåê", "çæåø");
	 		if(listitem == 1)
			{
				new bankamount = DOF2_GetInt(File(playerid), "Bank");
		 		if(bankamount <= 0) return SendClientMessage(playerid, White, "ñëåí äëñó ùàúä îåùê çééá ìäéåú âãåì 0$");
				AntiCheat_GivePlayerMoney(playerid, true, bankamount);
				DOF2_SetInt(File(playerid), "Bank", DOF2_GetInt(File(playerid), "Bank")-bankamount);
				SendClientMessage(playerid, Yellow, "îùëú îäáð÷ àú ëì äëñó ùäéä áå");
			}
		}
	}
	if(dialogid == 10_2_2_1)
	{
		if(!response) ShowPlayerDialog(playerid, 10_2_2, DIALOG_STYLE_LIST, "îòøëú áð÷ - ëñôåîè", "îùéëú ñëåí îñåééí\nîùéëú ëì äëñó ùéù ìê ááð÷", "áçø", "çæåø");
		else
		{
			new bankamount = strval(inputtext);
			if(!bankamount) return SendClientMessage(playerid, White, "äéðê öøéê ìëúåá ñëåí ëìùäå");
			if(bankamount <= 0) return SendClientMessage(playerid, White, "ñëåí äëñó ùàúä îåùê çééá ìäéåú âãåì 0$");
			if(bankamount > DOF2_GetInt(File(playerid), "Bank")) return SendClientMessage(playerid, White, "àéï ìê àú ñëåí äëñó ùáøöåðê ìîùåê");
			AntiCheat_GivePlayerMoney(playerid, true, bankamount);
			DOF2_SetInt(File(playerid), "Bank", DOF2_GetInt(File(playerid), "Bank")-bankamount);
			format(string, sizeof(string), "{FFFFFF}%s${FFFF00} îùëú îäáð÷ àú äñëåí", GetNum(bankamount));
			SendClientMessage(playerid, Yellow, string);
		}
	}

    if(dialogid == 13_0 && response)
	{
       	if(listitem == 0) ShowPlayerDialog(playerid, 13_1, DIALOG_STYLE_MSGBOX, "Help - Credits", "1# - ìéàåø | L1or - îúëðú äîåã áãéîåñ\n\n2# - àìé | DisPlay - îúëðú äîåã + îðäì ä÷äéìä", "áéèåì", "çæåø");
       	if(listitem == 1) ShowPlayerDialog(playerid, 13_2, DIALOG_STYLE_LIST, "Help - Rules", "çå÷ îñ' 1\nçå÷ îñ' 2\nçå÷ îñ' 3\nçå÷ îñ' 4\nçå÷ îñ' 5\nçå÷ îñ' 6", "áçø", "çæåø");
       	if(listitem == 2) ShowPlayerDialog(playerid, 13_4, DIALOG_STYLE_LIST, "Help - Systems", "Admin Mode\nSupport\nStats\nVip\nBank\nWeapons\nAFK\nPvp Areas\nActivity Status\nMoney Ship\nPunishment History\nDiary\nAutomatic Activities\nBattle\nCars\nClans\nGangs\nATM\nProperties\nTAB\nMissions", "áçø", "çæåø");
       	if(listitem == 3) ShowPlayerDialog(playerid, 13_6, DIALOG_STYLE_LIST, "Help - Commands", "Server - ô÷åãåú ùøú ëììéåú\nAdmin - ô÷åãåú ìàãîéðéí", "áçø", "çæåø");
		if(listitem == 4) ShowPlayerDialog(playerid, 13_7, DIALOG_STYLE_MSGBOX, "Help - Teleports", "/MoneyShip - ñôéðú äëñó\n/Race - àæåø äîéøåöéí\n/Drift - àæåø äãøéôèéí\n/Ammu - çðåú äðù÷éí\n/Hospital - áéú äçåìéí\n/Airport - ùãä äúòåôä\n/Jeeps - àæåø äâ'éôéí\n/Bank - äáð÷\n/Station - äúçðä äîøëæéú\n/Tower - äîâãì äâáåää\n/Ramp - àæåø äøàîôåú\n/Police - úçðú äîùèøä\n/Carpark - îâøù äçðééä\n/Military - áñéñ öáàé\n/SanFierro - ñàï ôééøå\n/LosSantos - ìåñ ñàðèåñ\n/LasVenturas - ìàñ åðèåøàñ", "éöéàä", "çæåø");
	}
    if(dialogid == 13_4)
