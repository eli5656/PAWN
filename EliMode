	{
		if(GetAdminLevel(playerid) < 1 && GetTempAdminLevel(playerid) < 1 && GetSpyAdminLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì1");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/SetActivityStatus(Sas) [playerid] :öåøú ùéîåù");
