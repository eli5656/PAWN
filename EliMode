		else
		{
			new bankamount = strval(inputtext);
			if(!bankamount) return SendClientMessage(playerid, White, "äéðê öøéê ìëúåá ñëåí ëìùäå");
			if(bankamount <= 0) return SendClientMessage(playerid, White, "ñëåí äëñó ùàúä îô÷éã çééá ìäéåú âãåì 0$");
			if(bankamount > AntiCheat_GetPlayerMoney(playerid)) return SendClientMessage(playerid, White, "àéï ìê àú ñëåí äëñó ùáøöåðê ìäô÷éã");
	 		if((bankamount+DOF2_GetInt(File(playerid), "Bank")) > 99999999) return SendClientMessage(playerid, White, "àéðê éëåì ìäô÷éã éåúø î99,999,999$");
