			if(!strlen(inputtext)) return ShowPlayerDialog(playerid, 15_4, DIALOG_STYLE_INPUT, "îòøëú ðëñéí - ùéðåé øååçå ùì ðëñ ÷ééí", "àðà äëðñ àú äàéãé ùì äðëñ\nòì îðú ìùðåú àú øååçå\n{FF0000}àðà äëðñ àéãé áëãé ìäîùéê", "áçø", "çæåø");
            new pid = strval(inputtext);
			format(PropFile, sizeof(PropFile), "Properties/%d.ini", pid);
			for(new i=0; i<MAX_PROPS; i++)
			{
				if(!DOF2_FileExists(PropFile)) return ShowPlayerDialog(playerid, 15_3, DIALOG_STYLE_INPUT, "îòøëú ðëñéí - ùéðåé øååçå ùì ðëñ ÷ééí", "àðà äëðñ àú äùí ùì äðëñ\nòì îðú ìùðåú àú øååçå\n{FF0000}äàéãé ùì äðëñ ùöééðú àéðå ÷ééí", "áçø", "çæåø");
				PropName[playerid] = DOF2_GetString(PropFile, "Name");
				PropSerialNum[playerid] = DOF2_GetInt(PropFile, "ID");
				format(string, sizeof(string), "àðà äëðñ àú äøååç\n%s ùáøöåðê ìùðåú ìðëñ", PropName[playerid]);
				ShowPlayerDialog(playerid, 15_4_1, DIALOG_STYLE_INPUT, "îòøëú ðëñéí - ùéðåé øååçå ùì ðëñ ÷ééí", string, "áçø", "çæåø");
				return 1;
			}
		}
	}
	if(dialogid == 15_4_1)
	{
		if(!response) ShowPlayerDialog(playerid, 15_4, DIALOG_STYLE_INPUT, "îòøëú ðëñéí - ùéðåé øååçå ùì ðëñ ÷ééí", "àðà äëðñ àú äàéãé ùì äðëñ\nòì îðú ìùðåú àú øååçå", "áçø", "çæåø");
		else
		{
			format(string, sizeof(string), "àðà äëðñ àú äøååç\n%s ùáøöåðê ìùðåú ìðëñ\n{FF0000}äøååç ùäæðú ùâåé", PropName[playerid]);
			if(!IsNumeric(inputtext)) return ShowPlayerDialog(playerid, 15_4_1, DIALOG_STYLE_INPUT, "îòøëú ðëñéí - ùéðåé øååçå ùì ðëñ ÷ééí", string, "áçø", "çæåø");
			format(string, sizeof(string), "àðà äëðñ àú äøååç\n%s ùáøöåðê ìùðåú ìðëñ\n{FF0000}òìéê ìøùåí øååç ëãé ìäîùéê", PropName[playerid]);
			if(!strlen(inputtext)) return ShowPlayerDialog(playerid, 15_4_1, DIALOG_STYLE_INPUT, "îòøëú ðëñéí - ùéðåé øååçå ùì ðëñ ÷ééí", string, "áçø", "çæåø");
			format(PropFile, sizeof(PropFile), "Properties/%d.ini", PropSerialNum[playerid]);
			DOF2_SetInt(PropFile, "Amount", strval(inputtext));
			Delete3DTextLabel(PropText[PropSerialNum[playerid]]);
			DestroyPickup(PropPickup[PropSerialNum[playerid]]);
			format(string, sizeof(string), "{AA3333}%s {FFFFFF}:ùí äðëñ\n{AA3333}%s {FFFFFF}:áòìéí\n{AA3333}%s$ {FFFFFF}:îçéø äðëñ\n{AA3333}%s$ {FFFFFF}:øååç ì%s ã÷åú\n{AA3333}ùáî÷ìãú {FFFFFF}\"F\"{AA3333} ìùéîåù ìçõ òì î÷ù", DOF2_GetString(PropFile, "Name"), DOF2_GetString(PropFile, "Owner"), GetNum(DOF2_GetInt(PropFile, "Value")), GetNum(DOF2_GetInt(PropFile, "Amount")), GetNum(DOF2_GetInt("Properties/Setting.ini", "AmountMinutes")));
			PropText[PropSerialNum[playerid]] = Create3DTextLabel(string, Gold, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z")+0.5, 60.0, 0, 1);
			PropPickup[PropSerialNum[playerid]] = CreatePickup(1273, 1, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z"), 0);
			format(string,sizeof(string), "! {FFFFFF}%s{AA3333} ìøååç {FFFFFF}%s{AA3333} ùéðéú àú øååçå ùì äðëñ", GetNum(strval(inputtext)), PropName[playerid]);
			SendClientMessage(playerid, Red, string);
			return 1;
     	}
	}
 	if(dialogid == 15_5)
	{
		if(!response) ShowPlayerDialog(playerid, 15_0, DIALOG_STYLE_LIST, "îòøëú ðëñéí", "éöéøú ðëñ çãù\nîçé÷ú ðëñ ÷øåá\näææú îé÷åí ùì ðëñ ÷ééí\nùéðåé òøê ùì ðëñ ÷ééí\nùéðåé øååçå ùì ðëñ ÷ééí\nùéðåé æîï ÷áìú äøååç\náãé÷ú àéãé ùì ðëñ ÷øåá", "áçø", "áéèåì");
		else
		{
			if(!IsNumeric(inputtext)) return ShowPlayerDialog(playerid, 15_5, DIALOG_STYLE_INPUT, "îòøëú ðëñéí - ùéðåé æîï ÷áìú äøååç", "àðà äëðñ àú äæîï áã÷åú áìáã\n{FF0000}äæîï ùäæðú ùâåé", "áçø", "çæåø");
			if(!strlen(inputtext)) return ShowPlayerDialog(playerid, 15_5, DIALOG_STYLE_INPUT, "îòøëú ðëñéí - ùéðåé æîï ÷áìú äøååç", "àðà äëðñ àú äæîï áã÷åú áìáã\n{FF0000}àðà äëðñ æîï áëãé ìäîùéê", "áçø", "çæåø");
            DOF2_SetInt("Properties/Setting.ini", "AmountMinutes", strval(inputtext));
			for(new i=0; i<MAX_PROPS; i++)
			{
				format(PropFile, sizeof(PropFile), "Properties/%d.ini", i);
				if(DOF2_FileExists(PropFile))
				{
					Delete3DTextLabel(PropText[i]);
					DestroyPickup(PropPickup[i]);
					format(string, sizeof(string), "{AA3333}%s {FFFFFF}:ùí äðëñ\n{AA3333}%s {FFFFFF}:áòìéí\n{AA3333}%s$ {FFFFFF}:îçéø äðëñ\n{AA3333}%s$ {FFFFFF}:øååç ì%s ã÷åú\n{AA3333}ùáî÷ìãú {FFFFFF}\"F\"{AA3333} ìùéîåù ìçõ òì î÷ù", DOF2_GetString(PropFile, "Name"), DOF2_GetString(PropFile, "Owner"), GetNum(DOF2_GetInt(PropFile, "Value")), GetNum(DOF2_GetInt(PropFile, "Amount")), GetNum(DOF2_GetInt("Properties/Setting.ini", "AmountMinutes")));
					PropText[i] = Create3DTextLabel(string, Gold, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z")+0.5, 60.0, 0, 1);
					PropPickup[i] = CreatePickup(1273, 1, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z"), 0);
				}
			}
			format(string,sizeof(string), "!ùéðéú àú æîï ÷áìú äøååç îðëñéí ì{FFFFFF}%s{AA3333} ã÷åú", GetNum(strval(inputtext)));
			SendClientMessage(playerid, Red, string);
		}
	}
	
	if(dialogid == 15_2 && response)
	{
       	if(listitem == 0)
		{
			if(HaveProperties(playerid) >= 3) return SendClientMessage(playerid, White, "éù áøùåúê ëáø 3 ðëñéí");
			for(new i=0; i<MAX_PROPS; i++)
			{
		   		format(PropFile, sizeof(PropFile), "Properties/%d.ini", i);
			   	if(DOF2_FileExists(PropFile) && PlayerToPoint(1, playerid, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z")))
				{
					if(strcmp(DOF2_GetString(PropFile, "Owner"), GetName(playerid), true) == 0) return SendClientMessage(playerid, White, "àúä ëáø äáòìéí äðåëçé ùì ðëñ æä");
					if(AntiCheat_GetPlayerMoney(playerid) < DOF2_GetInt(PropFile, "Value")) return SendClientMessage(playerid, White, "àéï ìê àú ñëåí äëñó äãøåù ì÷ðééú ðëñ æä");
					DOF2_SetInt(File(playerid), "Properties", DOF2_GetInt(File(playerid), "Properties")+1);
					DOF2_SetInt(PropFile, "Owned", 1);
					DOF2_SetString(PropFile, "Owner", GetName(playerid));
					AntiCheat_GivePlayerMoney(playerid, false, DOF2_GetInt(PropFile, "Value"));
					Delete3DTextLabel(PropText[i]);
					DestroyPickup(PropPickup[i]);
					format(string, sizeof(string), "{AA3333}%s {FFFFFF}:ùí äðëñ\n{AA3333}%s {FFFFFF}:áòìéí\n{AA3333}%s$ {FFFFFF}:îçéø äðëñ\n{AA3333}%s$ {FFFFFF}:øååç ì%s ã÷åú\n{AA3333}ùáî÷ìãú {FFFFFF}\"F\"{AA3333} ìùéîåù ìçõ òì î÷ù", DOF2_GetString(PropFile, "Name"), DOF2_GetString(PropFile, "Owner"), GetNum(DOF2_GetInt(PropFile, "Value")), GetNum(DOF2_GetInt(PropFile, "Amount")), GetNum(DOF2_GetInt("Properties/Setting.ini", "AmountMinutes")));
					PropText[i] = Create3DTextLabel(string, Gold, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z")+0.5, 60.0, 0, 1);
					PropPickup[i] = CreatePickup(1273, 1, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z"), 0);
					SendClientMessage(playerid, Red, "! ÷ðéú ðëñ æä áäöìçä");
					return 1;
			   	}
			}
		}
       	if(listitem == 1)
		{
			if(!HaveProperties(playerid)) return SendClientMessage(playerid, White, "àéï ìê ðëñéí ìîëéøä");
            for(new i=0; i<MAX_PROPS; i++)
			{
		   		format(PropFile, sizeof(PropFile), "Properties/%d.ini", i);
			   	if(DOF2_FileExists(PropFile) && PlayerToPoint(1, playerid, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z")))
				{
					if(strcmp(DOF2_GetString(PropFile, "Owner"), "None", true) == 0) return SendClientMessage(playerid, White, "àúä ìà äáòìéí ùì ðëñ æä");
					DOF2_SetInt(File(playerid), "Properties", DOF2_GetInt(File(playerid), "Properties")-1);
					DOF2_SetInt(PropFile, "Owned", 0);
