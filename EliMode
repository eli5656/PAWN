		weap = strval(tmp1);
		if(weap < 1 || weap > 46) return SendClientMessage(playerid, White, "äàééãé ðù÷ ùöééðú ùâåé");
		if(weap == 38 || weap == 39 || weap == 40 || weap == 43 || weap == 44 || weap == 45) return SendClientMessage(playerid, White, "äðù÷ ùöééðú àéðå éëåì ìäéåú ìùç÷ï îëéååï ùäéðå çñåí ìùéîåù");
		format(string, sizeof(string), "{FFFFFF}%s{33AA33} àéôñ ìê àú äðù÷ {FFFFFF}%s{33AA33} äîðäì", WeaponNames[weap], GetName(playerid));
	 	SendClientMessage(id, Green ,string);
		format(string, sizeof(string), "{FFFFFF}%s{33AA33} àú äðù÷ {FFFFFF}%s{33AA33} àéôñú ìùç÷ï", WeaponNames[weap], GetName(id));
	 	SendClientMessage(playerid, Green ,string);
		ResetPlayerWeapon(id, weap);
        return 1;
	}
