	if(strcmp(cmd, "/Clan", true) == 0)
	{
		tmp = strtok(cmdtext,idx);
	 	if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Clan [Manager | Invite | Join | Leave | SetLevel | Kick | Chat | Color | Msg | Data] :öåøú ùéîåù");
		if(strcmp(tmp, "Manager", true) == 0)
	 	{
			if(GetAdminLevel(playerid) < 5 && GetSpyAdminLevel(playerid) < 5 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì5");
	 	    tmp1 = strtok(cmdtext,idx);
	 	    if(!strlen(tmp1)) return SendClientMessage(playerid,White, "/Clan Manager [Create | Delete | Rename | GiveHq | pSpawn | vSpawn | AddManager | RemoveManager | SetBankClan] :öåøú äùéîåù");
		    if(strcmp(tmp1, "Create", true) == 0)
			{
			    tmp2 = strtok(cmdtext,idx);
			    format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", tmp2);
			 	if(!strlen(tmp2)) return SendClientMessage(playerid, White, "/Clan Manager Create [fullname] [playerid] :öåøú ùéîåù");
	        	for(new i = 0; i < strlen(tmp2); i++)
	 			if(tmp2[i] < 'ú' && tmp2[i] > 'à') return SendClientMessage(playerid, White, "òìéê ìáçåø ùí ì÷ìàï áàðâìéú áìáã");
			    if(DOF2_FileExists(ClanFile)) return SendClientMessage(playerid, White, "äùí ùöééðú úôåñ òì éãé ÷ìàï øùîé àçø");
			    tmp3 = strtok(cmdtext,idx);
			 	if(!strlen(tmp3)) return SendClientMessage(playerid, White, "/Clan Manager Create [fullname] [playerid] :öåøú ùéîåù");
				id = strval(tmp3);
				if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
				if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
				if(DOF2_GetInt(File(id), "InClan") == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ëáø ðîöà á÷ìàï øùîé");
				//------------------------------------------------------------------
				DOF2_CreateFile(ClanFile);
			    DOF2_SetString(ClanFile, "ClanName", tmp2);
				//------------------------------------------------------------------
				DOF2_SetInt(ClanFile, "Players", 1);
				DOF2_SetInt(ClanFile, "ClanChat", 0);
				DOF2_SetInt(ClanFile, "Bank", 0);
				DOF2_SetInt(ClanFile, "HaveHq", 0);
				DOF2_SetInt(ClanFile, "HaveMsg", 0);
				DOF2_SetString(ClanFile, "ClanMsg", "None");
				DOF2_SetInt(ClanFile, "HaveSpawn", 0);
				DOF2_SetFloat(ClanFile, "SpawnX", 0);
				DOF2_SetFloat(ClanFile, "SpawnY", 0);
				DOF2_SetFloat(ClanFile, "SpawnZ", 0);
				DOF2_SetFloat(ClanFile, "SpawnA", 0);
				DOF2_SetFloat(ClanFile, "SpawncarX", 0);
				DOF2_SetFloat(ClanFile, "SpawncarY", 0);
				DOF2_SetFloat(ClanFile, "SpawncarZ", 0);
				DOF2_SetFloat(ClanFile, "SpawncarA", 0);
	   			DOF2_SetInt(ClanFile, "HaveClanChatColor", 0);
