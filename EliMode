     	if(IsPlayerInAnyVehicle(id))
  		{
   			SetVehiclePos(GetPlayerVehicleID(id), X, Y, Z);
  		 	LinkVehicleToInterior(GetPlayerVehicleID(id), pinterior);
 			format(string, sizeof(string), "òí øëáê {FFFFFF}%s{33AA33} ùéâø àåúê ìùç÷ï {FFFFFF}%s{33AA33} äîðäì", GetName(id1), GetName(playerid));
		 	SendClientMessage(id, Green ,string);
 			format(string, sizeof(string), "àìééê òí øëáå {FFFFFF}%s{33AA33} ùéâø àú äùç÷ï {FFFFFF}%s{33AA33} äîðäì", GetName(playerid), GetName(id));
		 	SendClientMessage(id1, Green ,string);
 			format(string, sizeof(string), "òí øëáå {FFFFFF}%s{33AA33} ìùç÷ï {FFFFFF}%s{33AA33} ùéâøú àú äùç÷ï", GetName(id1), GetName(id));
