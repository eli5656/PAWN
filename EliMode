					{
						for(new a; a<MAX_PLAYERS; a++)
						{
							ShowPlayerNameTagForPlayer(a, i, true);
						}
						if(ActivityStatus[i] == 1)
						{
							ActLevel[i] = Create3DTextLabel("îöá ôòéìåú", Brown, X, Y, Z, 40.0, 0, 1);
							if(CheckAdminLevel(i) > 1)
							{
								Attach3DTextLabelToPlayer(ActLevel[i], i, 0.0, 0.0, 0.4);
							}
							else
							{
								Attach3DTextLabelToPlayer(ActLevel[i], i, 0.0, 0.0, 0.3);
							}
						}
						SetPlayerColor(i, RandomColor());
				 	}
				}
			}
			format(string, sizeof(string), "ùéðä ìðøàä ìùç÷ðéí áøãéåñ ùì {FFFFFF}%d.0{33AA33} öòãéí îîðå {FFFFFF}%s{33AA33} äîðäì", GetRadius(playerid), GetName(playerid));
			OL_SendClientMessageToAll(Green ,string);
	    	return 1;
	    }
