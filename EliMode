		 	SendClientMessage(id, Green ,string);
 			format(string, sizeof(string), "òí øëáê {FFFFFF}%s{33AA33} äùúâøú àì äùç÷ï", GetName(id));
		 	SendClientMessage(playerid, Green ,string);
  		}
  		else
  		{
   			SetPlayerPos(playerid, X, Y, Z+2);
   			SetPlayerInterior(playerid, pinterior);
 			format(string, sizeof(string), "äùúâø àìééê {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
		 	SendClientMessage(id, Green ,string);
 			format(string, sizeof(string), "{FFFFFF}%s{33AA33} äùúâøú àì äùç÷ï", GetName(id));
		 	SendClientMessage(playerid, Green ,string);
		}
  		return 1;
	}
	////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Goton", true) == 0 || strcmp(cmd, "/Gtn", true) == 0)
	{
		if(GetAdminLevel(playerid) < 1 && GetTempAdminLevel(playerid) < 1 && GetSpyAdminLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì1");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Goto(Gt) [Name] :öåøú äùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
  		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		if(IsPlayerInPvpArea(id) == 1) return SendClientMessage(playerid, White, "áùøú Pvpùç÷ï æä ðîöà áàçã îàæåøé ä");
		if(IsPlayerInMission(id) == 1) return SendClientMessage(playerid, White, "ùç÷ï æä îáöò ëøâò àú àçú îîùéîåú äùøú");
		if(AutomaticACT_In[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áôòéìåú àåèåîèéú");
		if(ActivityStatus[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ôòéìåú");
		if(InBattle[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà á÷øá àéùé");
		if(inJail[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áëìà");
		if(inAFK[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ìà ðîöà");
		for(new i = 0;i<MAX_PLAYERS;i++)
		{
			if(strcmp(tmp,GetName(i), true) == 0)
		    {
    			GetPlayerPos(i, X, Y, Z);
				pinterior = GetPlayerInterior(i);
				if(IsPlayerInAnyVehicle(playerid))
		  		{
		   			SetVehiclePos(GetPlayerVehicleID(playerid), X, Y, Z+2);
				 	LinkVehicleToInterior(GetPlayerVehicleID(playerid), pinterior);
		 			format(string, sizeof(string), "äùúâø àìééê òí øëáå {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
				 	SendClientMessage(i, Green ,string);
		 			format(string, sizeof(string), "òí øëáê {FFFFFF}%s{33AA33} äùúâøú àì äùç÷ï", GetName(i));
				 	SendClientMessage(playerid, Green ,string);
		  		}
		  		else
		  		{
		   			SetPlayerPos(playerid, X, Y, Z+2);
		   			SetPlayerInterior(playerid, pinterior);
		 			format(string, sizeof(string), "äùúâø àìééê {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
				 	SendClientMessage(i, Green ,string);
		 			format(string, sizeof(string), "{FFFFFF}%s{33AA33} äùúâøú àì äùç÷ï", GetName(i));
				 	SendClientMessage(playerid, Green ,string);
				}
		        break;
		    }
		}
  		return 1;
	}
