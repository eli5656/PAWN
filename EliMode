	if(strcmp(cmd, "/ReadRules", true) == 0 || strcmp(cmd, "/RR", true) == 0)
	{
		if(GetAdminLevel(playerid) < 2 && GetTempAdminLevel(playerid) < 2 && GetSpyAdminLevel(playerid) < 2) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì2");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/ReadRules(RR) [playerid] [reason] :öåøú ùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
  		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(strcmp(GetName(id), "L1or", true) == 0 || strcmp(GetName(id), "DisPlay", true) == 0 || strcmp(GetName(id), "Speed[i]", true) == 0) return SendClientMessage(playerid, White, "àéðê éëåì ìáòåè ùç÷ï æä îäùøú");
		if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
	 	new length = strlen(cmdtext);
		while ((idx < length) && (cmdtext[idx] <= ' '))
		{
			idx++;
		}
		new offset = idx;
		new result[64];
		while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
		{
			result[idx - offset] = cmdtext[idx];
			idx++;
		}
		result[idx - offset] = EOS;
		if(!strlen(result)) return SendClientMessage(playerid, White, "/ReadRules(RR) [playerid] [reason] :öåøú ùéîåù");
	 	format(string, sizeof(string), "{FFFFFF}%s{33AA33} ðáòè åäåëøç ì÷øåà çå÷éí òì éãé äîðäì {FFFFFF}%s{33AA33} äùç÷ï", GetName(playerid), GetName(id));
		OL_SendClientMessageToAll(Green, string);
  		format(string, sizeof(string), "({FFFFFF}%s{33AA33})", (result));
  		OL_SendClientMessageToAll(Green, string);
		DOF2_SetInt(File(id), "RRKick", DOF2_GetInt(File(id), "RRKick")+1);
		DOF2_SetInt(File(id), "ReadRules", 0);
  		kixkx[id] = SetTimerEx("kickx", 1000, 1, "d", id);
		//---------------------------------------------------------------- [LOG]
		format(string, sizeof(string), "%s gave rr-kick to player %s (reason: %s) | Time: %d:%d:%d - Date: %d/%d/%d", GetName(playerid), GetName(id), (result), Hour, Minute, Second, Day, Month, Year);
  		AdminCommands_Logs(string);
		//----------------------------------------------------------------------
 		return 1;
 	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/All", true) == 0)
	{
		if(GetAdminLevel(playerid) < 5 && GetSpyAdminLevel(playerid) < 5 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì5");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/All [function] :öåøú ùéîåù");
		if(strcmp(tmp, "Kick", true) == 0 || strcmp(tmp, "K", true) == 0)
		{
		 	format(string, sizeof(string), "áòè àú ëì äùç÷ðéí îäùøú {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
			OL_SendClientMessageToAll(Green, string);
			for(new i=0; i<MAX_PLAYERS; i++)
			{
				if(IsPlayerConnected(i))
				{
				    if(Logged[i] == true)
				    {
					    kixkx[i] = SetTimerEx("kickx", 1000, 1, "d", i);
					}
				}
			}
	 		return 1;
	 	}
	 	else if(strcmp(tmp, "Get", true) == 0 || strcmp(tmp, "G", true) == 0)
		{
	 		GetPlayerPos(playerid, X, Y, Z);
			for(new i=0; i<MAX_PLAYERS; i++)
			{
				if(IsPlayerConnected(i))
				{
				    if(Logged[i] == true)
				    {
				        if(!inJail[i] && !IsPlayerInPvpArea(i) && !ActivityStatus[i] && !AutomaticACT_In[i] && !IsPlayerInMission(i))
						{
							SetPlayerPos(i, X, Y, Z);
						}
					}
				}
			}
		 	format(string, sizeof(string), "ùéâø àìéå àú ëì äùç÷ðéí áùøú {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
			OL_SendClientMessageToAll(Green, string);
	 		return 1;
	 	}
 	 	else if(strcmp(tmp, "Freeze", true) == 0 || strcmp(tmp, "F", true) == 0)
