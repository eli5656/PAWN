			    	if(IsPlayerInAnyVehicle(i))
			    	{
						if(IsPlayerNearPlayer(GetRadius(playerid), playerid, i))
						{
		 					RepairVehicle(GetPlayerVehicleID(id));
		 				}
		 			}
		 		}
		 	}
			format(string, sizeof(string), "úé÷ï àú äøëáéí ìøãéåñ ùì {FFFFFF}%d.0{33AA33} öòãéí îîðå {FFFFFF}%s{33AA33} äîðäì", GetRadius(playerid), GetName(playerid));
			OL_SendClientMessageToAll(Green ,string);
	    	return 1;
	    }
		if(strcmp(tmp, "Invisable", true) == 0 || strcmp(tmp, "Inv", true) == 0)
		{
			for(new i=0; i<MAX_PLAYERS; i++)
			{
   				if(i != playerid)
				{
					if(IsPlayerNearPlayer(GetRadius(playerid), playerid, i))
					{
						for(new a; a<MAX_PLAYERS; a++)
						{
							ShowPlayerNameTagForPlayer(a, i, false);
						}
						Delete3DTextLabel(ActLevel[i]);
	 					SetPlayerColor(i, Invisible);
					}
				}
			}
			format(string, sizeof(string), "ùéðä ìáìúé ðøàä ìùç÷ðéí áøãéåñ ùì {FFFFFF}%d.0{33AA33} öòãéí îîðå {FFFFFF}%s{33AA33} äîðäì", GetRadius(playerid), GetName(playerid));
