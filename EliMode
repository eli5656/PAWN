				if(!strlen(tmp2)) return SendClientMessage(playerid, White, "/Diary(Dia) Edit 7 [Remove | Rewrite] :öåøú ùéîåù");
				if(strcmp(tmp2, "Remove", true) == 0)
				{
					SendClientMessage(playerid, Darkblue, "îç÷ú àú ùåøä îñôø 7 áéåîï ùìê");
					DOF2_SetString(File(playerid), "DiaryLine7", "None");
					return 1;
				}
				if(strcmp(tmp2, "Rewrite", true) == 0)
				{
					new length = strlen(cmdtext);
					while ((idx < length) && (cmdtext[idx] <= ' '))
					{
						idx++;
					}
					new offset = idx;
					new result[64];
					while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
					{
						result[idx - offset] = cmdtext[idx];
						idx++;
					}
					result[idx - offset] = EOS;
					if(!strlen(result)) return SendClientMessage(playerid, White, "/Diary(Dia) Edit 7 Rewrite [text] :öåøú ùéîåù");
					DOF2_SetString(File(playerid), "DiaryLine7", (result));
					SendClientMessage(playerid, Darkblue, "òøëú àú ùåøä îñôø 7 áéåîï ùìê");
					format(string, sizeof(string), "(%s)", (result));
					SendClientMessage(playerid, Darkblue ,string);
					return 1;
				}
				else
				{
					SendClientMessage(playerid, White, "{FFFFFF}ùâåééä {FF0000}/Diary Edit 7 {FFFFFF}àôùøåú");
				}
				return 1;
			}
			if(strcmp(tmp1, "8", true) == 0)
			{
		    	tmp2 = strtok(cmdtext,idx);
				if(!strlen(tmp2)) return SendClientMessage(playerid, White, "/Diary(Dia) Edit 8 [Remove | Rewrite] :öåøú ùéîåù");
