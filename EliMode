						{
					    	TogglePlayerControllable(i, 0);
					    }
				    }
				}
			}
		 	format(string, sizeof(string), "ä÷ôéà àú ëì äùç÷ðéí áùøú {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
