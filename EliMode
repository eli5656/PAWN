	{
  		if(inAFK[playerid] == 0)
  		{
  		    SendClientMessage(playerid, Grellow, "{FFFFFF}[SYSTEM]{ADFF2F} òáøú ìîöá ìà ðîöà {FFFFFF}•••");
   			format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}òáø ìîöá ìà ðîöà {FF0000}%s {FFFFFF}äîùúîù", GetName(playerid));
			OL_SendClientMessageToAllEx(playerid, Grey, string);
			TextDrawShowForPlayer(playerid, Afk);
			TextDrawShowForPlayer(playerid, Afk1);
			TogglePlayerControllable(playerid, 0);
			afkvirtual = GetPlayerVirtualWorld(playerid);
			SetPlayerVirtualWorld(playerid, 1);
      		inAFK[playerid] = 1;
  		}
