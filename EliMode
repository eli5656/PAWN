	return 1;
}

//------------------------------------------------------------------------------

public Monster_Start()
{
	KillTimer(Monster_ACT_Timer_Start);
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú ùäçìä:{99FFFF} îåðñèøéí");
	OL_SendClientMessageToAll(White, "{FFFFFF}æîï äøùîä:{99FFFF} 30 ùðéåú");
	new string[256];
	AutomaticACT_RandomPrice = randomex(100000, 150000);
	AutomaticACT_Price = AutomaticACT_RandomPrice;
	format(string, sizeof(string), "{FFFFFF}ôøñ: {99FFFF}%s$", GetNum(AutomaticACT_Price));
	OL_SendClientMessageToAll(White, string);
	OL_SendClientMessageToAll(White, "/{99FFFF}Join {FFFFFF}:ô÷åãä ìäøùîä");
	Monster_ACT_Timer_Register = SetTimer("Monster_CloseRegister", 1000*30*1, 1);
	Monster_CountDown_1(30);
	return 1;
}

public Monster_CloseRegister()
{
	KillTimer(Monster_ACT_Timer_Register);
	if(Monster_ACT_AllPlayers <= 1) return Monster_End(0);
	new string[256];
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}äôòéìåú:{99FFFF} äîåðñèøéí - úúçéì áòåã 10 ùðéåú");
	format(string, sizeof(string), "{FFFFFF}ëîåú äîùúúôéí: {99FFFF}%d", Monster_ACT_AllPlayers);
	OL_SendClientMessageToAll(White, string);
	Monster_ACT_Start = 0;
	Monster_ACT_Running = 1;
	Monster_ACT_Timer_Play = SetTimer("Monster_Play", 1000*10*1, 1);
	Monster_CountDown_2(10);
	return 1;
}

public Monster_Play()
{
	KillTimer(Monster_ACT_Timer_Play);
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} äîåðñèøéí - äçìä");
	OL_SendClientMessageToAll(White, "ùéäéä ìëì äîùúúôéí áäöìçä !");
	Monster_ACT_Running = 0;
	Monster_ACT_On = 1;
	for(new i=0; i<MAX_PLAYERS; i++)
   	{
   	    if(Monster_ACT_In[i] == 1)
   	    {
   	        TogglePlayerControllable(i, 1);
   	        ResetPlayerWeapons(i);
			inFreeze[i] = 0;
   	    }
	}
	return 1;
}

public Monster_CheckWinner(playerid)
{
	for(new i=0; i<MAX_PLAYERS; i++) if(Monster_ACT_On == 1)
	{
		if(Monster_ACT_AllPlayers == 1)
		{
			if(Monster_ACT_In[i] == 1)
	    	{
				new string[256];
				OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
				OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} äîåðñèøéí - äñúééîä");
				format(string, sizeof(string), "{99FFFF}%s{FFFFFF} :äîðöç áôòéìåú äåà", GetName(i));
				OL_SendClientMessageToAll(White, string);
				DOF2_SetInt(File(i), "Bank", DOF2_GetInt(File(i), "Bank")+AutomaticACT_Price);
				DOF2_SetInt(File(i), "MonsterWins", DOF2_GetInt(File(i), "MonsterWins")+1);
				SetVehicleToRespawn(GetPlayerVehicleID(i));
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				SetPlayerInterior(i, 0);
				Monster_ACT_On = 0;
				Monster_ACT_AllPlayers = 0;
				Monster_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				AutomaticACT_Price = 0;
				AutomaticACT_AlreadyON = 0;
				DestroyVehicle(Monster[0]);
				DestroyVehicle(Monster[1]);
				DestroyVehicle(Monster[2]);
				DestroyVehicle(Monster[3]);
				DestroyVehicle(Monster[4]);
				DestroyVehicle(Monster[5]);
				DestroyVehicle(Monster[6]);
				DestroyVehicle(Monster[7]);
				DestroyVehicle(Monster[8]);
				DestroyVehicle(Monster[9]);
				DestroyVehicle(Monster[10]);
				DestroyVehicle(Monster[11]);
				DestroyVehicle(Monster[12]);
				DestroyVehicle(Monster[13]);
				DestroyVehicle(Monster[14]);
				Monster_ACT_Place[0] = 0;
				Monster_ACT_Place[1] = 0;
				Monster_ACT_Place[2] = 0;
				Monster_ACT_Place[3] = 0;
				Monster_ACT_Place[4] = 0;
				Monster_ACT_Place[5] = 0;
				Monster_ACT_Place[6] = 0;
				Monster_ACT_Place[7] = 0;
				Monster_ACT_Place[8] = 0;
				Monster_ACT_Place[9] = 0;
				Monster_ACT_Place[10] = 0;
				Monster_ACT_Place[11] = 0;
				Monster_ACT_Place[12] = 0;
				Monster_ACT_Place[13] = 0;
				Monster_ACT_Place[14] = 0;
			}
		}
		else if(!Monster_ACT_AllPlayers)
		{
			OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
			OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} äîåðñèøéí - äñúééîä");
			OL_SendClientMessageToAll(White, "{FFFFFF}äîðöç áôòéìåú äåà:{99FFFF} àéï îðöç");
			Monster_ACT_On = 0;
			Monster_ACT_AllPlayers = 0;
			AutomaticACT_Price = 0;
			AutomaticACT_AlreadyON = 0;
			DestroyVehicle(Monster[0]);
			DestroyVehicle(Monster[1]);
			DestroyVehicle(Monster[2]);
			DestroyVehicle(Monster[3]);
			DestroyVehicle(Monster[4]);
			DestroyVehicle(Monster[5]);
			DestroyVehicle(Monster[6]);
			DestroyVehicle(Monster[7]);
			DestroyVehicle(Monster[8]);
			DestroyVehicle(Monster[9]);
			DestroyVehicle(Monster[10]);
			DestroyVehicle(Monster[11]);
			DestroyVehicle(Monster[12]);
			DestroyVehicle(Monster[13]);
			DestroyVehicle(Monster[14]);
			Monster_ACT_Place[0] = 0;
			Monster_ACT_Place[1] = 0;
			Monster_ACT_Place[2] = 0;
			Monster_ACT_Place[3] = 0;
			Monster_ACT_Place[4] = 0;
			Monster_ACT_Place[5] = 0;
			Monster_ACT_Place[6] = 0;
			Monster_ACT_Place[7] = 0;
			Monster_ACT_Place[8] = 0;
			Monster_ACT_Place[9] = 0;
			Monster_ACT_Place[10] = 0;
			Monster_ACT_Place[11] = 0;
			Monster_ACT_Place[12] = 0;
			Monster_ACT_Place[13] = 0;
			Monster_ACT_Place[14] = 0;
		}
		Monster_ACT_PlayerPlace[i][0] = 0;
		Monster_ACT_PlayerPlace[i][1] = 0;
		Monster_ACT_PlayerPlace[i][2] = 0;
		Monster_ACT_PlayerPlace[i][3] = 0;
		Monster_ACT_PlayerPlace[i][4] = 0;
		Monster_ACT_PlayerPlace[i][5] = 0;
		Monster_ACT_PlayerPlace[i][6] = 0;
		Monster_ACT_PlayerPlace[i][7] = 0;
		Monster_ACT_PlayerPlace[i][8] = 0;
		Monster_ACT_PlayerPlace[i][9] = 0;
		Monster_ACT_PlayerPlace[i][10] = 0;
		Monster_ACT_PlayerPlace[i][11] = 0;
		Monster_ACT_PlayerPlace[i][12] = 0;
		Monster_ACT_PlayerPlace[i][13] = 0;
		Monster_ACT_PlayerPlace[i][14] = 0;
	}
	return 1;
}

public Monster_End(endid)
{
	if(endid == 0)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îåðñèøéí - áåèìä");
		OL_SendClientMessageToAll(White, "(îéðéîåí ùç÷ðéí ìäôòìú äôòéìåú äéðä 2 ùç÷ðéí)");
	}
	else if(endid == 1)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îåðñèøéí - áåèìä");
		OL_SendClientMessageToAll(White, "(äîðäì áéèì àú äôòéìåú)");
	}
	else if(endid == 2)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îåðñèøéí - áåèìä");
		OL_SendClientMessageToAll(White, "(äôòéìåú äàåèåîèéú äúðâùä áôòéìåú äàåèåîèéú ùäåôòìä òì éãé äîðäì)");
	}
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(Monster_ACT_Start == 1 && Monster_ACT_Running == 0 && Monster_ACT_On == 0)
		{
			if(Monster_ACT_In[i] == 1)
			{
				SetVehicleToRespawn(GetPlayerVehicleID(i));
			    SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Monster_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
		else if(Monster_ACT_Start == 0 && Monster_ACT_Running == 1 && Monster_ACT_On == 0)
		{
			if(Monster_ACT_In[i] == 1)
			{
				SetVehicleToRespawn(GetPlayerVehicleID(i));
			    SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Monster_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
		else if(Monster_ACT_Start == 0 && Monster_ACT_Running == 0 && Monster_ACT_On == 1)
		{
			if(Monster_ACT_In[i] == 1)
			{
				SetVehicleToRespawn(GetPlayerVehicleID(i));
			    SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Monster_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
		Monster_ACT_PlayerPlace[i][0] = 0;
		Monster_ACT_PlayerPlace[i][1] = 0;
		Monster_ACT_PlayerPlace[i][2] = 0;
		Monster_ACT_PlayerPlace[i][3] = 0;
		Monster_ACT_PlayerPlace[i][4] = 0;
		Monster_ACT_PlayerPlace[i][5] = 0;
		Monster_ACT_PlayerPlace[i][6] = 0;
		Monster_ACT_PlayerPlace[i][7] = 0;
		Monster_ACT_PlayerPlace[i][8] = 0;
		Monster_ACT_PlayerPlace[i][9] = 0;
		Monster_ACT_PlayerPlace[i][10] = 0;
		Monster_ACT_PlayerPlace[i][11] = 0;
		Monster_ACT_PlayerPlace[i][12] = 0;
		Monster_ACT_PlayerPlace[i][13] = 0;
		Monster_ACT_PlayerPlace[i][14] = 0;
	}
	Monster_ACT_Start = 0;
	Monster_ACT_AllPlayers = 0;
	Monster_ACT_On = 0;
	AutomaticACT_Price = 0;
	AutomaticACT_AlreadyON = 0;
	DestroyVehicle(Monster[0]);
	DestroyVehicle(Monster[1]);
	DestroyVehicle(Monster[2]);
	DestroyVehicle(Monster[3]);
	DestroyVehicle(Monster[4]);
	DestroyVehicle(Monster[5]);
	DestroyVehicle(Monster[6]);
	DestroyVehicle(Monster[7]);
	DestroyVehicle(Monster[8]);
	DestroyVehicle(Monster[9]);
	DestroyVehicle(Monster[10]);
	DestroyVehicle(Monster[11]);
	DestroyVehicle(Monster[12]);
	DestroyVehicle(Monster[13]);
	DestroyVehicle(Monster[14]);
	Monster_ACT_Place[0] = 0;
	Monster_ACT_Place[1] = 0;
	Monster_ACT_Place[2] = 0;
	Monster_ACT_Place[3] = 0;
	Monster_ACT_Place[4] = 0;
	Monster_ACT_Place[5] = 0;
	Monster_ACT_Place[6] = 0;
	Monster_ACT_Place[7] = 0;
	Monster_ACT_Place[8] = 0;
	Monster_ACT_Place[9] = 0;
	Monster_ACT_Place[10] = 0;
	Monster_ACT_Place[11] = 0;
	Monster_ACT_Place[12] = 0;
	Monster_ACT_Place[13] = 0;
	Monster_ACT_Place[14] = 0;
	KillTimer(Monster_ACT_Timer_Start);
	KillTimer(Monster_ACT_Timer_Register);
	KillTimer(Monster_ACT_Timer_Play);
	KillTimer(Monster_ACT_Countdown_1);
	KillTimer(Monster_ACT_Countdown_2);
	return 1;
}

public Monster_CountDown_1(cd)
{
	new string[256];
	if(cd > 10)
	{
		format(string, sizeof(string), "~b~Monster will start in: ~w~%d ~n~~y~Register command is: ~w~/Join", cd);
		GameTextForAll(string, 1000, 3);
	}
	else if(cd <= 10)
	{
		format(string, sizeof(string), "~b~Monster will start in: ~w~%d ~n~~y~Register command is: ~w~/Join ~n~~r~HURRY UP TO REGISTER!!!", cd);
		GameTextForAll(string, 1000, 3);
	}
	if(cd > 0)
	{
		cd--;
		Monster_ACT_Countdown_1 = SetTimerEx("Monster_CountDown_1", 1000, 0, "%d", cd);
	}
	else KillTimer(Monster_ACT_Countdown_1);
	return 1;
}

public Monster_CountDown_2(cd)
{
	new string[256];
	format(string, sizeof(string), "~b~Monster will start in: ~r~%d", cd);
	GameTextForAll(string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		Monster_ACT_Countdown_2 = SetTimerEx("Monster_CountDown_2", 1000, 0, "%d", cd);
	}
	else
	{
		GameTextForAll("~b~Monster was ~w~started", 1000, 3);
		KillTimer(Monster_ACT_Countdown_2);
		CountdownON = 0;
	}
	return 1;
}

//------------------------------------------------------------------------------

public Karting_Start()
{
	KillTimer(Karting_ACT_Timer_Start);
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú ùäçìä:{99FFFF} ÷àøèéðâ");
	OL_SendClientMessageToAll(White, "{FFFFFF}æîï äøùîä:{99FFFF} 30 ùðéåú");
	new string[256];
	AutomaticACT_RandomPrice = randomex(100000, 150000);
	AutomaticACT_Price = AutomaticACT_RandomPrice;
	format(string, sizeof(string), "{FFFFFF}ôøñ: {99FFFF}%s$", GetNum(AutomaticACT_Price));
	OL_SendClientMessageToAll(White, string);
	OL_SendClientMessageToAll(White, "/{99FFFF}Join {FFFFFF}:ô÷åãä ìäøùîä");
	Karting_ACT_Timer_Register = SetTimer("Karting_CloseRegister", 1000*30*1, 1);
	Karting_CountDown_1(30);
	return 1;
}

public Karting_CloseRegister()
{
	KillTimer(Karting_ACT_Timer_Register);
	if(Karting_ACT_AllPlayers <= 1) return Karting_End(0);
	new string[256];
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}äôòéìåú:{99FFFF} ä÷àøèéðâ - úúçéì áòåã 10 ùðéåú");
	format(string, sizeof(string), "{FFFFFF}ëîåú äîùúúôéí: {99FFFF}%d", Karting_ACT_AllPlayers);
	OL_SendClientMessageToAll(White, string);
	Karting_ACT_Start = 0;
	Karting_ACT_Running = 1;
	Karting_ACT_Timer_Play = SetTimer("Karting_Play", 1000*10*1, 1);
	Karting_CountDown_2(10);
	return 1;
}

public Karting_Play()
{
	KillTimer(Karting_ACT_Timer_Play);
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ä÷àøèéðâ - äçìä");
	OL_SendClientMessageToAll(White, "ùéäéä ìëì äîùúúôéí áäöìçä !");
	Karting_ACT_Running = 0;
	Karting_ACT_On = 1;
	for(new i=0; i<MAX_PLAYERS; i++)
   	{
   	    if(Karting_ACT_In[i] == 1)
   	    {
   	        TogglePlayerControllable(i, 1);
   	        ResetPlayerWeapons(i);
			inFreeze[i] = 0;
			SetPlayerRaceCheckpoint(i,2,2887.1545,2285.5391,10.3869,            2907.4978,2363.7610,10.2343,15);
			CP_KartingActivity[i]++;
   	    }
	}
	return 1;
}

public Karting_CheckWinner()
{
	for(new i=0; i<MAX_PLAYERS; i++) if(Karting_ACT_On == 1)
	{
		if(Karting_ACT_AllPlayers == 1)
		{
			if(Karting_ACT_In[i] == 1)
	    	{
				new string[256];
				OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
				OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ä÷àøèéðâ - äñúééîä");
				format(string, sizeof(string), "{99FFFF}%s{FFFFFF} :äîðöç áôòéìåú äåà", GetName(i));
				OL_SendClientMessageToAll(White, string);
				DOF2_SetInt(File(i), "Bank", DOF2_GetInt(File(i), "Bank")+AutomaticACT_Price);
				DOF2_SetInt(File(i), "KartingWins", DOF2_GetInt(File(i), "KartingWins")+1);
				SetVehicleToRespawn(GetPlayerVehicleID(i));
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				SetPlayerInterior(i, 0);
				Karting_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				Karting_ACT_On = 0;
				Karting_ACT_AllPlayers = 0;
				AutomaticACT_Price = 0;
				AutomaticACT_AlreadyON = 0;
				DestroyVehicle(Karting[0]);
				DestroyVehicle(Karting[1]);
				DestroyVehicle(Karting[2]);
				DestroyVehicle(Karting[3]);
				DestroyVehicle(Karting[4]);
				DestroyVehicle(Karting[5]);
				DestroyVehicle(Karting[6]);
				DestroyVehicle(Karting[7]);
				DestroyVehicle(Karting[8]);
				DestroyVehicle(Karting[9]);
				DestroyVehicle(Karting[10]);
				DestroyVehicle(Karting[11]);
				DestroyVehicle(Karting[12]);
				DestroyVehicle(Karting[13]);
				DestroyVehicle(Karting[14]);
				DestroyVehicle(Karting[15]);
				Karting_ACT_Place[0] = 0;
				Karting_ACT_Place[1] = 0;
				Karting_ACT_Place[2] = 0;
				Karting_ACT_Place[3] = 0;
				Karting_ACT_Place[4] = 0;
				Karting_ACT_Place[5] = 0;
				Karting_ACT_Place[6] = 0;
				Karting_ACT_Place[7] = 0;
				Karting_ACT_Place[8] = 0;
				Karting_ACT_Place[9] = 0;
				Karting_ACT_Place[10] = 0;
				Karting_ACT_Place[11] = 0;
				Karting_ACT_Place[12] = 0;
				Karting_ACT_Place[13] = 0;
				Karting_ACT_Place[14] = 0;
				Karting_ACT_Place[15] = 0;
				DisablePlayerRaceCheckpoint(i);
  				CP_KartingActivity[i] = 0;
			}
		}
		else if(!Karting_ACT_AllPlayers)
		{
			OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
			OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ä÷àøèéðâ - äñúééîä");
			OL_SendClientMessageToAll(White, "{FFFFFF}äîðöç áôòéìåú äåà:{99FFFF} àéï îðöç");
			Karting_ACT_On = 0;
			Karting_ACT_AllPlayers = 0;
			AutomaticACT_Price = 0;
			AutomaticACT_AlreadyON = 0;
			DestroyVehicle(Karting[0]);
			DestroyVehicle(Karting[1]);
			DestroyVehicle(Karting[2]);
			DestroyVehicle(Karting[3]);
			DestroyVehicle(Karting[4]);
			DestroyVehicle(Karting[5]);
			DestroyVehicle(Karting[6]);
			DestroyVehicle(Karting[7]);
			DestroyVehicle(Karting[8]);
			DestroyVehicle(Karting[9]);
			DestroyVehicle(Karting[10]);
			DestroyVehicle(Karting[11]);
			DestroyVehicle(Karting[12]);
			DestroyVehicle(Karting[13]);
			DestroyVehicle(Karting[14]);
			DestroyVehicle(Karting[15]);
			Karting_ACT_Place[0] = 0;
			Karting_ACT_Place[1] = 0;
			Karting_ACT_Place[2] = 0;
			Karting_ACT_Place[3] = 0;
			Karting_ACT_Place[4] = 0;
			Karting_ACT_Place[5] = 0;
			Karting_ACT_Place[6] = 0;
			Karting_ACT_Place[7] = 0;
			Karting_ACT_Place[8] = 0;
			Karting_ACT_Place[9] = 0;
			Karting_ACT_Place[10] = 0;
			Karting_ACT_Place[11] = 0;
			Karting_ACT_Place[12] = 0;
			Karting_ACT_Place[13] = 0;
			Karting_ACT_Place[14] = 0;
			Karting_ACT_Place[15] = 0;
		}
		Karting_ACT_PlayerPlace[i][0] = 0;
		Karting_ACT_PlayerPlace[i][1] = 0;
		Karting_ACT_PlayerPlace[i][2] = 0;
		Karting_ACT_PlayerPlace[i][3] = 0;
		Karting_ACT_PlayerPlace[i][4] = 0;
		Karting_ACT_PlayerPlace[i][5] = 0;
		Karting_ACT_PlayerPlace[i][6] = 0;
		Karting_ACT_PlayerPlace[i][7] = 0;
		Karting_ACT_PlayerPlace[i][8] = 0;
		Karting_ACT_PlayerPlace[i][9] = 0;
		Karting_ACT_PlayerPlace[i][10] = 0;
		Karting_ACT_PlayerPlace[i][11] = 0;
		Karting_ACT_PlayerPlace[i][12] = 0;
		Karting_ACT_PlayerPlace[i][13] = 0;
		Karting_ACT_PlayerPlace[i][14] = 0;
		Karting_ACT_PlayerPlace[i][15] = 0;
	}
	return 1;
}

public Karting_End(endid)
{
	if(endid == 0)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ÷àøèéðâ - áåèìä");
		OL_SendClientMessageToAll(White, "(îéðéîåí ùç÷ðéí ìäôòìú äôòéìåú äéðä 2 ùç÷ðéí)");
	}
	else if(endid == 1)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ÷àøèéðâ - áåèìä");
		OL_SendClientMessageToAll(White, "(äîðäì áéèì àú äôòéìåú)");
	}
	else if(endid == 2)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ÷àøèéðâ - áåèìä");
		OL_SendClientMessageToAll(White, "(äôòéìåú äàåèåîèéú äúðâùä áôòéìåú äàåèåîèéú ùäåôòìä òì éãé äîðäì)");
	}
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(Karting_ACT_Start == 1 && Karting_ACT_Running == 0 && Karting_ACT_On == 0)
		{
			if(Karting_ACT_In[i] == 1)
			{
				SetVehicleToRespawn(GetPlayerVehicleID(i));
			    SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Karting_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				DisablePlayerRaceCheckpoint(i);
				CP_KartingActivity[i] = 0;
			}
		}
		else if(Karting_ACT_Start == 0 && Karting_ACT_Running == 1 && Karting_ACT_On == 0)
		{
			if(Karting_ACT_In[i] == 1)
			{
				SetVehicleToRespawn(GetPlayerVehicleID(i));
			    SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Karting_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				DisablePlayerRaceCheckpoint(i);
				CP_KartingActivity[i] = 0;
			}
		}
		else if(Karting_ACT_Start == 0 && Karting_ACT_Running == 0 && Karting_ACT_On == 1)
		{
			if(Karting_ACT_In[i] == 1)
			{
				SetVehicleToRespawn(GetPlayerVehicleID(i));
			    SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Karting_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				DisablePlayerRaceCheckpoint(i);
				CP_KartingActivity[i] = 0;
			}
		}
		Karting_ACT_PlayerPlace[i][0] = 0;
		Karting_ACT_PlayerPlace[i][1] = 0;
		Karting_ACT_PlayerPlace[i][2] = 0;
		Karting_ACT_PlayerPlace[i][3] = 0;
		Karting_ACT_PlayerPlace[i][4] = 0;
		Karting_ACT_PlayerPlace[i][5] = 0;
		Karting_ACT_PlayerPlace[i][6] = 0;
		Karting_ACT_PlayerPlace[i][7] = 0;
		Karting_ACT_PlayerPlace[i][8] = 0;
		Karting_ACT_PlayerPlace[i][9] = 0;
		Karting_ACT_PlayerPlace[i][10] = 0;
		Karting_ACT_PlayerPlace[i][11] = 0;
		Karting_ACT_PlayerPlace[i][12] = 0;
		Karting_ACT_PlayerPlace[i][13] = 0;
		Karting_ACT_PlayerPlace[i][14] = 0;
		Karting_ACT_PlayerPlace[i][15] = 0;
	}
	Karting_ACT_Start = 0;
	Karting_ACT_AllPlayers = 0;
	Karting_ACT_On = 0;
	AutomaticACT_Price = 0;
	AutomaticACT_AlreadyON = 0;
	DestroyVehicle(Karting[0]);
	DestroyVehicle(Karting[1]);
	DestroyVehicle(Karting[2]);
	DestroyVehicle(Karting[3]);
	DestroyVehicle(Karting[4]);
	DestroyVehicle(Karting[5]);
	DestroyVehicle(Karting[6]);
	DestroyVehicle(Karting[7]);
	DestroyVehicle(Karting[8]);
	DestroyVehicle(Karting[9]);
	DestroyVehicle(Karting[10]);
	DestroyVehicle(Karting[11]);
	DestroyVehicle(Karting[12]);
	DestroyVehicle(Karting[13]);
	DestroyVehicle(Karting[14]);
	DestroyVehicle(Karting[15]);
	Karting_ACT_Place[0] = 0;
	Karting_ACT_Place[1] = 0;
	Karting_ACT_Place[2] = 0;
	Karting_ACT_Place[3] = 0;
	Karting_ACT_Place[4] = 0;
	Karting_ACT_Place[5] = 0;
	Karting_ACT_Place[6] = 0;
	Karting_ACT_Place[7] = 0;
	Karting_ACT_Place[8] = 0;
	Karting_ACT_Place[9] = 0;
	Karting_ACT_Place[10] = 0;
	Karting_ACT_Place[11] = 0;
	Karting_ACT_Place[12] = 0;
	Karting_ACT_Place[13] = 0;
	Karting_ACT_Place[14] = 0;
	Karting_ACT_Place[15] = 0;
	KillTimer(Karting_ACT_Timer_Start);
	KillTimer(Karting_ACT_Timer_Register);
	KillTimer(Karting_ACT_Timer_Play);
	KillTimer(Karting_ACT_Countdown_1);
	KillTimer(Karting_ACT_Countdown_2);
	return 1;
}

public Karting_CountDown_1(cd)
{
	new string[256];
	if(cd > 10)
	{
		format(string, sizeof(string), "~b~Karting will start in: ~w~%d ~n~~y~Register command is: ~w~/Join", cd);
		GameTextForAll(string, 1000, 3);
	}
	else if(cd <= 10)
	{
		format(string, sizeof(string), "~b~Karting will start in: ~w~%d ~n~~y~Register command is: ~w~/Join ~n~~r~HURRY UP TO REGISTER!!!", cd);
		GameTextForAll(string, 1000, 3);
	}
	if(cd > 0)
	{
		cd--;
		Karting_ACT_Countdown_1 = SetTimerEx("Karting_CountDown_1", 1000, 0, "%d", cd);
	}
	else KillTimer(Karting_ACT_Countdown_1);
	return 1;
}

public Karting_CountDown_2(cd)
{
	new string[256];
	format(string, sizeof(string), "~b~Karting will start in: ~r~%d", cd);
	GameTextForAll(string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		Karting_ACT_Countdown_2 = SetTimerEx("Karting_CountDown_2", 1000, 0, "%d", cd);
	}
	else
	{
		GameTextForAll("~b~Karting was ~w~started", 1000, 3);
		KillTimer(Karting_ACT_Countdown_2);
		CountdownON = 0;
	}
	return 1;
}

//------------------------------------------------------------------------------
//------------------------------------------------------------------------------

public Drift_Start()
{
	KillTimer(Drift_ACT_Timer_Start);
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú ùäçìä:{99FFFF} äãøéôè");
	OL_SendClientMessageToAll(White, "{FFFFFF}æîï äøùîä:{99FFFF} 30 ùðéåú");
	new string[256];
	AutomaticACT_RandomPrice = randomex(100000, 150000);
	AutomaticACT_Price = AutomaticACT_RandomPrice;
	format(string, sizeof(string), "{FFFFFF}ôøñ: {99FFFF}%s$", GetNum(AutomaticACT_Price));
	OL_SendClientMessageToAll(White, string);
	OL_SendClientMessageToAll(White, "/{99FFFF}Join {FFFFFF}:ô÷åãä ìäøùîä");
	Drift_ACT_Timer_Register = SetTimer("Drift_CloseRegister", 1000*30*1, 1);
	Drift_CountDown_1(30);
	return 1;
}

public Drift_CloseRegister()
{
	KillTimer(Drift_ACT_Timer_Register);
	if(Drift_ACT_AllPlayers <= 1) return Drift_End(0);
	new string[256];
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}äôòéìåú:{99FFFF} äãøéôè - úúçéì áòåã 10 ùðéåú");
	format(string, sizeof(string), "{FFFFFF}ëîåú äîùúúôéí: {99FFFF}%d", Drift_ACT_AllPlayers);
	OL_SendClientMessageToAll(White, string);
	Drift_ACT_Start = 0;
	Drift_ACT_Running = 1;
	Drift_ACT_Timer_Play = SetTimer("Drift_Play", 1000*10*1, 1);
	Drift_CountDown_2(10);
	return 1;
}

public Drift_Play()
{
	KillTimer(Drift_ACT_Timer_Play);
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} äãøéôè - äçìä");
	OL_SendClientMessageToAll(White, "ùéäéä ìëì äîùúúôéí áäöìçä !");
	Drift_ACT_Running = 0;
	Drift_ACT_On = 1;
	for(new i=0; i<MAX_PLAYERS; i++)
   	{
   	    if(Drift_ACT_In[i] == 1)
   	    {
   	        TogglePlayerControllable(i, 1);
   	        ResetPlayerWeapons(i);
			inFreeze[i] = 0;
			SetPlayerRaceCheckpoint(i,2,2285.0803,1505.8159,42.5260,            2306.6877,1393.8424,36.1280,15);
			CP_DriftActivity[i]++;
   	    }
	}
	return 1;
}

public Drift_CheckWinner()
{
	for(new i=0; i<MAX_PLAYERS; i++) if(Drift_ACT_On == 1)
	{
		if(Drift_ACT_AllPlayers == 1)
		{
			if(Drift_ACT_In[i] == 1)
	    	{
				new string[256];
				OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
				OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} äãøéôè - äñúééîä");
				format(string, sizeof(string), "{99FFFF}%s{FFFFFF} :äîðöç áôòéìåú äåà", GetName(i));
				OL_SendClientMessageToAll(White, string);
				DOF2_SetInt(File(i), "Bank", DOF2_GetInt(File(i), "Bank")+AutomaticACT_Price);
				DOF2_SetInt(File(i), "DriftWins", DOF2_GetInt(File(i), "DriftWins")+1);
				SetVehicleToRespawn(GetPlayerVehicleID(i));
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				SetPlayerInterior(i, 0);
				Drift_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				Drift_ACT_On = 0;
				Drift_ACT_AllPlayers = 0;
				AutomaticACT_Price = 0;
				AutomaticACT_AlreadyON = 0;
				DestroyVehicle(Drift[0]);
				DestroyVehicle(Drift[1]);
				DestroyVehicle(Drift[2]);
				DestroyVehicle(Drift[3]);
				DestroyVehicle(Drift[4]);
				DestroyVehicle(Drift[5]);
				DestroyVehicle(Drift[6]);
				DestroyVehicle(Drift[7]);
				DestroyVehicle(Drift[8]);
				Drift_ACT_Place[0] = 0;
				Drift_ACT_Place[1] = 0;
				Drift_ACT_Place[2] = 0;
				Drift_ACT_Place[3] = 0;
				Drift_ACT_Place[4] = 0;
				Drift_ACT_Place[5] = 0;
				Drift_ACT_Place[6] = 0;
				Drift_ACT_Place[7] = 0;
				Drift_ACT_Place[8] = 0;
				DisablePlayerRaceCheckpoint(i);
  				CP_DriftActivity[i] = 0;
			}
		}
		else if(!Drift_ACT_AllPlayers)
		{
			OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
			OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} äãøéôè - äñúééîä");
			OL_SendClientMessageToAll(White, "{FFFFFF}äîðöç áôòéìåú äåà:{99FFFF} àéï îðöç");
			Drift_ACT_On = 0;
			Drift_ACT_AllPlayers = 0;
			AutomaticACT_Price = 0;
			AutomaticACT_AlreadyON = 0;
			DestroyVehicle(Drift[0]);
			DestroyVehicle(Drift[1]);
			DestroyVehicle(Drift[2]);
			DestroyVehicle(Drift[3]);
			DestroyVehicle(Drift[4]);
			DestroyVehicle(Drift[5]);
			DestroyVehicle(Drift[6]);
			DestroyVehicle(Drift[7]);
			DestroyVehicle(Drift[8]);
			Drift_ACT_Place[0] = 0;
			Drift_ACT_Place[1] = 0;
			Drift_ACT_Place[2] = 0;
			Drift_ACT_Place[3] = 0;
			Drift_ACT_Place[4] = 0;
			Drift_ACT_Place[5] = 0;
			Drift_ACT_Place[6] = 0;
			Drift_ACT_Place[7] = 0;
			Drift_ACT_Place[8] = 0;
		}
		Drift_ACT_PlayerPlace[i][0] = 0;
		Drift_ACT_PlayerPlace[i][1] = 0;
		Drift_ACT_PlayerPlace[i][2] = 0;
		Drift_ACT_PlayerPlace[i][3] = 0;
		Drift_ACT_PlayerPlace[i][4] = 0;
		Drift_ACT_PlayerPlace[i][5] = 0;
		Drift_ACT_PlayerPlace[i][6] = 0;
		Drift_ACT_PlayerPlace[i][7] = 0;
		Drift_ACT_PlayerPlace[i][8] = 0;
	}
	return 1;
}

public Drift_End(endid)
{
	if(endid == 0)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} äãøéôè - áåèìä");
		OL_SendClientMessageToAll(White, "(îéðéîåí ùç÷ðéí ìäôòìú äôòéìåú äéðä 2 ùç÷ðéí)");
	}
	else if(endid == 1)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} äãøéôè - áåèìä");
		OL_SendClientMessageToAll(White, "(äîðäì áéèì àú äôòéìåú)");
	}
	else if(endid == 2)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} äãøéôè - áåèìä");
		OL_SendClientMessageToAll(White, "(äôòéìåú äàåèåîèéú äúðâùä áôòéìåú äàåèåîèéú ùäåôòìä òì éãé äîðäì)");
	}
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(Drift_ACT_Start == 1 && Drift_ACT_Running == 0 && Drift_ACT_On == 0)
		{
			if(Drift_ACT_In[i] == 1)
			{
				SetVehicleToRespawn(GetPlayerVehicleID(i));
			    SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Drift_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				DisablePlayerRaceCheckpoint(i);
				CP_DriftActivity[i] = 0;
			}
		}
		else if(Drift_ACT_Start == 0 && Drift_ACT_Running == 1 && Drift_ACT_On == 0)
		{
			if(Drift_ACT_In[i] == 1)
			{
				SetVehicleToRespawn(GetPlayerVehicleID(i));
			    SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Drift_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				DisablePlayerRaceCheckpoint(i);
				CP_DriftActivity[i] = 0;
			}
		}
		else if(Drift_ACT_Start == 0 && Drift_ACT_Running == 0 && Drift_ACT_On == 1)
		{
			if(Drift_ACT_In[i] == 1)
			{
				SetVehicleToRespawn(GetPlayerVehicleID(i));
			    SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Drift_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				DisablePlayerRaceCheckpoint(i);
				CP_DriftActivity[i] = 0;
			}
		}
		Drift_ACT_PlayerPlace[i][0] = 0;
		Drift_ACT_PlayerPlace[i][1] = 0;
		Drift_ACT_PlayerPlace[i][2] = 0;
		Drift_ACT_PlayerPlace[i][3] = 0;
		Drift_ACT_PlayerPlace[i][4] = 0;
		Drift_ACT_PlayerPlace[i][5] = 0;
		Drift_ACT_PlayerPlace[i][6] = 0;
		Drift_ACT_PlayerPlace[i][7] = 0;
		Drift_ACT_PlayerPlace[i][8] = 0;
	}
	Drift_ACT_Start = 0;
	Drift_ACT_AllPlayers = 0;
	Drift_ACT_On = 0;
	AutomaticACT_Price = 0;
	AutomaticACT_AlreadyON = 0;
	DestroyVehicle(Drift[0]);
	DestroyVehicle(Drift[1]);
	DestroyVehicle(Drift[2]);
	DestroyVehicle(Drift[3]);
	DestroyVehicle(Drift[4]);
	DestroyVehicle(Drift[5]);
	DestroyVehicle(Drift[6]);
	DestroyVehicle(Drift[7]);
	DestroyVehicle(Drift[8]);
	Drift_ACT_Place[0] = 0;
	Drift_ACT_Place[1] = 0;
	Drift_ACT_Place[2] = 0;
	Drift_ACT_Place[3] = 0;
	Drift_ACT_Place[4] = 0;
	Drift_ACT_Place[5] = 0;
	Drift_ACT_Place[6] = 0;
	Drift_ACT_Place[7] = 0;
	Drift_ACT_Place[8] = 0;
	KillTimer(Drift_ACT_Timer_Start);
	KillTimer(Drift_ACT_Timer_Register);
	KillTimer(Drift_ACT_Timer_Play);
	KillTimer(Drift_ACT_Countdown_1);
	KillTimer(Drift_ACT_Countdown_2);
	return 1;
}

public Drift_CountDown_1(cd)
{
	new string[256];
	if(cd > 10)
	{
		format(string, sizeof(string), "~b~Drift will start in: ~w~%d ~n~~y~Register command is: ~w~/Join", cd);
		GameTextForAll(string, 1000, 3);
	}
	else if(cd <= 10)
	{
		format(string, sizeof(string), "~b~Drift will start in: ~w~%d ~n~~y~Register command is: ~w~/Join ~n~~r~HURRY UP TO REGISTER!!!", cd);
		GameTextForAll(string, 1000, 3);
	}
	if(cd > 0)
	{
		cd--;
		Drift_ACT_Countdown_1 = SetTimerEx("Drift_CountDown_1", 1000, 0, "%d", cd);
	}
	else KillTimer(Drift_ACT_Countdown_1);
	return 1;
}

public Drift_CountDown_2(cd)
{
	new string[256];
	format(string, sizeof(string), "~b~Drift will start in: ~r~%d", cd);
	GameTextForAll(string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		Drift_ACT_Countdown_2 = SetTimerEx("Drift_CountDown_2", 1000, 0, "%d", cd);
	}
	else
	{
		GameTextForAll("~b~Drift was ~w~started", 1000, 3);
		KillTimer(Drift_ACT_Countdown_2);
		CountdownON = 0;
	}
	return 1;
}
//------------------------------------------------------------------------------
public SawnoffKing_Start()
{
	KillTimer(SawnoffKing_ACT_Timer_Start);
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú ùäçìä:{99FFFF} îìê äñàåï-àåó ùåèâàï");
	OL_SendClientMessageToAll(White, "{FFFFFF}æîï äøùîä:{99FFFF} 30 ùðéåú");
	new string[256];
	AutomaticACT_RandomPrice = randomex(100000, 150000);
	AutomaticACT_Price = AutomaticACT_RandomPrice;
	format(string, sizeof(string), "{FFFFFF}ôøñ: {99FFFF}%s$", GetNum(AutomaticACT_Price));
	OL_SendClientMessageToAll(White, string);
	OL_SendClientMessageToAll(White, "/{99FFFF}Join {FFFFFF}:ô÷åãä ìäøùîä");
	SawnoffKing_ACT_Timer_Register = SetTimer("SawnoffKing_CloseRegister", 1000*30*1, 1);
	SawnoffKing_CountDown_1(30);
	return 1;
}

public SawnoffKing_CloseRegister()
{
	KillTimer(SawnoffKing_ACT_Timer_Register);
	if(SawnoffKing_ACT_AllPlayers <= 1) return SawnoffKing_End(0);
	new string[256];
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}äôòéìåú:{99FFFF} îìê äñàåï-àåó ùåèâàï - úúçéì áòåã 10 ùðéåú");
	format(string, sizeof(string), "{FFFFFF}ëîåú äîùúúôéí: {99FFFF}%d", SawnoffKing_ACT_AllPlayers);
	OL_SendClientMessageToAll(White, string);
	SawnoffKing_ACT_Start = 0;
	SawnoffKing_ACT_Running = 1;
	SawnoffKing_CountDown_2(10);
	SawnoffKing_ACT_Timer_Play = SetTimer("SawnoffKing_Play", 1000*10*1, 1);
	return 1;
}

public SawnoffKing_Play()
{
	KillTimer(SawnoffKing_ACT_Timer_Play);
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(SawnoffKing_ACT_In[i] == 1)
		{
			TogglePlayerControllable(i, 1);
			SetPlayerHealth(i, 100);
			SetPlayerArmour(i, 100);
			ResetPlayerWeapons(i);
			GivePlayerWeapon(i, 26, 99999999999);
		}
	}
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îìê äñàåï-àåó ùåèâàï - äçìä");
	OL_SendClientMessageToAll(White, "ùéäéä ìëì äîùúúôéí áäöìçä !");
	SawnoffKing_ACT_Running = 0;
	SawnoffKing_ACT_On = 1;
	return 1;
}

public SawnoffKing_CheckWinner()
{
	for(new i=0; i<MAX_PLAYERS; i++) if(SawnoffKing_ACT_On == 1)
	{
		if(SawnoffKing_ACT_AllPlayers == 1)
		{
			if(SawnoffKing_ACT_In[i] == 1)
	    	{
				new string[256];
				OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
				OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îìê äñàåï-àåó ùåèâàï - äñúééîä");
				format(string, sizeof(string), "{99FFFF}%s{FFFFFF} :äîðöç áôòéìåú äåà", GetName(i));
				OL_SendClientMessageToAll(White, string);
				DOF2_SetInt(File(i), "Bank", DOF2_GetInt(File(i), "Bank")+AutomaticACT_Price);
				DOF2_SetInt(File(i), "SawnoffKingWins", DOF2_GetInt(File(i), "SawnoffKingWins")+1);
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				SetPlayerInterior(i, 0);
				SawnoffKing_ACT_On = 0;
				SawnoffKing_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				SawnoffKing_ACT_AllPlayers = 0;
				AutomaticACT_Price = 0;
				AutomaticACT_AlreadyON = 0;
			}
		}
		else if(!SawnoffKing_ACT_AllPlayers)
		{
			OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
			OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îìê äñàåï-àåó ùåèâàï - äñúééîä");
			OL_SendClientMessageToAll(White, "{FFFFFF}äîðöç áôòéìåú äåà:{99FFFF} àéï îðöç");
			SawnoffKing_ACT_On = 0;
			SawnoffKing_ACT_AllPlayers = 0;
			AutomaticACT_Price = 0;
			AutomaticACT_AlreadyON = 0;
		}
	}
	return 1;
}

public SawnoffKing_End(endid)
{
	if(endid == 0)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îìê äñàåï-àåó ùåèâàï - áåèìä");
		OL_SendClientMessageToAll(White, "(îéðéîåí ùç÷ðéí ìäôòìú äôòéìåú äéðä 2 ùç÷ðéí)");
	}
	else if(endid == 1)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îìê äñàåï-àåó ùåèâàï - áåèìä");
		OL_SendClientMessageToAll(White, "(äîðäì áéèì àú äôòéìåú)");
	}
	else if(endid == 2)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îìê äñàåï-àåó ùåèâàï - áåèìä");
		OL_SendClientMessageToAll(White, "(äôòéìåú äàåèåîèéú äúðâùä áôòéìåú äàåèåîèéú ùäåôòìä òì éãé äîðäì)");
	}
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(SawnoffKing_ACT_Start == 1 && SawnoffKing_ACT_Running == 0 && SawnoffKing_ACT_On == 0)
		{
			if(SawnoffKing_ACT_In[i] == 1)
			{
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				SawnoffKing_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
		else if(SawnoffKing_ACT_Start == 0 && SawnoffKing_ACT_Running == 1 && SawnoffKing_ACT_On == 0)
		{
			if(SawnoffKing_ACT_In[i] == 1)
			{
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				SawnoffKing_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
		else if(SawnoffKing_ACT_Start == 0 && SawnoffKing_ACT_Running == 0 && SawnoffKing_ACT_On == 1)
		{
			if(SawnoffKing_ACT_In[i] == 1)
			{
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				SawnoffKing_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
	}
	SawnoffKing_ACT_Start = 0;
	SawnoffKing_ACT_AllPlayers = 0;
	SawnoffKing_ACT_On = 0;
	AutomaticACT_Price = 0;
	AutomaticACT_AlreadyON = 0;
	KillTimer(SawnoffKing_ACT_Timer_Start);
	KillTimer(SawnoffKing_ACT_Timer_Register);
	KillTimer(SawnoffKing_ACT_Timer_Play);
	KillTimer(SawnoffKing_ACT_Countdown_1);
	KillTimer(SawnoffKing_ACT_Countdown_2);
	return 1;
}

public SawnoffKing_CountDown_1(cd)
{
	new string[256];
	if(cd > 10)
	{
		format(string, sizeof(string), "~b~SawnoffKing will start in: ~w~%d ~n~~y~Register command is: ~w~/Join", cd);
		GameTextForAll(string, 1000, 3);
	}
	else if(cd <= 10)
	{
		format(string, sizeof(string), "~b~SawnoffKing will start in: ~w~%d ~n~~y~Register command is: ~w~/Join ~n~~r~HURRY UP TO REGISTER!!!", cd);
		GameTextForAll(string, 1000, 3);
	}
	if(cd > 0)
	{
		cd--;
		SawnoffKing_ACT_Countdown_1 = SetTimerEx("SawnoffKing_CountDown_1", 1000, 0, "%d", cd);
	}
	else KillTimer(SawnoffKing_ACT_Countdown_1);
	return 1;
}

public SawnoffKing_CountDown_2(cd)
{
	new string[256];
	format(string, sizeof(string), "~b~SawnoffKing will start in: ~r~%d", cd);
	GameTextForAll(string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		SawnoffKing_ACT_Countdown_2 = SetTimerEx("SawnoffKing_CountDown_2", 1000, 0, "%d", cd);
	}
	else
	{
		GameTextForAll("~b~SawnoffKing was ~w~started", 1000, 3);
		KillTimer(SawnoffKing_ACT_Countdown_2);
		CountdownON = 0;
	}
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerPickUpPickup(playerid, pickupid)
{
	if(pickupid == Pickup_Bank_Enter)
	{
		SetPlayerPosEx(playerid, 2144.1689, 1630.6648, 993.5761, 357.5513);
		SetPlayerInterior(playerid, 1);
	   	SetCameraBehindPlayer(playerid);
		return 1;
	}
	if(pickupid == Pickup_Weapons_Enter)
	{
		SetPlayerPosEx(playerid, 304.4738,-129.0103,999.6016, 215.7184);
		SetPlayerInterior(playerid, 7);
		SetCameraBehindPlayer(playerid);
		return 1;
	}
	if(pickupid == Pickup_Bank_Exit)
	{
		SetPlayerPosEx(playerid, 1217.7982, -1659.7842, 11.7969, 266.1774);
  		SetPlayerInterior(playerid, 0);
		SetCameraBehindPlayer(playerid);
		return 1;
	}
	if(pickupid == Pickup_Weapons_Exit)
	{
		SetPlayerPosEx(playerid, 1365.4742, -1279.3766, 13.5469, 89.8519);
  		SetPlayerInterior(playerid, 0);
		SetCameraBehindPlayer(playerid);
		return 1;
	}

	if(pickupid == MoneyMission_Pickup)
	{
	    if(IsPlayerInMission(playerid) == 1) return 0;
		else ShowPlayerDialog(playerid, 9_0, DIALOG_STYLE_MSGBOX, "îùéîú äëñó", "'ëãé ìäúçéì àú îùéîú äëñó ìçöå òì ëôúåø 'äúçì", "äúçì", "áéèåì");
	}
	if(pickupid == RaceMission_Pickup)
	{
	    if(IsPlayerInMission(playerid) == 1) return 0;
		else ShowPlayerDialog(playerid, 19_0, DIALOG_STYLE_MSGBOX, "îùéîú äîéøåõ", "'ëãé ìäúçéì àú îùéîú äîéøåõ ìçöå òì ëôúåø 'äúçì", "äúçì", "áéèåì");
 	}
	if(pickupid == RobberyMission_Pickup)
	{
	    if(IsPlayerInMission(playerid) == 1) return 0;
		else ShowPlayerDialog(playerid, 20_0, DIALOG_STYLE_MSGBOX, "îùéîú ùãéãú áð÷", "'ëãé ìäúçéì àú îùéîú äùãéãä ìçöå òì ëôúåø 'äúçì", "äúçì", "áéèåì");
	}
	if(pickupid == StuntsMission_Pickup)
	{
	    if(IsPlayerInMission(playerid) == 1) return 0;
		else ShowPlayerDialog(playerid, 21_0, DIALOG_STYLE_MSGBOX, "îùéîú äñèàðèéí", "'ëãé ìäúçéì àú îùéîú äñèàðèéí ìçöå òì ëôúåø 'äúçì", "äúçì", "áéèåì");
	}
	if(pickupid == DriftMission_Pickup)
	{
	    if(IsPlayerInMission(playerid) == 1) return 0;
		else ShowPlayerDialog(playerid, 22_0, DIALOG_STYLE_MSGBOX, "îùéîú äãøéôè", "'ëãé ìäúçéì àú îùéîú äãøéôè ìçöå òì ëôúåø 'äúçì", "äúçì", "áéèåì");
	}
	if(pickupid == ParachutingMission_Pickup)
	{
	    if(IsPlayerInMission(playerid) == 1) return 0;
		else ShowPlayerDialog(playerid, 23_0, DIALOG_STYLE_MSGBOX, "îùéîú äöðéçä", "'ëãé ìäúçéì àú îùéîú äöðéçä ìçöå òì ëôúåø 'äúçì", "äúçì", "áéèåì");
	}
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerEnterCheckpoint(playerid)
{
	new string[256];
	
//-----------------------------------------------------------------[ Pvp Areas ]
	if(CPS_IsPlayerInCheckpoint(playerid, CP_War))
	{
		SendClientMessage(playerid, Pilot, "o•o•o•o•o•o[ {FFFFFF}War Area{33CCFF} - {FFFFFF}àæåø äîìçîä{33CCFF} ]o•o•o•o•o•o");
		SendClientMessage(playerid, Gold, "- áøåëéí äáàéí ìàæåø äîìçîä ùì äùøú");
		SendClientMessage(playerid, White, "áàæåø æä ú÷áìå àú äðù÷éí ñàåï òåæé åàî4 ììà äâáìä åúåëìå ìäìçí ðâã ùç÷ðéí àçøéí");
		SendClientMessage(playerid, White, "{AFAFAF}/Check{FFFFFF} - áãé÷ú ùç÷ðé îìçîä îçåáøéí");
		SendClientMessage(playerid, Brightred, "áøâò ùúîåú úåçæø ìàæåø äîìçîä áàåôï à÷øàé");
		SendClientMessage(playerid, Brown, "ùáî÷ìãú {FFFFFF}\"F\"{4B351B} ìéöéàä îäàæåø ìçõ òì î÷ù");
	}

	if(CPS_IsPlayerInCheckpoint(playerid, CP_DM))
	{
		SendClientMessage(playerid, Pilot, "o•o•o•o•o•o[ {FFFFFF}HeavyWeapons Area{33CCFF} - {FFFFFF}àæåø äðù÷éí äëáãéí{33CCFF} ]o•o•o•o•o•o");
		SendClientMessage(playerid, Gold, "- áøåëéí äáàéí ìàæåø äðù÷éí äëáãéí ùì äùøú");
		SendClientMessage(playerid, White, "áî÷åí æä ú÷áìå ììà äâáìä àú äðù÷éí: ãéâì, àî4, ÷åîáè-ùåèâàï");
		SendClientMessage(playerid, White, "áàæåø éù îìà àåáéé÷èéí ùðéúï ìäúçáà áäí ëãé ùéäéä îùç÷ îàúâø éåúø");
		SendClientMessage(playerid, White, "{AFAFAF}/Check{FFFFFF} - áãé÷ú ùç÷ðé ðù÷éí ëáãéí îçåáøéí");
		SendClientMessage(playerid, Brightred, "áøâò ùúîåú úåçæø ìàæåø äðù÷éí äëáãéí áàåôï à÷øàé");
		SendClientMessage(playerid, Brown, "ùáî÷ìãú {FFFFFF}\"F\"{4B351B} ìéöéàä îäàæåø ìçõ òì î÷ù");
	}

	if(CPS_IsPlayerInCheckpoint(playerid, CP_Hardcore))
	{
		SendClientMessage(playerid, Pilot, "o•o•o•o•o•o[ {FFFFFF}Hardcore Area{33CCFF} - {FFFFFF}àæåø ääàøã÷åø{33CCFF} ]o•o•o•o•o•o");
		SendClientMessage(playerid, Gold, "- áøåëéí äáàéí ìàæåø äîéðéâàï ùì äùøú");
		SendClientMessage(playerid, White, "áî÷åí æä ú÷áìå ëì ôòí áøðãåîàìéåú áæå÷ä àå îéðéâàï");
		SendClientMessage(playerid, White, "(!áàæåø îôåæøéí îâååï ñôàåðéí ëãé ùéäéä àôùø ìùç÷ òì ùèç øçá éåúø (èéô: úòìå òì äââåú");
		SendClientMessage(playerid, White, "{AFAFAF}/Check{FFFFFF} - áãé÷ú ùç÷ðé äàøã÷åø îçåáøéí");
		SendClientMessage(playerid, Brightred, "áøâò ùúîåú úåçæø ìàæåø ääàøã÷åø áàåôï à÷øàé");
		SendClientMessage(playerid, Brown, "ùáî÷ìãú {FFFFFF}\"F\"{4B351B} ìéöéàä îäàæåø ìçõ òì î÷ù");
	}

	if(CPS_IsPlayerInCheckpoint(playerid, CP_Funbattle))
	{
		SendClientMessage(playerid, Pilot, "o•o•o•o•o•o[ {FFFFFF}Funbattle Area{33CCFF} - {FFFFFF}\"àæåø ä\"÷øá äëéó{33CCFF} ]o•o•o•o•o•o");
		SendClientMessage(playerid, Gold, "- áøåëéí äáàéí ìàæåø ä\"÷øá äëéó\" ùì äùøú");
		SendClientMessage(playerid, White, "áàæåø æä ú÷áìå àú äðù÷ ñëéï, áøçáé äî÷åí îôåæøéí ðù÷éí ðåñôéí ùéòæøå ìëí ìäøåâ ùç÷ðéí");
		SendClientMessage(playerid, White, "(-; áàæåø æä âí îôåæøéí îìà îå÷ùéí, àæ äæäøå ìà ìäúôåöõ");
		SendClientMessage(playerid, White, "{AFAFAF}/Check{FFFFFF} - áãé÷ú ùç÷ðé ä\"÷øá äëéó\" äîçåáøéí");
		SendClientMessage(playerid, Brightred, "áøâò ùúîåú úåçæø ìàæåø ä\"÷øá äëéó\" áàåôï à÷øàé");
		SendClientMessage(playerid, Brown, "ùáî÷ìãú {FFFFFF}\"F\"{4B351B} ìéöéàä îäàæåø ìçõ òì î÷ù");
	}
	
//------------------------------------------------------------------[Gain Areas]
	if(CPS_IsPlayerInCheckpoint(playerid, CP_MoneyShip))
	{
		SendClientMessage(playerid, Pilot, "=========================[ ñôéðú äëñó ]=========================");
		SendClientMessage(playerid, Gold, "** áøåëéí äáàéí ìñôéðú äëñó ùì äùøú **");
		SendClientMessage(playerid, White, "ëàï úåëìå ì÷áì áëì 3 ùðéåú ëñó, ëîåú äëñó âåãìú á50$ ìøîä");
		if(GetLevel(playerid) == 1) SendClientMessage(playerid, White, "áøîúê äðåëçéú (1) äéðê î÷áì 50$");
		else if(GetLevel(playerid) == 2) SendClientMessage(playerid, White, "áøîúê äðåëçéú (2) äéðê î÷áì 100$");
		else if(GetLevel(playerid) == 3) SendClientMessage(playerid, White, "áøîúê äðåëçéú (3) äéðê î÷áì 150$");
		else if(GetLevel(playerid) == 4) SendClientMessage(playerid, White, "áøîúê äðåëçéú (4) äéðê î÷áì 200$");
		else if(GetLevel(playerid) == 5) SendClientMessage(playerid, White, "áøîúê äðåëçéú (5) äéðê î÷áì 250$");
		else if(GetLevel(playerid) == 6) SendClientMessage(playerid, White, "áøîúê äðåëçéú (6) äéðê î÷áì 300$");
		else if(GetLevel(playerid) == 7) SendClientMessage(playerid, White, "áøîúê äðåëçéú (7) äéðê î÷áì 350$");
		else if(GetLevel(playerid) == 8) SendClientMessage(playerid, White, "áøîúê äðåëçéú (8) äéðê î÷áì 400$");
		else if(GetLevel(playerid) == 9) SendClientMessage(playerid, White, "áøîúê äðåëçéú (9) äéðê î÷áì 450$");
		else if(GetLevel(playerid) == 10) SendClientMessage(playerid, White, "áøîúê äðåëçéú (10) äéðê î÷áì 500$");
	}
	
	if(CPS_IsPlayerInCheckpoint(playerid, CP_MoneyMission))
	{
	    if(MoneyMission_In[playerid])
	    {
			MoneyMission_Start = 0;
			MoneyMission_In[playerid] = 0;
		 	MoneyMission_WasFinish[playerid] = 1;
			format(string, sizeof(string), "• òáøú àú îùéîú äëñó\n%s$ äøååçú -", GetNum(MoneyMission_RandomPrice));
			ShowPlayerDialog(playerid, 9_2, DIALOG_STYLE_MSGBOX, "îùéîú äëñó", string, "áéèåì", "");
			KillTimer(MoneyMission_Timer_1);
			KillTimer(MoneyMission_Timer_2);
			format(string, sizeof(string), "{FFFFFF}[Money Mission]{AA3333} ñééí àú îùéîú äëñó áäöìçä {FFFFFF}%s{AA3333} äùç÷ï", GetName(MoneyMission_Player));
			OL_SendClientMessageToAllEx(playerid, Red, string);
			AntiCheat_GivePlayerMoney(playerid, true, MoneyMission_RandomPrice);
			DestroyVehicle(MoneyMission_Bike);
			RemovePlayerAttachedObject(playerid, 2); // MoneyBag
		}
	}
	if(CPS_IsPlayerInCheckpoint(playerid, CP_StuntsMission))
	{
	    if(StuntsMission_In[playerid])
	    {
			StuntsMission_Start = 0;
			StuntsMission_In[playerid] = 0;
		 	StuntsMission_WasFinish[playerid] = 1;
			format(string, sizeof(string), "• òáøú àú îùéîú äñèàðèéí\n%s$ äøååçú -", GetNum(StuntsMission_RandomPrice));
			ShowPlayerDialog(playerid, 21_2, DIALOG_STYLE_MSGBOX, "îùéîú äñèàðèéí", string, "áéèåì", "");
			DestroyVehicle(StuntsMission_Car);
			KillTimer(StuntsMission_Timer_1);
			KillTimer(StuntsMission_Timer_2);
			format(string, sizeof(string), "{FFFFFF}[Stunts Mission]{AA3333} ñééí àú îùéîú äñèàðèéí áäöìçä {FFFFFF}%s{AA3333} äùç÷ï", GetName(StuntsMission_Player));
			OL_SendClientMessageToAllEx(playerid, Red, string);
			AntiCheat_GivePlayerMoney(playerid, true, StuntsMission_RandomPrice);
			SetPlayerVirtualWorld(playerid, 0);
		}
	}
	return 1;
}

//------------------------------------------------------------------------------

public OnPlayerEnterRaceCheckpoint(playerid)
{
	new string[256];
	if(CP_RaceMission[playerid] == 1)
    {
    	DisablePlayerRaceCheckpoint(playerid);
      	SetPlayerRaceCheckpoint(playerid,2,1420.5844,-2135.5859,12.9549,        0,0,0,15);
		format(string, sizeof(string), "{FFFFFF}[Race Mission]{AA3333} ñééîú [{FFFFFF}%d/18{AA3333}] ö'÷ôåéðèéí.", CP_RaceMission[playerid]);
		SendClientMessage(playerid, Red, string);
		CP_RaceMission[playerid]++;
	}
	else if(CP_RaceMission[playerid] == 2)
    {
    	DisablePlayerRaceCheckpoint(playerid);
      	SetPlayerRaceCheckpoint(playerid,2,1657.9432,-2037.3597,21.8462,        0,0,0,15);
