		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
  		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		format(string, sizeof(string), "{FFFFFF}%s{33AA33} ðúú ÷øàù ìùç÷ï", GetName(id));
 		SendClientMessage(playerid, Green ,string);
		SetPlayerAttachedObject(id, 3, 18693, 0);
		return 1;
 	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Spector", true) == 0 || strcmp(cmd, "/Spec", true) == 0)
	{
       	if(GetAdminLevel(playerid) < 2 && GetTempAdminLevel(playerid) < 2 && GetSpyAdminLevel(playerid) < 2) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì2");
  		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Spector(Spec) [playerid] :öåøú ùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
		if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		//format(string, sizeof(string), "öåôä áê %s äîðäì", GetName(playerid));
 		//SendClientMessage(id, Green ,string);
		format(string, sizeof(string), "{FFFFFF}%s{33AA33} àúä öåôä áùç÷ï", GetName(id));
 		SendClientMessage(playerid, Green ,string);
 		pvirtual = GetPlayerVirtualWorld(id);
 		pinterior = GetPlayerInterior(id);
 		SetPlayerVirtualWorld(playerid, pvirtual);
  		SetPlayerInterior(id, pinterior);
	    TogglePlayerSpectating(playerid, 1);
		inSpec[playerid] = 1;
		pSpecting[playerid] = id;
		SpecTimer[playerid] = SetTimerEx("SetPlayerSpecOnOtherPlayer", 1000*1*1, 0, "d", playerid);
  		return 1;
 	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Uuspector", true) == 0 || strcmp(cmd, "/Unspec", true) == 0)
	{
       	if(GetAdminLevel(playerid) < 2 && GetTempAdminLevel(playerid) < 2 && GetSpyAdminLevel(playerid) < 2) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì2");
		if(!inSpec[playerid]) return SendClientMessage(playerid, White, "àúä ìà áîöá öôééä");
		SendClientMessage(playerid, Green , "éöàú îîöá öôééä áùç÷ï");
		SetPlayerVirtualWorld(playerid, 0);
	    TogglePlayerSpectating(playerid, 0);
	    inSpec[playerid] = 0;
	    KillTimer(SpecTimer[playerid]);
  		return 1;
 	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Slap", true) == 0 || strcmp(cmd, "/Sla", true) == 0)
	{
       	if(GetAdminLevel(playerid) < 2 && GetTempAdminLevel(playerid) < 2 && GetSpyAdminLevel(playerid) < 2) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì2");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Slap(Sla) [playerid] :öåøú ùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(IsPlayerInPvpArea(id) == 1) return SendClientMessage(playerid, White, "áùøú Pvpùç÷ï æä ðîöà áàçã îàæåøé ä");
		if(IsPlayerInMission(id) == 1) return SendClientMessage(playerid, White, "ùç÷ï æä îáöò ëøâò àú àçú îîùéîåú äùøú");
		if(AutomaticACT_In[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áôòéìåú àåèåîèéú");
		if(ActivityStatus[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ôòéìåú");
		if(InBattle[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà á÷øá àéùé");
		if(inJail[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áëìà");
		if(inAFK[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ìà ðîöà");
		if(CheckAdminLevel(playerid) < CheckAdminLevel(id)) return SendClientMessage(playerid, White, "! àúä ìà éëåì ìòùåú ô÷åãä æå òì îðäì áøîú ðéäåì ùååä å/àå âáåä îùìê");
		if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
 		format(string, sizeof(string), "{FFFFFF}%s{33AA33} ëôëó àú äùç÷ï {FFFFFF}%s{33AA33} äîðäì", GetName(id), GetName(playerid));
    	OL_SendClientMessageToAll(Green, string);
		GivePlayerHealth(id, false, 10);
  		return 1;
 	}
