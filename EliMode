////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Flip", true) == 0)
	{
		if(DOF2_GetInt(File(playerid), "HaveCar") == 0) return SendClientMessage(playerid, White, "àéï ááòìåúê øëá àéùé");
		if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, White, "àúä ìà ðîöà áøëá");
		if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, White, "àúä ìà äðäâ áøëá");
		if(DOF2_GetInt(CarFile(vehicleid), "Public") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
		if(DOF2_GetInt(CarFile(vehicleid), "Private") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
		if(vehicleid != GetPlayerCar(playerid)) return SendClientMessage(playerid, White, "øëá àéùé æä àéðå ùééê ìê");
		SetVehicleZAngle(GetPlayerVehicleID(playerid), 0);
		SendClientMessage(playerid, Olive, "äôëú àú øëáê äàéùé");
    	return 1;
  	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Fix", true) == 0)
	{
		if(DOF2_GetInt(File(playerid), "HaveCar") == 0) return SendClientMessage(playerid, White, "àéï ááòìåúê øëá àéùé");
		if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, White, "àúä ìà ðîöà áøëá");
		if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, White, "àúä ìà äðäâ áøëá");
		if(DOF2_GetInt(CarFile(vehicleid), "Public") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
		if(DOF2_GetInt(CarFile(vehicleid), "Private") == 1) return SendClientMessage(playerid, White, "øëá æä àéðå øëá àéùé");
		if(vehicleid != GetPlayerCar(playerid)) return SendClientMessage(playerid, White, "øëá àéùé æä àéðå ùééê ìê");
		RepairVehicle(GetPlayerCar(playerid));
		SendClientMessage(playerid, Olive, "úé÷ðú àú øëáê äàéùé");
    	return 1;
  	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Callcar", true) == 0)
	{
		if(DOF2_GetInt(File(playerid), "HaveCar") == 0) return SendClientMessage(playerid, White, "àéï ááòìåúê øëá àéùé");
		if(IsPlayerInVehicle(playerid, GetPlayerCar(playerid))) return SendClientMessage(playerid, White, "àúä ëáø áøëáê äàéùé");
		GetPlayerPos(playerid, X, Y, Z);
		GetPlayerFacingAngle(playerid, A);
		RemovePlayerFromVehicle(GetDriver(GetPlayerCar(playerid)));
		SetVehiclePos(GetPlayerCar(playerid), X, Y, Z);
		SetVehicleZAngle(GetPlayerCar(playerid), A);
		if(GetPlayerState(playerid) == 2)
		{
  			RemovePlayerFromVehicle(playerid);
		}
		PutPlayerInVehicle(playerid, GetPlayerCar(playerid), 0);
		SendClientMessage(playerid, Olive, "ùéâøú àú øëáê äàéùé");
    	return 1;
  	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Setting", true) == 0)
	{
		ShowPlayerDialog(playerid, 7_0, DIALOG_STYLE_LIST, "îòøëú ùéðåé äâãøåú ùç÷ï", "çñéîä/áéèåì çñéîä ì÷áìú äåãòåú ôøèéåú\náéèåì/äôòìú äúçáøåú àåèåîèéú ìôé àéôé\nùîéøä/áéèåì ñ÷éï\näâãøú ùòä úîéãéú\näöâú ùòä åúàøéê\nùéðåé ðù÷ äúçìúé", "áçø", "áéèåì");
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Barrel", true) == 0 || strcmp(cmd, "/Barr", true) == 0)
	{
		if(WasBarrel[playerid] == 2) return SendClientMessage(playerid, White, "àúä éëåì ìäùúîù áô÷åæä æå ôòîééí áîùç÷");
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Barrel(Barr) [playerid] :öåøú äùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		if(!IsPlayerNearPlayer(20, playerid, id)) return SendClientMessage(playerid, White, "àúä ìà ÷øåá ìùç÷ï æä");
	 	if(IsPlayerInPvpArea(id) == 1) return SendClientMessage(playerid, White, "áùøú Pvpùç÷ï æä ðîöà áàçã îàæåøé ä");
	 	if(IsPlayerInMission(id) == 1) return SendClientMessage(playerid, White, "ùç÷ï æä îáöò ëøâò àú àçú îîùéîåú äùøú");
	 	if(AutomaticACT_In[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áôòéìåú àåèåîèéú");
		if(ActivityStatus[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ôòéìåú");
		if(InBattle[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà á÷øá àéùé");
		if(inJail[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áëìà");
		if(inAFK[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ìà ðîöà");
		format(string, sizeof(string), "{FFFFFF}[SYSTEM]{ADFF2F} áôööåú, ôåöõ îäø ìôðé ùéáøç! - òåã 10 ùðéåú äôööåú éòìîå %s ä÷ôú àú äùç÷ï {FFFFFF}•••", GetName(id));
		SendClientMessage(playerid, Grellow, string);
		format(string, sizeof(string), "ä÷éó àåúê áôööåú! áøç îäø %s äùç÷ï", GetName(playerid));
		SendClientMessage(id, Grellow, string);
		CreateBarrels(id);
   		WasBarrel[playerid] +=1;
   		SoundData(playerid, true, 1137);
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Rocket", true) == 0)
	{
		if(WasRocket[playerid] == 2) return SendClientMessage(playerid, White, "àúä éëåì ìäùúîù áô÷åæä æå ôòîééí áîùç÷");
		if(RocketOn == 1) return SendClientMessage(playerid, White, "îéùäå ùåìç áøâòéí àìä èéì òå÷á ìùç÷ï");
		if(AntiCheat_GetPlayerMoney(playerid) < 100000) return SendClientMessage(playerid, White, "[100,000] - àéï ìê îñôé÷ ëñó áëãé ìùìåç èéì òå÷á ìùç÷ï");
	    tmp = strtok(cmdtext,idx);
	    if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Rocket [playerid] :öåøú äùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		if(!IsPlayerNearPlayer(100, playerid, id)) return SendClientMessage(playerid, White, "àúä ìà ÷øåá ìùç÷ï æä");
	 	if(IsPlayerInPvpArea(id) == 1) return SendClientMessage(playerid, White, "áùøú Pvpùç÷ï æä ðîöà áàçã îàæåøé ä");
	 	if(IsPlayerInMission(id) == 1) return SendClientMessage(playerid, White, "ùç÷ï æä îáöò ëøâò àú àçú îîùéîåú äùøú");
	 	if(AutomaticACT_In[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áôòéìåú àåèåîèéú");
		if(ActivityStatus[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ôòéìåú");
		if(InBattle[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà á÷øá àéùé");
		if(inJail[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áëìà");
		if(inAFK[id] == 1) return SendClientMessage(playerid, White, "ùç÷ï æä ðîöà áîöá ìà ðîöà");
		GetPlayerPos(id, X, Y, Z);
		AntiCheat_GivePlayerMoney(playerid, false, 100000);
        RocketObj[id] = CreateObject(345, X, Y, Z+15, 0.0000, 0.0000, 0.0000);
        format(string,sizeof(string), "{FFFFFF}[SYSTEM]{ADFF2F} %s ùìçú èéì òå÷á òì äùç÷ï {FFFFFF}•••", GetName(id));
		SendClientMessage(playerid, Grellow, string);
  		format(string,sizeof(string), "ùìç àìééê èéì òå÷á! áøç ëì òåã àúä éëåì - òåã 30 ùðéåú äèéì òå÷á äúôåöõ %s äùç÷ï", GetName(playerid));
		SendClientMessage(id, Grellow, string);
		RocketTimer[id] = SetTimerEx("SendRocketObject", 1000, 1, "d", id);
		RocketEndTimer[id] = SetTimerEx("StopSendRocketObject", 1000*30, 1, "d", id);
		MoveObjectEx(RocketObj[id], X, Y, Z, 300);
		WasRocket[playerid] +=1;
		RocketOn = 1;
   		SoundData(playerid, true, 1137);
		return 1;
	}
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Helpme", true) == 0)
	{
	    for(new i=0; i<MAX_PLAYERS; i++)
	    {
	        if(GetSupportLevel(i) >= 1)
	        {
	            any++;
			}
		}
		if(!any) return SendClientMessage(playerid, White, "àéï úåîëéí îçåáøéí áùøú");
		new length = strlen(cmdtext);
		while ((idx < length) && (cmdtext[idx] <= ' '))
		{
			idx++;
		}
		new offset = idx;
		new result[64];
		while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
		{
			result[idx - offset] = cmdtext[idx];
			idx++;
		}
		result[idx - offset] = EOS;
		if(!strlen(result)) return SendClientMessage(playerid, White, "/Helpme [text] :öåøú ùéîåù");
		SendClientMessage(playerid, Whiteorange, "÷øéàú äòæøä ùìê ðùìçä ìöååú äúåîëéí äîçåáø áùøú, ðçæåø àìéê òí úùåáä ëëì äàôùø");
		format(string, sizeof(string), "**Helpme (%s): äùç÷ï öøéê òæøä, äàééãé ùìå äåà %d", GetName(playerid), playerid);
		SendSupportsMessage(Orange, string);
		format(string, sizeof(string), "({FFFFFF}%s{FFA500})", (result));
		SendSupportsMessage(Orange, string);
		SoundData(playerid, true, 1137);
		return 1;
	}
