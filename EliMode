		if(any == 0 || any == 1) return SendClientMessage(playerid, White, "àéðê éëåì ìáçåø àú äîñôøéí 0 å1");
		rnnum = randomex(1, any);
		format(string, sizeof(string), "äâøéì àú äîñôø {FFFFFF}%s{33AA33} {FFFFFF}%s{33AA33} äîðäì", GetNum(rnnum), GetName(playerid));
 		OL_SendClientMessageToAll(Green, string);
        return 1;
    }
