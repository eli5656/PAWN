    if(dialogid == 13_1 && !response) ShowPlayerDialog(playerid, 13_0, DIALOG_STYLE_LIST, "Help", "Credits - ÷øãéèéí\nRules - çå÷é äùøú\nSystems - îòøëåú äùøú\nCommands - ô÷åãåú äùøú\nTeleports - ùéâåøéí", "áçø", "áéèåì");

	if(dialogid == 13_2)
	{
		if(!response) ShowPlayerDialog(playerid, 13_0, DIALOG_STYLE_LIST, "Help", "Credits - ÷øãéèéí\nRules - çå÷é äùøú\nSystems - îòøëåú äùøú\nCommands - ô÷åãåú äùøú\nTeleports - ùéâåøéí", "áçø", "áéèåì");
