	if(strcmp(cmd, "/Support", true) == 0 || strcmp(cmd, "/Sup", true) == 0)
	{
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Support [Set | Say | Mute | Unmute | Jail | Unjail | Kick] :öåøú ùéîåù");
		if(strcmp(tmp, "Set", true) == 0 || strcmp(tmp, "S", true) == 0)
		{
			if(GetAdminLevel(playerid) < 5 && GetSupportLevel(playerid) < 3 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "äøîú úåîê ùìê ðîåëä å/àå ìà ùååä ì3");
	  		tmp = strtok(cmdtext, idx);
	  		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Support Set(Sup S) [playerid] [level] :öåøú ùéîåù");
			if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
			id = strval(tmp);
  			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
			tmp1 = strtok(cmdtext, idx);
	     	if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Support Set(Sup S) [playerid] [level] :öåøú ùéîåù");
	  		level = strval(tmp1);
	  		if(level < 0 || level > 3) return SendClientMessage(playerid, White, "èååç äøîåú ùì äúåîëéí äåà áéï 1 ì3");
			if(GetSupportLevel(id) == level) return SendClientMessage(playerid, White, "ìùç÷ï éù ëáø àú øîú äúåîê ùáøöåðê ìùéí");
			if(level > GetSupportLevel(id))
			{
				format(string, sizeof(string), "äòìä àåúê ìøîú úåîê {FFFFFF}%d{FFCC33} {FFFFFF}%s{FFCC33} àçøàé äúåîëéí", level, GetName(playerid));
				SendClientMessage(id, Whiteorange, string);
				format(string, sizeof(string), "ìøîú äúåîê {FFFFFF}%d{FFCC33} {FFFFFF}%s{FFCC33} äòìú àú äùç÷ï", level, GetName(id));
				SendClientMessage(playerid, Whiteorange, string);
			}
			else if(level == 0)
			{
				format(string, sizeof(string), "äñéø ìê øîú äúåîê %s àçøàé äúåîëéí", GetName(playerid));
				SendClientMessage(id, Whiteorange, string);
				format(string, sizeof(string), "àú øîú äúåîê %s äñøú ìùç÷ï", GetName(id));
				SendClientMessage(playerid, Whiteorange, string);
			}
			else if(level < GetSupportLevel(id))
			{
				format(string, sizeof(string), "äåøéã àåúê ìøîú úåîê {FFFFFF}%d{FFCC33} {FFFFFF}%s{FFCC33} àçøàé äúåîëéí", level, GetName(playerid));
				SendClientMessage(id, Whiteorange, string);
				format(string, sizeof(string), "ìøîú äúåîê {FFFFFF}%d{FFCC33} {FFFFFF}%s{FFCC33} äåøãú àú äùç÷ï", level, GetName(id));
				SendClientMessage(playerid, Whiteorange, string);
			}
			if(level == 0)
			{
			  	DOF2_Unset(File(id), "Tag");
			}
			else if(level == 1)
			{
			  	DOF2_SetString(File(id), "Tag", "Attempt Supporter");
			}
			else if(level == 2)
			{
			  	DOF2_SetString(File(id), "Tag", "Supporter");
			}
			else if(level == 3)
			{
			  	DOF2_SetString(File(id), "Tag", "Main Supporter");
			}
	  		DOF2_SetInt(File(id), "SupportLevel", level);
	  		return 1;
	  	}
	 	if(strcmp(tmp, "Say", true) == 0 || strcmp(tmp, "S", true) == 0)
		{
			if(GetSupportLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú úåîê ùìê ðîåëä å/àå ìà ùååä ì1");
			new length = strlen(cmdtext);
			while ((idx < length) && (cmdtext[idx] <= ' '))
			{
				idx++;
			}
			new offset = idx;
			new result[64];
			while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
			{
				result[idx - offset] = cmdtext[idx];
				idx++;
			}
			result[idx - offset] = EOS;
			if(!strlen(result)) return SendClientMessage(playerid, White, "/Say(S) [text] :öåøú ùéîåù");
		 	format(string, sizeof(string), "**Support ({FFFFFF}%s{FFCC33}): %s", GetName(playerid), (result));
			OL_SendClientMessageToAll(Whiteorange, string);
			return 1;
	 	}
		if(strcmp(tmp, "Mute", true) == 0 || strcmp(tmp, "M", true) == 0)
		{
			if(GetSupportLevel(playerid) < 1) return SendClientMessage(playerid, White, "äøîú úåîê ùìê ðîåëä å/àå ìà ùååä ì1");
