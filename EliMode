//------------------------------------------------------------------------------

public Minigun_Start()
{
	KillTimer(Minigun_ACT_Timer_Start);
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú ùäçìä:{99FFFF} îéðéâàï");
	OL_SendClientMessageToAll(White, "{FFFFFF}æîï äøùîä:{99FFFF} 30 ùðéåú");
	new string[256];
	AutomaticACT_RandomPrice = randomex(100000, 150000);
	AutomaticACT_Price = AutomaticACT_RandomPrice;
	format(string, sizeof(string), "{FFFFFF}ôøñ: {99FFFF}%s$", GetNum(AutomaticACT_Price));
	OL_SendClientMessageToAll(White, string);
	OL_SendClientMessageToAll(White, "/{99FFFF}Join {FFFFFF}:ô÷åãä ìäøùîä");
	Minigun_ACT_Timer_Register = SetTimer("Minigun_CloseRegister", 1000*30*1, 1);
	Minigun_CountDown_1(30);
	return 1;
}

public Minigun_CloseRegister()
{
	KillTimer(Minigun_ACT_Timer_Register);
	if(Minigun_ACT_AllPlayers <= 1) return Minigun_End(0);
	new string[256];
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}äôòéìåú:{99FFFF} îéðéâàï - úúçéì áòåã 10 ùðéåú");
	format(string, sizeof(string), "{FFFFFF}ëîåú äîùúúôéí: {99FFFF}%d", Minigun_ACT_AllPlayers);
	OL_SendClientMessageToAll(White, string);
	Minigun_ACT_Start = 0;
	Minigun_ACT_Running = 1;
	Minigun_CountDown_2(10);
	Minigun_ACT_Timer_Play = SetTimer("Minigun_Play", 1000*10*1, 1);
	return 1;
}

public Minigun_Play()
{
	KillTimer(Minigun_ACT_Timer_Play);
	for(new i=0; i<MAX_PLAYERS; i++) if(Minigun_ACT_In[i] == 1)
	{
		TogglePlayerControllable(i, 1);
		SetPlayerHealth(i, 100);
		SetPlayerArmour(i, 100);
		ResetPlayerWeapons(i);
		GivePlayerWeapon(i, 38, 99999999999);
	}
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îéðéâàï - äçìä");
	OL_SendClientMessageToAll(White, "ùéäéä ìëì äîùúúôéí áäöìçä !");
	Minigun_ACT_Running = 0;
 	Minigun_ACT_On = 1;
	return 1;
}

public Minigun_CheckWinner()
{
	for(new i=0; i<MAX_PLAYERS; i++) if(Minigun_ACT_On == 1)
	{
		if(Minigun_ACT_AllPlayers == 1)
		{
			if(Minigun_ACT_In[i] == 1)
	    	{
				new string[256];
				OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
				OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îéðéâàï - äñúééîä");
				format(string, sizeof(string), "{99FFFF}%s{FFFFFF} :äîðöç áôòéìåú äåà", GetName(i));
				OL_SendClientMessageToAll(White, string);
				DOF2_SetInt(File(i), "Bank", DOF2_GetInt(File(i), "Bank")+AutomaticACT_Price);
				DOF2_SetInt(File(i), "MinigunWins", DOF2_GetInt(File(i), "MinigunWins")+1);
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Minigun_ACT_On = 0;
				Minigun_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				Minigun_ACT_AllPlayers = 0;
				AutomaticACT_Price = 0;
				AutomaticACT_AlreadyON = 0;
			}
		}
		else if(!Minigun_ACT_AllPlayers)
		{
			OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
			OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îéðéâàï - äñúééîä");
			OL_SendClientMessageToAll(White, "{FFFFFF}äîðöç áôòéìåú äåà:{99FFFF} àéï îðöç");
			Minigun_ACT_On = 0;
			Minigun_ACT_AllPlayers = 0;
			AutomaticACT_Price = 0;
			AutomaticACT_AlreadyON = 0;
		}
	}
	return 1;
}

public Minigun_End(endid)
{
	if(endid == 0)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îéðéâàï - áåèìä");
		OL_SendClientMessageToAll(White, "(îéðéîåí ùç÷ðéí ìäôòìú äôòéìåú äéðä 2 ùç÷ðéí)");
	}
	else if(endid == 1)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îéðéâàï - áåèìä");
		OL_SendClientMessageToAll(White, "(äîðäì áéèì àú äôòéìåú)");
	}
	else if(endid == 2)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} îéðéâàï - áåèìä");
		OL_SendClientMessageToAll(White, "(äôòéìåú äàåèåîèéú äúðâùä áôòéìåú äàåèåîèéú ùäåôòìä òì éãé äîðäì)");
	}
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(Minigun_ACT_Start == 1 && Minigun_ACT_Running == 0 && Minigun_ACT_On == 0)
		{
			if(Minigun_ACT_In[i] == 1)
			{
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Minigun_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
		else if(Minigun_ACT_Start == 0 && Minigun_ACT_Running == 1 && Minigun_ACT_On == 0)
		{
			if(Minigun_ACT_In[i] == 1)
			{
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Minigun_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
		else if(Minigun_ACT_Start == 0 && Minigun_ACT_Running == 0 && Minigun_ACT_On == 1)
		{
			if(Minigun_ACT_In[i] == 1)
			{
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				Minigun_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
	}
	Minigun_ACT_Start = 0;
	Minigun_ACT_AllPlayers = 0;
	Minigun_ACT_On = 0;
	AutomaticACT_Price = 0;
	AutomaticACT_AlreadyON = 0;
	KillTimer(Minigun_ACT_Timer_Start);
	KillTimer(Minigun_ACT_Timer_Register);
	KillTimer(Minigun_ACT_Timer_Play);
	KillTimer(Minigun_ACT_Countdown_1);
	KillTimer(Minigun_ACT_Countdown_2);
	return 1;
}

public Minigun_CountDown_1(cd)
{
	new string[256];
	if(cd > 10)
	{
		format(string, sizeof(string), "~b~Minigun will start in: ~w~%d ~n~~y~Register command is: ~w~/Join", cd);
		GameTextForAll(string, 1000, 3);
	}
	else if(cd <= 10)
	{
		format(string, sizeof(string), "~b~Minigun will start in: ~w~%d ~n~~y~Register command is: ~w~/Join ~n~~r~HURRY UP TO REGISTER!!!", cd);
		GameTextForAll(string, 1000, 3);
	}
	if(cd > 0)
	{
		cd--;
		Minigun_ACT_Countdown_1 = SetTimerEx("Minigun_CountDown_1", 1000, 0, "%d", cd);
	}
	else KillTimer(Minigun_ACT_Countdown_1);
	return 1;
}

public Minigun_CountDown_2(cd)
{
	new string[256];
	format(string, sizeof(string), "~b~Minigun will start in: ~r~%d", cd);
	GameTextForAll(string, 1000, 3);
	if(cd > 0)
	{
		cd--;
		Minigun_ACT_Countdown_2 = SetTimerEx("Minigun_CountDown_2", 1000, 0, "%d", cd);
	}
	else
	{
		GameTextForAll("~b~Minigun was ~w~started", 1000, 3);
		KillTimer(Minigun_ACT_Countdown_2);
		CountdownON = 0;
	}
	return 1;
}

//------------------------------------------------------------------------------

public RandomWeapon_Start()
{
	KillTimer(RandomWeapon_ACT_Timer_Start);
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú ùäçìä:{99FFFF} ðù÷éí øðãåîàìééí");
	OL_SendClientMessageToAll(White, "{FFFFFF}æîï äøùîä:{99FFFF} 30 ùðéåú");
	new string[256];
	AutomaticACT_RandomPrice = randomex(100000, 150000);
	AutomaticACT_Price = AutomaticACT_RandomPrice;
	format(string, sizeof(string), "{FFFFFF}ôøñ: {99FFFF}%s$", GetNum(AutomaticACT_Price));
	OL_SendClientMessageToAll(White, string);
	OL_SendClientMessageToAll(White, "/{99FFFF}Join {FFFFFF}:ô÷åãä ìäøùîä");
	RandomWeapon_ACT_Timer_Register = SetTimer("RandomWeapon_CloseRegister", 1000*30*1, 1);
	RandomWeapon_CountDown_1(30);
	return 1;
}

public RandomWeapon_CloseRegister()
{
	KillTimer(RandomWeapon_ACT_Timer_Register);
	if(RandomWeapon_ACT_AllPlayers <= 1) return RandomWeapon_End(0);
	new string[256];
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}äôòéìåú:{99FFFF} ðù÷éí øðãåîàìééí - úúçéì áòåã 10 ùðéåú");
	format(string, sizeof(string), "{FFFFFF}ëîåú äîùúúôéí: {99FFFF}%d", RandomWeapon_ACT_AllPlayers);
	OL_SendClientMessageToAll(White, string);
	RandomWeapon_ACT_Start = 0;
	RandomWeapon_ACT_Running = 1;
	RandomWeapon_CountDown_2(10);
	RandomWeapon_ACT_Timer_Play = SetTimer("RandomWeapon_Play", 1000*10*1, 1);
	return 1;
}

public RandomWeapon_Play()
{
	KillTimer(RandomWeapon_ACT_Timer_Play);
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(RandomWeapon_ACT_In[i] == 1)
		{
			TogglePlayerControllable(i, 1);
			SetPlayerHealth(i, 100);
			SetPlayerArmour(i, 100);
			ResetPlayerWeapons(i);
			switch(random(12))
			{
				case 0:{GivePlayerWeapon(i, 22, 99999999999);}
				case 1:{GivePlayerWeapon(i, 23, 99999999999);}
				case 2:{GivePlayerWeapon(i, 24, 99999999999);}
				case 3:{GivePlayerWeapon(i, 25, 99999999999);}
				case 4:{GivePlayerWeapon(i, 26, 99999999999);}
				case 5:{GivePlayerWeapon(i, 27, 99999999999);}
				case 6:{GivePlayerWeapon(i, 28, 99999999999);}
				case 7:{GivePlayerWeapon(i, 29, 99999999999);}
				case 8:{GivePlayerWeapon(i, 30, 99999999999);}
				case 9:{GivePlayerWeapon(i, 31, 99999999999);}
				case 10:{GivePlayerWeapon(i, 32, 99999999999);}
				case 11:{GivePlayerWeapon(i, 33, 99999999999);}
			}
		}
	}
	OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
	OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ðù÷éí øðãåîàìééí - äçìä");
	OL_SendClientMessageToAll(White, "ùéäéä ìëì äîùúúôéí áäöìçä !");
	RandomWeapon_ACT_Running = 0;
	RandomWeapon_ACT_On = 1;
	return 1;
}

public RandomWeapon_CheckWinner()
{
	for(new i=0; i<MAX_PLAYERS; i++) if(RandomWeapon_ACT_On == 1)
	{
		if(RandomWeapon_ACT_AllPlayers == 1)
		{
			if(RandomWeapon_ACT_In[i] == 1)
	    	{
				new string[256];
				OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
				OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ðù÷éí øðãåîàìééí - äñúééîä");
				format(string, sizeof(string), "{99FFFF}%s{FFFFFF} :äîðöç áôòéìåú äåà", GetName(i));
				OL_SendClientMessageToAll(White, string);
				DOF2_SetInt(File(i), "Bank", DOF2_GetInt(File(i), "Bank")+AutomaticACT_Price);
				DOF2_SetInt(File(i), "RandomWeaponWins", DOF2_GetInt(File(i), "RandomWeaponWins")+1);
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				RandomWeapon_ACT_On = 0;
				RandomWeapon_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
				RandomWeapon_ACT_AllPlayers = 0;
				AutomaticACT_Price = 0;
				AutomaticACT_AlreadyON = 0;
			}
		}
		else if(!RandomWeapon_ACT_AllPlayers)
		{
			OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
			OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ðù÷éí øðãåîàìééí - äñúééîä");
			OL_SendClientMessageToAll(White, "{FFFFFF}äîðöç áôòéìåú äåà:{99FFFF} àéï îðöç");
			RandomWeapon_ACT_On = 0;
			RandomWeapon_ACT_AllPlayers = 0;
			AutomaticACT_Price = 0;
			AutomaticACT_AlreadyON = 0;
		}
	}
	return 1;
}

public RandomWeapon_End(endid)
{
	if(endid == 0)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ðù÷éí øðãåîàìééí - áåèìä");
		OL_SendClientMessageToAll(White, "(îéðéîåí ùç÷ðéí ìäôòìú äôòéìåú äéðä 2 ùç÷ðéí)");
	}
	else if(endid == 1)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ðù÷éí øðãåîàìééí - áåèìä");
		OL_SendClientMessageToAll(White, "(äîðäì áéèì àú äôòéìåú)");
	}
	else if(endid == 2)
	{
		OL_SendClientMessageToAll(Bluewhite, "============[{FFFFFF} îòøëú ôòéìåéåú àåèåîèéåú {99FFFF}]============");
		OL_SendClientMessageToAll(White, "{FFFFFF}ôòéìåú:{99FFFF} ðù÷éí øðãåîàìééí - áåèìä");
		OL_SendClientMessageToAll(White, "(äôòéìåú äàåèåîèéú äúðâùä áôòéìåú äàåèåîèéú ùäåôòìä òì éãé äîðäì)");
	}
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(RandomWeapon_ACT_Start == 1 && RandomWeapon_ACT_Running == 0 && RandomWeapon_ACT_On == 0)
		{
			if(RandomWeapon_ACT_In[i] == 1)
			{
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				RandomWeapon_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
		else if(RandomWeapon_ACT_Start == 0 && RandomWeapon_ACT_Running == 1 && RandomWeapon_ACT_On == 0)
		{
			if(RandomWeapon_ACT_In[i] == 1)
			{
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				RandomWeapon_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
		else if(RandomWeapon_ACT_Start == 0 && RandomWeapon_ACT_Running == 0 && RandomWeapon_ACT_On == 1)
		{
			if(RandomWeapon_ACT_In[i] == 1)
			{
				SetPlayerVirtualWorld(i, 0);
				SpawnPlayer(i);
				RandomWeapon_ACT_In[i] = 0;
				AutomaticACT_In[i] = 0;
			}
		}
	}
	RandomWeapon_ACT_Start = 0;
	RandomWeapon_ACT_AllPlayers = 0;
	RandomWeapon_ACT_On = 0;
	AutomaticACT_Price = 0;
	AutomaticACT_AlreadyON = 0;
	KillTimer(RandomWeapon_ACT_Timer_Start);
	KillTimer(RandomWeapon_ACT_Timer_Register);
	KillTimer(RandomWeapon_ACT_Timer_Play);
	KillTimer(RandomWeapon_ACT_Countdown_1);
	KillTimer(RandomWeapon_ACT_Countdown_2);
	return 1;
}

public RandomWeapon_CountDown_1(cd)
{
	new string[256];
	if(cd > 10)
	{
		format(string, sizeof(string), "~b~RandomWeapon will start in: ~w~%d ~n~~y~Register command is: ~w~/Join", cd);
		GameTextForAll(string, 1000, 3);
	}
	else if(cd <= 10)
	{
		format(string, sizeof(string), "~b~RandomWeapon will start in: ~w~%d ~n~~y~Register command is: ~w~/Join ~n~~r~HURRY UP TO REGISTER!!!", cd);
		GameTextForAll(string, 1000, 3);
	}
	if(cd > 0)
	{
		cd--;
		RandomWeapon_ACT_Countdown_1 = SetTimerEx("RandomWeapon_CountDown_1", 1000, 0, "%d", cd);
	}
	else KillTimer(RandomWeapon_ACT_Countdown_1);
	return 1;
}

public RandomWeapon_CountDown_2(cd)
{
	new string[256];
	format(string, sizeof(string), "~b~RandomWeapon will start in: ~r~%d", cd);
	GameTextForAll(string, 1000, 3);
	if(cd > 0)
	{
		cd--;
