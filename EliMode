					format(string, sizeof(string), "{FFFF00}ATM ID: {FFFFFF}#%d\n{FFFF00}áøåëéí äáàéí{FFFFFF} ìëñôåîè\n{FFFF00}ùáî÷ìãú {FFFFFF}\"F\"{FFFF00} ìùéîåù ìçõ òì î÷ù", DOF2_GetInt(AtmFile, "ID"));
					AtmText[aid] = Create3DTextLabel(string, Gold, DOF2_GetFloat(AtmFile, "X"), DOF2_GetFloat(AtmFile, "Y"), DOF2_GetFloat(AtmFile, "Z")+0.5, 60.0, 0, 1);
					AtmPickup[aid] = CreatePickup(1274, 1, DOF2_GetFloat(AtmFile, "X"), DOF2_GetFloat(AtmFile, "Y"), DOF2_GetFloat(AtmFile, "Z"), 0);
					format(string,sizeof(string), "! äææú àú ëñôåîè îñôø {FFFFFF}%d{FFFF00} ìîé÷åîê äðåëçé", aid);
					SendClientMessage(playerid, Yellow, string);
					return 1;
     			}
			}
		}
	}
	if(dialogid == 10_2 && response)
	{
        if(listitem == 0) ShowPlayerDialog(playerid, 10_2_1, DIALOG_STYLE_LIST, "îòøëú áð÷ - ëñôåîè", "äô÷ãú ñëåí îñåééí\näô÷ãú ëì äëñó ùéù òìéê", "áçø", "çæåø");
        if(listitem == 1) ShowPlayerDialog(playerid, 10_2_2, DIALOG_STYLE_LIST, "îòøëú áð÷ - ëñôåîè", "îùéëú ñëåí îñåééí\nîùéëú ëì äëñó ùéù ìê ááð÷", "áçø", "çæåø");
