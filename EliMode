	if(strcmp(cmd, "/Unban", true) == 0 || strcmp(cmd, "/Unb", true) == 0)
	{
		if(GetAdminLevel(playerid) < 5 && GetSpyAdminLevel(playerid) < 5 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì5");
		tmp = strtok(cmdtext,idx);
		format(pfile, sizeof(pfile), "Users/%s.ini", tmp);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/UNBan(Unb) [name] :öåøú ùéîåù");
		if(!DOF2_FileExists(pfile)) return SendClientMessage(playerid, White, "äùí îùúîù ùöééðú ìà ÷ééí áùøú");
		if(DOF2_GetInt(pfile, "HaveBan") == 0) return SendClientMessage(playerid, White, "ìùç÷ï ùöééðú àéï áàï");
		DOF2_SetInt(pfile, "HaveBan", 0);
		DOF2_SetInt("/Setting.ini", "InBans", DOF2_GetInt("/BansIP.ini", "InBans")-1);
		DOF2_Unset("/BansIP.ini", DOF2_GetString(pfile, "IP"));
	 	format(string, sizeof(string), "{FFFFFF}%s{33AA33} áåèì äçñéîä òì éãé äîðäì {FFFFFF}%s{33AA33} ìùç÷ï", GetName(playerid), DOF2_GetString(pfile, "Nickname"));
		OL_SendClientMessageToAll(Green, string);
		//---------------------------------------------------------------- [LOG]
		format(string, sizeof(string), "%s removed the ban of player %s | Time: %d:%d:%d - Date: %d/%d/%d", GetName(playerid), GetName(id), Hour, Minute, Second, Day, Month, Year);
  		AdminCommands_Logs(string);
		//----------------------------------------------------------------------
		return 1;
	}
