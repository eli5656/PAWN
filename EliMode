				if(IsPlayerConnected(i))
				{
					if(inWar[i])
					{
						format(string, sizeof(string), "%s[#%d]", GetName(i), i);
						SendClientMessage(playerid, White, string);
					}
				}
			}
			return 1;
 		}
		else if(inFunbattle[playerid] == 1)
	    {
			SendClientMessage(playerid, Grellow, ":ùç÷ðé ä\"÷øá äëéó\" äîçåáøéí");
