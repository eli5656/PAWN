	if(strcmp(cmd, "/SetAdmin", true) == 0 || strcmp(cmd, "/Sa", true) == 0)
	{
  		if(GetAdminLevel(playerid) < 5 && GetSpyAdminLevel(playerid) < 5 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "äøîú ðéäåì ùìê ðîåëä å/àå ìà ùååä ì5");
		tmp = strtok(cmdtext, idx);
  		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/SetAdmin(Sa) [playerid] [level] :öåøú ùéîåù");
		if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "äàééãé ùäæðú ùâåé");
		id = strval(tmp);
  		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "ùç÷ï æä ìà îçåáø");
		if(Logged[id] == false) return SendClientMessage(playerid, White, "ùç÷ï æä ìà ðëðñ ìîùç÷");
		//if(IsYou(id, playerid)) return SendClientMessage(playerid, White, "àéðê éëåì ìáöò ô÷åãä æå òì òöîê");
		tmp1 = strtok(cmdtext, idx);
     	if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/SetAdmin(Sa) [playerid] [level] :öåøú ùéîåù");
  		level = strval(tmp1);
  		if(level < 0 || level > 5) return SendClientMessage(playerid, White, "èååç äøîåú ùì äîðäì äåà áéï 1 ì5");
		if(GetAdminLevel(id) == level) return SendClientMessage(playerid, White, "ìùç÷ï éù ëáø àú øîú äðéäåì ùáøöåðê ìùéí");
		if(level > GetAdminLevel(id))
		{
			format(string, sizeof(string), "äòìä àåúê ìøîú äðéäåì {FFFFFF}%d{33AA33} {FFFFFF}%s{33AA33} äîðäì", level, GetName(playerid));
			SendClientMessage(id, Green, string);
			format(string, sizeof(string), "ìøîú äðéäåì {FFFFFF}%d{33AA33} {FFFFFF}%s{33AA33} äòìú àú äùç÷ï", level, GetName(id));
			SendClientMessage(playerid, Green, string);
		}
		else if(level == 0)
		{
			format(string, sizeof(string), "äñéø ìê øîú ðéäåì {FFFFFF}%s{33AA33} äîðäì", GetName(playerid));
			SendClientMessage(id, Green, string);
			format(string, sizeof(string), "àú øîú äðéäåì {FFFFFF}%s{33AA33} äñøú ìùç÷ï", GetName(id));
			SendClientMessage(playerid, Green, string);
		}
		else if(level < GetAdminLevel(id))
		{
			format(string, sizeof(string), "äåøéã àåúê ìøîú äðéäåì {FFFFFF}%d{33AA33} {FFFFFF}%s{33AA33} äîðäì", level, GetName(playerid));
			SendClientMessage(id, Green, string);
			format(string, sizeof(string), "ìøîú äðéäåì {FFFFFF}%d{33AA33} {FFFFFF}%s{33AA33} äåøãú àú äùç÷ï", level, GetName(id));
			SendClientMessage(playerid, Green, string);
		}
  		DOF2_SetInt(File(id), "AdminLevel", level);
  		DOF2_SetInt(File(id), "TempAdminLevel", 0);
     	return 1;
 	}
