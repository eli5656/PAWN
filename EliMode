		any = strval(tmp1);
		if(any < 1 || any > 100) return SendClientMessage(playerid, White, "ëîåú äçééí ùáøöåðê ìùéí ìøëá ùì äùç÷ï ùâåéä");
		format(string, sizeof(string), "{FFFFFF}%d{33AA33}ùéðä àú ëîåú äçééí ùì øëáê ì {FFFFFF}%s{33AA33} äîðäì", any, GetName(playerid));
		SendClientMessage(id, Green ,string);
		format(string, sizeof(string), "{FFFFFF}%d{33AA33}àú ëîåú äçééí ùì øëáå ì {FFFFFF}%s{33AA33} ùéðéú ìùç÷ï", any, GetName(id));
		SendClientMessage(playerid, Green ,string);
		SetVehicleHealth(GetPlayerVehicleID(id),  any*10);
		return 1;
