				//==============================================================
			    format(string, sizeof(string), "øîú ðéäåì: àçøàé ôòéìåéåú | %s[#%d]", GetName(i), i);
				if(GetAdminLevel(i) == 1) SendClientMessage(playerid, White, string);
			    format(string, sizeof(string), "øîú ðéäåì: îðäì áðéñéåï | %s[#%d]", GetName(i), i);
				if(GetAdminLevel(i) == 2) SendClientMessage(playerid, White, string);
			    format(string, sizeof(string), "øîú ðéäåì: îðäì | %s[#%d]", GetName(i), i);
				if(GetAdminLevel(i) == 3) SendClientMessage(playerid, White, string);
			    format(string, sizeof(string), "øîú ðéäåì: îðäì áëéø | %s[#%d]", GetName(i), i);
				if(GetAdminLevel(i) == 4) SendClientMessage(playerid, White, string);
			    format(string, sizeof(string), "øîú ðéäåì: àçøàé ÷äéìä | %s[#%d]", GetName(i), i);
				if(GetAdminLevel(i) == 5) SendClientMessage(playerid, White, string);
				//==============================================================
