				else if(Karting_ACT_PlayerPlace[playerid][12] == 1) return Karting_ACT_Place[12] = 0, Karting_ACT_PlayerPlace[playerid][12] = 0;
				else if(Karting_ACT_PlayerPlace[playerid][13] == 1) return Karting_ACT_Place[13] = 0, Karting_ACT_PlayerPlace[playerid][13] = 0;
				else if(Karting_ACT_PlayerPlace[playerid][14] == 1) return Karting_ACT_Place[14] = 0, Karting_ACT_PlayerPlace[playerid][14] = 0;
				else if(Karting_ACT_PlayerPlace[playerid][15] == 1) return Karting_ACT_Place[15] = 0, Karting_ACT_PlayerPlace[playerid][15] = 0;
			}
			else if(Drift_ACT_Start == 1 && Drift_ACT_Running == 0 && Drift_ACT_On == 0)
			{
				if(Drift_ACT_In[playerid] == 0) return SendClientMessage(playerid, White, "àúä ìà ðøùîú ìôòéìåú äãøéôè");
				DestroyVehicle(vehicleid);
				SetPlayerVirtualWorld(playerid, 0);
				SpawnPlayer(playerid);
				Drift_ACT_In[playerid] = 0;
				AutomaticACT_In[playerid] = 0;
				Drift_ACT_AllPlayers -=1;
				SendClientMessage(playerid, Bluewhite, "òæáú àú ôòéìåú ä÷àøèéðâ");
				if(Drift_ACT_PlayerPlace[playerid][0] == 1) return Drift_ACT_Place[0] = 0, Drift_ACT_PlayerPlace[playerid][0] = 0;
				else if(Drift_ACT_PlayerPlace[playerid][1] == 1) return Drift_ACT_Place[1] = 0, Drift_ACT_PlayerPlace[playerid][1] = 0;
				else if(Drift_ACT_PlayerPlace[playerid][2] == 1) return Drift_ACT_Place[2] = 0, Drift_ACT_PlayerPlace[playerid][2] = 0;
				else if(Drift_ACT_PlayerPlace[playerid][3] == 1) return Drift_ACT_Place[3] = 0, Drift_ACT_PlayerPlace[playerid][3] = 0;
				else if(Drift_ACT_PlayerPlace[playerid][4] == 1) return Drift_ACT_Place[4] = 0, Drift_ACT_PlayerPlace[playerid][4] = 0;
				else if(Drift_ACT_PlayerPlace[playerid][5] == 1) return Drift_ACT_Place[5] = 0, Drift_ACT_PlayerPlace[playerid][5] = 0;
				else if(Drift_ACT_PlayerPlace[playerid][6] == 1) return Drift_ACT_Place[6] = 0, Drift_ACT_PlayerPlace[playerid][6] = 0;
				else if(Drift_ACT_PlayerPlace[playerid][7] == 1) return Drift_ACT_Place[7] = 0, Drift_ACT_PlayerPlace[playerid][7] = 0;
				else if(Drift_ACT_PlayerPlace[playerid][8] == 1) return Drift_ACT_Place[8] = 0, Drift_ACT_PlayerPlace[playerid][8] = 0;
			}
			else if(SawnoffKing_ACT_Start == 1 && SawnoffKing_ACT_Running == 0 && SawnoffKing_ACT_On == 0)
			{
				if(SawnoffKing_ACT_Start == 0 && SawnoffKing_ACT_Running == 0 && SawnoffKing_ACT_On == 0) return SendClientMessage(playerid, White, "äôòéìåú îìê äñàåï-àåó ùåèâàï ìà îåôòìú");
				if(SawnoffKing_ACT_In[playerid] == 0) return SendClientMessage(playerid, White, "àúä ìà ðøùîú ìôòéìåú îìê äñàåï-àåó ùåèâàï");
				SetPlayerVirtualWorld(playerid, 0);
				SpawnPlayer(playerid);
				SawnoffKing_ACT_In[playerid] = 0;
				AutomaticACT_In[playerid] = 0;
				SawnoffKing_ACT_AllPlayers -=1;
				SendClientMessage(playerid, Bluewhite, "òæáú àú ôòéìåú îìê äñàåï-àåó ùåèâàï");
			}
		}
		else SendClientMessage(playerid, White, "àéï ëòú ôòéìåú àåèåîèéú îåôòìú");
		return 1;
	}
