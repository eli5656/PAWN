    }

	if(dialogid == 9_0 && response)
	{
		format(string, sizeof(string), "%s îùéîú äëñó ëáø ðòùú òì éãé äùç÷ï", GetName(MoneyMission_Player));
  		if(MoneyMission_Start == 1) return SendClientMessage(playerid, White, string);
	   	if(MoneyMission_WasFinish[playerid] == 1) return SendClientMessage(playerid, White, "ðéúï ìòùåú àú îùéîú äëñó ôòí áëðéñä ìîùç÷");
		if(AntiCheat_GetPlayerMoney(playerid) < 1000) return SendClientMessage(playerid, White, "àéï ìê 1000$ ëãé ìäúçéì àú äîùéîä");
		AntiCheat_GivePlayerMoney(playerid, false, 1000);
		MoneyMission_Start = 1;
		MoneyMission_In[playerid] = 1;
		MoneyMission_RandomPrice = randomex(10000, 20000);
		format(string, sizeof(string), "• äúçìú ëòú àú îùéîú äëñó\nLSéù ìê 3 ã÷åú ìäâéò ìö'÷ôåééðè ùðîöà áúçðú äîùèøä á\n%s$ áîéãä åúâéò ìö'÷ôåééðè ú÷áì", GetNum(MoneyMission_RandomPrice));
		ShowPlayerDialog(playerid, 9_1, DIALOG_STYLE_MSGBOX, "îùéîú äëñó", string, "áéèåì", "");
		GetPlayerPos(playerid, X, Y, Z);
		GetPlayerFacingAngle(playerid, A);
		MoneyMission_Player = playerid;
		MoneyMission_Countdown(180);
		MoneyMission_Timer_2 = SetTimerEx("MoneyMission_End", 1000*180*1, 0, "%d", playerid);
		format(string, sizeof(string), "{FFFFFF}[Money Mission]{AA3333} îúçéì áãøëå ìéòã ùì îùéîú äëñó - éù ìëí 3 ã÷åú ìòöåø àåúå åìäøååéç àú ñëåí äëñó äâðåá {FFFFFF}%s{AA3333} äùç÷ï", GetName(MoneyMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		MoneyMission_Bike = CreateVehicle(481, 824.5972,-1361.0049,-0.9473,316.9979,1,0, 50000);
		PutPlayerInVehicle(playerid, MoneyMission_Bike, 0);
		SetVehiclePosEx(playerid, 824.5972,-1361.0049,-0.9473,316.9979);
		SetPlayerAttachedObject(playerid, 2, 1550, 1, 0.100000, -0.200000, 0.000000, 160.000000, 90.000000, 0.000000, 1.000000, 1.000000, 1.000000);
	}
	
	if(dialogid == 10_0 && response)
	{
       	if(listitem == 0)
