public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
	if(!inAFK[playerid] && Logged[playerid] == true)
	{
	    KillTimer(AutoAFK_Timer[playerid]);
   		AutoAFK_Timer[playerid] = SetTimerEx("AutomaticAFK", 1000*60*2, 0, "d", playerid);
	}
	
    if(inAFK[playerid] == 1)
	{
  		    new string[256];
  		    SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} חזרת ממצב לא נמצא {FFFFFF}•••");
   			format(string, sizeof(string), "{FFA500}[FxpDeathMatch] {FFFFFF}חזר ממצב לא נמצא {FF0000}%s {FFFFFF}המשתמש", GetName(playerid));
			OL_SendClientMessageToAllEx(playerid, Grey, string);
			TextDrawHideForPlayer(playerid, Afk);
			TextDrawHideForPlayer(playerid, Afk1);
  			TogglePlayerControllable(playerid, 1);
			SetPlayerVirtualWorld(playerid, afkvirtual);
 			inAFK[playerid] = 0;
	}
	//new string[256];
    /*format(string, sizeof(string), "[%d] >> [%d]", oldkeys, newkeys);
	SendClientMessage(playerid, White, string);*/

	if(UsedVelocity[playerid] == 1 && IsKeyJustDown(KEY_FIRE, newkeys, oldkeys))
	{
	    if(IsPlayerInAnyVehicle(playerid))
	    {
			new Float:X, Float:Y, Float:Z;
			GetVehicleVelocity(GetPlayerVehicleID(playerid), X, Y, Z);
			SetVehicleVelocity(GetPlayerVehicleID(playerid), X*5.0, Y*5.0, Z);
		}
		else SendClientMessage(playerid, White, "! לצורך האצת הרכב עליך להיות בתוך רכב");
	}

	if(IsKeyJustDown(KEY_FIRE, newkeys, oldkeys) && inRCXD[playerid]) KillTimer(RCXDTimer[playerid]), DestroyRCXD(playerid);

	if(IsPlayerInArea(playerid, 2140.9573, 1627.5625, 2147.5081, 1642.9884))
	{
		if(IsKeyJustDown(KEY_FIRE, newkeys, oldkeys))
		{
		    SetPlayerArmour(playerid, 0);
		    SetPlayerHealth(playerid, 0);
		    SendClientMessage(playerid, White, "! {FFFFFF}אסור להרביץ לשחקנים {FFD720}בבנק");
		}
	}
	else if(IsPlayerInArea(playerid, 302.9533, -143.8248, 317.3446, -126.0462))
	{
		if(IsKeyJustDown(KEY_FIRE, newkeys, oldkeys))
		{
		    SetPlayerArmour(playerid, 0);
		    SetPlayerHealth(playerid, 0);
		    SendClientMessage(playerid, White, "! {FFFFFF}אסור להרביץ לשחקנים {FFD720}בחנות הנשקים");
		}
	}
	
	if(PRESSED(KEY_SECONDARY_ATTACK))
	{
		for(new i=0; i<MAX_ATMS; i++)
		{
	   		format(AtmFile, sizeof(AtmFile), "ATMs/%d.ini", i);
		   	if(DOF2_FileExists(AtmFile) && PlayerToPoint(1, playerid, DOF2_GetFloat(AtmFile, "X"), DOF2_GetFloat(AtmFile, "Y"), DOF2_GetFloat(AtmFile, "Z")))
		   	{
 				if(GetCreditCard(playerid) == 0) return SendClientMessage(playerid, White, "אין ברשותך כרטיס אשראי, לקניית כרטיס צור קשר עם הבנק");
				ShowPlayerDialog(playerid, 10_2, DIALOG_STYLE_LIST, "מערכת בנק - כספומט", "הפקדת כסף\nמשיכת כסף", "בחר", "ביטול");
			}
		}
	}
	
	if(PRESSED(KEY_SECONDARY_ATTACK))
	{
		for(new i=0; i<MAX_PROPS; i++)
		{
	   		format(PropFile, sizeof(PropFile), "Properties/%d.ini", i);
		   	if(DOF2_FileExists(PropFile) && PlayerToPoint(1, playerid, DOF2_GetFloat(PropFile, "X"), DOF2_GetFloat(PropFile, "Y"), DOF2_GetFloat(PropFile, "Z"))) ShowPlayerDialog(playerid, 15_2, DIALOG_STYLE_LIST, "מערכת נכסים", "קניית הנכס\nמכירת הנכס", "בחר", "ביטול");
		}
	}
	
	if(PRESSED(KEY_YES))
	{
		if(Survey_Have == 1)
		{
			if(Survey_Yes[playerid] == 1) return SendClientMessage(playerid, White, "הצבעת כבר בסקר זה");
			if(Survey_No[playerid] == 1) return SendClientMessage(playerid, White, "הצבעת כבר בסקר זה");
			Survey_Yes[playerid] = 1;
			Survey_AllYes ++;
			SendClientMessage(playerid, Green, "הצבעתך נקלטה במערכת, תודה");
		}
		return 1;
	}
	if(PRESSED(KEY_NO))
	{
		if(Survey_Have == 1)
		{
			if(Survey_Yes[playerid] == 1) return SendClientMessage(playerid, White, "הצבעת כבר בסקר זה");
			if(Survey_No[playerid] == 1) return SendClientMessage(playerid, White, "הצבעת כבר בסקר זה");
			Survey_No[playerid] = 1;
			Survey_AllNo ++;
			SendClientMessage(playerid, Green, "הצבעתך נקלטה במערכת, תודה");
		}
		return 1;
	}
	
	if(PRESSED(KEY_SECONDARY_ATTACK))
	{
		if(PlayerToPoint(1, playerid, 2146.4558, 1640.1334, 993.5761))
		{
            if(RobberyMission_In[playerid] == 1) return SendClientMessage(playerid, White, "אינך יכול לבצע פעולה זו שאתה במשימת שדידת הבנק");
			ShowPlayerDialog(playerid, 1_0, DIALOG_STYLE_LIST, "מערכת בנק", "הפקדת כספים\nמשיכת כספים\nמאזן הסכום\nהעברה בנקאית\nבנק קלאן\nכרטיס אשראי", "בחר", "ביטול");
		}
		else if(PlayerToPoint(1, playerid, 313.7229, -133.6927, 999.6016))
		{
            if(RobberyMission_In[playerid] == 1) return SendClientMessage(playerid, White, "אינך יכול לבצע פעולה זו שאתה במשימת שדידת הבנק");
