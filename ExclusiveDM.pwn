public OnPlayerText(playerid, text[])
{
    new string[256];
    
	if(!inAFK[playerid] && Logged[playerid] == true)
	{
	    KillTimer(AutoAFK_Timer[playerid]);
   		AutoAFK_Timer[playerid] = SetTimerEx("AutomaticAFK", 1000*60*2, 0, "d", playerid);
	}
    
	if(DOF2_FileExists(File(playerid)) && AccountLog[playerid] == false) return SendClientMessage(playerid, Redwhite, "אתה חייב להכנס למשחק כדי לכתוב בצ'אט"), 0;
	else if(!DOF2_FileExists(File(playerid))) return SendClientMessage(playerid, Redwhite, "אתה חייב להרשם לשרת כדי לכתוב בצ'אט"), 0;


	if(inMute[playerid] == 1) return SendClientMessage(playerid, Redwhite, "אתה מושתק, אינך יכול לכתוב בצ'אט"), 0;

	if(inAFK[playerid] == 1) return SendClientMessage(playerid, Redwhite, "אתה במצב לא נמצא, אינך יכול לכתוב בצ'אט"), 0;

	if(CheckChatBlock == 1 && CheckAdminLevel(playerid) <=  2) return SendClientMessage(playerid, Redwhite, "הצ'אט נעול, אינך יכול לכתוב בצ'אט"), 0;

	if(Survey_Have == 1 && CheckAdminLevel(playerid) <=  2) return SendClientMessage(playerid, Redwhite, "הסקר מופעל, אינך יכול לכתוב בצ'אט"), 0;
	
	if(CheckFlooding(playerid) && !GetAdminLevel(playerid) && !GetTempAdminLevel(playerid) && !GetSpyAdminLevel(playerid)) return SendClientMessage(playerid, Redwhite, "עברת את מספר ההודעות המורשה ל2 שניות, אנא המתן מספר שניות והמשך בכתיבה"), 0;


	for(new word; word<sizeof(BlockedWords); word++)
	{
    	if(strfind(text, BlockedWords[word]) != -1)
		{
			format(string, sizeof(string), "\"מערכת הצינזור איתרה מילה חסומה במשפט \"%s", BlockedWords[word]);
			SendClientMessage(playerid, Redwhite, string);
			SendClientMessage(playerid, Redwhite, "הודעתך לא נשלחה -");
			TimeBlockedWord[playerid] ++;
			DOF2_SetInt(File(playerid), "BlockedWord", DOF2_GetInt(File(playerid), "BlockedWord")+1);
			format(string, sizeof(string), "השתמשת ב%d מתוך 3 מחרוזות חסומות", TimeBlockedWord[playerid]);
			SendClientMessage(playerid, Redwhite, string);
			format(string, sizeof(string), "{FFFFFF}%s {33AA33}מערכת הצינזור צינזרה ניסיון פרסום מילה חסומה מהמשתמש", GetName(playerid));
			SendAdminsMessage(Green, string);
			format(string, sizeof(string), "(%s)", text);
			SendAdminsMessage(White, string);
			return 0;
		}
	}

	if(!IsVaildText(text))
	{
		SendClientMessage(playerid, Redwhite, "מערכת הצינזור איתרה פרסום שרת במשפטך");
		SendClientMessage(playerid, Redwhite, "הודעתך לא נשלחה -");
		TimeBlockedWord[playerid] ++;
		DOF2_SetInt(File(playerid), "BlockedWord", DOF2_GetInt(File(playerid), "BlockedWord")+1);
		format(string, sizeof(string), "השתמשת ב%d מתוך 3 מחרוזות חסומות", TimeBlockedWord[playerid]);
		SendClientMessage(playerid, Redwhite, string);
		format(string, sizeof(string), "{FFFFFF}%s {33AA33}מערכת הצינזור צינזרה ניסיון פרסום שרת מהמשתמש", GetName(playerid));
		SendAdminsMessage(Green, string);
		format(string, sizeof(string), "(%s)", text);
		SendAdminsMessage(White, string);
		return 0;
	}

	if(text[0] == '*')
	{
	    if(GetSupportLevel(playerid) >= 1)
  		{
			new ac[128], lang1text[20];
			strmid(ac, text, 1, strlen(text));
			if(DOF2_GetInt(File(playerid), "SupportLevel") == 1) lang1text = "בניסיון";
			else if(DOF2_GetInt(File(playerid), "SupportLevel") == 2) lang1text = "חבר";
			else if(DOF2_GetInt(File(playerid), "SupportLevel") == 3) lang1text = "אחראי";
 			format(string, sizeof(string), "[{FFFFFF}Supporters{FFA500} Chat] | {FFFFFF}%s{FFA500} | {FFFFFF}%s{FFA500}[#{FFFFFF}%d{FFA500}]: %s", lang1text, GetName(playerid), playerid, ac);
			SendSupportsMessage(Orange, string);
	  	}
		else SendClientMessage(playerid, White, "אתה לא תומך");
		return 0;
	}

	if(text[0] == '#')
	{
	    if(GetVipLevel(playerid) >= 1)
  		{
			new ac[128], lang1text[20];
			strmid(ac, text, 1, strlen(text));
			if(DOF2_GetInt(File(playerid), "VipLevel") == 1) lang1text = "חבר";
			else if(DOF2_GetInt(File(playerid), "VipLevel") == 2) lang1text = "אחראי";
 			format(string, sizeof(string), "[{FFFFFF}VIP{FF66FF} Chat] | {FFFFFF}%s{FF66FF} | {FFFFFF}%s{FF66FF}[#{FFFFFF}%d{FF66FF}]: %s", lang1text, GetName(playerid), playerid, ac);
			SendVipMessage(Pink, string);
	  	}
		else SendClientMessage(playerid, White, "אתה לא חבר ויאייפי");
		return 0;
	}

	if(text[0] == '@')
	{
		if(DOF2_GetInt(File(playerid), "InClan") >= 1)
		{
			format(ClanFile, sizeof(ClanFile), "Clans/%s.ini", DOF2_GetString(File(playerid), "Clan"));
			if(DOF2_GetInt(ClanFile, "ClanChat") == 0)
			{
			    new lang1text[20];
			    if(DOF2_GetInt(File(playerid), "ClanLevel") == 1) lang1text = "שחקן";
				else if(DOF2_GetInt(File(playerid), "ClanLevel") == 2) lang1text = "בוחן";
				else if(DOF2_GetInt(File(playerid), "ClanLevel") == 3) lang1text = "סגן מנהל";
				else if(DOF2_GetInt(File(playerid), "ClanLevel") == 4) lang1text = "מנהל";
				format(string, sizeof(string), "[%s Chat] | %s | %s[#%d]: %s", DOF2_GetString(File(playerid), "Clan"), lang1text, GetName(playerid), playerid, text[1]);
				for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && DOF2_GetInt(File(i), "InClan") == 1 && strcmp(DOF2_GetString(File(playerid), "Clan"), DOF2_GetString(File(i), "Clan"), true) == 0)
				{
					if(DOF2_GetInt(ClanFile, "HaveClanChatColor") == 1) SendClientMessage(i, rgba2hex(DOF2_GetInt(ClanFile, "ClanChatColorR"), DOF2_GetInt(ClanFile,"ClanChatColorG"), DOF2_GetInt(ClanFile,"ClanChatColorB"), 255), string);
					else SendClientMessage(i, Bluesea, string);
				}
			}
			else SendClientMessage(playerid, White, "צ'אט הקלאן נעול, אינך יכול לכתוב בצ'אט הקלאן");
		}
		else SendClientMessage(playerid, White, "אתה לא חבר קלאן רשמי");
 		return 0;
	}
	
	if(text[0] == '!')
	{
		if(DOF2_GetInt(File(playerid), "InGang") >= 1)
		{
			format(GangFile, sizeof(GangFile), "Gangs/%s.ini", DOF2_GetString(File(playerid), "Gang"));
			if(DOF2_GetInt(GangFile, "GangChat") == 0)
			{
			    new lang1text[20];
			    if(DOF2_GetInt(File(playerid), "GangLevel") == 1) lang1text = "שחקן";
				else if(DOF2_GetInt(File(playerid), "GangLevel") == 2) lang1text = "מנהל";
				format(string, sizeof(string), "[%s Chat] | %s | %s[#%d]: %s", DOF2_GetString(File(playerid), "Gang"), lang1text, GetName(playerid), playerid, text[1]);
				for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && DOF2_GetInt(File(i), "InGang") == 1 && strcmp(DOF2_GetString(File(playerid), "Gang"), DOF2_GetString(File(i), "Gang"), true) == 0) SendClientMessage(i, Whitebrown, string);
			}
			else SendClientMessage(playerid, White, "צ'אט הכנופייה נעול, אינך יכול לכתוב בצ'אט הכנופייה");
		}
		else SendClientMessage(playerid, White, "אתה לא חבר כנופייה");
 		return 0;
	}
	
	////////////////////////////////////////////////////////////////////////////
	format(string, sizeof(string), "[Activity Manager] %s:[{FFFFFF}#%d] {FFFFFF}%s", GetName(playerid), playerid,text);
 	if(GetAdminLevel(playerid) == 1 && inAdminSpy[playerid] == 0 && !DOF2_IsSet(File(playerid), "Tag")) return OL_SendClientMessageToAll(Whitegrey, string), 0;
	format(string, sizeof(string), "[Activity Manager] %s:[{FFFFFF}#%d][%s] {FFFFFF}%s", GetName(playerid), playerid, DOF2_GetString(pfile,"Tag"),text);
