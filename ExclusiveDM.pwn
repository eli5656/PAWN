		format(string, sizeof(string), "{FFFFFF}[Drift Mission]{AA3333} סיימת [{FFFFFF}%d/6{AA3333}] צ'קפוינטים.", CP_DriftMission[playerid]);
		SendClientMessage(playerid, Red, string);
		CP_DriftMission[playerid]++;
	}
	else if(CP_DriftMission[playerid] == 5)
    {
    	DisablePlayerRaceCheckpoint(playerid);
      	SetPlayerRaceCheckpoint(playerid,1,1426.3452,-1869.6849,13.3828,        0,0,0,15);
		format(string, sizeof(string), "{FFFFFF}[Drift Mission]{AA3333} סיימת [{FFFFFF}%d/6{AA3333}] צ'קפוינטים.", CP_DriftMission[playerid]);
		SendClientMessage(playerid, Red, string);
		CP_DriftMission[playerid]++;
	}
	else if(CP_DriftMission[playerid] == 6)
    {
		format(string, sizeof(string), "{FFFFFF}[Drift Mission]{AA3333} השלמת [{FFFFFF}%d/6{AA3333}] צ'קפוינטים.", CP_DriftMission[playerid]);
		SendClientMessage(playerid, Red, string);
		format(string, sizeof(string), "{FFFFFF}[Drift Mission]{AA3333} סיים את משימת הדריפט בהצלחה {FFFFFF}%s{AA3333} השחקן", GetName(DriftMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
    	DisablePlayerRaceCheckpoint(playerid);
      	CP_DriftMission[playerid] = 0;
		DriftMission_Start = 0;
		DriftMission_In[playerid] = 0;
		DriftMission_WasFinish[playerid] = 1;
		format(string, sizeof(string), "• עברת את משימת הדריפט\n%s$ הרווחת -", GetNum(DriftMission_RandomPrice));
		ShowPlayerDialog(playerid, 19_2, DIALOG_STYLE_MSGBOX, "משימת הדריפט", string, "ביטול", "");
		DestroyVehicle(DriftMission_Car);
		KillTimer(DriftMission_Timer_1);
		KillTimer(DriftMission_Timer_2);
		AntiCheat_GivePlayerMoney(playerid, true, DriftMission_RandomPrice);
		SetPlayerVirtualWorld(playerid, 0);
	}
	
	if(CP_ParachutingMission[playerid] == 1)
    {
		ParachutingMission_Start = 0;
		ParachutingMission_In[playerid] = 0;
	 	ParachutingMission_WasFinish[playerid] = 1;
		format(string, sizeof(string), "• עברת את משימת הצניחה\n%s$ הרווחת -", GetNum(ParachutingMission_RandomPrice));
		ShowPlayerDialog(playerid, 21_2, DIALOG_STYLE_MSGBOX, "משימת הצניחה", string, "ביטול", "");
		KillTimer(ParachutingMission_Timer_1);
		KillTimer(ParachutingMission_Timer_2);
		format(string, sizeof(string), "{FFFFFF}[Parachuting Mission]{AA3333} סיים את משימת הצניחה בהצלחה {FFFFFF}%s{AA3333} השחקן", GetName(ParachutingMission_Player));
		OL_SendClientMessageToAllEx(playerid, Red, string);
		DisablePlayerRaceCheckpoint(playerid);
  		CP_ParachutingMission[playerid] = 0;
		AntiCheat_GivePlayerMoney(playerid, true, ParachutingMission_RandomPrice);
		SetPlayerVirtualWorld(playerid, 0);
	}
    return 1;
}

