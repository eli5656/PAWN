stock SendMessageToClan(playerid, const string[])
{
	for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && DOF2_GetInt(File(i), "InClan") == 1 && strcmp(DOF2_GetString(File(playerid), "Clan"), DOF2_GetString(File(i), "Clan"), true) == 0) SendClientMessage(i, Bluesea, string);
	return 1;
}

stock IsNumeric(string[])
{
	for(new i = 0; i < strlen(string); i++) if(string[i] > '9' || string[i] < '0') return false;
	return true;
}

stock SendVipMessage(color, const string[])
{
    for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && GetVipLevel(i) >= 1) SendClientMessage(i, color, string);
	return 1;
}

stock SendAdminsMessage(color, const string[])
{
    for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && GetAdminLevel(i) >= 2 || GetTempAdminLevel(i) >= 2 || GetSpyAdminLevel(i) >= 2) SendClientMessage(i, color, string);
	return 1;
}

stock SendSupportsMessage(color, const string[])
{
    for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && GetSupportLevel(i) >= 1) SendClientMessage(i, color, string);
	return 1;
}

stock GetName(playerid)
{
    new Name[MAX_PLAYER_NAME];
    GetPlayerName(playerid, Name, sizeof(Name));
    return Name;
}

stock GetIP(playerid)
{
	new IP[16];
	GetPlayerIp(playerid, IP, sizeof(IP));
	return IP;
}

stock AtmsFile(id)
{
	format(pfile, sizeof(pfile), "ATMs/%s.ini", id);
	return pfile;
}

stock File(playerid)
{
	format(pfile, sizeof(pfile), "Users/%s.ini", GetName(playerid));
	return pfile;
}

stock CarFile(vehicleid)
{
	format(carfile, sizeof(carfile), "Cars/%d.ini", vehicleid);
	return carfile;
}

stock GivePlayerHealth(playerid, bool:Plus, Float:health1)
{
    new Float:health;
    GetPlayerHealth(playerid, health);
	return SetPlayerArmour(playerid, Plus == true? (health+health1):(health-health1));
}

stock GivePlayerArmour(playerid, bool:Plus, Float:armour1)
{
    new Float:armour;
    GetPlayerArmour(playerid, armour);
	return SetPlayerArmour(playerid, Plus == true? (armour+armour1):(armour-armour1));
}

stock SoundData(playerid, bool:play, sound)
{
	new Float:X, Float:Y, Float:Z;
	GetPlayerPos(playerid, X, Y, Z);
	PlayerPlaySound(playerid, play == true? (sound):(sound-1), X, Y, Z);
	return sound;
}

stock OL_SendClientMessageToAllEx(playerid, color, const string[])
