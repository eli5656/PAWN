public RunningAntiCheat()
{
	new string[256];
 	for(new i=0; i<MAX_PLAYERS; i++) if(Logged[i] == true && GetPlayerMoney(i) > AntiCheat_GetPlayerMoney(i)) return PlayerMoney[i] = GetPlayerMoney(i)-AntiCheat_GetPlayerMoney(i), GivePlayerMoney(i, -PlayerMoney[i]);

	for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerInAnyVehicle(i) && GetPlayerSpeed(i) >= 300 && GetVipLevel(i) == 0)
	{
	    if(!GetAdminLevel(i))
		{
			format(string, sizeof(string), "{33AA33}נבעט על ידי מערכת האנטי צ'יט {FFFFFF}%s {33AA33}השחקן", GetName(i));
			OL_SendClientMessageToAll(Green, string);
			OL_SendClientMessageToAll(Green, "({33AA33}שימוש בצ'יט {FFFFFF} ספיד האק{33AA33})");
			Kick(i);
		}
		else if(GetAdminLevel(i) >= 1) SendClientMessage(i, Green, "{FFFFFF}[AntiCheat]{33AA33} אנא הפסק להשתמש במיידי בצ'יט ספיד האק");
	}
	
	for(new i=0; i<MAX_PLAYERS; i++)
	{
	    format(string, sizeof(string), "נבעט על ידי מערכת הגבלת הפינג {FFFFFF}%s{33AA33} השחקן", GetName(i));
		if(Logged[i] == true && GetPlayerPing(i) >= 500) OL_SendClientMessageToAll(Green, string), OL_SendClientMessageToAll(Green, "({FFFFFF}כמות הפינג שלך עוקפת את הכמות הפינג המקסימאלית בשרת{33AA33})"), Kick(i);
	}
	return 1;
}

