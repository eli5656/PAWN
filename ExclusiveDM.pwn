stock AntiCheat_GetPlayerMoney(playerid) return AntiCheat_Money[playerid];

stock AntiCheat_GivePlayerMoney(playerid, bool:Plus, amount)
{
	GivePlayerMoney(playerid, Plus == true? (amount):(-amount));
	if(Plus == true) AntiCheat_Money[playerid] +=amount;
	else if(Plus == false) AntiCheat_Money[playerid] -=amount;
	return 1;
}

stock AntiCheat_SetPlayerMoney(playerid, oldamount)
{
	if(oldamount < AntiCheat_GetPlayerMoney(playerid))
	{
		newamount = AntiCheat_GetPlayerMoney(playerid)-oldamount;
		AntiCheat_GivePlayerMoney(playerid, false, newamount);
	}
	else if(oldamount > AntiCheat_GetPlayerMoney(playerid))
	{
		newamount = oldamount-AntiCheat_GetPlayerMoney(playerid);
		AntiCheat_GivePlayerMoney(playerid, true, newamount);
	}
	return 1;
}
