		}
	}
	
	if(PRESSED(KEY_SECONDARY_ATTACK))
	{
	    if(IsPlayerInPvpArea(playerid) == 1)
	    {
	        new Float:pHealth, Float:pArmour;
			GetPlayerHealth(playerid, pHealth);
			GetPlayerArmour(playerid, pArmour);
	        if(pHealth != 100 && pArmour != 100) return SendClientMessage(playerid, White, "אינך יכול לצאת מאזורי הלחימה כשאין לך חיים ומגן מלאים");
		    new string[256];
			if(inWar[playerid])
			{
				SendClientMessage(playerid, Orange, "{FFFFFF}[SYSTEM]{FFA500} יצאת מאזור המלחמה {FFFFFF}•••");
				SpawnPlayer(playerid);
	   			SetPlayerInterior(playerid, 0);
				SetPlayerVirtualWorld(playerid, 0);
				SetPlayerHealth(playerid, BeforePvpHealth[playerid]);
				SetPlayerArmour(playerid, BeforePvpArmour[playerid]);
				SoundData(playerid, true, 1137);
